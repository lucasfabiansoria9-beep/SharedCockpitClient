  <StringProperty Name="PREfastLog" Category="Advanced" Visible="false"/>
  <StringProperty Name="ProgramDataBaseFileName" Category="Output Files" Visible="false"/>
  <BoolProperty Name="RemoveUnreferencedCodeData" Category="Language" Visible="false"/>
  <BoolProperty Name="SDLCheck" Category="General" Visible="false"/>
  <BoolProperty Name="StringPooling" Category="Code Generation" Visible="false"/>
  <BoolProperty Name="SupportJustMyCode" Category="General" Visible="false"/>
