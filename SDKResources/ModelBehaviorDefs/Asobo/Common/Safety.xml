	<Template Name="ASOBO_SAFETY_Push_Warning_Template">
		<Parameters Type="Default">
			<VAR_SCOPE>O</VAR_SCOPE>
			<VAR_NAME>SAFETY_Push_Warning</VAR_NAME>
			<Switch Param="TYPE">
				<Case Value="AIRLINER">
					<SWITCH_POSITION_TYPE>I</SWITCH_POSITION_TYPE>
					<SWITCH_POSITION_VAR>SAFETY_Push_Warning_ButtonAnimVar</SWITCH_POSITION_VAR>
				</Case>
			</Switch>
		</Parameters>
		<Parameters Type="Default">
			<PUSH_EVENT>(&gt;K:MASTER_WARNING_ACKNOWLEDGE)</PUSH_EVENT>
			<IS_STATE_ACTIVE>(A:MASTER WARNING ACTIVE, Boolean) (A:MASTER WARNING ACKNOWLEDGED, Boolean) ! and (#VAR_SCOPE#:#VAR_NAME#) or</IS_STATE_ACTIVE>
			<TOOLTIP_TITLE>@TT_Package.SAFETY_PUSH_WARNING_TITLE</TOOLTIP_TITLE>
			<TT_DESCRIPTION_ID>@TT_Package.SAFETY_PUSH_WARNING_ACTION</TT_DESCRIPTION_ID>
		</Parameters>
		<Parameters Type="Override">
			<Switch Param="TYPE">
				<Case Value="AIRLINER">
					<IS_STATE_ACTIVE_SEQ1>#IS_STATE_ACTIVE# (#SWITCH_POSITION_TYPE#:#SWITCH_POSITION_VAR#_IsDown) or</IS_STATE_ACTIVE_SEQ1>
				</Case>
			</Switch>
		</Parameters>
		<UseTemplate Name="ASOBO_SAFETY_Push_Template">
			<PUSH_ID>Warning</PUSH_ID>
		</UseTemplate>
	</Template>
