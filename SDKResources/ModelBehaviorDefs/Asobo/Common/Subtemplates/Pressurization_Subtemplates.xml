			<WWISE_PREFIX>pneumatics</WWISE_PREFIX>
			<TOOLTIP_TITLE>@TT_Package.PNEUMATICS_VALVE_TITLE</TOOLTIP_TITLE>
			<TT_DESCRIPTION_ID>@TT_Package.PNEUMATICS_VALVE_ACTION</TT_DESCRIPTION_ID>
		</Parameters>
		<Switch Param="INTERACTION_TYPE">
			<Case Value="Switch">
				<UseTemplate Name="ASOBO_PNEUMATICS_Base_Template">
					<IE_NAME>IE_PNEUMATICS_#INTERACTION_TYPE#_2Valves_#VALVE_NAME_1#_#VALVE_NAME_2#</IE_NAME>					<!-- Unique name for the input -->
					<SET_STATE_ON>#VALVE_ID_1# (&gt;K:PNEUMATICS_VALVE_OPEN) #VALVE_ID_2# (&gt;K:PNEUMATICS_VALVE_OPEN)</SET_STATE_ON>
					<SET_STATE_OFF>#VALVE_ID_1# (&gt;K:PNEUMATICS_VALVE_CLOSE) #VALVE_ID_2# (&gt;K:PNEUMATICS_VALVE_CLOSE)</SET_STATE_OFF>
					<GET_STATE_EXTERNAL>(A:PNEUMATICS VALVE SWITCH:#VALVE_ID_1#, Bool)</GET_STATE_EXTERNAL>
				</UseTemplate>
			</Case>
			<Case Value="Knob">
				<UseTemplate Name="ASOBO_PNEUMATICS_Base_Template">
					<IE_NAME>IE_PNEUMATICS_#INTERACTION_TYPE#_2Valves_#VALVE_NAME_1#_#VALVE_NAME_2#</IE_NAME>					<!-- Unique name for the input -->
					<SET_STATE_EXTERNAL>(A:PNEUMATICS VALVE TARGET STATUS:#VALVE_ID_1#, percent) != if{ l0 #VALVE_ID_1# (&gt;K:2:PNEUMATICS_VALVE_SET) l0 #VALVE_ID_2# (&gt;K:2:PNEUMATICS_VALVE_SET) }</SET_STATE_EXTERNAL>
					<GET_STATE_EXTERNAL>(A:PNEUMATICS VALVE TARGET STATUS:#VALVE_ID_1#, percent)</GET_STATE_EXTERNAL>
				</UseTemplate>
			</Case>
		</Switch>
	</Template>

	<Template Name="ASOBO_PNEUMATICS_2ValvesSplit_SubTemplate">
		<Parameters Type="Default">
			<Condition>
				<Test>
					<And>
						<Arg NotEmpty="VALVE_1_NAME"/>
						<Arg NotEmpty="VALVE_2_NAME"/>
					</And>
				</Test>
				<True>
					<VALVE_ID_1>'#VALVE_1_NAME#'_n</VALVE_ID_1>
					<VALVE_ID_2>'#VALVE_2_NAME#'_n</VALVE_ID_2>
				</True>
				<False>
					<VALVE_1_NAME>#VALVE_ID_1#</VALVE_1_NAME>
					<VALVE_2_NAME>#VALVE_ID_2#</VALVE_2_NAME>
				</False>
			</Condition>
			<Switch Param="INTERACTION_TYPE">
				<Case Value="Lever">
					<DRAG_VALUE_TO_EVENT_SET>100 *</DRAG_VALUE_TO_EVENT_SET>
				</Case>
			</Switch>
		</Parameters>
		<Parameters Type="Default">
			<NODE_ID>PNEUMATICS_#INTERACTION_TYPE#_2ValvesSplit_#VALVE_1_NAME#_#VALVE_2_NAME#</NODE_ID>
			<ANIM_NAME>PNEUMATICS_#INTERACTION_TYPE#_2ValvesSplit_#VALVE_1_NAME#_#VALVE_2_NAME#</ANIM_NAME>
			<PART_ID>PNEUMATICS_#INTERACTION_TYPE#_2ValvesSplit</PART_ID>
			<WWISE_PREFIX>pneumatics</WWISE_PREFIX>
			<TOOLTIP_TITLE>@TT_Package.PNEUMATICS_VALVE_TITLE</TOOLTIP_TITLE>
			<TT_DESCRIPTION_ID>@TT_Package.PNEUMATICS_VALVE_ACTION</TT_DESCRIPTION_ID>
		</Parameters>
		<Parameters Type="Override">
			<IE_NAME>IE_PNEUMATICS_#INTERACTION_TYPE#_2ValvesSplit_#VALVE_1_NAME#_#VALVE_2_NAME#</IE_NAME>			<!-- Unique name for the input -->
		</Parameters>
		<UseTemplate Name="ASOBO_PNEUMATICS_Base_Template">
			<SET_STATE_EXTERNAL>(A:PNEUMATICS VALVE TARGET STATUS:#VALVE_ID_1#, percent) != if{ l0 #VALVE_ID_1# (&gt;K:2:PNEUMATICS_VALVE_SET) 100 l0 - #VALVE_ID_2# (&gt;K:2:PNEUMATICS_VALVE_SET) }</SET_STATE_EXTERNAL>
			<GET_STATE_EXTERNAL>(A:PNEUMATICS VALVE TARGET STATUS:#VALVE_ID_1#, percent)</GET_STATE_EXTERNAL>
			<INIT_CODE>
				(A:PNEUMATICS VALVE TARGET STATUS:#VALVE_ID_1#, percent) s0
				(A:PNEUMATICS VALVE TARGET STATUS:#VALVE_ID_2#, percent) + 100 != if{
					100 l0 - #VALVE_ID_2# (&gt;K:2:PNEUMATICS_VALVE_SET)
				}
				l0 (&gt;O:PNEUMATICS_#IE_NAME#_Position)
			</INIT_CODE>
			<SIMVAR_TO_WATCH_0>PNEUMATICS VALVE TARGET STATUS:#VALVE_ID_1#</SIMVAR_TO_WATCH_0>
			<SIMVAR_TO_WATCH_1>PNEUMATICS VALVE TARGET STATUS:#VALVE_ID_2#</SIMVAR_TO_WATCH_1>
		</UseTemplate>
	</Template>

	<Template Name="ASOBO_PNEUMATICS_Junction_Lines_Opening_Status_SubTemplate">
		<Parameters Type="Default">
			<Condition NotEmpty="JUNCTION_NAME">
				<JUNCTION_ID>'#JUNCTION_NAME#'_n</JUNCTION_ID>
			</Condition>
			<Condition NotEmpty="JUNCTION_ID">
				<JUNCTION_NAME>#JUNCTION_ID#</JUNCTION_NAME>
			</Condition>
			<Condition NotEmpty="INCREMENT">
				<INC_PARAM_0>#INCREMENT# p0 *</INC_PARAM_0>
				<INC_PARAM_0_IS_DYNAMIC>True</INC_PARAM_0_IS_DYNAMIC>
				<DEC_PARAM_0>#INCREMENT# p0 *</DEC_PARAM_0>
				<DEC_PARAM_0_IS_DYNAMIC>True</DEC_PARAM_0_IS_DYNAMIC>
			</Condition>
			<Switch Param="INTERACTION_TYPE">
				<Case Value="Lever">
					<DRAG_VALUE_TO_EVENT_SET>100 *</DRAG_VALUE_TO_EVENT_SET>
				</Case>
			</Switch>
		</Parameters>
		<Parameters Type="Default">
			<ANIM_NAME>#NODE_ID#</ANIM_NAME>
			<WWISE_PREFIX>pneumatics</WWISE_PREFIX>
			<TOOLTIP_TITLE>@TT_Package.PNEUMATICS_VALVE_TITLE</TOOLTIP_TITLE>
			<TT_DESCRIPTION_ID>@TT_Package.PNEUMATICS_VALVE_ACTION</TT_DESCRIPTION_ID>
		</Parameters>
		<Parameters Type="Override">
			<Loop>
				<Setup>
					<Param>LINE_ID</Param>
					<From>1</From>
					<Inc>1</Inc>
					<While>
						<Arg Valid="LINE_#LINE_ID#"/>
					</While>
				</Setup>
				<Do>
					<Parameters Type="Default" Lifetime="Loop">
						<SET_STATE_EXTERNAL_T/>
						<LINE_VALUE_T>l0</LINE_VALUE_T>
					</Parameters>
					<Parameters Type="Override" Lifetime="Loop">
						<LINE_ID_T Process="Param">LINE_#LINE_ID#</LINE_ID_T>
						<Condition NotEmpty="LINE_#LINE_ID#_VALUE">
							<LINE_VALUE_T Process="Param">LINE_#LINE_ID#_VALUE</LINE_VALUE_T>
						</Condition>
					</Parameters>
					<Parameters Type="Override" Lifetime="Loop">
						<SET_STATE_EXTERNAL_T>#SET_STATE_EXTERNAL_T# #LINE_VALUE_T# #LINE_ID_T# #JUNCTION_ID# (&gt;K:3:PNEUMATICS_JUNCTION_LINE_OPENING_STATUS_SET) </SET_STATE_EXTERNAL_T>
					</Parameters>
				</Do>
				<Then>
					<SET_STATE_EXTERNAL>#SET_STATE_EXTERNAL_T#</SET_STATE_EXTERNAL>
					<GET_STATE_EXTERNAL>#LINE_1# (A:1:PNEUMATICS JUNCTION LINE OPENING STATUS:#JUNCTION_ID#, percent)</GET_STATE_EXTERNAL>
				</Then>
			</Loop>
		</Parameters>
		<UseTemplate Name="ASOBO_PNEUMATICS_Base_Template">
			<IE_NAME>IE_PNEUMATICS_#INTERACTION_TYPE#_Junction_Line_Opening_Status_#JUNCTION_NAME#</IE_NAME>			<!-- Unique name for the input -->
			<SET_STATE_EXTERNAL>#SET_STATE_EXTERNAL#</SET_STATE_EXTERNAL>
			<GET_STATE_EXTERNAL>#GET_STATE_EXTERNAL#</GET_STATE_EXTERNAL>
		</UseTemplate>
	</Template>

</ModelBehaviors>