	<Template Name="ASOBO_LIGHTING_Update_SetLandingGearPotentiometer_Template">
		<Parameters Type="Default">
			<MAX_ANIM_PERCENT_FOR_LIGHTS>0.75</MAX_ANIM_PERCENT_FOR_LIGHTS>
		</Parameters>
		<Component ID="Light_LandingGear_Update_#POTENTIOMETER#">
			<UseTemplate Name="ASOBO_GT_Update">
				<FREQUENCY>1</FREQUENCY>
				<UPDATE_CODE>
					(A:GEAR TOTAL PCT EXTENDED, Percent) #MAX_ANIM_PERCENT_FOR_LIGHTS# &gt; 100 * #POTENTIOMETER# (&gt;K:2:LIGHT_POTENTIOMETER_SET)
				</UPDATE_CODE>
			</UseTemplate>
		</Component>
	</Template>
	<Template Name="ASOBO_LIGHTING_Switch_Test_Lights_Template">
		<Parameters Type="Default">
			<NODE_ID>LIGHTING_Switch_Test_Lights</NODE_ID>
			<ANIM_NAME>LIGHTING_Switch_Test_Lights</ANIM_NAME>
			<IE_NAME>Test_Lights</IE_NAME>
			<TT_DESCRIPTION_ID>@TT_Package.SAFETY_PUSH_TEST_ACTION</TT_DESCRIPTION_ID>
			<TOOLTIP_TITLE>@TT_Package.SAFETY_PUSH_TEST_TITLE</TOOLTIP_TITLE>
			<WWISE_EVENT_1>lights_test_push_button_on</WWISE_EVENT_1>
			<WWISE_EVENT_2>lights_test_push_button_off</WWISE_EVENT_2>
		</Parameters>
		<Parameters Type="Override">
			<SWITCH_POSITION_TYPE>L</SWITCH_POSITION_TYPE>
			<SWITCH_POSITION_VAR>XMLVAR_Lights_Test</SWITCH_POSITION_VAR>
		</Parameters>

		<UseTemplate Name="ASOBO_Lighting_Base_Template">
			<SET_STATE_EXTERNAL>(&gt;#SWITCH_POSITION_TYPE#:#SWITCH_POSITION_VAR#)</SET_STATE_EXTERNAL>
			<INTERACTION_TYPE>Push</INTERACTION_TYPE>
			<EXTRA_OPTION>_Held</EXTRA_OPTION>
			<ANIM_HANDLED_SEPARATELY>False</ANIM_HANDLED_SEPARATELY>
			<TT_VALUE>(#SWITCH_POSITION_TYPE#:#SWITCH_POSITION_VAR#) @TT_OnOff_Cond</TT_VALUE>
			<TT_VALUE_IS_DYNAMIC>True</TT_VALUE_IS_DYNAMIC>
		</UseTemplate>
	</Template>
	<Template Name="ASOBO_LIGHTING_Switch_Mode_Template">
		<Parameters Type="Default">
			<NODE_ID>LIGHTING_Switch_Mode</NODE_ID>
			<ANIM_NAME>LIGHTING_Switch_Mode</ANIM_NAME>
			<SWITCH_POSITION_TYPE>L</SWITCH_POSITION_TYPE>
			<SWITCH_POSITION_VAR>XMLVAR_Lights_Mode</SWITCH_POSITION_VAR>
			<LIMITED_INTENSITY_VALUE>0.25</LIMITED_INTENSITY_VALUE>
			<WWISE_EVENT>lighting_switch_generic</WWISE_EVENT>
		</Parameters>
		<Component ID="#NODE_ID#_Parent">
			<UseTemplate Name="ASOBO_GT_Update">
				<ONCE>True</ONCE>
				<!-- Init Factor to non-zero value is not set in FLT -->
				<UPDATE_CODE>
					(L:XMLVAR_Light_Mode_Factor) s1 #LIMITED_INTENSITY_VALUE# != l1 1 != and if{
						(#SWITCH_POSITION_TYPE#:#SWITCH_POSITION_VAR#) 2 == if{
							1 (&gt;L:XMLVAR_Light_Mode_Factor)
						} els{
							#LIMITED_INTENSITY_VALUE# (&gt;L:XMLVAR_Light_Mode_Factor)
						}
					}
				</UPDATE_CODE>
			</UseTemplate>
			<Parameters Type="Default">
				<NUM_STATES>3</NUM_STATES>
				<POS_NVG>0</POS_NVG>
				<POS_NITE>1</POS_NITE>
				<POS_DAY>2</POS_DAY>
				<TT_DESCRIPTION_ID>@TT_Package.AIRLINER_OVHD_KNOB_LIGHT_GLARESHIELD_PANELFLOOD_ACTION</TT_DESCRIPTION_ID>
			</Parameters>
			<UseTemplate Name="ASOBO_Lighting_Base_Template">
				<IE_NAME>INT_Lights_Mode</IE_NAME>
				<TOOLTIP_TITLE>@TT_Package.AIRLINER_OVHD_KNOB_LIGHT_GLARESHIELD_PANELFLOOD_TITLE</TOOLTIP_TITLE>
				<INTERACTION_TYPE>Switch</INTERACTION_TYPE>
				<GET_STATE_EXTERNAL>(#SWITCH_POSITION_TYPE#:#SWITCH_POSITION_VAR#)</GET_STATE_EXTERNAL>

				<SET_STATE_#POS_NVG#>0 (&gt;#SWITCH_POSITION_TYPE#:#SWITCH_POSITION_VAR#) #LIMITED_INTENSITY_VALUE# (&gt;L:XMLVAR_Light_Mode_Factor)</SET_STATE_#POS_NVG#>
				<SET_STATE_#POS_NITE#>1 (&gt;#SWITCH_POSITION_TYPE#:#SWITCH_POSITION_VAR#) #LIMITED_INTENSITY_VALUE# (&gt;L:XMLVAR_Light_Mode_Factor)</SET_STATE_#POS_NITE#>
				<SET_STATE_#POS_DAY#>2 (&gt;#SWITCH_POSITION_TYPE#:#SWITCH_POSITION_VAR#) 1 (&gt;L:XMLVAR_Light_Mode_Factor)</SET_STATE_#POS_DAY#>

				<STR_STATE_#POS_NVG#>NVG</STR_STATE_#POS_NVG#>
				<STR_STATE_#POS_NITE#>Nite</STR_STATE_#POS_NITE#>
				<STR_STATE_#POS_DAY#>Day</STR_STATE_#POS_DAY#>

				<TT_VALUE_#POS_NVG#>@TT_Package.GT_STATE_NVG</TT_VALUE_#POS_NVG#>
				<TT_VALUE_#POS_NITE#>@TT_Package.GT_STATE_NITE</TT_VALUE_#POS_NITE#>
				<TT_VALUE_#POS_DAY#>@TT_Package.GT_STATE_DAY</TT_VALUE_#POS_DAY#>
			</UseTemplate>
		</Component>
	</Template>
	<Template Name="ASOBO_LIGHTING_AOA_Template">
		<Parameters Type="Default">
			<NODE_ID>LIGHTING_AOA_</NODE_ID>
			<Switch Param="TYPE">
				<Case Value="GREEN">
					<MIN_AOA>8.8</MIN_AOA>
					<MAX_AOA>99</MAX_AOA>
				</Case>
				<Case Value="YELLOW">
					<MIN_AOA>6.9</MIN_AOA>
					<MAX_AOA>9.3</MAX_AOA>
				</Case>
				<Case Value="RED">
					<MIN_AOA>-99</MIN_AOA>
					<MAX_AOA>7.4</MAX_AOA>
				</Case>
			</Switch>
			<BRIGHTNESS_VAR>1</BRIGHTNESS_VAR>
		</Parameters>
		<UseTemplate Name="ASOBO_GT_Component_Emissive_Gauge">
			<EMISSIVE_CODE>
				(A:INCIDENCE ALPHA, degrees) s1 #MIN_AOA# &gt; l1 #MAX_AOA# &lt; and #BRIGHTNESS_VAR# *
			</EMISSIVE_CODE>
		</UseTemplate>
	</Template>
	<Template Name="ASOBO_LIGHTING_Switch_Light_Exterior_Template">
		<Parameters Type="Default">
			<SWITCH_POSITION_TYPE>L</SWITCH_POSITION_TYPE>
			<SWITCH_POSITION_VAR>XMLVAR_EXT_Lights_Mode</SWITCH_POSITION_VAR>
			<TT_DESCRIPTION_ID>@TT_Package.LIGHTING_SWITCH_EXT_LIGHTS_ACTION</TT_DESCRIPTION_ID>
		</Parameters>
		<Component ID="#NODE_ID#_Parent">
			<Parameters Type="Default">
				<NUM_STATES>3</NUM_STATES>
				<POS_NORM>0</POS_NORM>
				<POS_NVIS>1</POS_NVIS>
				<POS_OFF>2</POS_OFF>
			</Parameters>
			<UseTemplate Name="ASOBO_Lighting_Base_Template">
				<IE_NAME>EXT_Lights_Mode</IE_NAME>
				<TOOLTIP_TITLE>@TT_Package.LIGHTING_SWITCH_EXT_LIGHTS_TITLE</TOOLTIP_TITLE>
				<INTERACTION_TYPE>Switch</INTERACTION_TYPE>
				<GET_STATE_EXTERNAL>(#SWITCH_POSITION_TYPE#:#SWITCH_POSITION_VAR#)</GET_STATE_EXTERNAL>

				<Condition Valid="EXT_LTS_BUS_ID">
					<True>
						<SET_STATE_#POS_OFF#>2 (&gt;#SWITCH_POSITION_TYPE#:#SWITCH_POSITION_VAR#) #EXT_LTS_BUS_ID# #BUS_ID# (&gt;K:2:ELECTRICAL_BUS_TO_BUS_CONNECTION_TOGGLE) </SET_STATE_#POS_OFF#>
						<SET_STATE_#POS_NVIS#>1 (&gt;#SWITCH_POSITION_TYPE#:#SWITCH_POSITION_VAR#) (A:BUS CONNECTION ON:#EXT_LTS_BUS_ID#,Bool) 0 == if{ #EXT_LTS_BUS_ID# #BUS_ID# (&gt;K:2:ELECTRICAL_BUS_TO_BUS_CONNECTION_TOGGLE) }</SET_STATE_#POS_NVIS#>
						<SET_STATE_#POS_NORM#>0 (&gt;#SWITCH_POSITION_TYPE#:#SWITCH_POSITION_VAR#) </SET_STATE_#POS_NORM#>
					</True>
					<False>
						<SET_STATE_#POS_OFF#>2 (&gt;#SWITCH_POSITION_TYPE#:#SWITCH_POSITION_VAR#)</SET_STATE_#POS_OFF#>
						<SET_STATE_#POS_NVIS#>1 (&gt;#SWITCH_POSITION_TYPE#:#SWITCH_POSITION_VAR#)</SET_STATE_#POS_NVIS#>
						<SET_STATE_#POS_NORM#>0 (&gt;#SWITCH_POSITION_TYPE#:#SWITCH_POSITION_VAR#)</SET_STATE_#POS_NORM#>
					</False>
				</Condition>

				<STR_STATE_#POS_NORM#>Norm</STR_STATE_#POS_NORM#>
				<STR_STATE_#POS_NVIS#>NVis</STR_STATE_#POS_NVIS#>
				<STR_STATE_#POS_OFF#>Off</STR_STATE_#POS_OFF#>

				<TT_VALUE_#POS_NORM#>@TT_Package.GT_STATE_NORMAL</TT_VALUE_#POS_NORM#>
				<TT_VALUE_#POS_NVIS#>@TT_Package.GT_STATE_NIGHT</TT_VALUE_#POS_NVIS#>
				<TT_VALUE_#POS_OFF#>@TT_Package.GT_STATE_OFF</TT_VALUE_#POS_OFF#>
			</UseTemplate>
		</Component>
	</Template>
