	<Template Name="ASOBO_ELECTRICAL_Update_APU_Shutdown_After_Engine_Start_Template">
		<Parameters Type="Default">
			<ENGINE_COUNT>1</ENGINE_COUNT>
			<SHUTDOWN_DELAY>60</SHUTDOWN_DELAY>
		</Parameters>
		<UseTemplate Name="ASOBO_GT_Update">
			<FREQUENCY>1</FREQUENCY>
			<UPDATE_CODE>
				(A:APU SWITCH, Bool) ! if{ g1 }
				(A:GENERAL ENG COMBUSTION:1, Bool) ! if{ g1 }
				#ENGINE_COUNT# 2 &gt;= if{
				(A:GENERAL ENG COMBUSTION:2, Bool) ! if{ g1 }
				#ENGINE_COUNT# 3 &gt;= if{
				(A:GENERAL ENG COMBUSTION:3, Bool) ! if{ g1 }
				#ENGINE_COUNT# 4 &gt;= if{
				(A:GENERAL ENG COMBUSTION:4, Bool) ! if{ g1 }
				}
				}
				}
				(O:XMLVAR_APU_Shutdown_Time) 0 == if{
				(E:SIMULATION TIME, seconds) #SHUTDOWN_DELAY# + (&gt;O:XMLVAR_APU_Shutdown_Time)
				}
				(O:XMLVAR_APU_Shutdown_Time) (E:SIMULATION TIME, seconds) &lt; if{
				(&gt;K:APU_OFF_SWITCH)
				}
				quit :1 0 (&gt;O:XMLVAR_APU_Shutdown_Time) quit
			</UPDATE_CODE>
		</UseTemplate>
	</Template>
