	<Template Name="ASOBO_ELECTRICAL_Switch_Connection_Template">
		<Parameters Type="Default">
			<ID>1</ID>
		</Parameters>
		<Parameters Type="Default">
			<NODE_ID>ELECTRICAL_Switch_Connection</NODE_ID>
			<ANIM_NAME>ELECTRICAL_Switch_Connection</ANIM_NAME>
			<SOURCE_BUS_ID>1</SOURCE_BUS_ID>
			<DESTINATION_BUS>2</DESTINATION_BUS>
			<ANIM_LENGTH>100</ANIM_LENGTH>
			<TOGGLE_CODE/>
			<WWISE_EVENT_1>electrical_switch_on</WWISE_EVENT_1>
			<NORMALIZED_TIME_1>0.1</NORMALIZED_TIME_1>
			<WWISE_EVENT_2>electrical_switch_off</WWISE_EVENT_2>
			<NORMALIZED_TIME_2>0.5</NORMALIZED_TIME_2>
			<ON_CONNECTION_ON/>
			<ON_CONNECTION_OFF/>
			<IS_CONNECTION_ON/>
			<INPUT_EVENT_ID_SOURCE>ELECTRICAL</INPUT_EVENT_ID_SOURCE>
			<IE_CONNECTION_ID>Connection_#ID#</IE_CONNECTION_ID>
		</Parameters>
		<Parameters Type="Override">
			<SET_CONNECTION_STATE>l1 != if{ #DESTINATION_BUS# #SOURCE_BUS_ID# (&gt;K:2:ELECTRICAL_BUS_TO_BUS_CONNECTION_TOGGLE) }</SET_CONNECTION_STATE>
			<IS_CONNECTION_ON>#SOURCE_BUS_ID# (A:1:BUS CONNECTION ON:#DESTINATION_BUS#, Bool) #IS_CONNECTION_ON#</IS_CONNECTION_ON>
		</Parameters>
		<Parameters Type="Override">
			<BTN_ID>#IE_CONNECTION_ID#</BTN_ID>
			<GET_STATE_EXTERNAL>#IS_CONNECTION_ON# sp1</GET_STATE_EXTERNAL>
			<SIM_STATE_IS_ON_EXTERNAL>l1</SIM_STATE_IS_ON_EXTERNAL>
		</Parameters>
		<Parameters Type="Override">
			<SET_STATE_ON>1 #SET_CONNECTION_STATE# #ON_CONNECTION_ON#</SET_STATE_ON>
			<SET_STATE_OFF>0 #SET_CONNECTION_STATE# #ON_CONNECTION_OFF#</SET_STATE_OFF>
			<ON_BEFORE_SET_CODE>#GET_STATE_EXTERNAL# </ON_BEFORE_SET_CODE>
			<IS_CONNECTION_ON_EXTERNAL>(B:#INPUT_EVENT_ID_SOURCE#_#BTN_ID#, Bool)</IS_CONNECTION_ON_EXTERNAL>
			<LEFT_SINGLE_CODE>(&gt;B:#INPUT_EVENT_ID_SOURCE#_#BTN_ID#_Toggle)</LEFT_SINGLE_CODE>
			<TOOLTIP_ENTRY_0>#INPUT_EVENT_ID_SOURCE#_#BTN_ID#</TOOLTIP_ENTRY_0>
		</Parameters>
		<Parameters Type="Default">
			<Condition Valid="IS_AIRLINER">
				<SEQ1_EMISSIVE_CODE>#IS_CONNECTION_ON_EXTERNAL#</SEQ1_EMISSIVE_CODE>
				<SEQ2_EMISSIVE_CODE>#IS_CONNECTION_ON_EXTERNAL# !</SEQ2_EMISSIVE_CODE>
			</Condition>
		</Parameters>
		<Condition NotEmpty="ENSURE_VARIABLE_CONSISTENCY">
			<UseTemplate Name="ASOBO_GT_Update">
				<FREQUENCY>5</FREQUENCY>
				<UPDATE_CODE>#ENSURE_VARIABLE_CONSISTENCY#</UPDATE_CODE>
			</UseTemplate>
		</Condition>
		<Condition Valid="IS_AIRLINER">
			<True>
				<UseTemplate Name="ASOBO_GT_Push_Button_Airliner">
					<INPUT_EVENT_ID>ELECTRICAL</INPUT_EVENT_ID>
					<Condition Valid="INVERT_EMISSIVE_STATE">
						<True>
							<SEQ1_EMISSIVE_CODE>#SEQ2_EMISSIVE_CODE#</SEQ1_EMISSIVE_CODE>
							<SEQ2_EMISSIVE_CODE>#SEQ1_EMISSIVE_CODE#</SEQ2_EMISSIVE_CODE>
						</True>
					</Condition>
				</UseTemplate>
			</True>
			<False>
				<Component ID="#NODE_ID#_#ID#" Node="#NODE_ID#">
					<UseInputEvent ID="ELECTRICAL">
						<ANIM>EXTERNAL</ANIM>
					</UseInputEvent>
					<UseTemplate Name="ASOBO_GT_Switch_Code">
						<ANIM_CODE>#IS_CONNECTION_ON_EXTERNAL# #ANIM_LENGTH# *</ANIM_CODE>
					</UseTemplate>
				</Component>
			</False>
		</Condition>
	</Template>
