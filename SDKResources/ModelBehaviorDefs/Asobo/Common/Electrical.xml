	<Template Name="ASOBO_ELECTRICAL_Switch_Swap_Bus_Template">
		<Parameters Type="Default">
			<ID>1</ID>
		</Parameters>
		<Parameters Type="Default">
			<NODE_ID>ELECTRICAL_Switch_Swap_Bus_#ID#</NODE_ID>
			<IE_NAME>ELECTRICAL_Switch_Swap_Bus_#ID#</IE_NAME>
		</Parameters>
		<Parameters Type="Default">
			<ANIM_NAME>#NODE_ID#</ANIM_NAME>
		</Parameters>
		<Parameters Type="Override">
			<ENABLE_BUS_1>#SOURCE_BUS_ID_1# (A:1:BUS CONNECTION ON:#TARGET_BUS_ID_1#, Bool) ! if{ #SOURCE_BUS_ID_1# #TARGET_BUS_ID_1# (&gt;K:2:ELECTRICAL_BUS_TO_BUS_CONNECTION_TOGGLE) }</ENABLE_BUS_1>
			<DISABLE_BUS_1>#SOURCE_BUS_ID_1# (A:1:BUS CONNECTION ON:#TARGET_BUS_ID_1#, Bool) if{ #SOURCE_BUS_ID_1# #TARGET_BUS_ID_1# (&gt;K:2:ELECTRICAL_BUS_TO_BUS_CONNECTION_TOGGLE) }</DISABLE_BUS_1>
			<ENABLE_BUS_2>#SOURCE_BUS_ID_2# (A:1:BUS CONNECTION ON:#TARGET_BUS_ID_2#, Bool) ! if{ #SOURCE_BUS_ID_2# #TARGET_BUS_ID_2# (&gt;K:2:ELECTRICAL_BUS_TO_BUS_CONNECTION_TOGGLE) }</ENABLE_BUS_2>
			<DISABLE_BUS_2>#SOURCE_BUS_ID_2# (A:1:BUS CONNECTION ON:#TARGET_BUS_ID_2#, Bool) if{ #SOURCE_BUS_ID_2# #TARGET_BUS_ID_2# (&gt;K:2:ELECTRICAL_BUS_TO_BUS_CONNECTION_TOGGLE) }</DISABLE_BUS_2>
		</Parameters>
		<Parameters Type="Override">
			<INTERACTION_TYPE>Switch</INTERACTION_TYPE>
			<GET_STATE_EXTERNAL>#SOURCE_BUS_ID_2# (A:1:BUS CONNECTION ON:#TARGET_BUS_ID_2#, Bool) sp0</GET_STATE_EXTERNAL>	
			<SET_STATE_EXTERNAL>
				p0 if{
					#ENABLE_BUS_2#
					#DISABLE_BUS_1#
				} els{
					#ENABLE_BUS_1#
					#DISABLE_BUS_2#
				}
			</SET_STATE_EXTERNAL>	
			<ON_BEFORE_INIT_CODE>#ENABLE_BUS_1# #DISABLE_BUS_2#</ON_BEFORE_INIT_CODE>
		</Parameters>
	
		<UseTemplate Name="ASOBO_Electrical_Base_Template">				
		</UseTemplate>
	</Template>
