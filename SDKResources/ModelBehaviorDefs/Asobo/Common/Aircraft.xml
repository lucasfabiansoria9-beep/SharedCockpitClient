	<Template Name="ASOBO_AIRFRAME_Warning_Door_Unlock_Template">
		<Parameters Type="Default">
			<TYPE>Front</TYPE>
			<INDEX>0</INDEX>
		</Parameters>
		<UseTemplate Name="ASOBO_AIRFRAME_Warning_Door_Unlock_SubTemplate">
		</UseTemplate>
	</Template>
	<Template Name="ASOBO_AIRFRAME_Warning_Door_Unlock_SubTemplate">
		<Parameters Type="Default">
			<NODE_ID>AIRFRAME_Warning_Door_Unlock_#TYPE#</NODE_ID>
			<ANIM_NAME>AIRFRAME_Warning_Door_Unlock_#TYPE#</ANIM_NAME>
			<ANIM_LENGTH>1</ANIM_LENGTH>
		</Parameters>
		<Component ID="#NODE_ID#" Node="#NODE_ID#">
			<UseTemplate Name="ASOBO_GT_Anim_Code">
				<ANIM_CODE>(A:INTERACTIVE POINT OPEN:#INDEX#, Percent over 100) 0 == </ANIM_CODE>
			</UseTemplate>
		</Component>
	</Template>
	<Template Name="ASOBO_AIRFRAME_Door_Template">
		<Parameters Type="Default">
			<INDEX>0</INDEX>
		</Parameters>
		<UseTemplate Name="ASOBO_AIRFRAME_Door_SubTemplate">
		</UseTemplate>
	</Template>
	<Template Name="ASOBO_AIRFRAME_Door_SubTemplate">
		<Parameters Type="Default">
			<ANIM_NAME>AIRFRAME_Door_#INDEX#</ANIM_NAME>
		</Parameters>
		<UseTemplate Name="ASOBO_GT_Anim_Code">
			<ANIM_CODE>(A:INTERACTIVE POINT OPEN:#INDEX#, Percent)</ANIM_CODE>
		</UseTemplate>
	</Template>
	<Template Name="ASOBO_AIRFRAME_AerialApplication_Hook_Template">
		<Parameters Type="Default">
			<ANIM_NAME_RELEASE>HOOK_RELEASE</ANIM_NAME_RELEASE>
			<ANIM_LAG>200</ANIM_LAG>
		</Parameters>

		<Condition NotEmpty="ANIM_NAME_LOWER">
			<UseTemplate Name="ASOBO_GT_Anim_Code">
				<ANIM_NAME>#ANIM_NAME_LOWER#</ANIM_NAME>
				<ANIM_CODE>
					(L:1:XMLVAR_IsBannerHooked) if{
						35
					} els{
						(A:LEAD POLE SET, Percent)
					}
				</ANIM_CODE>
			</UseTemplate>
		</Condition>
		<UseTemplate Name="ASOBO_GT_Anim_Code">
			<ANIM_NAME>#ANIM_NAME_RELEASE#</ANIM_NAME>
			<ANIM_CODE>(A:GRAPPLE HOOK SET, Percent)</ANIM_CODE>
		</UseTemplate>
	</Template>
	
	<!-- Requires ModelBehaviors Version 2+ to allow for partial visibility -->
	<Template Name="ASOBO_AIRFRAME_Dirt_Template">
		<TemplateAlias>ASOBO_EXTERIOR_Dirt_Template</TemplateAlias>
		<Parameters Type="Default">
			<Switch Param="TYPE">
				<Case Value="REGULAR">
					<DIRT_SIMVAR>REGULAR DIRT PCT</DIRT_SIMVAR>
				</Case>
				<Case Value="RUNWAY">
					<DIRT_SIMVAR>RUNWAY DIRT PCT</DIRT_SIMVAR>
				</Case>
				<Case Value="MUD">
					<DIRT_SIMVAR>MUD DIRT PCT</DIRT_SIMVAR>
				</Case>
				<Case Value="LEAKS">
					<DIRT_SIMVAR>LEAKS DIRT PCT</DIRT_SIMVAR>
				</Case>
				<Case Value="DUST">
					<DIRT_SIMVAR>DUST DIRT PCT</DIRT_SIMVAR>
				</Case>
				<Case Value="SCRATCHES">
					<DIRT_SIMVAR>SCRATCHES DIRT PCT</DIRT_SIMVAR>
				</Case>
			</Switch>
		</Parameters>
		<Parameters Type="Default">
			<GET_DIRT_LEVEL>(A:#DIRT_SIMVAR#, Percent over 100)</GET_DIRT_LEVEL>
		</Parameters>
		
		<Component ID="#NODE_ID#" Node="#NODE_ID#">
			<UseTemplate Name="ASOBO_GT_Visibility_Code">
				<VISIBILITY_CODE>#GET_DIRT_LEVEL#</VISIBILITY_CODE>
			</UseTemplate>
		</Component>
	</Template>

	<Template Name="ASOBO_AIRFRAME_Dirt_Multiple_Template">
		<TemplateAlias>ASOBO_EXTERIOR_Dirt_Multiple_Template</TemplateAlias>
		<Loop>
			<Setup>
				<Param>NODE_ID</Param>
				<Parameters>
					<LoadParameters ID="Dirt_NODE_IDs"/>
					<RemoveSavedParameters ID="Dirt_NODE_IDs"/>
				</Parameters>
			</Setup>
			<Do>
				<UseTemplate Name="ASOBO_AIRFRAME_Dirt_Template"/>
			</Do>
		</Loop>
	</Template>
