							<ON_EVENT_LONG_1>#ON_PULL_EVENT_LONG_1#</ON_EVENT_LONG_1>
						</Condition>
						<Condition NotEmpty="ON_PULL_EVENT_LONG_2">
							<ON_EVENT_LONG_2>#ON_PULL_EVENT_LONG_2#</ON_EVENT_LONG_2>
						</Condition>
					</Case>
					<Default>
						<Condition NotEmpty="ON_PULL_EVENT_LONG">
							<ON_EVENT_LONG>#ON_PULL_EVENT_LONG#</ON_EVENT_LONG>
						</Condition>
