<ModelBehaviors>
	<!-- Units conversion -->
	<Macro Name="16K">16384</Macro>
	<Macro Name="S16KToU16K">@16K + 2 /</Macro>
	<Macro Name="S16KTo01">@S16KToU16K @16K /</Macro>
	<Macro Name="U16KTo01">@16K /</Macro>
	<Macro Name="S16KToS1">@16K /</Macro>
	<Macro Name="S1To01">1 + 2 /</Macro>
	<!-- TTs formating -->
	<Macro Name="Sprintf">(F:Format)</Macro>	<!-- Ex: 12.5 '%.2f%%' @Sprintf = 12.50% -->
	<!-- Var Get/Set -->
	<Macro Name="Get_O_Var">(F:VarO)</Macro>	<!-- Ex: 'hello' @Get_O_Var returns the var hello value -->
	<Macro Name="Set_O_Var">(&gt;F:VarO)</Macro>	<!-- Ex: 42 'hello' @Set_O_Var set the var hello value to 42 -->
	<Macro Name="Get_I_Var">(F:VarO)</Macro>	<!-- Ex: 'hello' @Get_I_Var returns the var hello value -->
	<Macro Name="Set_I_Var">(&gt;F:VarO)</Macro>	<!-- Ex: 42 'hello' @Set_I_Var set the var hello value to 42 -->
	<Macro Name="Get_Local_Var">(F:1:VarL)</Macro>	<!-- Ex: 'hello' @Get_Local_Var returns the var hello value -->
	<Macro Name="Set_Local_Var">(&gt;F:1:VarL)</Macro>	<!-- Ex: 42 'hello' @Set_Local_Var set the var hello value to 42 -->
	<Macro Name="Get_Global_Var">(F:VarL)</Macro>	<!-- Ex: 'hello' @Get_Global_Var returns the var hello value -->
	<Macro Name="Set_Global_Var">(&gt;F:VarL)</Macro>	<!-- Ex: 42 'hello' @Set_Global_Var set the var hello value to 42 -->
	<Macro Name="Send_Key_Event">(&gt;F:KeyEvent)</Macro>	<!-- Ex: @16k * 'SPOILERS_SET' @Send_Key_Event send the key event SPOILERS_SET with the value @16k -->
	<Macro Name="Get_Input_Event">(F:InputEvent)</Macro>	<!-- Ex: 'HANDLING_SPEED_BRAKES' @Get_Input_Event get the input event HANDLING_SPEED_BRAKES value -->
	<Macro Name="Send_Input_Event">(&gt;F:InputEvent)</Macro>	<!-- Ex: 0.5 'HANDLING_SPEED_BRAKES_SET' @Send_Input_Event send the input event HANDLING_SPEED_BRAKES_SET with the value 0.5 -->
	<!-- Behavior variables -->
	<Macro Name="Get_Anim_Pos">(F:AnimPos)</Macro>	<!-- Ex: 'hello' @Get_Anim_Pos returns the animation hello raw position -->
	<Macro Name="Get_Anim_Pos_Pct">(F:AnimPos, percent)</Macro>	<!-- Ex: 'hello' @Get_Anim_Pos_Pct returns the animation hello normalized position in percent -->
	<Macro Name="Get_Emissive_Pos">(F:EmissivePos)</Macro>	<!-- Ex: 'hello' @Get_Emissive_Pos returns the node hello raw emissive value -->
	<Macro Name="Get_Emissive_Pos_Pct">(F:EmissivePos, percent)</Macro>	<!-- Ex: 'hello' @Get_Emissive_Pos_Pct returns the emissive factor of the node hello in percent -->
	<Macro Name="Get_Visibility_Pos">(F:VisibilityPos)</Macro>	<!-- Ex: 'hello' @Get_Visibility_Pos returns the node hello raw visibility value -->
	<Macro Name="Get_Visibility_Pos_Pct">(F:VisibilityPos, percent)</Macro>	<!-- Ex: 'hello' @Get_Visibility_Pos_Pct returns the visibility factor of the node hello in percent -->
	<!-- RPN Functions -->
	<Macro Name="Lerp">(F:Lerp)</Macro>	<!-- Ex: 0.5 0 100 @Lerp = 50 -->
	<Macro Name="MapRange">(F:MapRange)</Macro>	<!-- Ex: 12550 100 25000 0 1 @MapRange = 0.5 -->
	<Macro Name="Clamp">(F:Clamp)</Macro>	<!-- Ex: 110 0 100 @Clamp = 100 -->
	<Macro Name="Ratio">(F:Ratio)</Macro>	<!-- Ex: 125 50 150 @Ratio = 0.75 -->
	<!-- RPN Snippets -->
	<Macro Name="MR_Event">(M:Event)</Macro>
	<Macro Name="MR_Abs_X">(M:X)</Macro>
	<Macro Name="MR_Abs_Y">(M:Y)</Macro>
	<Macro Name="MR_Rel_X">(M:RelativeX)</Macro>
	<Macro Name="MR_Rel_Y">(M:RelativeY)</Macro>
	<Macro Name="MR_DragPercent">(M:DragPercent)</Macro>
	<Macro Name="MR_IsUsingRelativePos">(M:InputType) 1 ==</Macro>
	<Macro Name="MR_IsUsingVRController">(M:InputType) 2 ==</Macro>
	<Macro Name="MR_Case_InputType">3 (M:InputType) case</Macro>
	<Macro Name="MR_DragLockResetTimer_Absolute">0.10</Macro>
	<Macro Name="MR_DragLockResetTimer_Relative">0.05</Macro>
	<Macro Name="MR_DragLockResetTimer">@MR_DragLockResetTimer_Relative @MR_DragLockResetTimer_Absolute @MR_IsUsingRelativePos ?</Macro>
	<Macro Name="MR_DefaultDeadZoneRadius">0.002</Macro>
	<Macro Name="E_AbsoluteTime">(E:ABSOLUTE TIME, second)</Macro>
	<Macro Name="E_SimulationTime">(E:SIMULATION TIME, second)</Macro>
	<Macro Name="E_SimulationDeltaTime">(E:SIMULATION DELTA TIME, second)</Macro>
	<Macro Name="E_SimulationRate">(E:SIMULATION RATE, number)</Macro>
	<Macro Name="E_LocalTime_Seconds">(E:LOCAL TIME, second)</Macro>
	<Macro Name="E_LocalTime_Minutes">(E:LOCAL TIME, minutes)</Macro>
	<Macro Name="E_LocalTime_Hours">(E:LOCAL TIME, hours)</Macro>
	<Macro Name="E_IsInVR">(E:IS IN VR, Bool)</Macro>
	<Macro Name="E_UnitsOfMesure">(E:UNITS OF MEASURE, enum)</Macro>	<!-- 0=english, 1=metric w/altitude in feet, 2= metric -->
	<Macro Name="E_TimeOfDay">(E:TIME OF DAY, enum)</Macro>	<!-- 0=dawn, 1=day, 2=dusk, 3=night -->
	<Macro Name="E_AnimationDeltaTime">(A:ANIMATION DELTA TIME, second)</Macro>
</ModelBehaviors>