<ModelBehaviors>
	<!-- ParametersFn Definitions -->
	<ParametersFn Name="ASOBO_PartId_Base_Parameters">
		<Parameters Type="Override">
			<Condition NotEmpty="ID">
				<INDEXED_PART_ID>#PART_ID#_#ID#</INDEXED_PART_ID>				
			</Condition>
		</Parameters>
		<ReturnParameters>
			<PART_ID>#PART_ID#</PART_ID>
			<Condition NotEmpty="INDEXED_PART_ID">
				<INDEXED_PART_ID>#INDEXED_PART_ID#</INDEXED_PART_ID>
			</Condition>
		</ReturnParameters>
	</ParametersFn>
	<!-- Template Definitions -->
	<Template Name="ASOBO_PartId_Template">
		<Parameters Type="Default">
			<Condition NotEmpty="PART_ID">
				<PART_ID_0>#PART_ID#</PART_ID_0>
			</Condition>
		</Parameters>
		<Condition NotEmpty="PART_ID_0">
			<UseTemplate Name="ASOBO_PartIds_Template"/>
		</Condition>
	</Template>
	<Template Name="ASOBO_PartIds_Template">
		<Loop>
			<Setup>
				<Param>CUR_ID</Param>
				<From>0</From>
				<Inc>1</Inc>
				<While>
					<Arg NotEmpty="PART_ID_#CUR_ID#"/>
				</While>
			</Setup>
			<Do>
				<Parameters Type="Default">
					<PART_ID_BASE_TEMPLATE>ASOBO_PartId_Base_Template</PART_ID_BASE_TEMPLATE>
					<PART_ID Process="Param">PART_ID_#CUR_ID#</PART_ID>
				</Parameters>
				<UseTemplate Name="#PART_ID_BASE_TEMPLATE#"/>
			</Do>
		</Loop>
	</Template>
	<Template Name="ASOBO_PartId_Base_Template">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_PFN_Call_Overridable_ParametersFn_Helper">
				<PFN_PARAM_DEFAULT>ASOBO_PartId_Base_Parameters</PFN_PARAM_DEFAULT>
				<PFN_PARAM_NAME>PART_ID_BASE_PARAMETERS_FN</PFN_PARAM_NAME>
			</UseParametersFn>
		</Parameters>
		<PartId>#PART_ID#</PartId>
		<Condition NotEmpty="INDEXED_PART_ID">
			<PartId>#INDEXED_PART_ID#</PartId>
		</Condition>
	</Template>
</ModelBehaviors>