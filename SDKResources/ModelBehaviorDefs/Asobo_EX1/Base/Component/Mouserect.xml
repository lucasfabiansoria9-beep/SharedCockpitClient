<ModelBehaviors>
	<!-- Macro Definitions -->
	<Macro Name="PrimaryPressed">LeftSingle</Macro>
	<Macro Name="SecondaryPressed">RightSingle</Macro>
	<Macro Name="TertiaryPressed">MiddleSingle</Macro>
	<Macro Name="PrimaryDown">LeftDrag</Macro>
	<Macro Name="SecondaryDown">RightDrag</Macro>
	<Macro Name="TertiaryDown">MiddleDrag</Macro>
	<Macro Name="PrimaryReleased">LeftRelease</Macro>
	<Macro Name="SecondaryReleased">RightRelease</Macro>
	<Macro Name="TertiaryReleased">MiddleRelease</Macro>
	<Macro Name="Increase">WheelUp</Macro>
	<Macro Name="Decrease">WheelDown</Macro>
	<Macro Name="EnterLock">Lock</Macro>
	<Macro Name="ExitLock">Unlock</Macro>
	<Macro Name="HoverMouserect">Move</Macro>
	<Macro Name="LeftMouserect">Leave</Macro>
	<Macro Name="Exit">Exit</Macro>
	<Macro Name="Enter">Enter</Macro>
	<Macro Name="FL_PrimaryPressedReleased">@PrimaryPressed+@PrimaryReleased</Macro>
	<Macro Name="FL_SecondaryPressedReleased">@SecondaryPressed+@SecondaryReleased</Macro>
	<Macro Name="FL_TertiaryPressedReleased">@TertiaryPressed+@TertiaryReleased</Macro>
	<Macro Name="FL_PrimaryAll">@FL_PrimaryPressedReleased+@PrimaryDown</Macro>
	<Macro Name="FL_SecondaryAll">@FL_SecondaryPressedReleased+@SecondaryDown</Macro>
	<Macro Name="FL_TertiaryAll">@FL_TertiaryPressedReleased+@TertiaryDown</Macro>
	<Macro Name="FL_IncreaseDecrease">@Increase+@Decrease</Macro>
	<Macro Name="FL_Lock">@EnterLock+@ExitLock</Macro>
	<Macro Name="IsPrimaryPressed">@MR_Event '@PrimaryPressed' scmi 0 ==</Macro>
	<Macro Name="IsSecondaryPressed">@MR_Event '@SecondaryPressed' scmi 0 ==</Macro>
	<Macro Name="IsTertiaryPressed">@MR_Event '@TertiaryPressed' scmi 0 ==</Macro>
	<Macro Name="IsPrimaryDown">@MR_Event '@PrimaryDown' scmi 0 ==</Macro>
	<Macro Name="IsSecondaryDown">@MR_Event '@SecondaryDown' scmi 0 ==</Macro>
	<Macro Name="IsTertiaryDown">@MR_Event '@TertiaryDown' scmi 0 ==</Macro>
	<Macro Name="IsPrimaryReleased">@MR_Event '@PrimaryReleased' scmi 0 ==</Macro>
	<Macro Name="IsSecondaryReleased">@MR_Event '@SecondaryReleased' scmi 0 ==</Macro>
	<Macro Name="IsTertiaryReleased">@MR_Event '@TertiaryReleased' scmi 0 ==</Macro>
	<Macro Name="IsIncreasing">@MR_Event '@Increase' scmi 0 ==</Macro>
	<Macro Name="IsDecreasing">@MR_Event '@Decrease' scmi 0 ==</Macro>
	<Macro Name="IsEnteringLock">@MR_Event '@EnterLock' scmi 0 ==</Macro>
	<Macro Name="IsExitingLock">@MR_Event '@ExitLock' scmi 0 ==</Macro>
	<Macro Name="IsHoveringMouserect">@MR_Event '@HoverMouserect' scmi 0 ==</Macro>
	<Macro Name="IsLeavingMouserect">@MR_Event '@LeftMouserect' scmi 0 ==</Macro>
	<Macro Name="IsExiting">@MR_Event '@Exit' scmi 0 ==</Macro>
	<Macro Name="IsEntering">@MR_Event '@Enter' scmi 0 ==</Macro>
	<Macro Name="IM_Default">DEFAULT_IM</Macro><!-- FSX/FS2020 backcompat -->
	<Macro Name="IM_Drag">DRAG_IM</Macro><!-- FS2020/FS2024 -->
	<!-- ParametersFn Definitions -->
	<ParametersFn Name="ASOBO_Mouserect_IM_Indexed_Parameters">
		<Parameters Type="Default">
			<IN_PARAM_NAME/>
		</Parameters>
		<Parameters Type="Default">
			<OUT_PARAM_NAME>#IN_PARAM_NAME#</OUT_PARAM_NAME>
		</Parameters>
		<Parameters Type="Override">
			<__PARAM_VALUE__ Process="Param">#IN_PARAM_NAME#</__PARAM_VALUE__>
		</Parameters>
		<Parameters Type="Override">
			<Condition Check="#IN_PARAM_NAME#_@IM_Default">
				<True>
					<__PARAM_VALUE_IM_DEFAULT__ Process="Param">#IN_PARAM_NAME#_@IM_Default</__PARAM_VALUE_IM_DEFAULT__>
				</True>
				<False>
					<__PARAM_VALUE_IM_DEFAULT__>#__PARAM_VALUE__#</__PARAM_VALUE_IM_DEFAULT__>
				</False>
			</Condition>
			<Condition Check="#IN_PARAM_NAME#_@IM_Drag">
				<True>
					<__PARAM_VALUE_IM_DRAG__ Process="Param">#IN_PARAM_NAME#_@IM_Drag</__PARAM_VALUE_IM_DRAG__>
				</True>
				<False>
					<__PARAM_VALUE_IM_DRAG__>#__PARAM_VALUE__#</__PARAM_VALUE_IM_DRAG__>
				</False>
			</Condition>
		</Parameters>
		<ReturnParameters>
			<#OUT_PARAM_NAME#_@IM_Default>#__PARAM_VALUE_IM_DEFAULT__#</#OUT_PARAM_NAME#_@IM_Default>
			<#OUT_PARAM_NAME#_@IM_Drag>#__PARAM_VALUE_IM_DRAG__#</#OUT_PARAM_NAME#_@IM_Drag>
		</ReturnParameters>
	</ParametersFn>
	<ParametersFn Name="ASOBO_Mouserect_IM_Indexed_List_Parameters">
		<ReturnParameters>
			<Loop>
				<Setup>
					<Param>CUR_ID</Param>
					<From>1</From>
					<Inc>1</Inc>
					<While>
						<Arg Check="PARAM_#CUR_ID#"/>
					</While>
				</Setup>
				<Do>
					<UseParametersFn Name="ASOBO_Mouserect_IM_Indexed_Parameters">
						<IN_PARAM_NAME Process="Param">PARAM_#CUR_ID#</IN_PARAM_NAME>
					</UseParametersFn>
				</Do>
			</Loop>
		</ReturnParameters>
	</ParametersFn>
	<ParametersFn Name="ASOBO_Mouserect_Cursor_Default_Parameters">
		<Parameters Type="Default">
			<CURSOR_CENTER_RADIUS>0</CURSOR_CENTER_RADIUS>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Mouserect_IM_Indexed_List_Parameters">
				<PARAM_1>CURSOR</PARAM_1>
				<PARAM_2>CURSOR_LEFT</PARAM_2>
				<PARAM_3>CURSOR_RIGHT</PARAM_3>
				<PARAM_4>CURSOR_UP</PARAM_4>
				<PARAM_5>CURSOR_DOWN</PARAM_5>
				<PARAM_6>CURSOR_CENTER</PARAM_6>
				<PARAM_7>CURSOR_CENTER_RADIUS</PARAM_7>
			</UseParametersFn>
		</ReturnParameters>
	</ParametersFn>
	<ParametersFn Name="ASOBO_Mouserect_Mouseflags_Default_Parameters">
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Mouserect_IM_Indexed_Parameters">
				<IN_PARAM_NAME>MOUSEFLAGS</IN_PARAM_NAME>
			</UseParametersFn>
		</ReturnParameters>
	</ParametersFn>
	<ParametersFn Name="ASOBO_Mouserect_Tooltips_Default_Parameters">
		<Parameters Type="Default">
			<TOOLTIPS_HELPID/>
			<TOOLTIPS_TOOLTIPID/>
			<TOOLTIPS_ANIMREF_ID/>
			<TOOLTIPS_ANIMCURSOR_MIN/>
			<TOOLTIPS_ANIMCURSOR_MAX/>
			<TOOLTIPS_ANIMCURSOR_DIR>1.0</TOOLTIPS_ANIMCURSOR_DIR>
			<TOOLTIPS_ANIMTIP_LOOP/>
			<TOOLTIPS_DISABLED>False</TOOLTIPS_DISABLED>
			<TOOLTIPS_TITLE/>
			<TOOLTIPS_TITLE_IS_DYNAMIC>False</TOOLTIPS_TITLE_IS_DYNAMIC>
			<TOOLTIPS_WEAR_AND_TEAR/>
		</Parameters>
		<ReturnParameters>
			<HELPID>#TOOLTIPS_HELPID#</HELPID>
			<TOOLTIPID>#TOOLTIPS_TOOLTIPID#</TOOLTIPID>
			<Condition NotEmpty="TOOLTIPS_ANIMREF_ID">
				<ANIMREF_ID>#TOOLTIPS_ANIMREF_ID#</ANIMREF_ID>
			</Condition>
			<ANIMCURSOR_MIN>#TOOLTIPS_ANIMCURSOR_MIN#</ANIMCURSOR_MIN>
			<ANIMCURSOR_MAX>#TOOLTIPS_ANIMCURSOR_MAX#</ANIMCURSOR_MAX>
			<ANIMCURSOR_DIR>#TOOLTIPS_ANIMCURSOR_DIR#</ANIMCURSOR_DIR>
			<Condition NotEmpty="TOOLTIPS_ANIMTIP_LOOP">
				<ANIMTIP_LOOP>#TOOLTIPS_ANIMTIP_LOOP#</ANIMTIP_LOOP>
			</Condition>
			<Loop>
				<Setup>
					<Param>CUR_ID</Param>
					<From>0</From>
					<Inc>1</Inc>
					<While>
						<Arg NotEmpty="TOOLTIPS_ANIMTIP_#CUR_ID#"/>
					</While>
				</Setup>
				<Do>
					<Parameters Type="Default">
						<TOOLTIPS_ANIMTIP_#CUR_ID#_ON_PERCENT>False</TOOLTIPS_ANIMTIP_#CUR_ID#_ON_PERCENT>
						<TOOLTIPS_ANIMTIP_#CUR_ID#_ON_CURSOR/>
						<TOOLTIPS_ANIMTIP_#CUR_ID#_ON_HITBOX/>
					</Parameters>
					<ANIMTIP_#CUR_ID# Process="Param">TOOLTIPS_ANIMTIP_#CUR_ID#</ANIMTIP_#CUR_ID#>
					<ANIMTIP_#CUR_ID#_ON_PERCENT Process="Param">TOOLTIPS_ANIMTIP_#CUR_ID#_ON_PERCENT</ANIMTIP_#CUR_ID#_ON_PERCENT>
					<ANIMTIP_#CUR_ID#_ON_CURSOR Process="Param">TOOLTIPS_ANIMTIP_#CUR_ID#_ON_CURSOR</ANIMTIP_#CUR_ID#_ON_CURSOR>
					<ANIMTIP_#CUR_ID#_ON_HITBOX Process="Param">TOOLTIPS_ANIMTIP_#CUR_ID#_ON_HITBOX</ANIMTIP_#CUR_ID#_ON_HITBOX>
				</Do>
			</Loop>
			<TOOLTIPS_DISABLED>#TOOLTIPS_DISABLED#</TOOLTIPS_DISABLED>
			<TOOLTIPS_TITLE>#TOOLTIPS_TITLE#</TOOLTIPS_TITLE>
			<TOOLTIPS_TITLE_IS_DYNAMIC>#TOOLTIPS_TITLE_IS_DYNAMIC#</TOOLTIPS_TITLE_IS_DYNAMIC>
			<TOOLTIPS_WEAR_AND_TEAR>#TOOLTIPS_WEAR_AND_TEAR#</TOOLTIPS_WEAR_AND_TEAR>
			<Loop>
				<Setup>
					<Param>CUR_ID</Param>
					<From>0</From>
					<Inc>1</Inc>
					<While>
						<Arg NotEmpty="TOOLTIPS_ENTRY_#CUR_ID#"/>
					</While>
				</Setup>
				<Do>
					<TOOLTIPS_ENTRY_#CUR_ID# Process="Param">TOOLTIPS_ENTRY_#CUR_ID#</TOOLTIPS_ENTRY_#CUR_ID#>
				</Do>
			</Loop>
		</ReturnParameters>
	</ParametersFn>
	<!-- For lock inputs users, working in a relative position environement -->
	<!-- IN: (opt): INVERT_DRAG_SCALAR (If true the drag scalar will be mulitplied with -1) -->
	<!-- IN: (opt): Mouserect_Anim_Lag_Targets (list of anim index (starting at 0) for which we should not skip anim lag when locked) -->
	<ParametersFn Name="ASOBO_Mouserect_Drag_Default_Common_Parameters">
		<Parameters Type="Override">
			<BIT_MASK_8_BITS Process="Int">1 9 &lt;&lt; 1 -</BIT_MASK_8_BITS>
		</Parameters>
		<Parameters Type="Default">
			<CALLBACK_DRAG_USE_ANIM_LAG>False</CALLBACK_DRAG_USE_ANIM_LAG>
			<CALLBACK_DRAG_ANIM_LAG_TARGET>#BIT_MASK_8_BITS#</CALLBACK_DRAG_ANIM_LAG_TARGET>
			<CALLBACK_DRAG_SCALAR>1</CALLBACK_DRAG_SCALAR>
			<CALLBACK_DRAG_MOUSEFLAGS_LOCKABLE>@PrimaryDown+@SecondaryDown+@TertiaryDown</CALLBACK_DRAG_MOUSEFLAGS_LOCKABLE>
		</Parameters>
		<Parameters Type="Override">
			<Condition CheckSavedParameters="Mouserect_Anim_Lag_Targets">
				<Loop>
					<Setup>
						<Value>ANIM_ID</Value>
						<Parameters>
							<LoadParameters ID="Mouserect_Anim_Lag_Targets"/>
							<RemoveSavedParameters ID="Mouserect_Anim_Lag_Targets"/>
						</Parameters>
					</Setup>
					<Do>
						<!-- 11111111 ANIM_ID_TO_BIT_POS_MASKED & -->
						<CALLBACK_DRAG_ANIM_LAG_TARGET Process="Int">#CALLBACK_DRAG_ANIM_LAG_TARGET# #BIT_MASK_8_BITS# 1 #ANIM_ID# &lt;&lt; ~ &amp;</CALLBACK_DRAG_ANIM_LAG_TARGET>
					</Do>
				</Loop>
			</Condition>
		</Parameters>
		<Parameters Type="Override">
			<Condition Valid="INVERT_DRAG_SCALAR">
				<CALLBACK_DRAG_SCALAR Process="Float">#CALLBACK_DRAG_SCALAR# -1 *</CALLBACK_DRAG_SCALAR>
			</Condition>
		</Parameters>
		<Parameters Type="Override">
			<Condition NotEmpty="CALLBACK_DRAG_SCALAR_BOOST">
				<CALLBACK_DRAG_SCALAR Process="Float">#CALLBACK_DRAG_SCALAR# #CALLBACK_DRAG_SCALAR_BOOST# *</CALLBACK_DRAG_SCALAR>
			</Condition>
		</Parameters>
		<ReturnParameters>
			<DRAG_USE_ANIM_LAG>#CALLBACK_DRAG_USE_ANIM_LAG#</DRAG_USE_ANIM_LAG>
			<DRAG_ANIM_LAG_TARGET>#CALLBACK_DRAG_ANIM_LAG_TARGET#</DRAG_ANIM_LAG_TARGET>
			<DRAG_SCALAR>#CALLBACK_DRAG_SCALAR#</DRAG_SCALAR>
			<DRAG_MOUSEFLAGS_LOCKABLE>#CALLBACK_DRAG_MOUSEFLAGS_LOCKABLE#</DRAG_MOUSEFLAGS_LOCKABLE>
		</ReturnParameters>
	</ParametersFn>
	<ParametersFn Name="ASOBO_Mouserect_Drag_Default_Parameters">
		<Parameters Type="Default">
			<CALLBACK_DRAG_ANIM_SYNCED_WITH_DRAG>False</CALLBACK_DRAG_ANIM_SYNCED_WITH_DRAG>
			<CALLBACK_DRAG_ANIM_NAME/>
			<CALLBACK_DRAG_NODE_ID/>
			<CALLBACK_DRAG_AXIS>Any</CALLBACK_DRAG_AXIS>
			<!-- Only relevant when in CALLBACK_DRAG_MODE Trajectory -->
			<Condition Check="AXIS" Match="TRAJECTORY">
				<True>
					<CALLBACK_DRAG_MODE>Trajectory</CALLBACK_DRAG_MODE>
				</True>
				<False>
					<CALLBACK_DRAG_MODE>Default</CALLBACK_DRAG_MODE>
				</False>
			</Condition>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Mouserect_Drag_Default_Common_Parameters"/>
			<ANIM_SYNCED_WITH_DRAG>#CALLBACK_DRAG_ANIM_SYNCED_WITH_DRAG#</ANIM_SYNCED_WITH_DRAG>
			<DRAG_ANIM_NAME>#CALLBACK_DRAG_ANIM_NAME#</DRAG_ANIM_NAME>
			<DRAG_NODE_ID>#CALLBACK_DRAG_NODE_ID#</DRAG_NODE_ID>
			<DRAG_AXIS>#CALLBACK_DRAG_AXIS#</DRAG_AXIS>
			<DRAG_MODE>#CALLBACK_DRAG_MODE#</DRAG_NODE_ID>
		</ReturnParameters>
	</ParametersFn>
	<ParametersFn Name="ASOBO_Mouserect_Callback_Code_Default_Parameters">
		<Parameters Type="Default">
			<CALLBACK_CODE/>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Mouserect_IM_Indexed_Parameters">
				<IN_PARAM_NAME>CALLBACK_CODE</IN_PARAM_NAME>
				<OUT_PARAM_NAME>CODE</OUT_PARAM_NAME>
			</UseParametersFn>
			<UseParametersFn Name="ASOBO_Mouserect_Drag_Default_Parameters"/>
		</ReturnParameters>
	</ParametersFn>
	<ParametersFn Name="ASOBO_Mouserect_Callback_EventID_Default_Parameters">
		<Parameters Type="Default">
			<CALLBACK_EVENT_ID/>
		</Parameters>
		<ReturnParameters>
			<EVENT_ID>#CALLBACK_EVENT_ID#</EVENT_ID>
		</ReturnParameters>
	</ParametersFn>
	<ParametersFn Name="ASOBO_Mouserect_Callback_Dragging_Default_Parameters">
		<Parameters Type="Default">
			<CALLBACK_DRAG_SIMVAR/>
			<CALLBACK_DRAG_SIMVAR_UNITS/>
			<CALLBACK_DRAG_SCALE Process="Float">@16k 10 /</CALLBACK_DRAG_SCALE>
			<CALLBACK_DRAG_AXIS_X_SCALE>0</CALLBACK_DRAG_AXIS_X_SCALE>
			<CALLBACK_DRAG_AXIS_Y_SCALE>0</CALLBACK_DRAG_AXIS_Y_SCALE>
			<CALLBACK_DRAG_AXIS_Z_SCALE>0</CALLBACK_DRAG_AXIS_Z_SCALE>
			<CALLBACK_DRAG_MIN_VALUE>0</CALLBACK_DRAG_MIN_VALUE>
			<CALLBACK_DRAG_MAX_VALUE>@16k</CALLBACK_DRAG_MAX_VALUE>
			<CALLBACK_DRAG_EVENT_ID_SET/>
			<CALLBACK_DRAG_IS_RELATIVE>True</CALLBACK_DRAG_IS_RELATIVE>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Mouserect_Drag_Default_Parameters"/>
			<DRAG_SIMVAR>#CALLBACK_DRAG_SIMVAR#</DRAG_SIMVAR>
			<DRAG_SIMVAR_UNITS>#CALLBACK_DRAG_SIMVAR_UNITS#</DRAG_SIMVAR_UNITS>
			<DRAG_SCALE>#CALLBACK_DRAG_SCALE#</DRAG_SCALE>
			<DRAG_AXIS_X_SCALE>#CALLBACK_DRAG_AXIS_X_SCALE#</DRAG_AXIS_X_SCALE>
			<DRAG_AXIS_Y_SCALE>#CALLBACK_DRAG_AXIS_Y_SCALE#</DRAG_AXIS_Y_SCALE>
			<DRAG_AXIS_Z_SCALE>#CALLBACK_DRAG_AXIS_Z_SCALE#</DRAG_AXIS_Z_SCALE>
			<DRAG_MIN_VALUE>#CALLBACK_DRAG_MIN_VALUE#</DRAG_MIN_VALUE>
			<DRAG_MAX_VALUE>#CALLBACK_DRAG_MAX_VALUE#</DRAG_MAX_VALUE>
			<DRAG_EVENT_ID_SET>#CALLBACK_DRAG_EVENT_ID_SET#</DRAG_EVENT_ID_SET>
			<DRAG_IS_RELATIVE>#CALLBACK_DRAG_IS_RELATIVE#</DRAG_IS_RELATIVE>
		</ReturnParameters>
	</ParametersFn>
	<ParametersFn Name="ASOBO_Mouserect_Callback_Movement_Default_Parameters">
		<Parameters Type="Default">
			<CALLBACK_DRAG_DELTA>0</CALLBACK_DRAG_DELTA>
			<CALLBACK_DRAG_SCALAR>1</CALLBACK_DRAG_SCALAR>
			<CALLBACK_EVENT_ID_INC/>
			<CALLBACK_EVENT_ID_DEC/>
		</Parameters>
		<ReturnParameters>
			<DELTA>#CALLBACK_DRAG_DELTA#</DELTA>
			<SCALAR>#CALLBACK_DRAG_SCALAR#</SCALAR>
			<EVENT_ID_INC>#CALLBACK_EVENT_ID_INC#</EVENT_ID_INC>
			<EVENT_ID_DEC>#CALLBACK_EVENT_ID_DEC#</EVENT_ID_DEC>
		</ReturnParameters>
	</ParametersFn>
	<ParametersFn Name="ASOBO_Mouserect_Callback_X_Movement_Default_Parameters">
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Mouserect_Callback_Movement_Default_Parameters"/>
			<UseParametersFn Name="ASOBO_Mouserect_Drag_Default_Common_Parameters"/>
		</ReturnParameters>
	</ParametersFn>
	<ParametersFn Name="ASOBO_Mouserect_Callback_Y_Movement_Default_Parameters">
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Mouserect_Callback_Movement_Default_Parameters"/>
			<UseParametersFn Name="ASOBO_Mouserect_Drag_Default_Common_Parameters"/>
		</ReturnParameters>
	</ParametersFn>
	<ParametersFn Name="ASOBO_Mouserect_Callback_Default_Parameters">
		<ReturnParameters>
			<Switch Param="CALLBACK_TYPE">
				<Case Value="CODE">
					<UseParametersFn Name="ASOBO_PFN_Call_Overridable_ParametersFn_Helper">
						<PFN_PARAM_DEFAULT>ASOBO_Mouserect_Callback_Code_Default_Parameters</PFN_PARAM_DEFAULT>
						<PFN_PARAM_NAME>CALLBACK_CODE_PARAMETERS_FN</PFN_PARAM_NAME>
					</UseParametersFn>
				</Case>
				<Case Value="EVENT_ID">
					<UseParametersFn Name="ASOBO_PFN_Call_Overridable_ParametersFn_Helper">
						<PFN_PARAM_DEFAULT>ASOBO_Mouserect_Callback_EventID_Default_Parameters</PFN_PARAM_DEFAULT>
						<PFN_PARAM_NAME>CALLBACK_EVENT_ID_PARAMETERS_FN</PFN_PARAM_NAME>
					</UseParametersFn>
				</Case>
				<Case Value="DRAGGING">
					<UseParametersFn Name="ASOBO_PFN_Call_Overridable_ParametersFn_Helper">
						<PFN_PARAM_DEFAULT>ASOBO_Mouserect_Callback_Dragging_Default_Parameters</PFN_PARAM_DEFAULT>
						<PFN_PARAM_NAME>CALLBACK_DRAGGING_PARAMETERS_FN</PFN_PARAM_NAME>
					</UseParametersFn>
				</Case>
				<Case Value="X_MOVEMENT">
					<UseParametersFn Name="ASOBO_PFN_Call_Overridable_ParametersFn_Helper">
						<PFN_PARAM_DEFAULT>ASOBO_Mouserect_Callback_X_Movement_Default_Parameters</PFN_PARAM_DEFAULT>
						<PFN_PARAM_NAME>CALLBACK_X_MOVEMENT_PARAMETERS_FN</PFN_PARAM_NAME>
					</UseParametersFn>
				</Case>
				<Case Value="Y_MOVEMENT">
					<UseParametersFn Name="ASOBO_PFN_Call_Overridable_ParametersFn_Helper">
						<PFN_PARAM_DEFAULT>ASOBO_Mouserect_Callback_Y_Movement_Default_Parameters</PFN_PARAM_DEFAULT>
						<PFN_PARAM_NAME>CALLBACK_Y_MOVEMENT_PARAMETERS_FN</PFN_PARAM_NAME>
					</UseParametersFn>
				</Case>
			</Switch>
		</ReturnParameters>
	</ParametersFn>
	<ParametersFn Name="ASOBO_Mouserect_Common_Default_Parameters">
		<Parameters Type="Default">
			<MOUSERECT_DISABLED_IN_VR>False</MOUSERECT_DISABLED_IN_VR>
			<MOUSERECT_LOCK_ENABLED>True</MOUSERECT_LOCK_ENABLED>
			<MOUSERECT_TEMPORARY_LOCK_FLAGS>@PrimaryPressed</MOUSERECT_TEMPORARY_LOCK_FLAGS>
			<Condition Valid="NO_MOUSERECT_HIGHLIGHT_NODE_ID">
				<True>
					<MOUSERECT_HIGHLIGHT_NODE_ID>__NO_HIGHLIGHT__</MOUSERECT_HIGHLIGHT_NODE_ID><!-- Force removal of highlight node id -->
					<MOUSERECT_HIGHLIGHT_DISABLED>True</MOUSERECT_HIGHLIGHT_DISABLED>
				</True>
				<False>
					<MOUSERECT_HIGHLIGHT_NODE_ID/><!-- Using the component node if not specified -->
					<MOUSERECT_HIGHLIGHT_DISABLED>False</MOUSERECT_HIGHLIGHT_DISABLED>
				</False>
			</Condition>
			<MOUSERECT_HIGHLIGHT_PART_ID/>
			<MOUSERECT_IGNORE_ZTEST>False</MOUSERECT_IGNORE_ZTEST>
			<MOUSERECT_PRIORITIZE_VCOCKPITS>False</MOUSERECT_PRIORITIZE_VCOCKPITS>
		</Parameters>
		<ReturnParameters>
			<MOUSERECT_HIGHLIGHT_DISABLED>#MOUSERECT_HIGHLIGHT_DISABLED#</MOUSERECT_HIGHLIGHT_DISABLED>
			<MOUSERECT_DISABLED_IN_VR>#MOUSERECT_DISABLED_IN_VR#</MOUSERECT_DISABLED_IN_VR>
			<MOUSERECT_LOCK_ENABLED>#MOUSERECT_LOCK_ENABLED#</MOUSERECT_LOCK_ENABLED>
			<MOUSERECT_TEMPORARY_LOCK_FLAGS>#MOUSERECT_TEMPORARY_LOCK_FLAGS#</MOUSERECT_TEMPORARY_LOCK_FLAGS>
			<MOUSERECT_HIGHLIGHT_NODE_ID>#MOUSERECT_HIGHLIGHT_NODE_ID#</MOUSERECT_HIGHLIGHT_NODE_ID>
			<MOUSERECT_HIGHLIGHT_PART_ID>#MOUSERECT_HIGHLIGHT_PART_ID#</MOUSERECT_HIGHLIGHT_PART_ID>
			<MOUSERECT_IGNORE_ZTEST>#MOUSERECT_IGNORE_ZTEST#</MOUSERECT_IGNORE_ZTEST>
			<MOUSERECT_PRIORITIZE_VCOCKPITS>#MOUSERECT_PRIORITIZE_VCOCKPITS#</MOUSERECT_PRIORITIZE_VCOCKPITS>
		</ReturnParameters>
	</ParametersFn>
	<!-- Template Definitions -->
	<Template Name="ASOBO_Mouserect_Template">
		<Parameters Type="Default">
			<CALLBACK_TYPE>CODE</CALLBACK_TYPE>
		</Parameters>
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_PFN_Call_Overridable_ParametersFn_Helper">
				<PFN_PARAM_DEFAULT>ASOBO_Mouserect_Cursor_Default_Parameters</PFN_PARAM_DEFAULT>
				<PFN_PARAM_NAME>CURSOR_BASE_PARAMETERS_FN</PFN_PARAM_NAME>
			</UseParametersFn>
			<UseParametersFn Name="ASOBO_PFN_Call_Overridable_ParametersFn_Helper">
				<PFN_PARAM_DEFAULT>ASOBO_Mouserect_Mouseflags_Default_Parameters</PFN_PARAM_DEFAULT>
				<PFN_PARAM_NAME>MOUSEFLAGS_BASE_PARAMETERS_FN</PFN_PARAM_NAME>
			</UseParametersFn>
			<UseParametersFn Name="ASOBO_PFN_Call_Overridable_ParametersFn_Helper">
				<PFN_PARAM_DEFAULT>ASOBO_Mouserect_Tooltips_Default_Parameters</PFN_PARAM_DEFAULT>
				<PFN_PARAM_NAME>TOOLTIPS_BASE_PARAMETERS_FN</PFN_PARAM_NAME>
			</UseParametersFn>
			<UseParametersFn Name="ASOBO_PFN_Call_Overridable_ParametersFn_Helper">
				<PFN_PARAM_DEFAULT>ASOBO_Mouserect_Callback_Default_Parameters</PFN_PARAM_DEFAULT>
				<PFN_PARAM_NAME>CALLBACK_BASE_PARAMETERS_FN</PFN_PARAM_NAME>
			</UseParametersFn>
			<UseParametersFn Name="ASOBO_PFN_Call_Overridable_ParametersFn_Helper">
				<PFN_PARAM_DEFAULT>ASOBO_Mouserect_Common_Default_Parameters</PFN_PARAM_DEFAULT>
				<PFN_PARAM_NAME>MOUSERECT_BASE_PARAMETERS_FN</PFN_PARAM_NAME>
			</UseParametersFn>
		</Parameters>
		<MouseRect>
			<IMCursorsInstances>
				<IMDefault>
					<CursorLeft>#CURSOR_LEFT_DEFAULT_IM#</CursorLeft>
					<CursorRight>#CURSOR_RIGHT_DEFAULT_IM#</CursorRight>
					<CursorUp>#CURSOR_UP_DEFAULT_IM#</CursorUp>
					<CursorDown>#CURSOR_DOWN_DEFAULT_IM#</CursorDown>
					<CursorCenter>#CURSOR_CENTER_DEFAULT_IM#</CursorCenter>
					<Cursor>#CURSOR_DEFAULT_IM#</Cursor>
					<CursorCenterRadius>#CURSOR_CENTER_RADIUS_DEFAULT_IM#</CursorCenterRadius>
				</IMDefault>
				<IMDrag>
					<CursorLeft>#CURSOR_LEFT_DRAG_IM#</CursorLeft>
					<CursorRight>#CURSOR_RIGHT_DRAG_IM#</CursorRight>
					<CursorUp>#CURSOR_UP_DRAG_IM#</CursorUp>
					<CursorDown>#CURSOR_DOWN_DRAG_IM#</CursorDown>
					<CursorCenter>#CURSOR_CENTER_DRAG_IM#</CursorCenter>
					<Cursor>#CURSOR_DRAG_IM#</Cursor>
					<CursorCenterRadius>#CURSOR_CENTER_RADIUS_DRAG_IM#</CursorCenterRadius>
				</IMDrag>
			</IMCursorsInstances>
			<IMMouseFlagsInstances>
				<IMDefault>#MOUSEFLAGS_DEFAULT_IM#</IMDefault>
				<IMDrag>#MOUSEFLAGS_DRAG_IM#</IMDrag>
			</IMMouseFlagsInstances>
			<IMTooltipsInstances>
				<IMDefault>
					<HelpID>#HELPID#</HelpID>
					<TooltipID>#TOOLTIPID#</TooltipID>
					<Animated>
						<Condition Check="ANIMREF_ID">
							<AnimRefID>#ANIMREF_ID#</AnimRefID>
						</Condition>
						<AnimCursor min="#ANIMCURSOR_MIN#" max="#ANIMCURSOR_MAX#">#ANIMCURSOR_DIR#</AnimCursorDir>
						<Condition Check="ANIMTIP_LOOP">
							<AnimTooltipLoop>#ANIMTIP_LOOP#</AnimTooltipLoop>
						</Condition>
						<Loop>
							<Setup>
								<Param>CUR_ID</Param>
								<From>0</From>
								<Inc>1</Inc>
								<While>
									<Arg NotEmpty="ANIMTIP_#CUR_ID#"/>
								</While>
							</Setup>
							<Do>
								<Parameters Type="Default" Lifetime="Iteration">
									<ANIMTIP_#CUR_ID#_ON_PERCENT>False</ANIMTIP_#CUR_ID#_ON_PERCENT>
									<ANIMTIP_#CUR_ID#_ON_CURSOR/>
									<ANIMTIP_#CUR_ID#_ON_HITBOX/>
								</Parameters>
								<Parameters Type="Override" Lifetime="Iteration">
									<ANIMTIP_ON_PERCENT Process="Param">ANIMTIP_#CUR_ID#_ON_PERCENT</ANIMTIP_ON_PERCENT>
									<ANIMTIP_ON_CURSOR Process="Param">ANIMTIP_#CUR_ID#_ON_CURSOR</ANIMTIP_ON_CURSOR>
									<ANIMTIP_ON_HITBOX Process="Param">ANIMTIP_#CUR_ID#_ON_HITBOX</ANIMTIP_ON_HITBOX>
									<ANIMTIP Process="Param">ANIMTIP_#CUR_ID#</ANIMTIP>
								</Parameters>
								<AnimTooltip percent="#ANIMTIP_ON_PERCENT#" cursor="#ANIMTIP_ON_CURSOR#" hitbox="#ANIMTIP_ON_HITBOX#">#ANIMTIP#</AnimTooltip>
							</Do>
						</Loop>
					</Animated>
				</IMDefault>
				<IMDrag>
					<WearAndTearElements>
						<Loop>
							<Setup>
								<Param>WEAR_AND_TEAR_ELT_CATEGORY</Param>
								<Value>WEAR_AND_TEAR_ELT_INDEX</Value>
								<Parameters>
									<LoadParameters ID="WearAndTearElements"/>
									<RemoveSavedParameters ID="WearAndTearElements"/>
								</Parameters>
							</Setup>
							<Do>	
								<Parameters Type="Override">
									<Condition NotEmpty="WEAR_AND_TEAR_ELT_INDEX">
										<True>
											<TOOLTIPS_WEAR_AND_TEAR>#WEAR_AND_TEAR_ELT_CATEGORY#:#WEAR_AND_TEAR_ELT_INDEX#</TOOLTIPS_WEAR_AND_TEAR>
										</True>
										<False>
											<TOOLTIPS_WEAR_AND_TEAR>#WEAR_AND_TEAR_ELT_CATEGORY#</TOOLTIPS_WEAR_AND_TEAR>
										</False>
									</Condition>
								</Parameters>
								<WearAndTearElement>#TOOLTIPS_WEAR_AND_TEAR#</WearAndTearElement>
							</Do>
						</Loop>
					</WearAndTearElements>
					<TooltipEntries Disabled="#TOOLTIPS_DISABLED#">
						<Condition NotEmpty="TOOLTIPS_TITLE">
							<TTTitle RPN="#TOOLTIPS_TITLE_IS_DYNAMIC#">#TOOLTIPS_TITLE#</TTTitle>
							<Loop>
								<Setup>
									<Param>CUR_ID</Param>
									<From>0</From>
									<Inc>1</Inc>
									<While>
										<Arg NotEmpty="TOOLTIPS_ENTRY_#CUR_ID#"/>
									</While>
								</Setup>
								<Do>
									<Parameters Type="Override" Lifetime="Iteration">
										<TT_ID Process="Param">TOOLTIPS_ENTRY_#CUR_ID#</TT_ID>
									</Parameters>
									<TooltipEntry ID="#TT_ID#"/>
								</Do>
							</Loop>
						</Condition>
					</TooltipEntries>
				</IMDrag>
			</IMTooltipsInstances>
			<HighlightDisabled>#MOUSERECT_HIGHLIGHT_DISABLED#</HighlightDisabled>
			<DisabledInVr>#MOUSERECT_DISABLED_IN_VR#</DisabledInVr>
			<IgnoreZTest>#MOUSERECT_IGNORE_ZTEST#</IgnoreZTest>
			<PrioritizeVCockpits>#MOUSERECT_PRIORITIZE_VCOCKPITS#</PrioritizeVCockpits>
			<Lock>#MOUSERECT_LOCK_ENABLED#</Lock>
			<LockFlagsTemporary>#MOUSERECT_TEMPORARY_LOCK_FLAGS#</LockFlagsTemporary>
			<HighlightNodeId>#MOUSERECT_HIGHLIGHT_NODE_ID#</HighlightNodeId>
			<HighlightPartId>#MOUSERECT_HIGHLIGHT_PART_ID#</HighlightPartId>
			<Switch Param="CALLBACK_TYPE">
				<Case Value="CODE">
					<CallbackCode>
						<IMCodeInstances>
							<IMDefault>#CODE_DEFAULT_IM#</IMDefault>
							<IMDrag>#CODE_DRAG_IM#</IMDrag>
						</IMCodeInstances>
						<DragAnimName>#DRAG_ANIM_NAME#</DragAnimName>
						<DragNodeId>#DRAG_NODE_ID#</DragNodeId>
						<DragAnimSynced>#ANIM_SYNCED_WITH_DRAG#</DragAnimSynced>
						<DragUseAnimLag>#DRAG_USE_ANIM_LAG#</DragUseAnimLag>
						<DragAnimLagTarget>#DRAG_ANIM_LAG_TARGET#</DragAnimLagTarget>
						<DragMode>#DRAG_MODE#</DragMode>
						<DragAxis>#DRAG_AXIS#</DragAxis>
						<DragScalar>#DRAG_SCALAR#</DragScalar>
						<DragFlagsLockable>#DRAG_MOUSEFLAGS_LOCKABLE#</DragFlagsLockable>
					</CallbackCode>
				</Case>
				<Case Value="EVENT_ID">
					<EventID>#EVENTID#</EventID>
				</Case>
				<Case Value="DRAGGING">
					<CallbackDragging>
						<Variable>#DRAG_SIMVAR#</Variable>
						<Units>#DRAG_SIMVAR_UNITS#</Units>
						<Scale>#DRAG_SCALE#</Scale>
						<XScale>#DRAG_AXIS_X_SCALE#</XScale>
						<YScale>#DRAG_AXIS_Y_SCALE#</YScale>
						<ZScale>#DRAG_AXIS_Z_SCALE#</ZScale>
						<MinValue>#DRAG_MIN_VALUE#</MinValue>
						<MaxValue>#DRAG_MAX_VALUE#</MaxValue>
						<EventID>#DRAG_EVENTID_SET#</EventID>
						<IsRelative>#DRAG_IS_RELATIVE#</IsRelative>
						<DragAnimName>#DRAG_ANIM_NAME#</DragAnimName>
						<DragNodeId>#DRAG_NODE_ID#</DragNodeId>
						<DragAnimSynced>#ANIM_SYNCED_WITH_DRAG#</DragAnimSynced>
						<DragUseAnimLag>#DRAG_USE_ANIM_LAG#</DragUseAnimLag>
						<DragAnimLagTarget>#DRAG_ANIM_LAG_TARGET#</DragAnimLagTarget>
						<DragMode>#DRAG_MODE#</DragMode>
						<DragAxis>#DRAG_AXIS#</DragAxis>
						<DragScalar>#DRAG_SCALAR#</DragScalar>
						<DragFlagsLockable>#DRAG_MOUSEFLAGS_LOCKABLE#</DragFlagsLockable>
					</CallbackDragging>
				</Case>
				<Case Value="X_MOVEMENT">
					<CallbackJumpDragging>
						<XMovement>
							<Delta>#DELTA#</Delta>
							<DragScalar>#SCALAR#</DragScalar>
							<EventIdInc>#EVENT_ID_INC#</EventIdInc>
							<EventIdDec>#EVENT_ID_DEC#</EventIdDec>
						</XMovement>
						<DragUseAnimLag>#DRAG_USE_ANIM_LAG#</DragUseAnimLag>
						<DragAnimLagTarget>#DRAG_ANIM_LAG_TARGET#</DragAnimLagTarget>
						<DragScalar>#DRAG_SCALAR#</DragScalar>
						<DragFlagsLockable>#DRAG_MOUSEFLAGS_LOCKABLE#</DragFlagsLockable>
					</CallbackJumpDragging>
				</Case>
				<Case Value="Y_MOVEMENT">
					<CallbackJumpDragging>
						<YMovement>
							<Delta>#DELTA#</Delta>
							<DragScalar>#SCALAR#</DragScalar>
							<EventIdInc>#EVENT_ID_INC#</EventIdInc>
							<EventIdDec>#EVENT_ID_DEC#</EventIdDec>
						</YMovement>
						<DragUseAnimLag>#DRAG_USE_ANIM_LAG#</DragUseAnimLag>
						<DragAnimLagTarget>#DRAG_ANIM_LAG_TARGET#</DragAnimLagTarget>
						<DragScalar>#DRAG_SCALAR#</DragScalar>
						<DragFlagsLockable>#DRAG_MOUSEFLAGS_LOCKABLE#</DragFlagsLockable>
					</CallbackJumpDragging>
				</Case>
			</Switch>
		</MouseRect>
	</Template>
</ModelBehaviors>