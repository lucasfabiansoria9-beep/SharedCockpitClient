	</Template>
	<!-- InputEvent Definitions -->
	<InputEvent ID="ASOBO_GIE_Base">
		<Presets>
			<Preset ID="#PRESET_ID#">
				<Tooltip Inop="#TT_INOP#">
					<Icon>#TT_ICON#</Icon>
					<Interaction>#TT_INTERACTION#</Interaction>
					<InteractionLockable>#TT_INTERACTION_LOCKABLE#</InteractionLockable>
					<TTDescription RPN="#TT_DESCRIPTION_IS_DYNAMIC#">#TT_DESCRIPTION#</TTDescription>
					<TTValue RPN="#TT_VALUE_IS_DYNAMIC#">#TT_VALUE#</TTValue>
				</Tooltip>
				<Value>
					<Units>#VALUE_UNITS#</Units>
					<Code>#VALUE_CODE#</Code>
					<Init>#VALUE_INIT_CODE#</Init>
					<WatchVars>
						<Loop>
							<Setup>
								<Param>CUR_ID</Param>
								<From>0</From>
								<Inc>1</Inc>
								<While>
									<Arg Valid="SIMVAR_TO_WATCH_#CUR_ID#"/>
								</While>
							</Setup>
							<Do>
								<Parameters Type="Override">
									<SIMVAR_TO_WATCH Process="Param">SIMVAR_TO_WATCH_#CUR_ID#</SIMVAR_TO_WATCH>
								</Parameters>
								<Simvar ID="#SIMVAR_TO_WATCH#"/>
							</Do>
						</Loop>
						<Loop>
							<Setup>
								<Param>CUR_ID</Param>
								<From>0</From>
								<Inc>1</Inc>
								<While>
									<Arg Valid="LOCAL_VAR_TO_WATCH_#CUR_ID#"/>
								</While>
							</Setup>
							<Do>
								<Parameters Type="Override">
									<LOCAL_VAR_TO_WATCH Process="Param">LOCAL_VAR_TO_WATCH_#CUR_ID#</LOCAL_VAR_TO_WATCH>
									<LOCAL_VAR_SCOPE Process="Param">LOCAL_VAR_TO_WATCH_#CUR_ID#_SCOPE</LOCAL_VAR_SCOPE>
								</Parameters>
								<Variable ID="#LOCAL_VAR_TO_WATCH#" Scope="#LOCAL_VAR_SCOPE#"/>
							</Do>
						</Loop>
						<Loop>
							<Setup>
								<Param>CUR_ID</Param>
								<From>0</From>
								<Inc>1</Inc>
								<While>
									<Arg Valid="GLOBAL_VAR_TO_WATCH_#CUR_ID#"/>
								</While>
							</Setup>
							<Do>
								<Parameters Type="Override">
									<GLOBAL_VAR_TO_WATCH Process="Param">GLOBAL_VAR_TO_WATCH_#CUR_ID#</GLOBAL_VAR_TO_WATCH>
								</Parameters>
								<GlobalVar ID="#GLOBAL_VAR_TO_WATCH#"/>
							</Do>
						</Loop>
					</WatchVars>
				</Value>
				<Inc>
					<Action>
						<Type>#INC_ACTION_TYPE#</Type>
						<TT_Name>#INC_ACTION_TT_NAME#</TT_Name>
						<TT_Description>#INC_ACTION_TT_DESCRIPTION#</TT_Description>
						<TT_Function>#INC_ACTION_TT_FUNCTION#</TT_Function>
						<TT_Tag>#INC_ACTION_TT_TAG#</TT_Tag>
					</Action>
					<Code>#INC_CODE#</Code>
					<Parameters>
						<Condition>
							<Test>
								<GreaterOrEqual>
									<Number>#INC_ARG_LAST_ID#</Number>
									<Number>0</Number>
								</GreaterOrEqual>
							</Test>
							<True>
								<Loop>
									<Setup>
										<Param>ARG_ID</Param>
										<From>0</From>
										<Inc>1</Inc>
										<To>#INC_ARG_LAST_ID#</To>
									</Setup>
									<Do>
										<Parameters Type="Override">
											<UseParametersFn Name="ASOBO_PFN_Call_Overridable_ParametersFn_Helper">
												<IN_EVENT_PARAM_NAME>INC_PARAM_#ARG_ID#</IN_EVENT_PARAM_NAME>
												<PFN_PARAM_DEFAULT>ASOBO_Inc_Event_Params_GIE_Base_Parameters</PFN_PARAM_DEFAULT>
												<PFN_PARAM_NAME>INC_PARAMS_GIE_BASE_PARAMETERS_FN</PFN_PARAM_NAME>
											</UseParametersFn>
										</Parameters>
										<Param Type="#PARAM_TYPE#" RPN="#PARAM_IS_DYNAMIC#">#PARAM_VALUE#</Param>
									</Do>
								</Loop>
							</True>
						</Condition>
					</Parameters>
					<Bindings>
						<Loop>
							<Setup>
								<Param>ID</Param>
								<From>0</From>
								<Inc>1</Inc>
								<While>
									<Arg NotEmpty="BINDING_INC_ALIAS_ID_#ID#"/>
								</While>
							</Setup>
							<Do>
								<Parameters Type="Override">
									<IN_EVENT_BINDING_TYPE>ALIAS_ID</IN_EVENT_BINDING_TYPE>
								</Parameters>
								<Parameters Type="Override">
									<UseParametersFn Name="ASOBO_PFN_Call_Overridable_ParametersFn_Helper">
										<IN_EVENT_BINDING_NAME Process="Param">BINDING_INC_ALIAS_ID_#ID#</IN_EVENT_BINDING_NAME>
										<IN_EVENT_BINDING_ACTION_TYPE Process="Param">BINDING_INC_ALIAS_ID_#ID#_ACTION_TYPE</IN_EVENT_BINDING_ACTION_TYPE>
										<IN_EVENT_BINDING_ACTION_TT_NAME Process="Param">BINDING_INC_ALIAS_ID_#ID#_ACTION_TT_NAME</IN_EVENT_BINDING_ACTION_TT_NAME>
										<IN_EVENT_BINDING_ACTION_TT_DESCRIPTION Process="Param">BINDING_INC_ALIAS_ID_#ID#_ACTION_TT_DESCRIPTION</IN_EVENT_BINDING_ACTION_TT_DESCRIPTION>
										<IN_EVENT_BINDING_ACTION_TT_FUNCTION Process="Param">BINDING_INC_ALIAS_ID_#ID#_ACTION_TT_FUNCTION</IN_EVENT_BINDING_ACTION_TT_FUNCTION>
										<IN_EVENT_BINDING_ACTION_TT_TAG Process="Param">BINDING_INC_ALIAS_ID_#ID#_ACTION_TT_TAG</IN_EVENT_BINDING_ACTION_TT_TAG>
										<PFN_PARAM_DEFAULT>ASOBO_Inc_Event_Bindings_GIE_Base_Parameters</PFN_PARAM_DEFAULT>
										<PFN_PARAM_NAME>BINDING_INC_ALIAS_ID_GIE_BASE_PARAMETERS_FN</PFN_PARAM_NAME>
									</UseParametersFn>
								</Parameters>
								<Binding Alias="#BINDING_ID#">
									<Action>
										<Type>#BINDING_ACTION_TYPE#</Type>
										<TT_Name>#BINDING_ACTION_TT_NAME#</TT_Name>
										<TT_Description>#BINDING_ACTION_TT_DESCRIPTION#</TT_Description>
										<TT_Function>#BINDING_ACTION_TT_FUNCTION#</TT_Function>
										<TT_Tag>#BINDING_ACTION_TT_TAG#</TT_Tag>
									</Action>
									<Parameters>
										<Condition>
											<Test>
												<GreaterOrEqual>
													<Number>#INC_ARG_LAST_ID#</Number>
													<Number>0</Number>
												</GreaterOrEqual>
											</Test>
											<True>
												<Loop>
													<Setup>
														<Param>ARG_ID</Param>
														<From>0</From>
														<Inc>1</Inc>
														<To>#INC_ARG_LAST_ID#</To>
													</Setup>
													<Do>
														<Parameters Type="Override">
															<UseParametersFn Name="ASOBO_PFN_Call_Overridable_ParametersFn_Helper">
																<IN_EVENT_PARAM_NAME>BINDING_INC_ALIAS_ID_#ID#_PARAM_#ARG_ID#</IN_EVENT_PARAM_NAME>
																<PFN_PARAM_DEFAULT>ASOBO_Inc_Event_Params_GIE_Base_Parameters</PFN_PARAM_DEFAULT>
																<PFN_PARAM_NAME>BINDING_INC_ALIAS_ID_PARAMS_GIE_BASE_PARAMETERS_FN</PFN_PARAM_NAME>
															</UseParametersFn>
														</Parameters>
														<Param RPN="#PARAM_IS_DYNAMIC#">#PARAM_VALUE#</Param>
													</Do>
												</Loop>
											</True>
										</Condition>
									</Parameters>
								</Binding>
							</Do>
						</Loop>
						<Loop>
							<Setup>
								<Param>ID</Param>
								<From>0</From>
								<Inc>1</Inc>
								<While>
									<Arg NotEmpty="BINDING_INC_EVENT_ID_#ID#"/>
								</While>
							</Setup>
							<Do>
								<Parameters Type="Override">
									<IN_EVENT_BINDING_TYPE>EVENT_ID</IN_EVENT_BINDING_TYPE>
								</Parameters>
								<Parameters Type="Override">
									<UseParametersFn Name="ASOBO_PFN_Call_Overridable_ParametersFn_Helper">
										<IN_EVENT_BINDING_NAME Process="Param">BINDING_INC_EVENT_ID_#ID#</IN_EVENT_BINDING_NAME>
										<IN_EVENT_BINDING_ACTION_TYPE Process="Param">BINDING_INC_EVENT_ID_#ID#_ACTION_TYPE</IN_EVENT_BINDING_ACTION_TYPE>
										<IN_EVENT_BINDING_ACTION_TT_NAME Process="Param">BINDING_INC_EVENT_ID_#ID#_ACTION_TT_NAME</IN_EVENT_BINDING_ACTION_TT_NAME>
