	<!-- IN_EVENT_PARAM_NAME: Name of the parameters to read data from -->
	<!-- IN_EVENT_PARAM_DEFAULT: if IN_EVENT_PARAM_NAME isn't specified, used to default the param value -->
	<!-- OUT: PARAM_VALUE, PARAM_TYPE, PARAM_IS_DYNAMIC -->
	<ParametersFn Name="ASOBO_GIE_EventParam_Base_Parameters">
		<Parameters Type="Default">
			<Condition NotEmpty="#IN_EVENT_PARAM_NAME#">
				<True>
					<IN_EVENT_PARAM_DEFAULT_TYPE>Float</IN_EVENT_PARAM_DEFAULT_TYPE>
					<IN_EVENT_PARAM_DEFAULT>0</IN_EVENT_PARAM_DEFAULT>
					<IN_EVENT_PARAM_DEFAULT_IS_DYNAMIC>False</IN_EVENT_PARAM_DEFAULT_IS_DYNAMIC>
				</True>
				<False>
					<IN_EVENT_PARAM_DEFAULT_TYPE>Float</IN_EVENT_PARAM_DEFAULT_TYPE>
					<IN_EVENT_PARAM_DEFAULT>p0</IN_EVENT_PARAM_DEFAULT>
					<IN_EVENT_PARAM_DEFAULT_IS_DYNAMIC>True</IN_EVENT_PARAM_DEFAULT_IS_DYNAMIC>
				</False>
			</Condition>
		</Parameters>
		<Parameters Type="Override">
			<Condition NotEmpty="#IN_EVENT_PARAM_NAME#">
				<True>
					<__PARAM_VALUE__ Process="Param">#IN_EVENT_PARAM_NAME#</__PARAM_VALUE__>
					<Condition NotEmpty="#IN_EVENT_PARAM_NAME#_TYPE">
						<True>
							<__PARAM_TYPE__ Process="Param">#IN_EVENT_PARAM_NAME#_TYPE</__PARAM_TYPE__>
						</True>
						<False>
							<__PARAM_TYPE__>#IN_EVENT_PARAM_DEFAULT_TYPE#</__PARAM_TYPE__>
						</False>
					</Condition>
					<Condition NotEmpty="#IN_EVENT_PARAM_NAME#_IS_DYNAMIC">
						<True>
							<__PARAM_IS_DYNAMIC__ Process="Param">#IN_EVENT_PARAM_NAME#_IS_DYNAMIC</__PARAM_IS_DYNAMIC__>
						</True>
						<False>
							<__PARAM_IS_DYNAMIC__>#IN_EVENT_PARAM_DEFAULT_IS_DYNAMIC#</__PARAM_IS_DYNAMIC__>
						</False>
					</Condition>
				</True>
				<False>
					<__PARAM_VALUE__>#IN_EVENT_PARAM_DEFAULT#</__PARAM_VALUE__>
					<__PARAM_TYPE__>#IN_EVENT_PARAM_DEFAULT_TYPE#</__PARAM_TYPE__>
					<__PARAM_IS_DYNAMIC__>#IN_EVENT_PARAM_DEFAULT_IS_DYNAMIC#</__PARAM_IS_DYNAMIC__>
				</False>
			</Condition>
		</Parameters>
		<ReturnParameters>
			<PARAM_VALUE>#__PARAM_VALUE__#</PARAM_VALUE>
			<PARAM_TYPE>#__PARAM_TYPE__#</PARAM_TYPE>
			<PARAM_IS_DYNAMIC>#__PARAM_IS_DYNAMIC__#</PARAM_IS_DYNAMIC>
		</ReturnParameters>
	</ParametersFn>
	<!-- IN: IN_EVENT_BINDING_NAME (Name of the binding parameter to read data from) -->
	<!-- OUT: BINDING_ID (ID of the binding) -->
	<ParametersFn Name="ASOBO_GIE_EventBinding_Base_Parameters">
		<ReturnParameters>
			<Switch Param="IN_EVENT_BINDING_TYPE">
				<Case Value="ALIAS_ID">
					<BINDING_ID>#PRESET_ID#_#IN_EVENT_BINDING_NAME#</BINDING_ID>
				</Case>
				<Case Value="EVENT_ID">
					<BINDING_ID>#IN_EVENT_BINDING_NAME#</BINDING_ID>
				</Case>
			</Switch>
			<BINDING_ACTION_TYPE>#IN_EVENT_BINDING_ACTION_TYPE#</BINDING_ACTION_TYPE>
			<BINDING_ACTION_TT_NAME>#IN_EVENT_BINDING_ACTION_TT_NAME#</BINDING_ACTION_TT_NAME>
			<BINDING_ACTION_TT_DESCRIPTION>#IN_EVENT_BINDING_ACTION_TT_DESCRIPTION#</BINDING_ACTION_TT_DESCRIPTION>
			<BINDING_ACTION_TT_FUNCTION>#IN_EVENT_BINDING_ACTION_TT_FUNCTION#</BINDING_ACTION_TT_FUNCTION>
			<BINDING_ACTION_TT_TAG>#IN_EVENT_BINDING_ACTION_TT_TAG#</BINDING_ACTION_TT_TAG>
		</ReturnParameters>
	</ParametersFn>
