	<!-- IN (opt): MR_ON_PRIMARY_PRESSED -->
	<!-- IN (opt): MR_ON_SECONDARY_PRESSED -->
	<!-- IN (opt): MR_ON_TERTIARY_PRESSED -->
	<!-- IN (opt): MR_ON_PRIMARY_DOWN -->
	<!-- IN (opt): MR_ON_SECONDARY_DOWN -->
	<!-- IN (opt): MR_ON_TERTIARY_DOWN -->
	<!-- IN (opt): MR_ON_PRIMARY_RELEASED -->
	<!-- IN (opt): MR_ON_SECONDARY_RELEASED -->
	<!-- IN (opt): MR_ON_TERTIARY_RELEASED -->
	<!-- IN (opt): MR_ON_INCREASE -->
	<!-- IN (opt): MR_ON_DECREASE -->
	<!-- IN (opt): MR_ON_HOVER -->
	<!-- IN (opt): MR_ON_LEAVE -->
	<!-- IN (opt): MR_ON_ENTER_LOCK -->
	<!-- IN (opt): MR_ON_EXIT_LOCK -->
	<!-- IN (opt): MR_ON_ENTER -->
	<!-- IN (opt): MR_ON_EXIT -->
	<!-- IN (opt): INTERACTION_MODEL (@IM_Drag or @IM_Default) -->
	<!-- IN (opt): OUT_PARAM_NAME_CODE (Name of the parameters where to store the resulted code) -->
	<!-- IN (opt): OUT_PARAM_NAME_FLAGS (IName of the parameters where to store the resulted code mouseflags) -->
	<!-- OUT: formatted OUT_PARAM_NAME_CODE & OUT_PARAM_NAME_FLAGS -->
	<ParametersFn Name="ASOBO_Get_Mouserect_Events_Callback_Code">
		<Parameters Type="Default">
			<Switch>
				<Case NotEmpty="INTERACTION_MODEL">
					<OUT_PARAM_NAME_CODE>CALLBACK_CODE_#INTERACTION_MODEL#</OUT_PARAM_NAME_CODE>
					<OUT_PARAM_NAME_FLAGS>MOUSEFLAGS_#INTERACTION_MODEL#</OUT_PARAM_NAME_FLAGS>
				</Case>
				<Default>
					<OUT_PARAM_NAME_CODE>CALLBACK_CODE</OUT_PARAM_NAME_CODE>
					<OUT_PARAM_NAME_FLAGS>MOUSEFLAGS</OUT_PARAM_NAME_FLAGS>
				</Default>
			</Switch>
			<UseParametersFn Name="ASOBO_Get_Next_Safe_Jump_Flag_Helper">
				<OUT_PARAM_NAME>FL_AFTER_CODE_EVENTS</OUT_PARAM_NAME>
			</UseParametersFn>
			<__CONCATENATED_EVENTS__/>
			<__CONCATENATED_FLAGS__/>
		</Parameters>
		<ReturnParameters>
			<Loop>
				<Setup>
					<Param>MR_EVENT_NAME</Param>
					<Value>MR_EVENT_VALUE</Value>
					<Parameters>
						<UseParametersFn Name="ASOBO_Get_Mouserect_Events"/>
					</Parameters>
				</Setup>
				<Do>
					<Parameters Type="Default">
						<RETURN_ON_SUCCESSFUL_EVENT_CHECK>True</RETURN_ON_SUCCESSFUL_EVENT_CHECK>
					</Parameters>
					<Parameters Type="Override">
						<UseParametersFn Name="ASOBO_Get_Mouserect_Ev_#MR_EVENT_NAME#_Parameters"/>
						<!-- Load MR_EVENT_TEST & MR_EVENT_FLAG -->
					</Parameters>
					<Parameters Type="Override">
						<Condition NotEmpty="MR_EVENT_VALUE">
							<Condition Valid="RETURN_ON_SUCCESSFUL_EVENT_CHECK">
								<True>
									<THIS_MR_EVENT_CODE>#MR_EVENT_TEST# if{ #MR_EVENT_VALUE# g#FL_AFTER_CODE_EVENTS# }</THIS_MR_EVENT_CODE>									
								</True>
								<False>
									<THIS_MR_EVENT_CODE>#MR_EVENT_TEST# if{ #MR_EVENT_VALUE# }</THIS_MR_EVENT_CODE>	
								</False>
							</Condition>
						</Condition>
					</Parameters>
					<Parameters Type="Override" Lifetime="Loop">
						<Condition NotEmpty="THIS_MR_EVENT_CODE">
							<Condition NotEmpty="__CONCATENATED_EVENTS__">
								<True>
									<__CONCATENATED_EVENTS__>#__CONCATENATED_EVENTS__# #THIS_MR_EVENT_CODE#</__CONCATENATED_EVENTS__>
									<Condition NotEmpty="MR_EVENT_FLAG">
										<__CONCATENATED_FLAGS__>#__CONCATENATED_FLAGS__#+#MR_EVENT_FLAG#</__CONCATENATED_FLAGS__>										
									</Condition>
								</True>
								<False>
									<__CONCATENATED_EVENTS__>#THIS_MR_EVENT_CODE#</__CONCATENATED_EVENTS__>
									<__CONCATENATED_FLAGS__>#MR_EVENT_FLAG#</__CONCATENATED_FLAGS__>
								</False>
							</Condition>
						</Condition>
					</Parameters>
				</Do>
				<Then>
					<#OUT_PARAM_NAME_CODE#>#__CONCATENATED_EVENTS__# :#FL_AFTER_CODE_EVENTS#</#OUT_PARAM_NAME_CODE#>
					<#OUT_PARAM_NAME_FLAGS#>#__CONCATENATED_FLAGS__#</#OUT_PARAM_NAME_FLAGS#>
				</Then>
			</Loop>
		</ReturnParameters>
	</ParametersFn>
	<!-- IN: ALIAS_ID -->
	<!-- IN: IE_PRESET_ID -->
	<!-- IN (opt): OUT_PARAM_NAME -->
	<ParametersFn Name="ASOBO_Get_IE_ALIAS_Code">
		<Parameters Type="Default">
			<OUT_PARAM_NAME>IE_ALIAS_CODE</OUT_PARAM_NAME>
		</Parameters>
		<ReturnParameters>
			<#OUT_PARAM_NAME#>(&gt;B:#IE_PRESET_ID#_#ALIAS_ID#)</#OUT_PARAM_NAME#>
		</ReturnParameters>
	</ParametersFn>
	<!-- IN: VALUE -->
	<!-- IN: IE_PRESET_ID -->
	<!-- IN (opt): OUT_PARAM_NAME -->
	<ParametersFn Name="ASOBO_Get_IE_VALUE_Check">
		<Parameters Type="Default">
			<OUT_PARAM_NAME>IE_ALIAS_CHECK</OUT_PARAM_NAME>
		</Parameters>
		<ReturnParameters>
			<#OUT_PARAM_NAME#>(B:#IE_PRESET_ID#) #VALUE# ==</#OUT_PARAM_NAME#>
		</ReturnParameters>
	</ParametersFn>
	<!-- IN: TOOLTIP_ID -->
	<!-- IN: IE_PRESET_ID -->
	<!-- IN (opt): OUT_PARAM_NAME -->
	<ParametersFn Name="ASOBO_Get_TOOLTIP_ID_Parameters">
		<Parameters Type="Default">
			<OUT_PARAM_NAME>TOOLTIP_ID_FORMATTED</OUT_PARAM_NAME>
		</Parameters>
		<ReturnParameters>
			<#OUT_PARAM_NAME#>(R:1:#TOOLTIP_ID#)</#OUT_PARAM_NAME#>
		</ReturnParameters>
	</ParametersFn>
	<!-- Process ON_INCREASE & ON_DECREASE invertion -->
	<ParametersFn Name="ASOBO_Process_Inc_Dec_Invertion">
		<Parameters Type="Default">
			<ON_INCREASE/>
			<ON_DECREASE/>
		</Parameters>
		<ReturnParameters>
			<Condition Valid="MR_INVERT_INC_DEC_EVENTS">
				<True>
					<ON_INCREASE>#ON_DECREASE#</ON_INCREASE>
					<ON_DECREASE>#ON_INCREASE#</ON_DECREASE>					
				</True>
				<False>
					<ON_INCREASE>#ON_INCREASE#</ON_INCREASE>
					<ON_DECREASE>#ON_DECREASE#</ON_DECREASE>
				</False>
			</Condition>
		</ReturnParameters>
	</ParametersFn>
</ModelBehaviors>