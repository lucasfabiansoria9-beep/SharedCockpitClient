<ModelBehaviors>
	<!-- ################################# INTERACTION ################################ -->
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_IE_Directional_Variables_Parameters">
		<Parameters Type="Default">
			<VAR_AXIS_POSITION_TYPE>O</VAR_AXIS_POSITION_TYPE>
			<VAR_AXIS_POSITION_X_NAME>#IE_PRESET_ID#_Axis_X_Position</VAR_AXIS_POSITION_X_NAME>
			<VAR_AXIS_POSITION_Y_NAME>#IE_PRESET_ID#_Axis_Y_Position</VAR_AXIS_POSITION_Y_NAME>
		</Parameters>
		<Parameters Type="Default">
			<GET_X_AXIS_POSITION>(#VAR_AXIS_POSITION_TYPE#:#VAR_AXIS_POSITION_X_NAME#)</GET_X_AXIS_POSITION>
			<SET_X_AXIS_POSITION>(&gt;#VAR_AXIS_POSITION_TYPE#:#VAR_AXIS_POSITION_X_NAME#)</SET_X_AXIS_POSITION>
			<GET_Y_AXIS_POSITION>(#VAR_AXIS_POSITION_TYPE#:#VAR_AXIS_POSITION_Y_NAME#)</GET_Y_AXIS_POSITION>
			<SET_Y_AXIS_POSITION>(&gt;#VAR_AXIS_POSITION_TYPE#:#VAR_AXIS_POSITION_Y_NAME#)</SET_Y_AXIS_POSITION>
		</Parameters>
		<ReturnParameters>
			<VAR_AXIS_POSITION_TYPE>#VAR_AXIS_POSITION_TYPE#</VAR_AXIS_POSITION_TYPE>
			<VAR_AXIS_POSITION_X_NAME>#VAR_AXIS_POSITION_X_NAME#</VAR_AXIS_POSITION_X_NAME>
			<VAR_AXIS_POSITION_Y_NAME>#VAR_AXIS_POSITION_Y_NAME#</VAR_AXIS_POSITION_Y_NAME>
			<GET_X_AXIS_POSITION>#GET_X_AXIS_POSITION#</GET_X_AXIS_POSITION>
			<SET_X_AXIS_POSITION>#SET_X_AXIS_POSITION#</SET_X_AXIS_POSITION>
			<GET_Y_AXIS_POSITION>#GET_Y_AXIS_POSITION#</GET_Y_AXIS_POSITION>
			<SET_Y_AXIS_POSITION>#SET_Y_AXIS_POSITION#</SET_Y_AXIS_POSITION>
		</ReturnParameters>
	</ParametersFn>
	<ParametersFn Name="ASOBO_Setup_Directional_Parameters">
		<Parameters Type="Default">
			<UseParametersFn Name="ASOBO_IE_Directional_Variables_Parameters"/>
			<IE_SET_ARG_COUNT>2</IE_SET_ARG_COUNT>
			<IE_INC_ARG_COUNT>2</IE_INC_ARG_COUNT>
			<IE_DEC_ARG_COUNT>2</IE_DEC_ARG_COUNT>
			<X_POSITION_TO_ANIM_CODE>1 + 50 *</X_POSITION_TO_ANIM_CODE>
			<Y_POSITION_TO_ANIM_CODE>1 + 50 *</Y_POSITION_TO_ANIM_CODE>
			<CALLBACK_DRAG_SCALAR>0.01</CALLBACK_DRAG_SCALAR>
			<AUTO_CREATE_SOUND_EVENTS>True</AUTO_CREATE_SOUND_EVENTS>
		</Parameters>
		<Parameters Type="Default">
			<SaveParameters ID="X_Axis_Animation_Parameters" Append="Default">
				<ANIM_NAME>#ANIM_NAME_X_AXIS#</ANIM_NAME>
				<ANIM_CODE>#GET_X_AXIS_POSITION# #X_POSITION_TO_ANIM_CODE#</ANIM_CODE>
			</SaveParameters>
			<SaveParameters ID="Y_Axis_Animation_Parameters" Append="Default">
				<ANIM_NAME>#ANIM_NAME_Y_AXIS#</ANIM_NAME>
				<ANIM_CODE>#GET_Y_AXIS_POSITION# #Y_POSITION_TO_ANIM_CODE#</ANIM_CODE>
			</SaveParameters>
		</Parameters>
		<Parameters Type="Override">
			<Switch Param="INTERACTION_VARIANT">
				<Case Value="DRAG_AXIS">
					<UseParametersFn Name="ASOBO_Setup_Directional_Axis_Parameters">
						<UseParametersFn Name="ASOBO_Load_Then_Remove_Parameters_Helper">
							<SAVED_PARAMETERS>PLOAD_CONFIG</SAVED_PARAMETERS>
						</UseParametersFn>
					</UseParametersFn>
				</Case>
				<Case Value="DIRECTIONAL_STATES">
					<UseParametersFn Name="ASOBO_Setup_Directional_States_Parameters">
						<UseParametersFn Name="ASOBO_Load_Then_Remove_Parameters_Helper">
							<SAVED_PARAMETERS>PLOAD_CONFIG</SAVED_PARAMETERS>
						</UseParametersFn>
					</UseParametersFn>
				</Case>
			</Switch>
		</Parameters>
		<Condition Valid="AUTO_CREATE_SOUND_EVENTS">
			<Parameters Type="Default">
				<SOUND_EVENT_COUNT>2</SOUND_EVENT_COUNT>
				<Condition NotEmpty="SOUND_EVENT_TYPE_ID">
					<True>
						<SOUND_EVENT_NAME>TYPE_#SOUND_EVENT_TYPE_ID#</SOUND_EVENT_NAME>
					</True>
					<False>
						<SOUND_EVENT_NAME>#IE_PRESET_ID#</SOUND_EVENT_NAME>
					</False>
				</Condition>
				<SOUND_EVENT_INTERACTION_TYPE>#INTERACTION_TYPE#</SOUND_EVENT_INTERACTION_TYPE>
			</Parameters>
			<Condition NotEmpty="SOUND_EVENT_SUFFIX">
				<Parameters Type="Override">
					<SOUND_EVENT_NAME>#SOUND_EVENT_NAME#_#SOUND_EVENT_SUFFIX#</SOUND_EVENT_NAME>
				</Parameters>
			</Condition>
			<Parameters Type="Default">
				<FULL_SOUND_EVENT_NAME>#SOUND_EVENT_INTERACTION_TYPE#_#SOUND_EVENT_NAME#</FULL_SOUND_EVENT_NAME>
			</Parameters>
			<Condition NotEmpty="SOUND_EVENT_GROUP">
				<Parameters Type="Override">
					<FULL_SOUND_EVENT_NAME>#SOUND_EVENT_GROUP#_#FULL_SOUND_EVENT_NAME#</FULL_SOUND_EVENT_NAME>
				</Parameters>
			</Condition>
		</Condition>
		<Parameters Type="Override">
			<SaveParameters ID="#IE_PARAMS#" Append="Default">
				<Condition Valid="AUTO_CREATE_SOUND_EVENTS">
					<UseParametersFn Name="ASOBO_Add_Sound_Event_Helper">
						<WWISE_EVENT_DIRECTION>Both</WWISE_EVENT_DIRECTION>
						<WWISE_EVENT_NAME>#FULL_SOUND_EVENT_NAME#</WWISE_EVENT_NAME>
						<WWISE_EVENT_COUNT>#SOUND_EVENT_COUNT#</WWISE_EVENT_COUNT>
					</UseParametersFn>
					<UseParametersFn Name="ASOBO_Add_Sound_Event_Helper">
						<WWISE_EVENT_DIRECTION>Forward</WWISE_EVENT_DIRECTION>
						<WWISE_EVENT_NAME>#FULL_SOUND_EVENT_NAME#_Forward</WWISE_EVENT_NAME>
						<WWISE_EVENT_COUNT>#SOUND_EVENT_COUNT#</WWISE_EVENT_COUNT>
					</UseParametersFn>
					<UseParametersFn Name="ASOBO_Add_Sound_Event_Helper">
						<WWISE_EVENT_DIRECTION>Backward</WWISE_EVENT_DIRECTION>
						<WWISE_EVENT_NAME>#FULL_SOUND_EVENT_NAME#_Backward</WWISE_EVENT_NAME>
						<WWISE_EVENT_COUNT>#SOUND_EVENT_COUNT#</WWISE_EVENT_COUNT>
					</UseParametersFn>
				</Condition>
				<UseParametersFn Name="ASOBO_Add_Animation_Helper">
					<LoadParameters ID="X_Axis_Animation_Parameters"/>
					<APPEND_WITH_SAVED_ANIMATION>X_Axis_Animation_Parameters</APPEND_WITH_SAVED_ANIMATION>
				</UseParametersFn>
				<Condition Valid="AUTO_CREATE_SOUND_EVENTS">
					<UseParametersFn Name="ASOBO_Add_Sound_Event_Helper">
						<WWISE_EVENT_DIRECTION>Both</WWISE_EVENT_DIRECTION>
						<WWISE_EVENT_NAME>#FULL_SOUND_EVENT_NAME#</WWISE_EVENT_NAME>
						<WWISE_EVENT_COUNT>#SOUND_EVENT_COUNT#</WWISE_EVENT_COUNT>
					</UseParametersFn>
					<UseParametersFn Name="ASOBO_Add_Sound_Event_Helper">
						<WWISE_EVENT_DIRECTION>Forward</WWISE_EVENT_DIRECTION>
						<WWISE_EVENT_NAME>#FULL_SOUND_EVENT_NAME#_Forward</WWISE_EVENT_NAME>
						<WWISE_EVENT_COUNT>#SOUND_EVENT_COUNT#</WWISE_EVENT_COUNT>
					</UseParametersFn>
					<UseParametersFn Name="ASOBO_Add_Sound_Event_Helper">
						<WWISE_EVENT_DIRECTION>Backward</WWISE_EVENT_DIRECTION>
						<WWISE_EVENT_NAME>#FULL_SOUND_EVENT_NAME#_Backward</WWISE_EVENT_NAME>
						<WWISE_EVENT_COUNT>#SOUND_EVENT_COUNT#</WWISE_EVENT_COUNT>
					</UseParametersFn>
				</Condition>
				<UseParametersFn Name="ASOBO_Add_Animation_Helper">
					<LoadParameters ID="Y_Axis_Animation_Parameters"/>
					<APPEND_WITH_SAVED_ANIMATION>Y_Axis_Animation_Parameters</APPEND_WITH_SAVED_ANIMATION>
				</UseParametersFn>
				<IE_SET_ARG_COUNT>#IE_SET_ARG_COUNT#</IE_SET_ARG_COUNT>
				<IE_INC_ARG_COUNT>#IE_INC_ARG_COUNT#</IE_INC_ARG_COUNT>
				<IE_DEC_ARG_COUNT>#IE_DEC_ARG_COUNT#</IE_DEC_ARG_COUNT>
				<INC_PARAM_0>p0</INC_PARAM_0>
				<INC_PARAM_0_TYPE>Float</INC_PARAM_0_TYPE>
				<INC_PARAM_0_IS_DYNAMIC>True</INC_PARAM_0_IS_DYNAMIC>
				<DEC_PARAM_0>p0</DEC_PARAM_0>
				<DEC_PARAM_0_TYPE>Float</DEC_PARAM_0_TYPE>
				<DEC_PARAM_0_IS_DYNAMIC>True</DEC_PARAM_0_IS_DYNAMIC>
				<SET_PARAM_0>p0</SET_PARAM_0>
				<SET_PARAM_0_TYPE>Float</SET_PARAM_0_TYPE>
				<SET_PARAM_0_IS_DYNAMIC>True</SET_PARAM_0_IS_DYNAMIC>
				<INC_PARAM_1>p1</INC_PARAM_1>
				<INC_PARAM_1_TYPE>Float</INC_PARAM_1_TYPE>
				<INC_PARAM_1_IS_DYNAMIC>True</INC_PARAM_1_IS_DYNAMIC>
				<DEC_PARAM_1>p1</DEC_PARAM_1>
				<DEC_PARAM_1_TYPE>Float</DEC_PARAM_1_TYPE>
				<DEC_PARAM_1_IS_DYNAMIC>True</DEC_PARAM_1_IS_DYNAMIC>
				<SET_PARAM_1>p1</SET_PARAM_1>
				<SET_PARAM_1_TYPE>Float</SET_PARAM_1_TYPE>
				<SET_PARAM_1_IS_DYNAMIC>True</SET_PARAM_1_IS_DYNAMIC>
			</SaveParameters>
		</Parameters>
		<ReturnParameters>
			<!-- Watch positions -->
			<UseParametersFn Name="ASOBO_Watch_LocalVar_Helper">
				<VAR_SCOPE>#VAR_AXIS_POSITION_TYPE#</VAR_SCOPE>
				<LOCAL_VAR>#VAR_AXIS_POSITION_X_NAME#</LOCAL_VAR>
			</UseParametersFn>
			<UseParametersFn Name="ASOBO_Watch_LocalVar_Helper">
				<VAR_SCOPE>#VAR_AXIS_POSITION_TYPE#</VAR_SCOPE>
				<LOCAL_VAR>#VAR_AXIS_POSITION_Y_NAME#</LOCAL_VAR>
			</UseParametersFn>
			<!-- Bind axis independant SET -->
			<UseParametersFn Name="ASOBO_Add_Set_Binding_Helper">
				<ALIAS_ID>X_Set</ALIAS_ID>
				<PARAM_0>p0</PARAM_0>
				<PARAM_0_IS_DYNAMIC>True</PARAM_0_IS_DYNAMIC>
				<PARAM_1>#GET_Y_AXIS_POSITION#</PARAM_1>
				<PARAM_1_IS_DYNAMIC>True</PARAM_1_IS_DYNAMIC>
			</UseParametersFn>
			<UseParametersFn Name="ASOBO_Add_Set_Binding_Helper">
				<ALIAS_ID>Y_Set</ALIAS_ID>
				<PARAM_0>#GET_X_AXIS_POSITION#</PARAM_0>
				<PARAM_0_IS_DYNAMIC>True</PARAM_0_IS_DYNAMIC>
				<PARAM_1>p0</PARAM_1>
				<PARAM_1_IS_DYNAMIC>True</PARAM_1_IS_DYNAMIC>
			</UseParametersFn>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Setup_Directional_States_Parameters">
		<Parameters Type="Default">
			<POSITION_TO_DEGREES>#GET_Y_AXIS_POSITION# #GET_X_AXIS_POSITION# atg2 pi / 180 *</POSITION_TO_DEGREES>
			<THRESHOLD_FOR_NEUTRAL>0.5</THRESHOLD_FOR_NEUTRAL>
			<POS_NEUTRAL_CODE/>
			<POS_L_CODE/>
			<POS_R_CODE/>
			<POS_DN_CODE/>
			<POS_UP_CODE/>
		</Parameters>
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Next_Safe_Jump_Flag_Helper">
				<OUT_PARAM_NAME>FL_AFTER_SET</OUT_PARAM_NAME>
			</UseParametersFn>
		</Parameters>
		<Parameters Type="Override">
			<Condition NotEmpty="POS_NEUTRAL_CHECK">
				<POS_NEUTRAL_CODE>#POS_NEUTRAL_CHECK# ! if{ #POS_NEUTRAL_CODE# }</POS_NEUTRAL_CODE>
			</Condition>
			<Condition NotEmpty="POS_L_CHECK">
				<POS_L_CODE>#POS_L_CHECK# ! if{ #POS_L_CODE# }</POS_L_CODE>
			</Condition>
			<Condition NotEmpty="POS_R_CHECK">
				<POS_R_CODE>#POS_R_CHECK# ! if{ #POS_R_CODE# }</POS_R_CODE>
			</Condition>
			<Condition NotEmpty="POS_DN_CHECK">
				<POS_DN_CODE>#POS_DN_CHECK# ! if{ #POS_DN_CODE# }</POS_DN_CODE>
			</Condition>
			<Condition NotEmpty="POS_UP_CHECK">
				<POS_UP_CODE>#POS_UP_CHECK# ! if{ #POS_UP_CODE# }</POS_UP_CODE>
			</Condition>
		</Parameters>
		<Parameters Type="Default">
			<ON_AFTER_POSITION_SET>
				#GET_Y_AXIS_POSITION# abs #THRESHOLD_FOR_NEUTRAL# &lt; #GET_X_AXIS_POSITION# abs #THRESHOLD_FOR_NEUTRAL# &lt; and if{ #POS_NEUTRAL_CODE# g#FL_AFTER_SET# }
				#POSITION_TO_DEGREES# sp0
				l0 0 &gt; sp1 (* top of the circle *)
				l0 abs 90 &gt; sp2 (* left of the circle *)
				45 135 l0 abs rng sp3 (* in a vertical 90&deg; quadrant *)
				l3 if{
					l1 if{
						(* UP *) #POS_UP_CODE#
					} els{
						(* DN *) #POS_DN_CODE#
					}
				} els{
					l2 if{
						(* L *) #POS_L_CODE#
					} els{
						(* R *) #POS_R_CODE#
					}
				} :#FL_AFTER_SET#
			</ON_AFTER_POSITION_SET>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Setup_Directional_Axis_Parameters"/>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Setup_Directional_Axis_Parameters">
		<Parameters Type="Default">
			<Condition Check="IE_TOOLTIP_VALUE">
				<False>
					<IE_TOOLTIP_VALUE_ID>@TT_Package.STATE.NA</IE_TOOLTIP_VALUE_ID>
				</False>
			</Condition>
			<POSITION_TO_DEGREES>#GET_Y_AXIS_POSITION# #GET_X_AXIS_POSITION# atg2 pi / 180 *</POSITION_TO_DEGREES>
			<IE_TOOLTIP_ICON>MOVE_AXIS</IE_TOOLTIP_ICON>
			<AXIS>BOTH</AXIS>
			<FORWARD_INC_DEC_TO_IE>True</FORWARD_INC_DEC_TO_IE>
			<MR_TO_IE_SET_VALUE/>
			<MR_ABSOLUTE_RANGE>0.015</MR_ABSOLUTE_RANGE>
			<VAR_IS_INTERACTING>O:_Interaction_Is_Locked</VAR_IS_INTERACTING>
			<RESET_POSITION_ON_EXIT>True</RESET_POSITION_ON_EXIT>
		</Parameters>
		<Parameters Type="Default">
			<VALUE_CODE>#POSITION_TO_DEGREES#</VALUE_CODE>
			<VALUE_UNITS>degrees</VALUE_UNITS>
		</Parameters>
		<Parameters Type="Default">
			<Condition Valid="RESET_POSITION_ON_EXIT">
				<RESET_POSITION>0 0 (&gt;B:#IE_PRESET_ID#_Set)</RESET_POSITION>
			</Condition>
			<CLAMP_POSITION>-1 1 @Clamp</CLAMP_POSITION>
			<NORMALIZE_MR_POSITION>-#MR_ABSOLUTE_RANGE# #MR_ABSOLUTE_RANGE# -1 1 @MapRange</NORMALIZE_MR_POSITION>
		</Parameters>
		<Parameters Type="Default">
			<Condition Valid="RESET_POSITION_ON_EXIT">
				<CHECK_AND_RESET_POSITION>(#VAR_IS_INTERACTING#) @MR_IsUsingRelativePos and if{ @MR_Abs_Y abs 0 == @MR_Abs_X abs 0 == and if{ #RESET_POSITION# } }</CHECK_AND_RESET_POSITION>
				<RESET_FREQUENCY>16</RESET_FREQUENCY>				<!-- only affects gamepads -->
			</Condition>
			<GET_RELATIVE_POS>@MR_Abs_Y @MR_Abs_X</GET_RELATIVE_POS>
			<GET_ABSOLUTE_POS>@MR_Rel_Y #NORMALIZE_MR_POSITION# @MR_Rel_X #NORMALIZE_MR_POSITION#</GET_ABSOLUTE_POS>
		</Parameters>
		<Parameters Type="Default">
			<ON_ENTER_LOCK>1 (&gt;#VAR_IS_INTERACTING#)</ON_ENTER_LOCK>
			<ON_EXIT_LOCK>0 (&gt;#VAR_IS_INTERACTING#)</ON_EXIT_LOCK>
			<ON_PRIMARY_DOWN>(#VAR_IS_INTERACTING#) if{ @MR_IsUsingRelativePos if{ #GET_RELATIVE_POS# } els{ #GET_ABSOLUTE_POS# } #MR_TO_IE_SET_VALUE# (&gt;B:#IE_PRESET_ID#_Set) }</ON_PRIMARY_DOWN>
		</Parameters>
		<Parameters Type="Override">
			<Condition Valid="RESET_POSITION_ON_EXIT">
				<ON_EXIT_LOCK>#ON_EXIT_LOCK# #RESET_POSITION#</ON_EXIT_LOCK>
			</Condition>
		</Parameters>
		<Parameters Type="Default">
			<IE_SET_STATE>p0 #CLAMP_POSITION# #SET_X_AXIS_POSITION# p1 #CLAMP_POSITION# #SET_Y_AXIS_POSITION#</IE_SET_STATE>
			<IE_INC_STATE>p0 #GET_X_AXIS_POSITION# +  p1 #GET_Y_AXIS_POSITION# + (&gt;B:#IE_PRESET_ID#_Set)</IE_INC_STATE>
			<IE_DEC_STATE>#GET_X_AXIS_POSITION# p0 -  #GET_Y_AXIS_POSITION# p1 - (&gt;B:#IE_PRESET_ID#_Set)</IE_DEC_STATE>
		</Parameters>
		<Parameters Type="Default">
			<SET_CODE>#IE_SET_STATE#</SET_CODE>
			<INC_CODE>#IE_INC_STATE#</INC_CODE>
			<DEC_CODE>#IE_DEC_STATE#</DEC_CODE>
		</Parameters>
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Append_Set_Code_Helper"/>
			<UseParametersFn Name="ASOBO_Append_Inc_Code_Helper"/>
			<UseParametersFn Name="ASOBO_Append_Dec_Code_Helper"/>
			<Condition Check="ON_BEFORE_POSITION_SET">
				<UseParametersFn Name="ASOBO_Append_Set_Code_Helper">
					<SET_CODE>#ON_BEFORE_POSITION_SET#</SET_CODE>
					<APPEND_BEFORE>True</APPEND_BEFORE>
				</UseParametersFn>
			</Condition>
			<Condition Check="ON_AFTER_POSITION_SET">
				<UseParametersFn Name="ASOBO_Append_Set_Code_Helper">
					<SET_CODE>#ON_AFTER_POSITION_SET#</SET_CODE>
				</UseParametersFn>
			</Condition>
			<Condition Valid="RESET_POSITION_ON_EXIT">
				<UseParametersFn Name="ASOBO_Add_Update_Helper">
					<UPDATE_CODE>#CHECK_AND_RESET_POSITION#</UPDATE_CODE>
					<UPDATE_FREQUENCY>#RESET_FREQUENCY#</UPDATE_FREQUENCY>
				</UseParametersFn>
			</Condition>
		</Parameters>
		<Parameters Type="Override">
			<SaveParameters ID="Temp_IE_Save">
				<UseParametersFn Name="ASOBO_Append_Value_Code_Helper"/>
				<UseParametersFn Name="ASOBO_Append_To_Mouserect_Events_Helper">
					<EV_PARAM>ON_PRIMARY_DOWN</EV_PARAM>
				</UseParametersFn>
				<UseParametersFn Name="ASOBO_Append_To_Mouserect_Events_Helper">
					<EV_PARAM>ON_ENTER_LOCK</EV_PARAM>
				</UseParametersFn>
				<UseParametersFn Name="ASOBO_Append_To_Mouserect_Events_Helper">
					<EV_PARAM>ON_EXIT_LOCK</EV_PARAM>
				</UseParametersFn>
				<Condition Valid="FORWARD_INC_DEC_TO_IE">
					<UseParametersFn Name="ASOBO_Append_To_Mouserect_Events_Helper">
						<EV_PARAM>ON_INCREASE</EV_PARAM>
					</UseParametersFn>
					<UseParametersFn Name="ASOBO_Append_To_Mouserect_Events_Helper">
						<EV_PARAM>ON_DECREASE</EV_PARAM>
					</UseParametersFn>
				</Condition>
				<SaveParameters ID="Mouserect_Parameters" Append="Default">
					<CURSOR>Grab</CURSOR>
					<CALLBACK_DRAG_SCALAR>#CALLBACK_DRAG_SCALAR#</CALLBACK_DRAG_SCALAR>
					<CALLBACK_DRAG_AXIS>#AXIS#</CALLBACK_DRAG_AXIS>
				</SaveParameters>
				<IE_TOOLTIP_ICON>#IE_TOOLTIP_ICON#</IE_TOOLTIP_ICON>
				<UseParametersFn Name="ASOBO_Format_Tooltip_Value_Helper"/>
			</SaveParameters>
		</Parameters>
		<Parameters Type="Override">
			<SaveParameters ID="#IE_PARAMS#" Append="Default">
				<LoadParameters ID="Temp_IE_Save"/>
			</SaveParameters>
		</Parameters>
	</ParametersFn>
	<!-- ############################################################################## -->
</ModelBehaviors>