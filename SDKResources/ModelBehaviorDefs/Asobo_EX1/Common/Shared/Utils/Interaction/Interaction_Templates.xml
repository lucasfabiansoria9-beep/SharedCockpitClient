			<Condition CheckSavedParameters="Global_Default_Joystick_Interaction_Config">
				<LoadParameters ID="Global_Default_Joystick_Interaction_Config"/>
			</Condition>
			<GET_COMMON_PARAMETERS_FN>ASOBO_Get_Joystick_Common_Parameters_Helper</GET_COMMON_PARAMETERS_FN>
		</Parameters>
		<Parameters Type="Default">
			<UseParametersFn Name="ASOBO_Get_Common_Parameters_Helper"/>
			<INTERACTION_TYPE>Joystick</INTERACTION_TYPE>
		</Parameters>
		<UseTemplate Name="ASOBO_CT_Interaction_Template"/>
	</Template>
    <!-- ############################################################################## -->
</ModelBehaviors>