	<!-- ############################################################################## -->
	<!-- @DOC TEMPLATE ASOBO_CT_Wheel_Template
		@Brief Create a wheel interaction
		@Param WHEEL_INTERACTION_TEMPLATE Template used to create the interaction, usually pre-set in Global_Default_Wheel_Interaction_Config
	-->
	<Template Name="ASOBO_CT_Wheel_Template">
		<TemplateAlias>ASOBO_IT_Wheel_Template</TemplateAlias>
