<ModelBehaviors>
	<!-- ################################## HANDLING ################################## -->
	<!-- ############################################################################## -->
	<InputEvent ID="HANDLING">
		<TT_Category_Main>TT:INPUT.CAT_1_CONTROL_SURFACES</TT_Category_Main>
		<Switch Param="TT_HANDLING_CATEGORY">
			<Case Value="CONTROL_SURFACES_PRIMARY">
				<TT_Category_Sub>TT:INPUT.CAT_2_CONTROL_SURFACES_PRIMARY</TT_Category_Sub>
			</Case>
			<Case Value="CONTROL_SURFACES_SECONDARY">
				<TT_Category_Sub>TT:INPUT.CAT_2_CONTROL_SURFACES_SECONDARY</TT_Category_Sub>
			</Case>
			<Case Value="CONTROL_SURFACES_TRIM">
				<TT_Category_Sub>TT:INPUT.CAT_2_CONTROL_SURFACES_TRIM</TT_Category_Sub>
			</Case>
		</Switch>
		<Presets>
			<Extend Target="#IE_BASE_TO_EXTEND_FROM#">
				<Parameters Type="Override">
					<UseParametersFn Name="ASOBO_PFN_Call_Overridable_ParametersFn_No_Default_Helper">
						<PFN_PARAM_NAME>HANDLING_IE_PARAMETERS_FN</PFN_PARAM_NAME>
					</UseParametersFn>
				</Parameters>
			</Extend>
		</Presets>
	</InputEvent>
</ModelBehaviors>