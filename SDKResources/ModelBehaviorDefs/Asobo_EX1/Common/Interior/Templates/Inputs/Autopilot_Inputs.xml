<ModelBehaviors>
	<!-- ################################## AUTOPILOT ################################# -->
	<!-- ############################################################################## -->
	<InputEvent ID="AUTOPILOT">
		<TT_Category_Main>TT:INPUT.CAT_1_AP</TT_Category_Main>
		<Condition NotEmpty="TT_CATEGORY_SUB">
			<TT_Category_Sub>TT:#TT_CATEGORY_SUB#</TT_Category_Sub>
		</Condition>
		<Presets>
			<Extend Target="#IE_BASE_TO_EXTEND_FROM#">
				<Parameters Type="Override">
					<UseParametersFn Name="ASOBO_PFN_Call_Overridable_ParametersFn_No_Default_Helper">
						<PFN_PARAM_NAME>AUTOPILOT_IE_PARAMETERS_FN</PFN_PARAM_NAME>
					</UseParametersFn>
				</Parameters>
			</Extend>
		</Presets>
	</InputEvent>
</ModelBehaviors>