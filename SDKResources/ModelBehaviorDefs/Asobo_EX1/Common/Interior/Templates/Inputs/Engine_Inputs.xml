<ModelBehaviors>
	<!-- ################################## ENGINE #################################### -->
	<!-- ############################################################################## -->
	<InputEvent ID="ENGINE">
		<TT_Category_Main>TT:INPUT.CAT_1_POWER_MANAGEMENT</TT_Category_Main>
		<Switch Param="TT_ENGINE_CATEGORY">
			<Case Value="CONDITION">
				<TT_Category_Sub>TT:INPUT.CAT_2_POWER_MANAGEMENT_CONDITION</TT_Category_Sub>
			</Case>
			<Case Value="MIXTURE">
				<TT_Category_Sub>TT:INPUT.CAT_2_POWER_MANAGEMENT_MIXTURE</TT_Category_Sub>
			</Case>
			<Case Value="PROPELLER">
				<TT_Category_Sub>TT:INPUT.CAT_2_POWER_MANAGEMENT_PROPELLER</TT_Category_Sub>
			</Case>
			<Case Value="THROTTLE">
				<TT_Category_Sub>TT:INPUT.CAT_2_POWER_MANAGEMENT_THROTTLE</TT_Category_Sub>
			</Case>
		</Switch>
		<Presets>
			<Extend Target="#IE_BASE_TO_EXTEND_FROM#">
				<Parameters Type="Override">
					<UseParametersFn Name="ASOBO_PFN_Call_Overridable_ParametersFn_No_Default_Helper">
						<PFN_PARAM_NAME>ENGINE_IE_PARAMETERS_FN</PFN_PARAM_NAME>
					</UseParametersFn>
				</Parameters>
			</Extend>
		</Presets>
	</InputEvent>
	<!-- ############################################################################## -->
	<InputEvent ID="ROTOR">
		<TT_Category_Main>TT:INPUT.CAT_1_POWER_MANAGEMENT</TT_Category_Main>
		<TT_Category_Sub>TT:INPUT.CAT_2_POWER_MANAGEMENT_ROTOR</TT_Category_Sub>
		<Presets>
			<Extend Target="#IE_BASE_TO_EXTEND_FROM#">
				<Parameters Type="Override">
					<UseParametersFn Name="ASOBO_PFN_Call_Overridable_ParametersFn_No_Default_Helper">
						<PFN_PARAM_NAME>ROTOR_IE_PARAMETERS_FN</PFN_PARAM_NAME>
					</UseParametersFn>
				</Parameters>
			</Extend>
		</Presets>
	</InputEvent>
	<!-- ############################################################################## -->
</ModelBehaviors>