<ModelBehaviors>
	<!-- ################################## LIGHTING ################################## -->
	<!-- ############################################################################## -->
	<InputEvent ID="LIGHTING">
		<TT_Category_Main>TT:INPUT.CAT_1_LIGHTS</TT_Category_Main>
		<Switch Param="TT_LIGHT_CATEGORY">
			<Case Value="INTERIOR_LIGHT">
				<TT_Category_Sub>TT:INPUT.CAT_2_LIGHTS_INT</TT_Category_Sub>
			</Case>
			<Case Value="EXTERIOR_LIGHT">
				<TT_Category_Sub>TT:INPUT.CAT_2_LIGHTS_EXT</TT_Category_Sub>
			</Case>
		</Switch>
		<Presets>
			<Extend Target="#IE_BASE_TO_EXTEND_FROM#">
				<Parameters Type="Override">
					<UseParametersFn Name="ASOBO_PFN_Call_Overridable_ParametersFn_No_Default_Helper">
						<PFN_PARAM_NAME>LIGHTING_IE_PARAMETERS_FN</PFN_PARAM_NAME>
					</UseParametersFn>
				</Parameters>
			</Extend>
		</Presets>
	</InputEvent>
</ModelBehaviors>