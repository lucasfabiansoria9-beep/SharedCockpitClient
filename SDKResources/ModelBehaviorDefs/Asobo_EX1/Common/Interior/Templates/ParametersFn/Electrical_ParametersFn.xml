	<!-- Given line id will be set connected, all others wil be disconnected -->
	<ParametersFn Name="ASOBO_One_Line_Active_Junction_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Param_Suffix_Helper" />
			<UseParametersFn Name="ASOBO_Get_Next_Safe_Jump_Flag_Helper">
				<OUT_PARAM_NAME>END_LOOP</OUT_PARAM_NAME>
			</UseParametersFn>
		</Parameters>
		<ReturnParameters>
			<Loop>
				<Setup>
					<Param>LINE_ID</Param>
					<Parameters>
						<LoadParameters ID="Lines_Parameters" />
						<RemoveSavedParameters ID="Lines_Parameters" />
					</Parameters>
				</Setup>
				<Do>
					<Parameters Type="Override" Lifetime="Loop">
						<UseParametersFn Name="ASOBO_Increment_ID_Helper">
							<OUT_PARAM_ID_NAME>LOOP_INDEX</OUT_PARAM_ID_NAME>
						</UseParametersFn>
					</Parameters>
					<Parameters Type="Default" Lifetime="Loop">
						<SET_JUNCTION_STATE>sp0</SET_JUNCTION_STATE>
						<GET_JUNCTION_STATE></GET_JUNCTION_STATE>
					</Parameters>
					<Parameters Type="Override" Lifetime="Loop">
						<UseParametersFn Name="ASOBO_Line_Connection_Parameters">
							<OUT_PARAM_SUFFIX />
							<LINE_ID>'#LINE_ID#'_n</LINE_ID>
						</UseParametersFn>
					</Parameters>
					<Parameters Type="Override" Lifetime="Loop">
						<GET_LINE_STATE>#GET_CONNECTION#</GET_LINE_STATE>
						<TOGGLE_LINE_STATE>#TOGGLE_CONNECTION#</TOGGLE_LINE_STATE>
					</Parameters>
					<Parameters Type="Override" Lifetime="Loop">
						<SET_JUNCTION_STATE>#SET_JUNCTION_STATE# l0 #LOOP_INDEX# == #GET_LINE_STATE# != if{ #TOGGLE_LINE_STATE# }</SET_JUNCTION_STATE>
						<GET_JUNCTION_STATE>#GET_JUNCTION_STATE# #GET_LINE_STATE# if{ #LOOP_INDEX# g#END_LOOP# }</GET_JUNCTION_STATE>
					</Parameters>
				</Do>
				<Then>
					<SET_JUNCTION#OUT_PARAM_SUFFIX#_STATE>#SET_JUNCTION_STATE#</SET_JUNCTION#OUT_PARAM_SUFFIX#_STATE>
					<GET_JUNCTION#OUT_PARAM_SUFFIX#_STATE>#GET_JUNCTION_STATE# :#END_LOOP#</GET_JUNCTION#OUT_PARAM_SUFFIX#_STATE>
				</Then>
			</Loop>
		</ReturnParameters>
	</ParametersFn>
