	<ParametersFn Name="ASOBO_Battery_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Param_Suffix_Helper"/>
		</Parameters>
		<Parameters Type="Default">
			<BATTERY_ID>1</BATTERY_ID>
		</Parameters>
		<Parameters Type="Default">
			<SIMVAR_BATTERY_SWITCH>ELECTRICAL MASTER BATTERY:#BATTERY_ID#</SIMVAR_BATTERY_SWITCH>
			<SIMVAR_BATTERY_VOLTAGE>ELECTRICAL BATTERY VOLTAGE:#BATTERY_ID#</SIMVAR_BATTERY_VOLTAGE>
			<SIMVAR_BATTERY_LOAD>ELECTRICAL BATTERY LOAD:#BATTERY_ID#</SIMVAR_BATTERY_LOAD>
			<UNITS_BATTERY_VOLTAGE>volts</UNITS_BATTERY_VOLTAGE>
			<UNITS_BATTERY_LOAD>amps</UNITS_BATTERY_LOAD>
		</Parameters>
		<Parameters Type="Default">
			<GET_BATTERY_SWITCH>(A:#SIMVAR_BATTERY_SWITCH#, Bool)</GET_BATTERY_SWITCH>
			<GET_BATTERY_VOLTAGE>(A:#SIMVAR_BATTERY_VOLTAGE#, #UNITS_BATTERY_VOLTAGE#)</GET_BATTERY_VOLTAGE>
			<GET_BATTERY_LOAD>(A:#SIMVAR_BATTERY_LOAD#, #UNITS_BATTERY_LOAD#)</GET_BATTERY_LOAD>
		</Parameters>
		<Parameters Type="Default">
			<TOGGLE_BATTERY>#BATTERY_ID# (&gt;K:TOGGLE_MASTER_BATTERY)</TOGGLE_BATTERY>
		</Parameters>
		<Parameters Type="Default">
			<SET_BATTERY>#GET_BATTERY_SWITCH# != if{ #TOGGLE_BATTERY# }</SET_BATTERY>
		</Parameters>
		<ReturnParameters>
			<TOGGLE_BATTERY#OUT_PARAM_SUFFIX#>#TOGGLE_BATTERY#</TOGGLE_BATTERY#OUT_PARAM_SUFFIX#>
			<SET_BATTERY#OUT_PARAM_SUFFIX#>#SET_BATTERY#</SET_BATTERY#OUT_PARAM_SUFFIX#>
			<GET_BATTERY#OUT_PARAM_SUFFIX#_SWITCH>#GET_BATTERY_SWITCH#</GET_BATTERY#OUT_PARAM_SUFFIX#_SWITCH>
			<GET_BATTERY#OUT_PARAM_SUFFIX#_VOLTAGE>#GET_BATTERY_VOLTAGE#</GET_BATTERY#OUT_PARAM_SUFFIX#_VOLTAGE>
			<GET_BATTERY#OUT_PARAM_SUFFIX#_LOAD>#GET_BATTERY_LOAD#</GET_BATTERY#OUT_PARAM_SUFFIX#_LOAD>
			<SIMVAR_BATTERY#OUT_PARAM_SUFFIX#_SWITCH>#SIMVAR_BATTERY_SWITCH#</SIMVAR_BATTERY#OUT_PARAM_SUFFIX#_SWITCH>
			<SIMVAR_BATTERY#OUT_PARAM_SUFFIX#_VOLTAGE>#SIMVAR_BATTERY_VOLTAGE#</SIMVAR_BATTERY#OUT_PARAM_SUFFIX#_VOLTAGE>
			<SIMVAR_BATTERY#OUT_PARAM_SUFFIX#_LOAD>#SIMVAR_BATTERY_LOAD#</SIMVAR_BATTERY#OUT_PARAM_SUFFIX#_LOAD>
