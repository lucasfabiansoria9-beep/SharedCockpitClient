	<ParametersFn Name="ASOBO_Alternator_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Param_Suffix_Helper"/>
		</Parameters>
		<Parameters Type="Default">
			<ALTERNATOR_ID>1</ALTERNATOR_ID>
		</Parameters>
		<Parameters Type="Default">
			<Switch Param="ELECTRICAL_SYSTEM">
				<Case Value="EX1">
					<SIMVAR_ALTERNATOR_SWITCH>ELECTRICAL GENERATOR SWITCH:#ALTERNATOR_ID#</SIMVAR_ALTERNATOR_SWITCH>
					<SIMVAR_ALTERNATOR_ACTIVE>ELECTRICAL GENERATOR ACTIVE:#ALTERNATOR_ID#</SIMVAR_ALTERNATOR_ACTIVE>
					<SIMVAR_ALTERNATOR_VOLTAGE>ELECTRICAL GENERATOR VOLTAGE:#ALTERNATOR_ID#</SIMVAR_ALTERNATOR_VOLTAGE>
					<SIMVAR_ALTERNATOR_LOAD>ELECTRICAL GENERATOR AMPS:#ALTERNATOR_ID#</SIMVAR_ALTERNATOR_LOAD>
				</Case>
				<Default>
					<SIMVAR_ALTERNATOR_SWITCH>GENERAL ENG MASTER ALTERNATOR:#ALTERNATOR_ID#</SIMVAR_ALTERNATOR_SWITCH>
					<SIMVAR_ALTERNATOR_VOLTAGE>ELECTRICAL GENALT BUS VOLTAGE:#ALTERNATOR_ID#</SIMVAR_ALTERNATOR_VOLTAGE>
					<SIMVAR_ALTERNATOR_LOAD>ELECTRICAL GENALT BUS AMPS:#ALTERNATOR_ID#</SIMVAR_ALTERNATOR_LOAD>
					<SIMVAR_ALTERNATOR_LOAD_RATIO>ELECTRICAL GENALT LOAD:#ALTERNATOR_ID#</SIMVAR_ALTERNATOR_LOAD_RATIO>
				</Default>
			</Switch>
			<UNITS_ALTERNATOR_VOLTAGE>volts</UNITS_ALTERNATOR_VOLTAGE>
			<UNITS_ALTERNATOR_LOAD>amps</UNITS_ALTERNATOR_LOAD>
			<UNITS_ALTERNATOR_LOAD_RATIO>percent</UNITS_ALTERNATOR_LOAD_RATIO>
		</Parameters>
		<Parameters Type="Default">
			<GET_ALTERNATOR_SWITCH>(A:#SIMVAR_ALTERNATOR_SWITCH#, Bool)</GET_ALTERNATOR_SWITCH>
			<GET_ALTERNATOR_VOLTAGE>(A:#SIMVAR_ALTERNATOR_VOLTAGE#, #UNITS_ALTERNATOR_VOLTAGE#)</GET_ALTERNATOR_VOLTAGE>
			<GET_ALTERNATOR_LOAD>(A:#SIMVAR_ALTERNATOR_LOAD#, #UNITS_ALTERNATOR_LOAD#)</GET_ALTERNATOR_LOAD>
			<Switch Param="ELECTRICAL_SYSTEM">
				<Case Value="EX1">
					<GET_ALTERNATOR_ACTIVE>(A:#SIMVAR_ALTERNATOR_ACTIVE#, Bool)</GET_ALTERNATOR_ACTIVE>
				</Case>
				<Default>
					<GET_ALTERNATOR_LOAD_RATIO>(A:#SIMVAR_ALTERNATOR_LOAD_RATIO#, #UNITS_ALTERNATOR_LOAD_RATIO#)</GET_ALTERNATOR_LOAD_RATIO>
				</Default>
			</Switch>
		</Parameters>
		<Parameters Type="Default">
			<SET_ALTERNATOR>#GET_ALTERNATOR_SWITCH# != if{ #ALTERNATOR_ID# (&gt;K:TOGGLE_MASTER_ALTERNATOR) }</SET_ALTERNATOR>
			<TOGGLE_ALTERNATOR>#ALTERNATOR_ID# (&gt;K:TOGGLE_MASTER_ALTERNATOR)</TOGGLE_ALTERNATOR>
		</Parameters>
		<ReturnParameters>
			<TOGGLE_ALTERNATOR#OUT_PARAM_SUFFIX#>#TOGGLE_ALTERNATOR#</TOGGLE_ALTERNATOR#OUT_PARAM_SUFFIX#>
			<SET_ALTERNATOR#OUT_PARAM_SUFFIX#>#SET_ALTERNATOR#</SET_ALTERNATOR#OUT_PARAM_SUFFIX#>
			<GET_ALTERNATOR#OUT_PARAM_SUFFIX#_SWITCH>#GET_ALTERNATOR_SWITCH#</GET_ALTERNATOR#OUT_PARAM_SUFFIX#_SWITCH>
			<GET_ALTERNATOR#OUT_PARAM_SUFFIX#_VOLTAGE>#GET_ALTERNATOR_VOLTAGE#</GET_ALTERNATOR#OUT_PARAM_SUFFIX#_VOLTAGE>
			<GET_ALTERNATOR#OUT_PARAM_SUFFIX#_LOAD>#GET_ALTERNATOR_LOAD#</GET_ALTERNATOR#OUT_PARAM_SUFFIX#_LOAD>
			<Switch Param="ELECTRICAL_SYSTEM">
				<Case Value="EX1">
					<GET_ALTERNATOR#OUT_PARAM_SUFFIX#_ACTIVE>#GET_ALTERNATOR_ACTIVE#</GET_ALTERNATOR#OUT_PARAM_SUFFIX#_ACTIVE>
				</Case>
				<Default>
					<GET_ALTERNATOR#OUT_PARAM_SUFFIX#_LOAD_RATIO>#GET_ALTERNATOR_LOAD_RATIO#</GET_ALTERNATOR#OUT_PARAM_SUFFIX#_LOAD_RATIO>
				</Default>
			</Switch>
			<SIMVAR_ALTERNATOR#OUT_PARAM_SUFFIX#_SWITCH>#SIMVAR_ALTERNATOR_SWITCH#</SIMVAR_ALTERNATOR#OUT_PARAM_SUFFIX#_SWITCH>
			<SIMVAR_ALTERNATOR#OUT_PARAM_SUFFIX#_VOLTAGE>#SIMVAR_ALTERNATOR_VOLTAGE#</SIMVAR_ALTERNATOR#OUT_PARAM_SUFFIX#_SWITCH>
			<SIMVAR_ALTERNATOR#OUT_PARAM_SUFFIX#_LOAD>#SIMVAR_ALTERNATOR_LOAD#</SIMVAR_ALTERNATOR#OUT_PARAM_SUFFIX#_LOAD>
			<SIMVAR_ALTERNATOR#OUT_PARAM_SUFFIX#_LOAD_RATIO>#UNITS_ALTERNATOR_LOAD_RATIO#</SIMVAR_ALTERNATOR#OUT_PARAM_SUFFIX#_LOAD_RATIO>
		</ReturnParameters>
	</ParametersFn>
