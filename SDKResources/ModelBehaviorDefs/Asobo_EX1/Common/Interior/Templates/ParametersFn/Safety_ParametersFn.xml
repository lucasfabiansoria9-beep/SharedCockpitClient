<ModelBehaviors>
	<!-- ################################## SAFETY #################################### -->
	<!-- ############################################################################## -->
	<Include RelativeFile="InteractionPresets\\Safety_Interactions.xml"/>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Passenger_Alerts_Parameters">
		<Parameters Type="Default">
			<SIMVAR_FASTEN_SEAT_BELTS>CABIN SEATBELTS ALERT SWITCH</SIMVAR_FASTEN_SEAT_BELTS>
			<SIMVAR_NO_SMOKING>CABIN NO SMOKING ALERT SWITCH</SIMVAR_NO_SMOKING>
		</Parameters>
		<Parameters Type="Default">
			<GET_FASTEN_SEAT_BELTS>(A:#SIMVAR_FASTEN_SEAT_BELTS#, Bool)</GET_FASTEN_SEAT_BELTS>
			<GET_NO_SMOKING>(A:#SIMVAR_NO_SMOKING#, Bool)</GET_NO_SMOKING>
		</Parameters>
		<Parameters Type="Default">
			<TOGGLE_FASTEN_SEAT_BELTS_SIGN>(&gt;K:CABIN_SEATBELTS_ALERT_SWITCH_TOGGLE)</TOGGLE_FASTEN_SEAT_BELTS_SIGN>
			<TOGGLE_NO_SMOKING_SIGN>(&gt;K:CABIN_NO_SMOKING_ALERT_SWITCH_TOGGLE)</TOGGLE_NO_SMOKING_SIGN>
			<SET_FASTEN_SEAT_BELTS_SIGN>#GET_FASTEN_SEAT_BELTS# != if{ (&gt;K:CABIN_SEATBELTS_ALERT_SWITCH_TOGGLE) }</SET_FASTEN_SEAT_BELTS_SIGN>
			<SET_NO_SMOKING_SIGN>#GET_NO_SMOKING# != if{ (&gt;K:CABIN_NO_SMOKING_ALERT_SWITCH_TOGGLE) }</SET_NO_SMOKING_SIGN>
		</Parameters>
		<ReturnParameters>
			<SIMVAR_FASTEN_SEAT_BELTS>#SIMVAR_FASTEN_SEAT_BELTS#</SIMVAR_FASTEN_SEAT_BELTS>
			<SIMVAR_NO_SMOKING>#SIMVAR_NO_SMOKING#</SIMVAR_NO_SMOKING>
			<GET_FASTEN_SEAT_BELTS>#GET_FASTEN_SEAT_BELTS#</GET_FASTEN_SEAT_BELTS>
			<GET_NO_SMOKING>#GET_NO_SMOKING#</GET_NO_SMOKING>
			<TOGGLE_FASTEN_SEAT_BELTS_SIGN>#TOGGLE_FASTEN_SEAT_BELTS_SIGN#</TOGGLE_FASTEN_SEAT_BELTS_SIGN>
			<TOGGLE_NO_SMOKING_SIGN>#TOGGLE_NO_SMOKING_SIGN#</TOGGLE_NO_SMOKING_SIGN>
			<SET_FASTEN_SEAT_BELTS_SIGN>#SET_FASTEN_SEAT_BELTS_SIGN#</SET_FASTEN_SEAT_BELTS_SIGN>
			<SET_NO_SMOKING_SIGN>#SET_NO_SMOKING_SIGN#</SET_NO_SMOKING_SIGN>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Engine_Fire_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Param_Suffix_Helper">
				<OUT_PARAM_NAME>ENGINE_FIRE_SUFFIX</OUT_PARAM_NAME>
			</UseParametersFn>
		</Parameters>
		<Parameters Type="Default">
			<ENG_ID>1</ENG_ID>
		</Parameters>
		<Parameters Type="Default">
			<BOTTLE_ID>#ENG_ID#</BOTTLE_ID>
			<SIMVAR_ENG_ON_FIRE>ENG ON FIRE:#ENG_ID#</SIMVAR_ENG_ON_FIRE>
		</Parameters>
		<Parameters Type="Default">
			<GET_ENG_ON_FIRE>(A:#SIMVAR_ENG_ON_FIRE#, Bool)</GET_ENG_ON_FIRE>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Fire_Bottle_Parameters">
				<Condition Check="BOTTLE_OUT_PARAM_ID">
					<OUT_PARAM_ID>#BOTTLE_OUT_PARAM_ID#</OUT_PARAM_ID>
				</Condition>
			</UseParametersFn>
			<SIMVAR_ENG#ENGINE_FIRE_SUFFIX#_ON_FIRE>#SIMVAR_ENG_ON_FIRE#</SIMVAR_ENG#ENGINE_FIRE_SUFFIX#_ON_FIRE>
			<GET_ENG#ENGINE_FIRE_SUFFIX#_ON_FIRE>#GET_ENG_ON_FIRE#</GET_ENG#ENGINE_FIRE_SUFFIX#_ON_FIRE>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_APU_Fire_Parameters">
		<Parameters Type="Default">
			<BOTTLE_ID Process="Int">(A:NUMBER OF ENGINES, Number) 1 +</BOTTLE_ID>
			<SIMVAR_APU_ON_FIRE>APU ON FIRE DETECTED</SIMVAR_APU_ON_FIRE>
		</Parameters>
		<Parameters Type="Default">
			<GET_APU_ON_FIRE>(A:#SIMVAR_APU_ON_FIRE#, Bool)</GET_APU_ON_FIRE>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Fire_Bottle_Parameters">
				<IS_APU>True</IS_APU>
			</UseParametersFn>
			<SIMVAR_APU_ON_FIRE>#SIMVAR_APU_ON_FIRE#</SIMVAR_APU_ON_FIRE>
			<GET_APU_ON_FIRE>#GET_APU_ON_FIRE#</GET_APU_ON_FIRE>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Fire_Bottle_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Param_Suffix_Helper">
				<OUT_PARAM_NAME>BOTTLE_SUFFIX</OUT_PARAM_NAME>
			</UseParametersFn>
		</Parameters>
		<Parameters Type="Default">
			<BOTTLE_ID>1</BOTTLE_ID>
		</Parameters>
		<Parameters Type="Default">
			<SIMVAR_FIRE_BOTTLE_SWITCH>FIRE BOTTLE SWITCH:#BOTTLE_ID#</SIMVAR_FIRE_BOTTLE_SWITCH>
			<SIMVAR_FIRE_BOTTLE_DISCHARGED>FIRE BOTTLE DISCHARGED:#BOTTLE_ID#</SIMVAR_FIRE_BOTTLE_DISCHARGED>
		</Parameters>
		<Parameters Type="Default">
			<GET_FIRE_BOTTLE_SWITCH>(A:#SIMVAR_FIRE_BOTTLE_SWITCH#, Bool)</GET_FIRE_BOTTLE_SWITCH>
			<GET_FIRE_BOTTLE_DISCHARGED>(A:#SIMVAR_FIRE_BOTTLE_DISCHARGED#, Bool)</GET_FIRE_BOTTLE_DISCHARGED>
		</Parameters>
		<Parameters Type="Default">
			<Switch>
				<Case NotEmpty="ENG_ID">
					<SET_FIRE_BOTTLE_SWITCH>#GET_FIRE_BOTTLE_SWITCH# != if{ #BOTTLE_ID##ENG_ID# (&gt;K:EXTINGUISH_ENGINE_FIRE) }</SET_FIRE_BOTTLE_SWITCH>
					<TOGGLE_FIRE_BOTTLE_SWITCH>#BOTTLE_ID##ENG_ID# (&gt;K:EXTINGUISH_ENGINE_FIRE)</TOGGLE_FIRE_BOTTLE_SWITCH>					
				</Case>
				<Case Valid="IS_APU">
					<SET_FIRE_BOTTLE_SWITCH>#GET_FIRE_BOTTLE_SWITCH# != if{ #BOTTLE_ID# (&gt;K:APU_EXTINGUISH_FIRE) }</SET_FIRE_BOTTLE_SWITCH>
					<TOGGLE_FIRE_BOTTLE_SWITCH>#BOTTLE_ID# (&gt;K:APU_EXTINGUISH_FIRE)</TOGGLE_FIRE_BOTTLE_SWITCH>	
				</Case>
			</Switch>
		</Parameters>
		<ReturnParameters>
			<SIMVAR_FIRE_BOTTLE#BOTTLE_SUFFIX#_SWITCH>#SIMVAR_FIRE_BOTTLE_SWITCH#</SIMVAR_FIRE_BOTTLE#BOTTLE_SUFFIX#_SWITCH>
			<SIMVAR_FIRE_BOTTLE#BOTTLE_SUFFIX#_DISCHARGED>#SIMVAR_FIRE_BOTTLE_DISCHARGED#</SIMVAR_FIRE_BOTTLE#BOTTLE_SUFFIX#_DISCHARGED>
			<GET_FIRE_BOTTLE#BOTTLE_SUFFIX#_SWITCH>#GET_FIRE_BOTTLE_SWITCH#</GET_FIRE_BOTTLE#BOTTLE_SUFFIX#_SWITCH>
			<GET_FIRE_BOTTLE#BOTTLE_SUFFIX#_DISCHARGED>#GET_FIRE_BOTTLE_DISCHARGED#</GET_FIRE_BOTTLE#BOTTLE_SUFFIX#_DISCHARGED>
			<Condition Check="SET_FIRE_BOTTLE_SWITCH">
				<SET_FIRE_BOTTLE#BOTTLE_SUFFIX#_SWITCH>#SET_FIRE_BOTTLE_SWITCH#</SET_FIRE_BOTTLE#BOTTLE_SUFFIX#_SWITCH>				
			</Condition>
			<Condition Check="TOGGLE_FIRE_BOTTLE_SWITCH">
				<TOGGLE_FIRE_BOTTLE#BOTTLE_SUFFIX#_SWITCH>#TOGGLE_FIRE_BOTTLE_SWITCH#</TOGGLE_FIRE_BOTTLE#BOTTLE_SUFFIX#_SWITCH>									
			</Condition>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_ELT_Parameters">
		<Parameters Type="Default">
			<SIMVAR_ELT>ELT ACTIVATED</SIMVAR_ELT>
		</Parameters>
		<Parameters Type="Default">
			<GET_ELT>(A:#SIMVAR_ELT#, Bool)</GET_ELT>
			<TOGGLE_ELT>(&gt;K:ELT_TOGGLE)</TOGGLE_ELT>
			<SET_ELT>(&gt;K:ELT_SET)</SET_ELT>
		</Parameters>
		<ReturnParameters>
			<SIMVAR_ELT>#SIMVAR_ELT#</SIMVAR_ELT>
			<GET_ELT>#GET_ELT#</GET_ELT>
			<TOGGLE_ELT>#TOGGLE_ELT#</TOGGLE_ELT>
			<SET_ELT>#SET_ELT#</SET_ELT>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_ELT_Light_Emissive_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_ELT_Parameters"/>
			<EMISSIVE_CODE>
				#GET_ELT# 
				@E_SimulationTime 6 * 2 % and
			</EMISSIVE_CODE>
		</Parameters>
		<Parameters Type="Override">
			<Condition NotEmpty="TEST_POS_CHECK">
				<EMISSIVE_CODE>
					#EMISSIVE_CODE#
					#TEST_POS_CHECK# or
				</EMISSIVE_CODE>
			</Condition>
		</Parameters>
		<ReturnParameters>
			<EMISSIVE_CODE>#EMISSIVE_CODE#</EMISSIVE_CODE>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
</ModelBehaviors>