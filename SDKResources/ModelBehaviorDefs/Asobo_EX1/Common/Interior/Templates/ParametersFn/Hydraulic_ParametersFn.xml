<ModelBehaviors>
	<!-- ################################## HYDRAULIC ################################# -->
	<!-- ############################################################################## -->
	<Include RelativeFile="InteractionPresets\\Hydraulic_Interactions.xml"/>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Hydraulic_Pump_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Param_Suffix_Helper"/>
		</Parameters>
		<Parameters Type="Default">
			<PUMP_ID>1</PUMP_ID>
		</Parameters>
		<Parameters Type="Default">
			<SIMVAR_PUMP_SWITCH>HYDRAULIC SWITCH:#PUMP_ID#</SIMVAR_PUMP_SWITCH>
			<SIMVAR_PUMP_PRESSURE>HYDRAULIC PRESSURE:#PUMP_ID#</SIMVAR_PUMP_PRESSURE>
			<SIMVAR_PUMP_RESERVOIR>HYDRAULIC SYSTEM RESERVOIR PERCENT:#PUMP_ID#</SIMVAR_PUMP_RESERVOIR>
			<SIMVAR_PUMP_SYSTEM_INTEGRITY>HYDRAULIC SYSTEM INTEGRITY:#PUMP_ID#</SIMVAR_PUMP_SYSTEM_INTEGRITY>
			<UNITS_PUMP_PRESSURE>psi</UNITS_PUMP_PRESSURE>
			<UNITS_PUMP_RESERVOIR>percent</UNITS_PUMP_RESERVOIR>
			<UNITS_PUMP_SYSTEM_INTEGRITY>percent</UNITS_PUMP_SYSTEM_INTEGRITY>
		</Parameters>
		<Parameters Type="Default">
			<GET_PUMP_SWITCH>(A:#SIMVAR_PUMP_SWITCH#, Bool)</GET_PUMP_SWITCH>
			<GET_PUMP_PRESSURE>(A:#SIMVAR_PUMP_PRESSURE#, #UNITS_PUMP_PRESSURE#)</GET_PUMP_PRESSURE>
			<GET_PUMP_RESERVOIR>(A:#SIMVAR_PUMP_RESERVOIR#, #UNITS_PUMP_RESERVOIR#)</GET_PUMP_RESERVOIR>
			<GET_PUMP_SYSTEM_INTEGRITY>(A:#SIMVAR_PUMP_SYSTEM_INTEGRITY#, #UNITS_PUMP_SYSTEM_INTEGRITY#)</GET_PUMP_SYSTEM_INTEGRITY>
		</Parameters>
		<Parameters Type="Default">
			<TOGGLE_PUMP>#PUMP_ID# (&gt;K:HYDRAULIC_SWITCH_TOGGLE)</TOGGLE_PUMP>
			<SET_PUMP>#GET_PUMP_SWITCH# != if{ #PUMP_ID# (&gt;K:HYDRAULIC_SWITCH_TOGGLE) }</SET_PUMP>
		</Parameters>
		<ReturnParameters>
			<SIMVAR_PUMP#OUT_PARAM_SUFFIX#_SWITCH>#SIMVAR_PUMP_SWITCH#</SIMVAR_PUMP#OUT_PARAM_SUFFIX#_SWITCH>
			<SIMVAR_PUMP#OUT_PARAM_SUFFIX#_PRESSURE>#SIMVAR_PUMP_PRESSURE#</SIMVAR_PUMP#OUT_PARAM_SUFFIX#_PRESSURE>
			<SIMVAR_PUMP#OUT_PARAM_SUFFIX#_RESERVOIR>#SIMVAR_PUMP_RESERVOIR#</SIMVAR_PUMP#OUT_PARAM_SUFFIX#_RESERVOIR>
			<SIMVAR_PUMP#OUT_PARAM_SUFFIX#_SYSTEM_INTEGRITY>#SIMVAR_PUMP_SYSTEM_INTEGRITY#</SIMVAR_PUMP#OUT_PARAM_SUFFIX#_SYSTEM_INTEGRITY>
			<GET_PUMP#OUT_PARAM_SUFFIX#_SWITCH>#GET_PUMP_SWITCH#</GET_PUMP#OUT_PARAM_SUFFIX#_SWITCH>
			<GET_PUMP#OUT_PARAM_SUFFIX#_PRESSURE>#GET_PUMP_PRESSURE#</GET_PUMP#OUT_PARAM_SUFFIX#_PRESSURE>
			<GET_PUMP#OUT_PARAM_SUFFIX#_RESERVOIR>#GET_PUMP_RESERVOIR#</GET_PUMP#OUT_PARAM_SUFFIX#_RESERVOIR>
			<GET_PUMP#OUT_PARAM_SUFFIX#_SYSTEM_INTEGRITY>#GET_PUMP_SYSTEM_INTEGRITY#</GET_PUMP#OUT_PARAM_SUFFIX#_SYSTEM_INTEGRITY>
			<TOGGLE_PUMP#OUT_PARAM_SUFFIX#>#TOGGLE_PUMP#</TOGGLE_PUMP#OUT_PARAM_SUFFIX#>
			<SET_PUMP#OUT_PARAM_SUFFIX#>#SET_PUMP#</SET_PUMP#OUT_PARAM_SUFFIX#>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Hydraulic_Valve_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Param_Suffix_Helper"/>
		</Parameters>
		<Parameters Type="Default">
			<VALVE_ID>1</VALVE_ID>
			<VALVE_POS_UNIT>percent over 100</VALVE_POS_UNIT>
			<THRESHOLD_OPEN_CLOSE_DETECTION_PCT>5</THRESHOLD_OPEN_CLOSE_DETECTION_PCT>
		</Parameters>
		<Parameters Type="Default">
			<SIMVAR_VALVE_POS>HYDRAULIC VALVE POS:#VALVE_ID#</SIMVAR_VALVE_POS>
			<SIMVAR_VALVE_TARGET>HYDRAULIC VALVE TARGET POS:#VALVE_ID#</SIMVAR_VALVE_TARGET>
			<OPEN_VALVE_PCT Process="Int">100 #THRESHOLD_OPEN_CLOSE_DETECTION_PCT# -</OPEN_VALVE_PCT>
			<CLOSED_VALVE_PCT>#THRESHOLD_OPEN_CLOSE_DETECTION_PCT#</CLOSED_VALVE_PCT>
		</Parameters>
		<Parameters Type="Default">
			<GET_VALVE_POS_PCT>(A:#SIMVAR_VALVE_POS#, percent)</GET_VALVE_POS_PCT>
			<GET_VALVE_POS>(A:#SIMVAR_VALVE_POS#, #VALVE_POS_UNIT#)</GET_VALVE_POS>
			<GET_VALVE_TARGET>(A:#SIMVAR_VALVE_TARGET#, #VALVE_POS_UNIT#)</GET_VALVE_TARGET>
		</Parameters>
		<Parameters Type="Default">
			<GET_VALVE_OPEN>#GET_VALVE_POS_PCT# &gt; #OPEN_VALVE_PCT#</GET_VALVE_OPEN>
			<GET_VALVE_CLOSED>#GET_VALVE_POS_PCT# &lt; #CLOSED_VALVE_PCT#</GET_VALVE_CLOSED>
			<GET_VALVE_BETWEEN>#CLOSED_VALVE_PCT# #OPEN_VALVE_PCT# #GET_VALVE_POS# rng</GET_VALVE_BETWEEN>
			<TOGGLE_VALVE>#VALVE_ID# (&gt;K:HYDRAULIC_SWITCH_TOGGLE)</TOGGLE_VALVE>
			<SET_VALVE>#VALVE_ID# (&gt;K:2:HYDRAULIC_VALVE_SET)</SET_VALVE>
			<OPEN_VALVE>#VALVE_ID# (&gt;K:HYDRAULIC_VALVE_OPEN)</OPEN_VALVE>
			<CLOSE_VALVE>#VALVE_ID# (&gt;K:HYDRAULIC_VALVE_CLOSE)</CLOSE_VALVE>
		</Parameters>
		<ReturnParameters>
			<TOGGLE_VALVE#OUT_PARAM_SUFFIX#>#TOGGLE_VALVE#</TOGGLE_VALVE#OUT_PARAM_SUFFIX#>
			<SET_VALVE#OUT_PARAM_SUFFIX#>#SET_VALVE#</SET_VALVE#OUT_PARAM_SUFFIX#>
			<OPEN_VALVE#OUT_PARAM_SUFFIX#>#OPEN_VALVE#</OPEN_VALVE#OUT_PARAM_SUFFIX#>
			<CLOSE_VALVE#OUT_PARAM_SUFFIX#>#CLOSE_VALVE#</CLOSE_VALVE#OUT_PARAM_SUFFIX#>
			<IS_VALVE#OUT_PARAM_SUFFIX#_OPEN>#GET_VALVE_OPEN#</IS_VALVE#OUT_PARAM_SUFFIX#_OPEN>
			<IS_VALVE#OUT_PARAM_SUFFIX#_IN_BETWEEN>#GET_VALVE_BETWEEN#</IS_VALVE#OUT_PARAM_SUFFIX#_IN_BETWEEN>
			<IS_VALVE#OUT_PARAM_SUFFIX#_CLOSED>#GET_VALVE_CLOSED#</IS_VALVE#OUT_PARAM_SUFFIX#_CLOSED>
			<GET_VALVE#OUT_PARAM_SUFFIX#>#GET_VALVE_POS#</GET_VALVE#OUT_PARAM_SUFFIX#>
			<SIMVAR_VALVE#OUT_PARAM_SUFFIX#_POS>#SIMVAR_VALVE_POS#</SIMVAR_VALVE#OUT_PARAM_SUFFIX#_POS>
			<GET_VALVE#OUT_PARAM_SUFFIX#_SWITCH>#GET_VALVE_TARGET#</GET_VALVE#OUT_PARAM_SUFFIX#_SWITCH>
			<SIMVAR_VALVE#OUT_PARAM_SUFFIX#>#SIMVAR_VALVE_TARGET#</SIMVAR_VALVE#OUT_PARAM_SUFFIX#>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Hydraulic_Actuator_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Param_Suffix_Helper"/>
		</Parameters>
		<Parameters Type="Default">
			<ACTUATOR_ID>1</ACTUATOR_ID>
			<UNIT_ACTUATOR_INTEGRITY>percent over 100</UNIT_ACTUATOR_INTEGRITY>
			<UNIT_ACTUATOR_PRESSURE>psi</UNIT_ACTUATOR_PRESSURE>
		</Parameters>
		<Parameters Type="Default">
			<SIMVAR_ACTUATOR_ACTIVE>HYDRAULIC ACTUATOR ACTIVE:#ACTUATOR_ID#</SIMVAR_ACTUATOR_ACTIVE>
			<SIMVAR_ACTUATOR_INTEGRITY>HYDRAULIC ACTUATOR INTEGRITY:#ACTUATOR_ID#</SIMVAR_ACTUATOR_INTEGRITY>
			<SIMVAR_ACTUATOR_PRESSURE>HYDRAULIC ACTUATOR PRESSURE:#ACTUATOR_ID#</SIMVAR_ACTUATOR_PRESSURE>
		</Parameters>
		<Parameters Type="Default">
			<GET_ACTUATOR_ACTIVE>(A:#SIMVAR_ACTUATOR_ACTIVE#, bool)</GET_ACTUATOR_ACTIVE>
			<GET_ACTUATOR_INTEGRITY>(A:#SIMVAR_ACTUATOR_INTEGRITY#, #UNIT_ACTUATOR_INTEGRITY#)</GET_ACTUATOR_INTEGRITY>
			<GET_ACTUATOR_PRESSURE>(A:#SIMVAR_ACTUATOR_PRESSURE#, #UNIT_ACTUATOR_PRESSURE#)</GET_ACTUATOR_PRESSURE>
		</Parameters>
		<ReturnParameters>
			<SIMVAR_ACTUATOR#OUT_PARAM_SUFFIX#_ACTIVE>#SIMVAR_ACTUATOR_ACTIVE#</SIMVAR_ACTUATOR#OUT_PARAM_SUFFIX#_ACTIVE>
			<SIMVAR_ACTUATOR#OUT_PARAM_SUFFIX#_INTEGRITY>#SIMVAR_ACTUATOR_INTEGRITY#</SIMVAR_ACTUATOR#OUT_PARAM_SUFFIX#_INTEGRITY>
			<SIMVAR_ACTUATOR#OUT_PARAM_SUFFIX#_PRESSURE>#SIMVAR_ACTUATOR_PRESSURE#</SIMVAR_ACTUATOR#OUT_PARAM_SUFFIX#_PRESSURE>
			<GET_ACTUATOR#OUT_PARAM_SUFFIX#_ACTIVE>#GET_ACTUATOR_ACTIVE#</GET_ACTUATOR#OUT_PARAM_SUFFIX#_ACTIVE>
			<GET_ACTUATOR#OUT_PARAM_SUFFIX#_INTEGRITY>#GET_ACTUATOR_INTEGRITY#</GET_ACTUATOR#OUT_PARAM_SUFFIX#_INTEGRITY>
			<GET_ACTUATOR#OUT_PARAM_SUFFIX#_PRESSURE>#GET_ACTUATOR_PRESSURE#</GET_ACTUATOR#OUT_PARAM_SUFFIX#_PRESSURE>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
</ModelBehaviors>