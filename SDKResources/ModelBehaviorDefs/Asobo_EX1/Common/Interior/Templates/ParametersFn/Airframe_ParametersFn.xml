<ModelBehaviors>
	<!-- ################################## AIRFRAME ################################# -->
	<!-- ############################################################################## -->
	<Include RelativeFile="InteractionPresets\\Airframe_Interactions.xml"/>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Door_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Param_Suffix_Helper"/>
		</Parameters>
		<Parameters Type="Default">
			<EXTERNAL_POWER_ID>1</EXTERNAL_POWER_ID>
		</Parameters>
		<Parameters Type="Default">
			<SIMVAR_DOOR_OPEN>INTERACTIVE POINT OPEN:#ID#</SIMVAR_DOOR_OPEN>
			<SIMVAR_DOOR_GOAL>INTERACTIVE POINT GOAL:#ID#</SIMVAR_DOOR_GOAL>
		</Parameters>
		<Parameters Type="Default">
			<GET_DOOR_OPEN>(A:#SIMVAR_DOOR_OPEN#, Percent)</GET_DOOR_OPEN>
			<GET_DOOR_GOAL>(A:#SIMVAR_DOOR_GOAL#, Percent)</GET_DOOR_GOAL>
			<SET_DOOR_OPEN>(&gt;A:#SIMVAR_DOOR_OPEN#, Percent)</SET_DOOR_OPEN>
			<SET_DOOR_GOAL>(&gt;A:#SIMVAR_DOOR_GOAL#, Percent)</SET_DOOR_GOAL>
		</Parameters>
		<ReturnParameters>
			<GET_DOOR_OPEN#OUT_PARAM_SUFFIX#>#GET_DOOR_OPEN#</GET_DOOR_OPEN#OUT_PARAM_SUFFIX#>
			<GET_DOOR_GOAL#OUT_PARAM_SUFFIX#>#GET_DOOR_GOAL#</GET_DOOR_GOAL#OUT_PARAM_SUFFIX#>
			<SET_DOOR_OPEN#OUT_PARAM_SUFFIX#>#SET_DOOR_OPEN#</SET_DOOR_OPEN#OUT_PARAM_SUFFIX#>
			<SET_DOOR_GOAL#OUT_PARAM_SUFFIX#>#SET_DOOR_GOAL#</SET_DOOR_GOAL#OUT_PARAM_SUFFIX#>
			
			<SIMVAR_DOOR_OPEN#OUT_PARAM_SUFFIX#>#SIMVAR_DOOR_OPEN#</SIMVAR_DOOR_OPEN#OUT_PARAM_SUFFIX#>
			<SIMVAR_DOOR_GOAL#OUT_PARAM_SUFFIX#>#SIMVAR_DOOR_GOAL#</SIMVAR_DOOR_GOAL#OUT_PARAM_SUFFIX#>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
</ModelBehaviors>