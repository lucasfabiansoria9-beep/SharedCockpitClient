	<ParametersFn Name="ASOBO_Baro_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Param_Suffix_Helper"/>
		</Parameters>
		<Parameters Type="Default">
			<BARO_ID>0</BARO_ID>
		</Parameters>
		<Parameters Type="Default">
			<SIMVAR_BARO_HG>KOHLSMAN SETTING HG:#BARO_ID#</SIMVAR_BARO_HG>
			<SIMVAR_BARO_MB>KOHLSMAN SETTING MB:#BARO_ID#</SIMVAR_BARO_MB>
			<SIMVAR_BARO_STD>KOHLSMAN SETTING STD:#BARO_ID#</SIMVAR_BARO_STD>
		</Parameters>
		<Parameters Type="Default">
			<SET_BARO_STD_PRESSURE>#BARO_ID# (&gt;K:BAROMETRIC_STD_PRESSURE)</SET_BARO_STD_PRESSURE>
			<GET_BARO_HG>(A:#SIMVAR_BARO_HG#, inHg)</GET_BARO_HG>
			<GET_BARO_MB>(A:#SIMVAR_BARO_MB#, Millibar)</GET_BARO_MB>
			<GET_BARO_STD>(A:#SIMVAR_BARO_STD#, Bool)</GET_BARO_STD>
			<SET_BARO_STD>(&gt;A:#SIMVAR_BARO_STD#, Bool)</SET_BARO_STD>
			<SET_BARO_MB>sp0 #BARO_ID# l0 16 * (&gt;K:2:KOHLSMAN_SET)</SET_BARO_MB>
			<SET_BARO_HG>33.863886667 / sp0 #BARO_ID# l0 16 * (&gt;K:2:KOHLSMAN_SET)</SET_BARO_HG>
		</Parameters>
		<ReturnParameters>
			<GET_BARO#OUT_PARAM_SUFFIX#_HG>#GET_BARO_HG#</GET_BARO#OUT_PARAM_SUFFIX#_HG>
			<GET_BARO#OUT_PARAM_SUFFIX#_MB>#GET_BARO_MB#</GET_BARO#OUT_PARAM_SUFFIX#_MB>
			<GET_BARO#OUT_PARAM_SUFFIX#_STD>#GET_BARO_STD#</GET_BARO#OUT_PARAM_SUFFIX#_STD>
			<SET_BARO#OUT_PARAM_SUFFIX#_MB>#SET_BARO_MB#</SET_BARO#OUT_PARAM_SUFFIX#_MB>
			<SET_BARO#OUT_PARAM_SUFFIX#_HG>#SET_BARO_HG#</SET_BARO#OUT_PARAM_SUFFIX#_HG>
			<SET_BARO#OUT_PARAM_SUFFIX#_STD>#SET_BARO_STD#</SET_BARO#OUT_PARAM_SUFFIX#_STD>
			<SET_BARO#OUT_PARAM_SUFFIX#_STD_PRESSURE>#SET_BARO_STD_PRESSURE#</SET_BARO#OUT_PARAM_SUFFIX#_STD_PRESSURE>
			<SIMVAR_BARO#OUT_PARAM_SUFFIX#_HG>#SIMVAR_BARO_HG#</SIMVAR_BARO#OUT_PARAM_SUFFIX#_HG>
			<SIMVAR_BARO#OUT_PARAM_SUFFIX#_MB>#SIMVAR_BARO_MB#</SIMVAR_BARO#OUT_PARAM_SUFFIX#_MB>
			<SIMVAR_BARO#OUT_PARAM_SUFFIX#_STD>#SIMVAR_BARO_STD#</SIMVAR_BARO#OUT_PARAM_SUFFIX#_STD>
		</ReturnParameters>
	</ParametersFn>
