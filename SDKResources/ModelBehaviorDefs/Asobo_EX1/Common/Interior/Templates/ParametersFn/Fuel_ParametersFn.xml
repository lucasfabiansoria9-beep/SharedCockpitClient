	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Pump_Low_Pressure_Parameters">
		<Parameters Type="Override">
			<Condition Valid="ONLY_CHECK_WHEN_PUMP_ON">
				<UseParametersFn Name="ASOBO_Fuel_Pump_Parameters"/>
			</Condition>
			<UseParametersFn Name="ASOBO_Fuel_Line_Parameters"/>
		</Parameters>
		<Parameters Type="Default">
			<LOW_PRESSURE_WARNING_PSI>20</LOW_PRESSURE_WARNING_PSI>
			<ONLY_CHECK_WHEN_PUMP_ON>False</ONLY_CHECK_WHEN_PUMP_ON>
		</Parameters>
		<Parameters Type="Default">
			<Condition Valid="ONLY_CHECK_WHEN_PUMP_ON">
				<PUMP_IS_ON>#GET_PUMP#</PUMP_IS_ON>
			</Condition>
		</Parameters>
		<Parameters Type="Default">
			<PRESSURE_CHECK>#GET_LINE_PRESSURE# #LOW_PRESSURE_WARNING_PSI# &lt;</PRESSURE_CHECK>
		</Parameters>
		<ReturnParameters>
			<Condition Valid="ONLY_CHECK_WHEN_PUMP_ON">
				<True>
					<IS_LOW_PRESSURE>#PRESSURE_CHECK# #PUMP_IS_ON# and</IS_LOW_PRESSURE>
				</True>
				<False>
					<IS_LOW_PRESSURE>#PRESSURE_CHECK#</IS_LOW_PRESSURE>
				</False>
			</Condition>
		</ReturnParameters>
	</ParametersFn>
	<ParametersFn Name="ASOBO_Pump_Low_Pressure_Emissive_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Pump_Low_Pressure_Parameters"/>
		</Parameters>
		<ReturnParameters>
			<EMISSIVE_CODE>#IS_LOW_PRESSURE#</EMISSIVE_CODE>
		</ReturnParameters>
	</ParametersFn>
</ModelBehaviors>