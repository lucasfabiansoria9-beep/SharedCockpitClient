<ModelBehaviors>
	<!-- ################################## FUEL ###################################### -->
	<!-- ############################################################################## -->
	<Include RelativeFile="InteractionPresets\\Fuel_Interactions.xml"/>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Fuel_Valve_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Param_Suffix_Helper"/>
		</Parameters>
		<Parameters Type="Default">
			<VALVE_ID>1</VALVE_ID>
			<THRESHOLD_OPEN_CLOSE_DETECTION_PCT>5</THRESHOLD_OPEN_CLOSE_DETECTION_PCT>
		</Parameters>
		<Parameters Type="Default">
			<SIMVAR_VALVE>FUELSYSTEM VALVE SWITCH:#VALVE_ID#</SIMVAR_VALVE>
			<SIMVAR_VALVE_OPEN>FUELSYSTEM VALVE OPEN:#VALVE_ID#</SIMVAR_VALVE_OPEN>
			<UNITS_VALVE_OPEN>percent</UNITS_VALVE_OPEN>
			<OPEN_VALVE_PCT Process="Int">100 #THRESHOLD_OPEN_CLOSE_DETECTION_PCT# -</OPEN_VALVE_PCT>
			<CLOSED_VALVE_PCT>#THRESHOLD_OPEN_CLOSE_DETECTION_PCT#</CLOSED_VALVE_PCT>
		</Parameters>
		<Parameters Type="Default">
			<GET_VALVE>(A:#SIMVAR_VALVE#, Bool)</GET_VALVE>
			<GET_VALVE_OPEN>(A:#SIMVAR_VALVE_OPEN#, #UNITS_VALVE_OPEN#)</GET_VALVE_OPEN>
			<GET_VALVE_OPENING_PCT>(A:#SIMVAR_VALVE_OPEN#, percent)</GET_VALVE_OPENING_PCT>
		</Parameters>
		<Parameters Type="Default">
			<TOGGLE_VALVE>#VALVE_ID# (&gt;K:FUELSYSTEM_VALVE_TOGGLE)</TOGGLE_VALVE>
			<SET_VALVE>#VALVE_ID# (&gt;K:2:FUELSYSTEM_VALVE_SET)</SET_VALVE>
		</Parameters>
		<Parameters Type="Default">
			<OPEN_CHECK>#GET_VALVE_OPENING_PCT# #OPEN_VALVE_PCT# &gt;</OPEN_CHECK>
			<IN_TRANSIT_CHECK>#CLOSED_VALVE_PCT# #OPEN_VALVE_PCT# #GET_VALVE_OPENING_PCT# rng</IN_TRANSIT_CHECK>
			<CLOSED_CHECK>#GET_VALVE_OPENING_PCT# #CLOSED_VALVE_PCT# &lt;</CLOSED_CHECK>
		</Parameters>
		<ReturnParameters>
			<TOGGLE_VALVE#OUT_PARAM_SUFFIX#>#TOGGLE_VALVE#</TOGGLE_VALVE#OUT_PARAM_SUFFIX#>
			<SET_VALVE#OUT_PARAM_SUFFIX#>#SET_VALVE#</SET_VALVE#OUT_PARAM_SUFFIX#>
			<GET_VALVE#OUT_PARAM_SUFFIX#>#GET_VALVE#</GET_VALVE#OUT_PARAM_SUFFIX#>
			<GET_VALVE#OUT_PARAM_SUFFIX#_OPEN>#GET_VALVE_OPEN#</GET_VALVE#OUT_PARAM_SUFFIX#_OPEN>
			<SIMVAR_VALVE#OUT_PARAM_SUFFIX#>#SIMVAR_VALVE#</SIMVAR_VALVE#OUT_PARAM_SUFFIX#>
			<SIMVAR_VALVE#OUT_PARAM_SUFFIX#_OPEN>#SIMVAR_VALVE_OPEN#</SIMVAR_VALVE#OUT_PARAM_SUFFIX#_OPEN>
			<IS_VALVE#OUT_PARAM_SUFFIX#_OPEN>#OPEN_CHECK#</IS_VALVE#OUT_PARAM_SUFFIX#_OPEN>
			<IS_VALVE#OUT_PARAM_SUFFIX#_IN_TRANSIT>#IN_TRANSIT_CHECK#</IS_VALVE#OUT_PARAM_SUFFIX#_IN_TRANSIT>
			<IS_VALVE#OUT_PARAM_SUFFIX#_CLOSED>#CLOSED_CHECK#</IS_VALVE#OUT_PARAM_SUFFIX#_CLOSED>
		</ReturnParameters>
	</ParametersFn>
