<ModelBehaviors>
	<!-- ################################## LANDING ################################### -->
	<!-- ############################################################################## -->
	<Include RelativeFile="InteractionPresets\\Landing_Interactions.xml"/>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Gears_Handle_Parameters">
		<Parameters Type="Default">
			<SIMVAR_GEAR_HANDLE_POSITION>GEAR HANDLE POSITION</SIMVAR_GEAR_HANDLE_POSITION>
			<SIMVAR_GEAR_EMERGENCY_HANDLE_POSITION>GEAR EMERGENCY HANDLE POSITION</SIMVAR_GEAR_EMERGENCY_HANDLE_POSITION>
		</Parameters>
		<Parameters Type="Default">
			<GET_GEAR_HANDLE_POSITION>(A:#SIMVAR_GEAR_HANDLE_POSITION#, Bool)</GET_GEAR_HANDLE_POSITION>
			<GET_GEAR_EMERGENCY_HANDLE_POSITION>(A:#SIMVAR_GEAR_EMERGENCY_HANDLE_POSITION#, Bool)</GET_GEAR_EMERGENCY_HANDLE_POSITION>
		</Parameters>
		<Parameters Type="Default">
			<SET_GEAR_HANDLE_POSITION>(&gt;K:GEAR_SET)</SET_GEAR_HANDLE_POSITION>
			<TOGGLE_GEAR_HANDLE_POSITION>(&gt;K:GEAR_TOGGLE)</TOGGLE_GEAR_HANDLE_POSITION>
			<SET_GEAR_EMERGENCY_HANDLE_POSITION>#GET_GEAR_EMERGENCY_HANDLE_POSITION# != if{ (&gt;K:GEAR_EMERGENCY_HANDLE_TOGGLE) }</SET_GEAR_EMERGENCY_HANDLE_POSITION>
			<TOGGLE_GEAR_EMERGENCY_HANDLE_POSITION>(&gt;K:GEAR_EMERGENCY_HANDLE_TOGGLE)</TOGGLE_GEAR_EMERGENCY_HANDLE_POSITION>
		</Parameters>
		<ReturnParameters>
			<SIMVAR_GEAR_HANDLE_POSITION>#SIMVAR_GEAR_HANDLE_POSITION#</SIMVAR_GEAR_HANDLE_POSITION>
			<SIMVAR_GEAR_EMERGENCY_HANDLE_POSITION>#SIMVAR_GEAR_EMERGENCY_HANDLE_POSITION#</SIMVAR_GEAR_EMERGENCY_HANDLE_POSITION>
			<SET_GEAR_HANDLE_POSITION>#SET_GEAR_HANDLE_POSITION#</SET_GEAR_HANDLE_POSITION>
			<TOGGLE_GEAR_HANDLE_POSITION>#TOGGLE_GEAR_HANDLE_POSITION#</TOGGLE_GEAR_HANDLE_POSITION>
			<SET_GEAR_EMERGENCY_HANDLE_POSITION>#SET_GEAR_EMERGENCY_HANDLE_POSITION#</SET_GEAR_EMERGENCY_HANDLE_POSITION>
			<TOGGLE_GEAR_EMERGENCY_HANDLE_POSITION>#TOGGLE_GEAR_EMERGENCY_HANDLE_POSITION#</TOGGLE_GEAR_EMERGENCY_HANDLE_POSITION>
			<GET_GEAR_HANDLE_POSITION>#GET_GEAR_HANDLE_POSITION#</GET_GEAR_HANDLE_POSITION>
			<GET_GEAR_EMERGENCY_HANDLE_POSITION>#GET_GEAR_EMERGENCY_HANDLE_POSITION#</GET_GEAR_EMERGENCY_HANDLE_POSITION>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<Macro Name="Enum_GearID_Center">0</Macro>
	<Macro Name="Enum_GearID_Left">1</Macro>
	<Macro Name="Enum_GearID_Right">2</Macro>
	<ParametersFn Name="ASOBO_GearID_Parameters">
		<Parameters Type="Override">
			<!-- Convert from string GEAR_ID to Enum value -->
			<Switch Param="GEAR_ID">
				<Case Value="CENTER"><GEAR_ID>@Enum_GearID_Center</GEAR_ID></Case>
				<Case Value="LEFT"><GEAR_ID>@Enum_GearID_Left</GEAR_ID></Case>
				<Case Value="RIGHT"><GEAR_ID>@Enum_GearID_Right</GEAR_ID></Case>
			</Switch>
		</Parameters>
		<ReturnParameters>
			<GEAR_ID>#GEAR_ID#</GEAR_ID>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<Macro Name="Enum_GearPosition_Up">0</Macro>
	<Macro Name="Enum_GearPosition_Down">100</Macro>
	<ParametersFn Name="ASOBO_GearPosition_Parameters">
		<Parameters Type="Override">
			<Switch Param="GEAR_POSITION">
				<Case Value="UP"><GEAR_POSITION>@Enum_GearPosition_Up</GEAR_POSITION></Case>
				<Case Value="DOWN"><GEAR_POSITION>@Enum_GearPosition_Down</GEAR_POSITION></Case>
				<Default><GEAR_POSITION>@Enum_GearPosition_Down</GEAR_POSITION></Default>
			</Switch>
		</Parameters>
		<ReturnParameters>
			<GEAR_POSITION>#GEAR_POSITION#</GEAR_POSITION>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Parking_Brake_Parameters">
		<Parameters Type="Default">
			<SIMVAR_PARKING_BRAKE_POSITION>BRAKE PARKING POSITION</SIMVAR_PARKING_BRAKE_POSITION>
		</Parameters>
		<Parameters Type="Default">
			<GET_PARKING_BRAKE_POSITION>(A:#SIMVAR_PARKING_BRAKE_POSITION#, Bool)</GET_PARKING_BRAKE_POSITION>
		</Parameters>
		<Parameters Type="Default">
			<SET_PARKING_BRAKE_POSITION>(&gt;K:PARKING_BRAKE_SET)</SET_PARKING_BRAKE_POSITION>
			<TOGGLE_PARKING_BRAKE_POSITION>(&gt;K:PARKING_BRAKES)</TOGGLE_PARKING_BRAKE_POSITION>
		</Parameters>
		<ReturnParameters>
			<SIMVAR_PARKING_BRAKE_POSITION>#SIMVAR_PARKING_BRAKE_POSITION#</SIMVAR_PARKING_BRAKE_POSITION>
			<SET_PARKING_BRAKE_POSITION>#SET_PARKING_BRAKE_POSITION#</SET_PARKING_BRAKE_POSITION>
			<TOGGLE_PARKING_BRAKE_POSITION>#TOGGLE_PARKING_BRAKE_POSITION#</TOGGLE_PARKING_BRAKE_POSITION>
			<GET_PARKING_BRAKE_POSITION>#GET_PARKING_BRAKE_POSITION#</GET_PARKING_BRAKE_POSITION>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Brake_Parameters">
		<Parameters Type="Default">
			<SIMVAR_L_BRAKE_POSITION>BRAKE LEFT POSITION EX1</SIMVAR_L_BRAKE_POSITION>
			<SIMVAR_R_BRAKE_POSITION>BRAKE RIGHT POSITION EX1</SIMVAR_R_BRAKE_POSITION>
			<UNITS_L_BRAKE_POSITION>position</UNITS_L_BRAKE_POSITION>
			<UNITS_R_BRAKE_POSITION>position</UNITS_R_BRAKE_POSITION>
		</Parameters>
		<Parameters Type="Default">
			<GET_L_BRAKE_POSITION>(A:#SIMVAR_L_BRAKE_POSITION#, #UNITS_L_BRAKE_POSITION#)</GET_L_BRAKE_POSITION>
			<GET_R_BRAKE_POSITION>(A:#SIMVAR_R_BRAKE_POSITION#, #UNITS_R_BRAKE_POSITION#)</GET_R_BRAKE_POSITION>
		</Parameters>
		<Parameters Type="Default">
			<SET_L_BRAKE_POSITION>@16k * (&gt;K:AXIS_LEFT_BRAKE_SET)</SET_L_BRAKE_POSITION>
			<SET_R_BRAKE_POSITION>@16k * (&gt;K:AXIS_RIGHT_BRAKE_SET)</SET_R_BRAKE_POSITION>
			<SET_L_BRAKE_POSITION_16K>(&gt;K:AXIS_LEFT_BRAKE_SET)</SET_L_BRAKE_POSITION_16K>
			<SET_R_BRAKE_POSITION_16K>(&gt;K:AXIS_RIGHT_BRAKE_SET)</SET_R_BRAKE_POSITION_16K>
		</Parameters>
		<ReturnParameters>
			<SIMVAR_L_BRAKE_POSITION>#SIMVAR_L_BRAKE_POSITION#</SIMVAR_L_BRAKE_POSITION>
			<SIMVAR_R_BRAKE_POSITION>#SIMVAR_R_BRAKE_POSITION#</SIMVAR_R_BRAKE_POSITION>
			<GET_L_BRAKE_POSITION>#GET_L_BRAKE_POSITION#</GET_L_BRAKE_POSITION>
			<GET_R_BRAKE_POSITION>#GET_R_BRAKE_POSITION#</GET_R_BRAKE_POSITION>
			<SET_L_BRAKE_POSITION>#SET_L_BRAKE_POSITION#</SET_L_BRAKE_POSITION>
			<SET_R_BRAKE_POSITION>#SET_R_BRAKE_POSITION#</SET_R_BRAKE_POSITION>
			<SET_L_BRAKE_POSITION_16K>#SET_L_BRAKE_POSITION_16K#</SET_L_BRAKE_POSITION_16K>
			<SET_R_BRAKE_POSITION_16K>#SET_R_BRAKE_POSITION_16K#</SET_R_BRAKE_POSITION_16K>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<Macro Name="Enum_Autobrake_RTO">0</Macro>
	<Macro Name="Enum_Autobrake_Off">1</Macro>
	<Macro Name="Enum_Autobrake_First_Position">2</Macro>
	<ParametersFn Name="ASOBO_Auto_Brake_Parameters">
		<Parameters Type="Default">
			<SIMVAR_AUTO_BRAKE_POSITION>AUTO BRAKE SWITCH CB</SIMVAR_AUTO_BRAKE_POSITION>
			<SIMVAR_AUTO_BRAKE_ACTIVE>AUTOBRAKES ACTIVE</SIMVAR_AUTO_BRAKE_ACTIVE>
			<SIMVAR_RTO_ACTIVE>REJECTED TAKEOFF BRAKES ACTIVE</SIMVAR_RTO_ACTIVE>
			<SIMVAR_ANTISKID_ACTIVE>ANTISKID BRAKES ACTIVE</SIMVAR_ANTISKID_ACTIVE>
		</Parameters>
		<Parameters Type="Default">
			<GET_AUTO_BRAKE_POSITION>(A:#SIMVAR_AUTO_BRAKE_POSITION#, Number)</GET_AUTO_BRAKE_POSITION>
			<GET_AUTO_BRAKE_ACTIVE>(A:#SIMVAR_AUTO_BRAKE_ACTIVE#, Bool)</GET_AUTO_BRAKE_ACTIVE>
			<GET_RTO_ACTIVE>(A:#SIMVAR_RTO_ACTIVE#, Bool)</GET_RTO_ACTIVE>
			<GET_ANTISKID_ACTIVE>(A:#SIMVAR_ANTISKID_ACTIVE#, Bool)</GET_ANTISKID_ACTIVE>
		</Parameters>
		<Parameters Type="Default">
			<SET_AUTO_BRAKE_POSITION>(&gt;K:SET_AUTOBRAKE_CONTROL)</SET_AUTO_BRAKE_POSITION>
			<SET_ANTISKID>#GET_ANTISKID_ACTIVE# != if{ (&gt;K:ANTISKID_BRAKES_TOGGLE) }</SET_ANTISKID>
			<TOGGLE_ANTISKID>(&gt;K:ANTISKID_BRAKES_TOGGLE)</TOGGLE_ANTISKID>
		</Parameters>
		<ReturnParameters>
			<SIMVAR_AUTO_BRAKE_POSITION>#SIMVAR_AUTO_BRAKE_POSITION#</SIMVAR_AUTO_BRAKE_POSITION>
			<SIMVAR_AUTO_BRAKE_ACTIVE>#SIMVAR_AUTO_BRAKE_ACTIVE#</SIMVAR_AUTO_BRAKE_ACTIVE>
			<SIMVAR_RTO_ACTIVE>#SIMVAR_RTO_ACTIVE#</SIMVAR_RTO_ACTIVE>
			<SIMVAR_ANTISKID_ACTIVE>#SIMVAR_ANTISKID_ACTIVE#</SIMVAR_ANTISKID_ACTIVE>
			<GET_AUTO_BRAKE_POSITION>#GET_AUTO_BRAKE_POSITION#</GET_AUTO_BRAKE_POSITION>
			<GET_AUTO_BRAKE_ACTIVE>#GET_AUTO_BRAKE_ACTIVE#</GET_AUTO_BRAKE_ACTIVE>
			<GET_RTO_ACTIVE>#GET_RTO_ACTIVE#</GET_RTO_ACTIVE>
			<GET_ANTISKID_ACTIVE>#GET_ANTISKID_ACTIVE#</GET_ANTISKID_ACTIVE>
			<SET_AUTO_BRAKE_POSITION>#SET_AUTO_BRAKE_POSITION#</SET_AUTO_BRAKE_POSITION>
			<SET_ANTISKID>#SET_ANTISKID#</SET_ANTISKID>
			<TOGGLE_ANTISKID>#TOGGLE_ANTISKID#</TOGGLE_ANTISKID>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
</ModelBehaviors>