<ModelBehaviors>
	<!-- ################################## COMMON #################################### -->
	<!-- ############################################################################## -->
	<Include RelativeFile="InteractionPresets\\Common_Interactions.xml"/>
	<!-- ############################################################################## -->
	<!-- Cover -->
	<ParametersFn Name="ASOBO_Cover_Interaction_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Setup_Interaction_Parameters">
				<IE_NAME>#COVER_ID#</IE_NAME>
				<IE_ID_SOURCE>COMMON</IE_ID_SOURCE>
			</UseParametersFn>
			<COVER_ID_UC Process="String">'#COVER_ID#' uc</COVER_ID_UC>
			<TOOLTIP_ID_WITH_NO_ID>True</TOOLTIP_ID_WITH_NO_ID>
			<Switch>
				<Case Check="USE_LOCK_NAMING">
					<DEFAULT_TOOLTIP_ID>LOCK</DEFAULT_TOOLTIP_ID>	
				</Case>
				<Case Check="USE_GUARD_NAMING">
					<DEFAULT_TOOLTIP_ID>GUARD</DEFAULT_TOOLTIP_ID>	
				</Case>
				<Default>
					<DEFAULT_TOOLTIP_ID>COVER</DEFAULT_TOOLTIP_ID>						
				</Default>
			</Switch>
		</Parameters>
		<Parameters Type="Default">
			<GET_#COVER_ID_UC#_SETTINGS_CONFIG>ASOBO_Cover_Settings_Config_Parameters</GET_#COVER_ID_UC#_SETTINGS_CONFIG>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Process_Interaction_Parameters"/>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Aircraft_Sim_Parameters">
		<Parameters Type="Default">
			<SIMVAR_IS_ON_GROUND>SIM ON GROUND</SIMVAR_IS_ON_GROUND>
			<SIMVAR_GROUND_SPEED>GPS GROUND SPEED</SIMVAR_GROUND_SPEED>
			<SIMVAR_AIRSPEED>AIRSPEED INDICATED</SIMVAR_AIRSPEED>
			<SIMVAR_PLANE_ALTITUDE>PLANE ALTITUDE</SIMVAR_PLANE_ALTITUDE>
			<SIMVAR_GROUND_ALTITUDE>GROUND ALTITUDE</SIMVAR_GROUND_ALTITUDE>
			<SIMVAR_PLANE_LATITUDE>PLANE LATITUDE</SIMVAR_PLANE_LATITUDE>
			<SIMVAR_PLANE_LONGITUDE>PLANE LONGITUDE</SIMVAR_PLANE_LONGITUDE>
			<SIMVAR_PLANE_HEADING>PLANE HEADING DEGREES GYRO</SIMVAR_PLANE_HEADING>
			<GROUND_SPEED_UNITS>Knots</GROUND_SPEED_UNITS>
			<AIRSPEED_UNITS>Knots</AIRSPEED_UNITS>
			<PLANE_ALTITUDE_UNITS>Feet</PLANE_ALTITUDE_UNITS>
			<GROUND_ALTITUDE_UNITS>Feet</GROUND_ALTITUDE_UNITS>
			<PLANE_LATITUDE_UNITS>Degrees</PLANE_LATITUDE_UNITS>
			<PLANE_LONGITUDE_UNITS>Degrees</PLANE_LONGITUDE_UNITS>
			<PLANE_HEADING_UNITS>Degrees</PLANE_HEADING_UNITS>
		</Parameters>
		<Parameters Type="Default">
			<GET_IS_ON_GROUND>(A:#SIMVAR_IS_ON_GROUND#, Bool)</GET_IS_ON_GROUND>
			<GET_GROUND_SPEED>(A:#SIMVAR_GROUND_SPEED#, #GROUND_SPEED_UNITS#)</GET_GROUND_SPEED>
			<GET_AIRSPEED>(A:#SIMVAR_AIRSPEED#, #AIRSPEED_UNITS#)</GET_AIRSPEED>
			<GET_PLANE_ALTITUDE>(A:#SIMVAR_PLANE_ALTITUDE#, #PLANE_ALTITUDE_UNITS#)</GET_PLANE_ALTITUDE>
			<GET_GROUND_ALTITUDE>(A:#SIMVAR_GROUND_ALTITUDE#, #GROUND_ALTITUDE_UNITS#)</GET_GROUND_ALTITUDE>
			<GET_PLANE_LATITUDE>(A:#SIMVAR_PLANE_LATITUDE#, #PLANE_LATITUDE_UNITS#)</GET_PLANE_LATITUDE>
			<GET_PLANE_LONGITUDE>(A:#SIMVAR_PLANE_LONGITUDE#, #PLANE_LONGITUDE_UNITS#)</GET_PLANE_LONGITUDE>
			<GET_PLANE_HEADING>(A:#SIMVAR_PLANE_HEADING#, #PLANE_HEADING_UNITS#)</GET_PLANE_HEADING>
		</Parameters>
		<ReturnParameters>
			<SIMVAR_IS_ON_GROUND>#SIMVAR_IS_ON_GROUND#</SIMVAR_IS_ON_GROUND>
			<SIMVAR_GROUND_SPEED>#SIMVAR_GROUND_SPEED#</SIMVAR_GROUND_SPEED>
			<SIMVAR_AIRSPEED>#SIMVAR_AIRSPEED#</SIMVAR_AIRSPEED>
			<SIMVAR_PLANE_ALTITUDE>#SIMVAR_PLANE_ALTITUDE#</SIMVAR_PLANE_ALTITUDE>
			<SIMVAR_GROUND_ALTITUDE>#SIMVAR_GROUND_ALTITUDE#</SIMVAR_GROUND_ALTITUDE>
			<SIMVAR_PLANE_LATITUDE>#SIMVAR_PLANE_LATITUDE#</SIMVAR_PLANE_LATITUDE>
			<SIMVAR_PLANE_LONGITUDE>#SIMVAR_PLANE_LONGITUDE#</SIMVAR_PLANE_LONGITUDE>
			<SIMVAR_PLANE_HEADING>#SIMVAR_PLANE_HEADING#</SIMVAR_PLANE_HEADING>
			<GET_IS_ON_GROUND>#GET_IS_ON_GROUND#</GET_IS_ON_GROUND>
			<GET_GROUND_SPEED>#GET_GROUND_SPEED#</GET_GROUND_SPEED>
			<GET_AIRSPEED>#GET_AIRSPEED#</GET_AIRSPEED>
			<GET_PLANE_ALTITUDE>#GET_PLANE_ALTITUDE#</GET_PLANE_ALTITUDE>
			<GET_GROUND_ALTITUDE>#GET_GROUND_ALTITUDE#</GET_GROUND_ALTITUDE>
			<GET_PLANE_LATITUDE>#GET_PLANE_LATITUDE#</GET_PLANE_LATITUDE>
			<GET_PLANE_LONGITUDE>#GET_PLANE_LONGITUDE#</GET_PLANE_LONGITUDE>
			<GET_PLANE_HEADING>#GET_PLANE_HEADING#</GET_PLANE_HEADING>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Air_Sim_Parameters">
		<Parameters Type="Default">
			<SIMVAR_AMBIENT_PRESSURE>AMBIENT PRESSURE</SIMVAR_AMBIENT_PRESSURE>
			<SIMVAR_AMBIENT_TEMPERATURE>AMBIENT TEMPERATURE</SIMVAR_AMBIENT_TEMPERATURE>
			<SIMVAR_SEA_LEVEL_PRESSURE>SEA LEVEL PRESSURE</SIMVAR_SEA_LEVEL_PRESSURE>
			<SIMVAR_SEA_LEVEL_TEMPERATURE>SEA LEVEL AMBIENT TEMPERATURE</SIMVAR_SEA_LEVEL_TEMPERATURE>
			<UNITS_PRESSURE>psi</UNITS_PRESSURE>
			<UNITS_TEMPERATURE>celsius</UNITS_TEMPERATURE>
		</Parameters>
		<Parameters Type="Default">
			<GET_AMBIENT_PRESSURE>(A:#SIMVAR_AMBIENT_PRESSURE#, #UNITS_PRESSURE#)</GET_AMBIENT_PRESSURE>
			<GET_AMBIENT_TEMPERATURE>(A:#SIMVAR_AMBIENT_TEMPERATURE#, #UNITS_TEMPERATURE#)</GET_AMBIENT_TEMPERATURE>
			<GET_SEA_LEVEL_PRESSURE>(A:#SIMVAR_SEA_LEVEL_PRESSURE#, #UNITS_PRESSURE#)</GET_SEA_LEVEL_PRESSURE>
			<GET_SEA_LEVEL_TEMPERATURE>(A:#SIMVAR_SEA_LEVEL_TEMPERATURE#, #UNITS_TEMPERATURE#)</GET_SEA_LEVEL_TEMPERATURE>
		</Parameters>
		<ReturnParameters>
			<SIMVAR_AMBIENT_PRESSURE>#SIMVAR_AMBIENT_PRESSURE#</SIMVAR_AMBIENT_PRESSURE>
			<SIMVAR_AMBIENT_TEMPERATURE>#SIMVAR_AMBIENT_TEMPERATURE#</SIMVAR_AMBIENT_TEMPERATURE>
			<SIMVAR_SEA_LEVEL_PRESSURE>#SIMVAR_SEA_LEVEL_PRESSURE#</SIMVAR_SEA_LEVEL_PRESSURE>
			<SIMVAR_SEA_LEVEL_TEMPERATURE>#SIMVAR_SEA_LEVEL_TEMPERATURE#</SIMVAR_SEA_LEVEL_TEMPERATURE>
			<GET_AMBIENT_PRESSURE>#GET_AMBIENT_PRESSURE#</GET_AMBIENT_PRESSURE>
			<GET_AMBIENT_TEMPERATURE>#GET_AMBIENT_TEMPERATURE#</GET_AMBIENT_TEMPERATURE>
			<GET_SEA_LEVEL_PRESSURE>#GET_SEA_LEVEL_PRESSURE#</GET_SEA_LEVEL_PRESSURE>
			<GET_SEA_LEVEL_TEMPERATURE>#GET_SEA_LEVEL_TEMPERATURE#</GET_SEA_LEVEL_TEMPERATURE>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<Macro Name="AutomaticBrightnessFactor">(A:AMBIENT LIGHT SENSOR, Number) #LIGHT_LUX_MIN# #LIGHT_LUX_MAX# #FACTOR_MIN# #FACTOR_MAX# (F:MapRange) #SCREEN_GAMMA# pow</Macro>
	<ParametersFn Name="ASOBO_Light_Parameters">
		<Parameters Type="Default">
			<LIGHT_LUX_MIN>1</LIGHT_LUX_MIN>
			<LIGHT_LUX_MAX>4000</LIGHT_LUX_MAX>
			<FACTOR_MIN>0.001</FACTOR_MIN>
			<FACTOR_MAX>1</FACTOR_MAX>
			<SCREEN_GAMMA>2.2</SCREEN_GAMMA>
			<SIMVAR_AMBIENT_LIGHT_SENSOR>AMBIENT LIGHT SENSOR</SIMVAR_AMBIENT_LIGHT_SENSOR>
			<UNITS_AMBIENT_LIGHT_SENSOR>Number</UNITS_AMBIENT_LIGHT_SENSOR>
		</Parameters>
		<Parameters Type="Default">
			<GET_AMBIENT_LIGHT_SENSOR>(A:#SIMVAR_AMBIENT_LIGHT_SENSOR#, #UNITS_AMBIENT_LIGHT_SENSOR#)</GET_AMBIENT_LIGHT_SENSOR>
		</Parameters>
		<ReturnParameters>
			<SIMVAR_AMBIENT_LIGHT_SENSOR>#SIMVAR_AMBIENT_LIGHT_SENSOR#</SIMVAR_AMBIENT_LIGHT_SENSOR>
			<GET_AMBIENT_LIGHT_SENSOR>#GET_AMBIENT_LIGHT_SENSOR#</GET_AMBIENT_LIGHT_SENSOR>
			<GET_AUTOMATIC_BRIGHTNESS_FACTOR>@AutomaticBrightnessFactor</GET_AUTOMATIC_BRIGHTNESS_FACTOR>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
</ModelBehaviors>