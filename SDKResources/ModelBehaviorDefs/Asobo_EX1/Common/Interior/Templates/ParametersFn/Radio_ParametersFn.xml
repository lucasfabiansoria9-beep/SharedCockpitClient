	<ParametersFn Name="ASOBO_Frequency_Parameters">
		<Parameters Type="Default">
			<OUT_PARAM_NAME_CLAMP_FREQUENCY>CLAMP_FREQUENCY</OUT_PARAM_NAME_CLAMP_FREQUENCY>
		</Parameters>
		<Parameters Type="Override">
			<FREQ_RANGE Process="Int">#MAX_FREQ# #MIN_FREQ# -</FREQ_RANGE>
		</Parameters>
		<ReturnParameters>
			<#OUT_PARAM_NAME_CLAMP_FREQUENCY#>
				s0 #MIN_FREQ# #MAX_FREQ# l0 rng ! if{
					l0 #MAX_FREQ# &gt; if{
						l0 #MIN_FREQ# - #FREQ_RANGE# % #MIN_FREQ# +
					} els{
						#FREQ_RANGE# l0 #MIN_FREQ# - abs #FREQ_RANGE# % - #MIN_FREQ# +
					}
				} els{ l0 }
			</#OUT_PARAM_NAME_CLAMP_FREQUENCY#>
		</ReturnParameters>
	</ParametersFn>
