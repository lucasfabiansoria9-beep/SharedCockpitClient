<ModelBehaviors>
	<!-- ################################## DEICE ##################################### -->
	<!-- ############################################################################## -->
	<Include RelativeFile="InteractionPresets\\Deice_Interactions.xml"/>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Engine_Deice_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Param_Suffix_Helper"/>
		</Parameters>
		<Parameters Type="Default">
			<ENG_ID>1</ENG_ID>
		</Parameters>
		<Parameters Type="Default">
			<SIMVAR_ENG_DEICE>ENG ANTI ICE:#ENG_ID#</SIMVAR_ENG_DEICE>
		</Parameters>
		<Parameters Type="Default">
			<GET_ENG_DEICE>(A:#SIMVAR_ENG_DEICE#, Bool)</GET_ENG_DEICE>
			<GET_ENG_DEICE_POSITION>(A:#SIMVAR_ENG_DEICE#, position)</GET_ENG_DEICE_POSITION>
		</Parameters>
		<Parameters Type="Default">
			<TOGGLE_ENG_DEICE>(&gt;K:ANTI_ICE_TOGGLE_ENG#ENG_ID#)</TOGGLE_ENG_DEICE>
			<SET_ENG_DEICE>(&gt;K:ANTI_ICE_SET_ENG#ENG_ID#)</SET_ENG_DEICE>
			<SET_ENG_DEICE_16K>(&gt;K:ANTI_ICE_GRADUAL_SET_ENG#ENG_ID#)</SET_ENG_DEICE_16K>
		</Parameters>
		<ReturnParameters>
			<SIMVAR_ENG_DEICE#OUT_PARAM_SUFFIX#>#SIMVAR_ENG_DEICE#</SIMVAR_ENG_DEICE#OUT_PARAM_SUFFIX#>
			<GET_ENG_DEICE#OUT_PARAM_SUFFIX#>#GET_ENG_DEICE#</GET_ENG_DEICE#OUT_PARAM_SUFFIX#>
			<GET_ENG_DEICE_POSITION#OUT_PARAM_SUFFIX#>#GET_ENG_DEICE_POSITION#</GET_ENG_DEICE_POSITION#OUT_PARAM_SUFFIX#>
			<TOGGLE_ENG_DEICE#OUT_PARAM_SUFFIX#>#TOGGLE_ENG_DEICE#</TOGGLE_ENG_DEICE#OUT_PARAM_SUFFIX#>
			<SET_ENG_DEICE#OUT_PARAM_SUFFIX#>#SET_ENG_DEICE#</SET_ENG_DEICE#OUT_PARAM_SUFFIX#>
			<SET_ENG_DEICE_16K#OUT_PARAM_SUFFIX#>#SET_ENG_DEICE_16K#</SET_ENG_DEICE_16K#OUT_PARAM_SUFFIX#>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Structural_Deice_Parameters">
		<Parameters Type="Default">
			<SIMVAR_STRUCTURAL_DEICE>STRUCTURAL DEICE SWITCH</SIMVAR_STRUCTURAL_DEICE>
		</Parameters>
		<Parameters Type="Default">
			<GET_STRUCTURAL_DEICE>(A:#SIMVAR_STRUCTURAL_DEICE#, Bool)</GET_STRUCTURAL_DEICE>
		</Parameters>
		<Parameters Type="Default">
			<TOGGLE_STRUCTURAL_DEICE>(&gt;K:TOGGLE_STRUCTURAL_DEICE)</TOGGLE_STRUCTURAL_DEICE>
			<SET_STRUCTURAL_DEICE>#GET_STRUCTURAL_DEICE# != if{ (&gt;K:TOGGLE_STRUCTURAL_DEICE) }</SET_STRUCTURAL_DEICE>
		</Parameters>
		<ReturnParameters>
			<SIMVAR_STRUCTURAL_DEICE>#SIMVAR_STRUCTURAL_DEICE#</SIMVAR_STRUCTURAL_DEICE>
			<GET_STRUCTURAL_DEICE>#GET_STRUCTURAL_DEICE#</GET_STRUCTURAL_DEICE>
			<TOGGLE_STRUCTURAL_DEICE>#TOGGLE_STRUCTURAL_DEICE#</TOGGLE_STRUCTURAL_DEICE>
			<SET_STRUCTURAL_DEICE>#SET_STRUCTURAL_DEICE#</SET_STRUCTURAL_DEICE>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Windshield_Deice_Parameters">
		<Parameters Type="Default">
			<SIMVAR_WINDSHIELD_DEICE>WINDSHIELD DEICE SWITCH</SIMVAR_WINDSHIELD_DEICE>
		</Parameters>
		<Parameters Type="Default">
			<GET_WINDSHIELD_DEICE>(A:#SIMVAR_WINDSHIELD_DEICE#, Bool)</GET_WINDSHIELD_DEICE>
		</Parameters>
		<Parameters Type="Default">
			<TOGGLE_WINDSHIELD_DEICE>(&gt;K:WINDSHIELD_DEICE_TOGGLE)</TOGGLE_WINDSHIELD_DEICE>
			<SET_WINDSHIELD_DEICE>(&gt;K:WINDSHIELD_DEICE_SET)</SET_WINDSHIELD_DEICE>
		</Parameters>
		<ReturnParameters>
			<SIMVAR_WINDSHIELD_DEICE>#SIMVAR_WINDSHIELD_DEICE#</SIMVAR_WINDSHIELD_DEICE>
			<GET_WINDSHIELD_DEICE>#GET_WINDSHIELD_DEICE#</GET_WINDSHIELD_DEICE>
			<TOGGLE_WINDSHIELD_DEICE>#TOGGLE_WINDSHIELD_DEICE#</TOGGLE_WINDSHIELD_DEICE>
			<SET_WINDSHIELD_DEICE>#SET_WINDSHIELD_DEICE#</SET_WINDSHIELD_DEICE>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Pitot_Heat_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Param_Suffix_Helper"/>
		</Parameters>
		<Parameters Type="Default">
			<PITOT_ID>0</PITOT_ID>
		</Parameters>
		<Parameters Type="Default">
			<SIMVAR_PITOT_HEAT>PITOT HEAT SWITCH:#PITOT_ID#</SIMVAR_PITOT_HEAT>
			<SIMVAR_PITOT_HEAT_ON>PITOT HEAT</SIMVAR_PITOT_HEAT_ON>
		</Parameters>
		<Parameters Type="Default">
			<GET_PITOT_HEAT>(A:#SIMVAR_PITOT_HEAT#, Bool)</GET_PITOT_HEAT>
			<GET_PITOT_HEAT_ON>(A:#SIMVAR_PITOT_HEAT_ON#, Bool)</GET_PITOT_HEAT_ON>
		</Parameters>
		<Parameters Type="Default">
			<TOGGLE_PITOT_HEAT>#PITOT_ID# (&gt;K:PITOT_HEAT_TOGGLE)</TOGGLE_PITOT_HEAT>
			<SET_PITOT_HEAT>#GET_PITOT_HEAT# != if{ #PITOT_ID# (&gt;K:PITOT_HEAT_TOGGLE) }</SET_PITOT_HEAT>
		</Parameters>
		<ReturnParameters>
			<SIMVAR_PITOT_HEAT#OUT_PARAM_SUFFIX#>#SIMVAR_PITOT_HEAT#</SIMVAR_PITOT_HEAT#OUT_PARAM_SUFFIX#>
			<SIMVAR_PITOT_HEAT_ON>#SIMVAR_PITOT_HEAT_ON#</SIMVAR_PITOT_HEAT_ON>
			<GET_PITOT_HEAT#OUT_PARAM_SUFFIX#>#GET_PITOT_HEAT#</GET_PITOT_HEAT#OUT_PARAM_SUFFIX#>
			<GET_PITOT_HEAT_ON>#GET_PITOT_HEAT_ON#</GET_PITOT_HEAT_ON>
			<TOGGLE_PITOT_HEAT#OUT_PARAM_SUFFIX#>#TOGGLE_PITOT_HEAT#</TOGGLE_PITOT_HEAT#OUT_PARAM_SUFFIX#>
			<SET_PITOT_HEAT#OUT_PARAM_SUFFIX#>#SET_PITOT_HEAT#</SET_PITOT_HEAT#OUT_PARAM_SUFFIX#>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
</ModelBehaviors>