<ModelBehaviors>
	<!-- ################################## LIGHTING ################################## -->
	<!-- ############################################################################## -->
	<Include RelativeFile="InteractionPresets\\Lighting_Interactions.xml"/>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Potentiometer_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Param_Suffix_Helper"/>
		</Parameters>
		<Parameters Type="Default">
			<POTENTIOMETER_ID>1</POTENTIOMETER_ID>
			<UNITS_POTENTIOMETER>Percent</UNITS_POTENTIOMETER>
		</Parameters>
		<Parameters Type="Default">
			<SIMVAR_POTENTIOMETER>LIGHT POTENTIOMETER:#POTENTIOMETER_ID#</SIMVAR_POTENTIOMETER>
		</Parameters>
		<Parameters Type="Default">
			<SET_POTENTIOMETER>#POTENTIOMETER_ID# (&gt;K:2:LIGHT_POTENTIOMETER_SET)</SET_POTENTIOMETER>
			<GET_POTENTIOMETER>(A:LIGHT POTENTIOMETER:#POTENTIOMETER_ID#, #UNITS_POTENTIOMETER#)</GET_POTENTIOMETER>
		</Parameters>
		<ReturnParameters>
			<SET_POTENTIOMETER#OUT_PARAM_SUFFIX#>#SET_POTENTIOMETER#</SET_POTENTIOMETER#OUT_PARAM_SUFFIX#>
			<GET_POTENTIOMETER#OUT_PARAM_SUFFIX#>#GET_POTENTIOMETER#</GET_POTENTIOMETER#OUT_PARAM_SUFFIX#>
			<SIMVAR_POTENTIOMETER#OUT_PARAM_SUFFIX#>#SIMVAR_POTENTIOMETER#</SIMVAR_LIGHT#OUT_PARAM_SUFFIX#>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<!-- Lights -->
	<ParametersFn Name="ASOBO_Light_Default_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Param_Suffix_Helper"/>
		</Parameters>
		<Parameters Type="Default">
			<LIGHT_ID>1</LIGHT_ID>
		</Parameters>
		<Parameters Type="Default">
			<SIMVAR_LIGHT_SWITCH>LIGHT #LIGHT_TYPE#:#LIGHT_ID#</SIMVAR_LIGHT_SWITCH>
			<SIMVAR_LIGHT_ON>LIGHT #LIGHT_TYPE# ON:#LIGHT_ID#</SIMVAR_LIGHT_ON>
			<SIMVAR_LIGHT_POWER_SETTING>LIGHT #LIGHT_TYPE# POWER SETTING:#LIGHT_ID#</SIMVAR_LIGHT_POWER_SETTING>
			<UNITS_LIGHT_INTENSITY>Percent</UNITS_LIGHT_INTENSITY>
			<KEY_ID_TOGGLE>TOGGLE_#LIGHT_TYPE#_LIGHTS</KEY_ID_TOGGLE>
			<KEY_ID_SET>#LIGHT_TYPE#_LIGHTS_SET</KEY_ID_SET>
		</Parameters>
		<Parameters Type="Default">
			<GET_LIGHT>(A:#SIMVAR_LIGHT_SWITCH#, Bool)</GET_LIGHT>
			<GET_LIGHT_ON>(A:#SIMVAR_LIGHT_ON#, Bool)</GET_LIGHT_ON>
			<GET_LIGHT_POWER_SETTING>(A:#SIMVAR_LIGHT_POWER_SETTING#, #UNITS_LIGHT_INTENSITY#)</GET_LIGHT_POWER_SETTING>
		</Parameters>
		<Parameters Type="Default">
			<TOGGLE_LIGHT>#LIGHT_ID# (&gt;K:#KEY_ID_TOGGLE#)</TOGGLE_LIGHT>
			<SET_LIGHT>#GET_LIGHT# != if{ #LIGHT_ID# (&gt;K:#KEY_ID_TOGGLE#) }</SET_LIGHT>
			<SET_LIGHT_POWER_SETTING>#LIGHT_ID# (&gt;K:2:#LIGHT_TYPE#_LIGHTS_POWER_SETTING_SET)</SET_LIGHT_POWER_SETTING>
		</Parameters>
		<Parameters Type="Default">
			<Switch>
				<Case NotEmpty="LIGHT_POTENTIOMETER">
					<SET_LIGHT_INTENSITY>#LIGHT_POTENTIOMETER# (&gt;K:2:LIGHT_POTENTIOMETER_SET)</SET_LIGHT_INTENSITY>
					<GET_LIGHT_INTENSITY>(A:LIGHT POTENTIOMETER:#LIGHT_POTENTIOMETER#, #UNITS_LIGHT_INTENSITY#)</GET_LIGHT_INTENSITY>
					<SIMVAR_LIGHT_INTENSITY>LIGHT POTENTIOMETER:#LIGHT_POTENTIOMETER#</SIMVAR_LIGHT_INTENSITY>
				</Case>
				<Default>
					<SET_LIGHT_INTENSITY>#SET_LIGHT_POWER_SETTING#</SET_LIGHT_INTENSITY>
					<GET_LIGHT_INTENSITY>#GET_LIGHT_POWER_SETTING#</GET_LIGHT_INTENSITY>
					<SIMVAR_LIGHT_INTENSITY>#SIMVAR_LIGHT_POWER_SETTING#</SIMVAR_LIGHT_INTENSITY>
				</Default>
			</Switch>
		</Parameters>
		<ReturnParameters>
			<TOGGLE_LIGHT#OUT_PARAM_SUFFIX#>#TOGGLE_LIGHT#</TOGGLE_LIGHT#OUT_PARAM_SUFFIX#>
			<SET_LIGHT#OUT_PARAM_SUFFIX#>#SET_LIGHT#</SET_LIGHT#OUT_PARAM_SUFFIX#>
			<GET_LIGHT#OUT_PARAM_SUFFIX#>#GET_LIGHT#</GET_LIGHT#OUT_PARAM_SUFFIX#>
			<GET_LIGHT#OUT_PARAM_SUFFIX#_ON>#GET_LIGHT_ON#</GET_LIGHT#OUT_PARAM_SUFFIX#_ON>
			<SET_LIGHT#OUT_PARAM_SUFFIX#_INTENSITY>#SET_LIGHT_INTENSITY#</SET_LIGHT#OUT_PARAM_SUFFIX#_INTENSITY>
			<GET_LIGHT#OUT_PARAM_SUFFIX#_INTENSITY>#GET_LIGHT_INTENSITY#</GET_LIGHT#OUT_PARAM_SUFFIX#_INTENSITY>
			<SIMVAR_LIGHT#OUT_PARAM_SUFFIX#_SWITCH>#SIMVAR_LIGHT_SWITCH#</SIMVAR_LIGHT#OUT_PARAM_SUFFIX#_SWITCH>
			<SIMVAR_LIGHT#OUT_PARAM_SUFFIX#_ON>#SIMVAR_LIGHT_ON#</SIMVAR_LIGHT#OUT_PARAM_SUFFIX#_ON>
			<SIMVAR_LIGHT#OUT_PARAM_SUFFIX#_POWER_SETTING>#SIMVAR_LIGHT_POWER_SETTING#</SIMVAR_LIGHT#OUT_PARAM_SUFFIX#_POWER_SETTING>
			<SIMVAR_LIGHT#OUT_PARAM_SUFFIX#_INTENSITY>#SIMVAR_LIGHT_INTENSITY#</SIMVAR_LIGHT#OUT_PARAM_SUFFIX#_INTENSITY>
		</ReturnParameters>
	</ParametersFn>
