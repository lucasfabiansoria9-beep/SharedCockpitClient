<ModelBehaviors>
	<!-- ################################## INSTRUMENT ################################ -->
	<!-- ############################################################################## -->
	<Include RelativeFile="InteractionPresets\\Instrument_Interactions.xml"/>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Instrument_Flag_Animation_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Instrument_Failure_Animation_Parameters"/>
		</Parameters>
		<ReturnParameters>
			<SHOW_FLAG_CONDITION>#IS_INSTRUMENT_FAILING#</SHOW_FLAG_CONDITION>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Instrument_Failure_Animation_Parameters">
		<Parameters Type="Default">
			<INSTRUMENT_TYPE>Pressurized</INSTRUMENT_TYPE>
			<MIN_VOLTAGE>1</MIN_VOLTAGE>
		</Parameters>
		<Parameters Type="Default">
			<Switch Param="INSTRUMENT_TYPE">
				<Case Value="Pressurized">
					<FAILURE>(A:SUCTION PRESSURE, inhg) 2.3 &lt;</FAILURE>
				</Case>
				<Case Value="Electrical">
					<Condition Check="INSTRUMENT_CIRCUIT_ID">
						<True>
							<FAILURE>(A:CIRCUIT ON:#INSTRUMENT_CIRCUIT_ID#, Bool) !</FAILURE>
						</True>
						<False>
							<FAILURE>(A:ELECTRICAL MAIN BUS VOLTAGE, Volts) #MIN_VOLTAGE# &lt;</FAILURE>
						</False>
					</Condition>
				</Case>
			</Switch>
		</Parameters>
		<ReturnParameters>
			<IS_INSTRUMENT_FAILING>#FAILURE#</IS_INSTRUMENT_FAILING>
			<IS_INSTRUMENT_WORKING>#FAILURE# !</IS_INSTRUMENT_WORKING>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<Macro Name="millibar_to_inHg">33.863886667 /</Macro>
	<Macro Name="inHg_to_millibar">33.863886667 *</Macro>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Altimeter_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Param_Suffix_Helper"/>
		</Parameters>
		<Parameters Type="Default">
			<ALTIMETER_ID>0</ALTIMETER_ID>
		</Parameters>
		<Parameters Type="Default">
			<SIMVAR_ALTIMETER_ALTITUDE>INDICATED ALTITUDE:#ALTIMETER_ID#</SIMVAR_ALTIMETER_ALTITUDE>
			<SIMVAR_RADAR_ALTIMETER_ALTITUDE>PLANE ALT ABOVE GROUND:#ALTIMETER_ID#</SIMVAR_RADAR_ALTIMETER_ALTITUDE>
			<SIMVAR_ALTIMETER_SETTING>KOHLSMAN SETTING MB:#ALTIMETER_ID#</SIMVAR_ALTIMETER_SETTING>
			<SIMVAR_ALTIMETER_STANDARD_MODE>KOHLSMAN SETTING STD:#ALTIMETER_ID#</SIMVAR_ALTIMETER_STANDARD_MODE>
			<UNITS_ALTIMETER_ALTITUDE>Feet</UNITS_ALTIMETER_ALTITUDE>
			<UNITS_ALTIMETER_SETTING>inHg</UNITS_ALTIMETER_SETTING>
		</Parameters>
		<Parameters Type="Default">
			<GET_ALTIMETER_ALTITUDE>(A:#SIMVAR_ALTIMETER_ALTITUDE#, #UNITS_ALTIMETER_ALTITUDE#)</GET_ALTIMETER_ALTITUDE>
			<GET_ALTIMETER_RADAR_ALTITUDE>(A:#SIMVAR_RADAR_ALTIMETER_ALTITUDE#, #UNITS_ALTIMETER_ALTITUDE#)</GET_ALTIMETER_RADAR_ALTITUDE>
			<GET_ALTIMETER_SETTING>(A:#SIMVAR_ALTIMETER_SETTING#, #UNITS_ALTIMETER_SETTING#)</GET_ALTIMETER_SETTING>
			<GET_ALTIMETER_SETTING_INHG>(A:#SIMVAR_ALTIMETER_SETTING#, inHg)</GET_ALTIMETER_SETTING_INHG>
			<GET_ALTIMETER_SETTING_MB>(A:#SIMVAR_ALTIMETER_SETTING#, Millibar)</GET_ALTIMETER_SETTING_MB>
			<GET_ALTIMETER_STANDARD_MODE>(A:#SIMVAR_ALTIMETER_STANDARD_MODE#, Bool)</GET_ALTIMETER_STANDARD_MODE>
		</Parameters>
		<Parameters Type="Default">
			<SET_ALTIMETER_ALTITUDE_STD>#ALTIMETER_ID# (&gt;K:BAROMETRIC_STD_PRESSURE)</SET_ALTIMETER_ALTITUDE_STD>
			<SET_ALTIMETER_STANDARD_MODE>(&gt;A:#SIMVAR_ALTIMETER_STANDARD_MODE#, Bool)</SET_ALTIMETER_STANDARD_MODE>
			<SET_ALTIMETER_SETTING_MB>sp0 #ALTIMETER_ID# l0 16 * (&gt;K:2:KOHLSMAN_SET)</SET_ALTIMETER_SETTING_MB>
			<SET_ALTIMETER_SETTING_HG>@inHg_to_millibar sp0 #ALTIMETER_ID# l0 16 * (&gt;K:2:KOHLSMAN_SET)</SET_ALTIMETER_SETTING_HG>
		</Parameters>
		<ReturnParameters>
			<GET_ALTIMETER#OUT_PARAM_SUFFIX#_ALTITUDE>#GET_ALTIMETER_ALTITUDE#</GET_ALTIMETER#OUT_PARAM_SUFFIX#_ALTITUDE>
			<GET_ALTIMETER#OUT_PARAM_SUFFIX#_RADAR_ALTITUDE>#GET_ALTIMETER_RADAR_ALTITUDE#</GET_ALTIMETER#OUT_PARAM_SUFFIX#_ALTITUDE>
			<GET_ALTIMETER#OUT_PARAM_SUFFIX#_SETTING>#GET_ALTIMETER_SETTING#</GET_ALTIMETER#OUT_PARAM_SUFFIX#_SETTING>
			<GET_ALTIMETER#OUT_PARAM_SUFFIX#_STANDARD_MODE>#GET_ALTIMETER_STANDARD_MODE#</GET_ALTIMETER#OUT_PARAM_SUFFIX#_STANDARD_MODE>
			<GET_ALTIMETER#OUT_PARAM_SUFFIX#_SETTING_MB>#GET_ALTIMETER_SETTING_MB#</GET_ALTIMETER#OUT_PARAM_SUFFIX#_SETTING_MB>
			<GET_ALTIMETER#OUT_PARAM_SUFFIX#_SETTING_INHG>#GET_ALTIMETER_SETTING_INHG#</GET_ALTIMETER#OUT_PARAM_SUFFIX#_SETTING_INHG>
			<SIMVAR_ALTIMETER#OUT_PARAM_SUFFIX#_ALTITUDE>#SIMVAR_ALTIMETER_ALTITUDE#</SIMVAR_ALTIMETER#OUT_PARAM_SUFFIX#_ALTITUDE>
			<SIMVAR_ALTIMETER#OUT_PARAM_SUFFIX#_SETTING>#SIMVAR_ALTIMETER_SETTING#</SIMVAR_ALTIMETER#OUT_PARAM_SUFFIX#_SETTING>
			<SIMVAR_ALTIMETER#OUT_PARAM_SUFFIX#_STANDARD_MODE>#SIMVAR_ALTIMETER_STANDARD_MODE#</SIMVAR_ALTIMETER#OUT_PARAM_SUFFIX#_STANDARD_MODE>
			<SET_ALTIMETER#OUT_PARAM_SUFFIX#_ALTITUDE_STD>#SET_ALTIMETER_ALTITUDE_STD#</SET_ALTIMETER#OUT_PARAM_SUFFIX#_ALTITUDE_STD>
			<SET_ALTIMETER#OUT_PARAM_SUFFIX#_STANDARD_MODE>#SET_ALTIMETER_STANDARD_MODE#</SET_ALTIMETER#OUT_PARAM_SUFFIX#_STANDARD_MODE>
			<SET_ALTIMETER#OUT_PARAM_SUFFIX#_SETTING_MB>#SET_ALTIMETER_SETTING_MB#</SET_ALTIMETER#OUT_PARAM_SUFFIX#_SETTING_MB>
			<SET_ALTIMETER#OUT_PARAM_SUFFIX#_SETTING_HG>#SET_ALTIMETER_SETTING_HG#</SET_ALTIMETER#OUT_PARAM_SUFFIX#_SETTING_HG>
		</ReturnParameters>
	</ParametersFn>
