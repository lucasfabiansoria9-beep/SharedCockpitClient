			<GET_ATTITUDE_CAGE>(A:#SIMVAR_ATTITUDE_CAGE#, Bool)</GET_ATTITUDE_CAGE>
		</Parameters>
		<Parameters Type="Default">
			<TOGGLE_ATTITUDE_CAGE>(&gt;K:ATTITUDE_CAGE_BUTTON)</TOGGLE_ATTITUDE_CAGE>
			<SET_ATTITUDE_CAGE>#GET_ATTITUDE_CAGE# != if{ (&gt;K:ATTITUDE_CAGE_BUTTON) }</SET_ATTITUDE_CAGE>
			<SET_ATTITUDE_BARS>#INDICATOR_ID# (&gt;K:2:ATTITUDE_BARS_POSITION_SET)</SET_ATTITUDE_BARS>
		</Parameters>
		<ReturnParameters>
			<GET_ATTITUDE_INDICATOR#OUT_PARAM_SUFFIX#_PITCH>#GET_ATTITUDE_INDICATOR_PITCH#</GET_ATTITUDE_INDICATOR#OUT_PARAM_SUFFIX#_PITCH>
			<GET_ATTITUDE_INDICATOR#OUT_PARAM_SUFFIX#_BANK>#GET_ATTITUDE_INDICATOR_BANK#</GET_ATTITUDE_INDICATOR#OUT_PARAM_SUFFIX#_BANK>
