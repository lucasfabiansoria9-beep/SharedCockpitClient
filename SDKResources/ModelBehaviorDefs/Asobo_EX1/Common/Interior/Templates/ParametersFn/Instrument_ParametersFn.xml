			<GET_ATTITUDE#OUT_PARAM_SUFFIX#_CAGE>#GET_ATTITUDE_CAGE#</GET_ATTITUDE#OUT_PARAM_SUFFIX#_CAGE>
			<SIMVAR_ATTITUDE_INDICATOR#OUT_PARAM_SUFFIX#_PITCH>#SIMVAR_ATTITUDE_INDICATOR_PITCH#</SIMVAR_ATTITUDE_INDICATOR#OUT_PARAM_SUFFIX#_PITCH>
			<SIMVAR_ATTITUDE_INDICATOR#OUT_PARAM_SUFFIX#_BANK>#SIMVAR_ATTITUDE_INDICATOR_BANK#</SIMVAR_ATTITUDE_INDICATOR#OUT_PARAM_SUFFIX#_BANK>
