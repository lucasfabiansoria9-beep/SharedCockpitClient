			<SIMVAR_ATTITUDE_INDICATOR_BANK>ATTITUDE INDICATOR BANK DEGREES</SIMVAR_ATTITUDE_INDICATOR_BANK>
			<SIMVAR_ATTITUDE_INDICATOR_BARS>ATTITUDE BARS POSITION</SIMVAR_ATTITUDE_INDICATOR_BARS>
			<SIMVAR_ATTITUDE_CAGE>ATTITUDE CAGE</SIMVAR_ATTITUDE_CAGE>
			<UNITS_ATTITUDE_INDICATOR_PITCH>Degrees</UNITS_ATTITUDE_INDICATOR_PITCH>
			<UNITS_ATTITUDE_INDICATOR_BANK>Degrees</UNITS_ATTITUDE_INDICATOR_BANK>
			<UNITS_ATTITUDE_INDICATOR_BARS>Percent</UNITS_ATTITUDE_INDICATOR_BARS>
		</Parameters>
		<Parameters Type="Default">
			<GET_ATTITUDE_INDICATOR_PITCH>(A:#SIMVAR_ATTITUDE_INDICATOR_PITCH#, #UNITS_ATTITUDE_INDICATOR_PITCH#)</GET_ATTITUDE_INDICATOR_PITCH>
