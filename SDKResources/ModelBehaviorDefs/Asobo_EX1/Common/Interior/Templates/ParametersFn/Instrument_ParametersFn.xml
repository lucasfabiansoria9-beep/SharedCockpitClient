			<TOGGLE_ATTITUDE#OUT_PARAM_SUFFIX#_CAGE>#TOGGLE_ATTITUDE_CAGE#</TOGGLE_ATTITUDE#OUT_PARAM_SUFFIX#_CAGE>
			<SET_ATTITUDE#OUT_PARAM_SUFFIX#_CAGE>#SET_ATTITUDE_CAGE#</SET_ATTITUDE#OUT_PARAM_SUFFIX#_CAGE>
			<SET_ATTITUDE#OUT_PARAM_SUFFIX#_BARS>#SET_ATTITUDE_BARS#</SET_ATTITUDE#OUT_PARAM_SUFFIX#_BARS>
		</ReturnParameters>
