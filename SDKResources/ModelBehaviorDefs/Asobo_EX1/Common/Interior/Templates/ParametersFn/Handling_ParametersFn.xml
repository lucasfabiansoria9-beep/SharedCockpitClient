	<ParametersFn Name="ASOBO_Trim_Disconnect_Parameters">
		<Parameters Type="Default">
			<SIMVAR_TRIM_DISCONNECTED>#TRIM_TYPE# TRIM DISABLED</SIMVAR_TRIM_DISCONNECTED>
		</Parameters>
		<Parameters Type="Override">
			<GET_TRIM_DISCONNECTED>(A:#SIMVAR_TRIM_DISCONNECTED#, Bool)</GET_TRIM_DISCONNECTED>
			<SET_TRIM_DISCONNECTED>(&gt;K:#TRIM_TYPE#_TRIM_DISABLED_SET)</SET_TRIM_DISCONNECTED>
		</Parameters>
		<Parameters Type="Override">
			<Condition NotEmpty="GET_TRIM_DISCONNECTED_ALL">
				<True>
					<GET_TRIM_DISCONNECTED_APPEND>#GET_TRIM_DISCONNECTED# and</GET_TRIM_DISCONNECTED_APPEND>
					<SET_TRIM_DISCONNECTED_APPEND>d #SET_TRIM_DISCONNECTED#</SET_TRIM_DISCONNECTED_APPEND>
				</True>
				<False>
					<GET_TRIM_DISCONNECTED_APPEND>#GET_TRIM_DISCONNECTED#</GET_TRIM_DISCONNECTED_APPEND>
					<SET_TRIM_DISCONNECTED_APPEND>#SET_TRIM_DISCONNECTED#</SET_TRIM_DISCONNECTED_APPEND>
				</False>
			</Condition>
		</Parameters>
		<ReturnParameters>
			<SIMVAR_#TRIM_TYPE#_TRIM_DISCONNECTED>#SIMVAR_TRIM_DISCONNECTED#</SIMVAR_#TRIM_TYPE#_TRIM_DISCONNECTED>
			<GET_#TRIM_TYPE#_TRIM_DISCONNECTED>#GET_TRIM_DISCONNECTED#</GET_#TRIM_TYPE#_TRIM_DISCONNECTED>
			<SET_#TRIM_TYPE#_TRIM_DISCONNECTED>#SET_TRIM_DISCONNECTED#</SET_#TRIM_TYPE#_TRIM_DISCONNECTED>
			<UseParametersFn Name="ASOBO_Append_If_Necessary_Helper">
				<PARAM>GET_TRIM_DISCONNECTED_APPEND</PARAM>
				<OUT_PARAM_NAME>GET_TRIM_DISCONNECTED_ALL</OUT_PARAM_NAME>
			</UseParametersFn>
			<UseParametersFn Name="ASOBO_Append_If_Necessary_Helper">
				<PARAM>SET_TRIM_DISCONNECTED_APPEND</PARAM>
				<OUT_PARAM_NAME>SET_TRIM_DISCONNECTED_ALL</OUT_PARAM_NAME>
				<APPEND_BEFORE>True</APPEND_BEFORE>
			</UseParametersFn>
		</ReturnParameters>
	</ParametersFn>
