<ModelBehaviors>
	<!-- ################################## HANDLING ################################## -->
	<!-- ############################################################################## -->
	<Include RelativeFile="InteractionPresets\\Handling_Interactions.xml"/>
	<Include RelativeFile="InteractionPresets\\Autopilot_Interactions.xml"/>
	<!-- ############################################################################## -->
	<!-- Yoke/Stick -->
	<ParametersFn Name="ASOBO_Yoke_Animation_Parameters">
		<Parameters Type="Default">
			<IN_SIMVAR_UNITS>percent</IN_SIMVAR_UNITS>
			<OUT_SIMVAR_BIAS>100</OUT_SIMVAR_BIAS>
			<OUT_SIMVAR_SCALE>0.5</OUT_SIMVAR_SCALE>
			<Condition Valid="AP_CAN_DRIVE_ANIMATION">
				<True>
					<ANIM_SIMVAR_X>YOKE X POSITION WITH AP</ANIM_SIMVAR_X>
					<ANIM_SIMVAR_X_LINEAR>YOKE X POSITION WITH AP LINEAR</ANIM_SIMVAR_X_LINEAR>
					<ANIM_SIMVAR_Y>YOKE Y POSITION WITH AP</ANIM_SIMVAR_Y>
				</True>
				<False>
					<ANIM_SIMVAR_X>YOKE X POSITION</ANIM_SIMVAR_X>
					<ANIM_SIMVAR_X_LINEAR>YOKE X POSITION LINEAR</ANIM_SIMVAR_X_LINEAR>
					<ANIM_SIMVAR_Y>YOKE Y POSITION</ANIM_SIMVAR_Y>
				</False>
			</Condition>
			<ANIM_NAME_YOKE_X>HANDLING_YOKE_LEVER_STICK_L_R</ANIM_NAME_YOKE_X>
			<ANIM_NAME_YOKE_Y>HANDLING_YOKE_LEVER_STICK_FORE_AFT</ANIM_NAME_YOKE_Y>
		</Parameters>
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Transform_Simvar_Helper">
				<IN_SIMVAR>#ANIM_SIMVAR_X#</IN_SIMVAR>
				<OUT_PARAM_NAME>ANIM_SIMVAR_X_FORMATTED</OUT_PARAM_NAME>
				<FORMAT_SIMVAR>True</FORMAT_SIMVAR>
			</UseParametersFn>
			<UseParametersFn Name="ASOBO_Transform_Simvar_Helper">
				<IN_SIMVAR>#ANIM_SIMVAR_X_LINEAR#</IN_SIMVAR>
				<OUT_PARAM_NAME>ANIM_SIMVAR_X_LINEAR_FORMATTED</OUT_PARAM_NAME>
				<FORMAT_SIMVAR>True</FORMAT_SIMVAR>
			</UseParametersFn>
			<UseParametersFn Name="ASOBO_Transform_Simvar_Helper">
				<IN_SIMVAR>#ANIM_SIMVAR_Y#</IN_SIMVAR>
				<OUT_PARAM_NAME>ANIM_SIMVAR_Y_FORMATTED</OUT_PARAM_NAME>
				<FORMAT_SIMVAR>True</FORMAT_SIMVAR>
			</UseParametersFn>
			<GET_YOKE_X_POSITION>(A:#ANIM_SIMVAR_X#, #IN_SIMVAR_UNITS#)</GET_YOKE_X_POSITION>
			<GET_YOKE_Y_POSITION>(A:#ANIM_SIMVAR_Y#, #IN_SIMVAR_UNITS#)</GET_YOKE_Y_POSITION>
		</Parameters>
		<ReturnParameters>
			<ANIM_SIMVAR_X_FORMATTED>#ANIM_SIMVAR_X_FORMATTED#</ANIM_SIMVAR_X_FORMATTED>
			<ANIM_SIMVAR_X_LINEAR_FORMATTED>#ANIM_SIMVAR_X_LINEAR_FORMATTED#</ANIM_SIMVAR_X_LINEAR_FORMATTED>
			<ANIM_SIMVAR_Y_FORMATTED>#ANIM_SIMVAR_Y_FORMATTED#</ANIM_SIMVAR_Y_FORMATTED>
			<ANIM_NAME_YOKE_X>#ANIM_NAME_YOKE_X#</ANIM_NAME_YOKE_X>
			<ANIM_NAME_YOKE_Y>#ANIM_NAME_YOKE_Y#</ANIM_NAME_YOKE_Y>
			<GET_YOKE_X_POSITION>#GET_YOKE_X_POSITION#</GET_YOKE_X_POSITION>
			<GET_YOKE_Y_POSITION>#GET_YOKE_Y_POSITION#</GET_YOKE_Y_POSITION>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<!-- Rudder pedals -->
	<ParametersFn Name="ASOBO_Rudder_Pedals_Animation_Parameters">
		<Parameters Type="Default">
			<USE_BRAKES>False</USE_BRAKES>
			<SIMVAR_RUDDER_PEDALS>RUDDER PEDAL POSITION</SIMVAR_RUDDER_PEDALS>
			<ANIM_SIMVAR_UNITS_PEDALS>percent</ANIM_SIMVAR_UNITS_PEDALS>
			<ANIM_SIMVAR_BIAS_PEDALS>100</ANIM_SIMVAR_BIAS_PEDALS>
			<ANIM_SIMVAR_SCALE_PEDALS>0.5</ANIM_SIMVAR_SCALE_PEDALS>
		</Parameters>
		<ReturnParameters>
			<USE_BRAKES>#USE_BRAKES#</USE_BRAKES>
			<SIMVAR_RUDDER_PEDALS>#SIMVAR_RUDDER_PEDALS#</SIMVAR_RUDDER_PEDALS>
			<ANIM_SIMVAR_UNITS_PEDALS>#ANIM_SIMVAR_UNITS_PEDALS#</ANIM_SIMVAR_UNITS_PEDALS>
			<ANIM_SIMVAR_BIAS_PEDALS>#ANIM_SIMVAR_BIAS_PEDALS#</ANIM_SIMVAR_BIAS_PEDALS>
			<ANIM_SIMVAR_SCALE_PEDALS>#ANIM_SIMVAR_SCALE_PEDALS#</ANIM_SIMVAR_SCALE_PEDALS>
		</ReturnParameters>
	</ParametersFn>
	<ParametersFn Name="ASOBO_Rudder_Pedals_Common_Parameters">
		<Parameters Type="Default">
			<NODE_ID_LEFT>#IN_BASE_NAME#_1#SUFFIX_ID#</NODE_ID_LEFT>
			<NODE_ID_RIGHT>#IN_BASE_NAME#_2#SUFFIX_ID#</NODE_ID_RIGHT>
			<ANIM_NAME>#IN_BASE_NAME#</ANIM_NAME>
			<PART_ID>#IN_BASE_NAME#</PART_ID>
			<Condition Valid="USE_BRAKES">
				<Condition Valid="DIFFERENTIATE_BRAKE_ANIM_L_R">
					<True>
						<BRAKE_ANIM_NAME_L>#IN_BASE_NAME#_Brake_L#SUFFIX_ID#</BRAKE_ANIM_NAME_L>
						<BRAKE_ANIM_NAME_R>#IN_BASE_NAME#_Brake_R#SUFFIX_ID#</BRAKE_ANIM_NAME_R>
					</True>
					<False>
						<BRAKE_ANIM_NAME>#IN_BASE_NAME#_Brakes#SUFFIX_ID#</BRAKE_ANIM_NAME>
					</False>
				</Condition>
			</Condition>
		</Parameters>
		<ReturnParameters>
			<NODE_ID_LEFT>#NODE_ID_LEFT#</NODE_ID_LEFT>
			<NODE_ID_RIGHT>#NODE_ID_RIGHT#</NODE_ID_RIGHT>
			<ANIM_NAME>#ANIM_NAME#</ANIM_NAME>
			<PART_ID>#PART_ID#</PART_ID>
			<Condition Valid="USE_BRAKES">
				<Condition>
					<Test>
						<And>
							<Arg NotEmpty="BRAKE_ANIM_NAME_L"/>
							<Arg NotEmpty="BRAKE_ANIM_NAME_R"/>
						</And>
					</Test>
					<True>
						<BRAKE_ANIM_NAME_L>#BRAKE_ANIM_NAME_L#</BRAKE_ANIM_NAME_L>
						<BRAKE_ANIM_NAME_R>#BRAKE_ANIM_NAME_R#</BRAKE_ANIM_NAME_R>
						<DIFFERENTIATE_BRAKE_ANIM_L_R>True</DIFFERENTIATE_BRAKE_ANIM_L_R>
					</True>
					<False>
						<BRAKE_ANIM_NAME>#BRAKE_ANIM_NAME#</BRAKE_ANIM_NAME>
						<DIFFERENTIATE_BRAKE_ANIM_L_R>False</DIFFERENTIATE_BRAKE_ANIM_L_R>
					</False>
				</Condition>
			</Condition>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Brakes_Animation_Parameters">
		<Parameters Type="Default">
			<ANIM_SIMVAR_UNITS_BRAKES>Percent</ANIM_SIMVAR_UNITS_BRAKES>
			<ANIM_SIMVAR_BIAS_BRAKES>0</ANIM_SIMVAR_BIAS_BRAKES>
			<ANIM_SIMVAR_SCALE_BRAKES>1</ANIM_SIMVAR_SCALE_BRAKES>
		</Parameters>
		<Parameters Type="Default">
			<Condition Valid="DONT_BRAKE_ON_PARKINGBRAKE">
				<True>
					<SIMVAR_RUDDER_PEDALS_BRAKE_L>BRAKE LEFT POSITION EX1</SIMVAR_RUDDER_PEDALS_BRAKE_L>
					<SIMVAR_RUDDER_PEDALS_BRAKE_R>BRAKE RIGHT POSITION EX1</SIMVAR_RUDDER_PEDALS_BRAKE_R>
				</True>
				<False>
					<SIMVAR_RUDDER_PEDALS_BRAKE_L>BRAKE LEFT POSITION</SIMVAR_RUDDER_PEDALS_BRAKE_L>
					<SIMVAR_RUDDER_PEDALS_BRAKE_R>BRAKE RIGHT POSITION</SIMVAR_RUDDER_PEDALS_BRAKE_R>
				</False>
			</Condition>
		</Parameters>
		<ReturnParameters>
			<DIFFERENTIATE_BRAKE_ANIM_L_R>#DIFFERENTIATE_BRAKE_ANIM_L_R#</DIFFERENTIATE_BRAKE_ANIM_L_R>
			<Condition Valid="DIFFERENTIATE_BRAKE_ANIM_L_R">
				<True>
					<SIMVAR_RUDDER_PEDALS_BRAKE_L>#SIMVAR_RUDDER_PEDALS_BRAKE_L#</SIMVAR_RUDDER_PEDALS_BRAKE_L>
					<SIMVAR_RUDDER_PEDALS_BRAKE_R>#SIMVAR_RUDDER_PEDALS_BRAKE_R#</SIMVAR_RUDDER_PEDALS_BRAKE_R>
				</True>
				<False>
					<SIMVAR_RUDDER_PEDALS_BRAKE>#SIMVAR_RUDDER_PEDALS_BRAKE_L#</SIMVAR_RUDDER_PEDALS_BRAKE>
				</False>
			</Condition>
			<ANIM_SIMVAR_UNITS_BRAKES>#ANIM_SIMVAR_UNITS_BRAKES#</ANIM_SIMVAR_UNITS_BRAKES>
			<ANIM_SIMVAR_BIAS_BRAKES>#ANIM_SIMVAR_BIAS_BRAKES#</ANIM_SIMVAR_BIAS_BRAKES>
			<ANIM_SIMVAR_SCALE_BRAKES>#ANIM_SIMVAR_SCALE_BRAKES#</ANIM_SIMVAR_SCALE_BRAKES>
		</ReturnParameters>
	</ParametersFn>
