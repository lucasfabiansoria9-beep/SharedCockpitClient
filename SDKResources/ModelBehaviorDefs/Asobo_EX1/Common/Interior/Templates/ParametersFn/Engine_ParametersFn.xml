			<SIMVAR_ENG#OUT_PARAM_SUFFIX#_MAGNETO_PLASMA_SWITCH>#SIMVAR_ENG_MAGNETO_PLASMA_SWITCH#</SIMVAR_ENG#OUT_PARAM_SUFFIX#_MAGNETO_PLASMA_SWITCH>
			<GET_ENG#OUT_PARAM_SUFFIX#_MAGNETO_PLASMA_SWITCH>#GET_ENG_MAGNETO_PLASMA_SWITCH#</GET_ENG#OUT_PARAM_SUFFIX#_MAGNETO_PLASMA_SWITCH>
			<SET_ENG#OUT_PARAM_SUFFIX#_MAGNETO_PLASMA_SWITCH>#SET_ENG_MAGNETO_PLASMA_SWITCH#</SET_ENG#OUT_PARAM_SUFFIX#_MAGNETO_PLASMA_SWITCH>
		</ReturnParameters>
	</ParametersFn>
