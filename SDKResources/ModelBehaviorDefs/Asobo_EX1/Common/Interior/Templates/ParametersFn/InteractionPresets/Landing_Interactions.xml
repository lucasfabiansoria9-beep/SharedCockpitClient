<ModelBehaviors>
	<!-- ################################## LANDING ################################### -->
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Gears_Settings_Config">
		<Parameters Type="Default">
			<USE_SIMPLE_TOGGLE_INTERACTION>False</USE_SIMPLE_TOGGLE_INTERACTION>
		</Parameters>
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Gears_Handle_Parameters"/>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Auto_Define_XStates_Variant_Helper"/>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>#SIMVAR_GEAR_HANDLE_POSITION#</SIMVAR>
			</UseParametersFn>
			<!-- Check position -->
			<POS_UP_CHECK>#GET_GEAR_HANDLE_POSITION# !</POS_UP_CHECK>
			<POS_DOWN_CHECK>#GET_GEAR_HANDLE_POSITION#</POS_DOWN_CHECK>
			<!-- Set position -->
			<POS_UP_CODE>0 #SET_GEAR_HANDLE_POSITION#</POS_UP_CODE>
			<POS_DOWN_CODE>1 #SET_GEAR_HANDLE_POSITION#</POS_DOWN_CODE>
			
			<USE_SIMPLE_TOGGLE_INTERACTION>#USE_SIMPLE_TOGGLE_INTERACTION#</USE_SIMPLE_TOGGLE_INTERACTION>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Gears_Electric_Settings_Config">
		<Parameters Type="Default">
			<GEAR_ID>0</GEAR_ID>
		</Parameters>
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Gears_Handle_Parameters"/>
			<UseParametersFn Name="ASOBO_Line_Connection_Parameters"/>
			<UseParametersFn Name="ASOBO_Gear_Parameters"/>
			<VAR_FORCE_TO_UP>O:XMLVAR_Force_To_Up</VAR_FORCE_TO_UP>
			<VAR_RESET>L:1:XMLVAR_Landing_Gear_Switch_Reset</VAR_RESET>
			<VAR_LAST_UP>L:1:XMLVAR_Landing_Gear_Switch_Last_Up</VAR_LAST_UP>
		</Parameters>
		<Parameters Type="Override">
			<GET_VAR_LAST_UP>(#VAR_LAST_UP#)</GET_VAR_LAST_UP>
			<SET_VAR_LAST_UP>(&gt;#VAR_LAST_UP#)</SET_VAR_LAST_UP>

			<GET_VAR_FORCE_TO_UP>(#VAR_FORCE_TO_UP#)</GET_VAR_FORCE_TO_UP>
			<SET_VAR_FORCE_TO_UP>(&gt;#VAR_FORCE_TO_UP#)</SET_VAR_FORCE_TO_UP>
			<GET_VAR_RESET>(#VAR_RESET#)</GET_VAR_RESET>
			<SET_VAR_RESET>(&gt;#VAR_RESET#)</SET_VAR_RESET>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Auto_Define_XStates_Variant_Helper"/>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>#SIMVAR_GEAR_POSITION#</SIMVAR>
			</UseParametersFn>
			
			<!-- Check position -->
			<POS_UP_CHECK>#GET_GEAR_HANDLE_POSITION# ! #GET_CONNECTION# and #GET_VAR_FORCE_TO_UP# or</POS_UP_CHECK>
			<POS_NEUTRAL_CHECK>#GET_GEAR_HANDLE_POSITION# #GET_VAR_RESET# or #GET_CONNECTION# ! and #GET_VAR_FORCE_TO_UP# ! and</POS_NEUTRAL_CHECK>
			<POS_DOWN_CHECK>#GET_GEAR_HANDLE_POSITION# #GET_CONNECTION# and #GET_VAR_FORCE_TO_UP#  ! and</POS_DOWN_CHECK>
			<!-- Set position -->
			<POS_UP_CODE>0 #SET_GEAR_HANDLE_POSITION# 1 #SET_CONNECTION# 1 #SET_VAR_LAST_UP#</POS_UP_CODE>
			<POS_NEUTRAL_CODE>
				0 #SET_VAR_RESET#
				#GET_VAR_FORCE_TO_UP# if{ 
					#GET_GEAR_POSITION# 0 &gt; if{ g2 }
					0 #SET_VAR_FORCE_TO_UP# 
				}
				#GET_VAR_LAST_UP# if{ 
					0 #SET_CONNECTION#
					0 #SET_VAR_LAST_UP# 
					1 #SET_VAR_RESET# (* Required to delay setting the gear handle by one frame, to ensure the gears do not start going down before the gear motor is off *)
					g2
				}
				0 #SET_CONNECTION# 1 #SET_GEAR_HANDLE_POSITION#
				:2
			</POS_NEUTRAL_CODE>
			<POS_DOWN_CODE>1 #SET_GEAR_HANDLE_POSITION# 1 #SET_CONNECTION#</POS_DOWN_CODE>

			<UseParametersFn Name="ASOBO_Add_Set_Binding_Helper">
				<EVENT_ID>GEAR_TOGGLE</EVENT_ID>
				<PARAM_0>#GET_GEAR_POSITION# 0 &gt; #GET_VAR_FORCE_TO_UP# ! and if{ 1 #SET_VAR_FORCE_TO_UP# 2 } els{ 0 #SET_VAR_FORCE_TO_UP# 0 }</PARAM_0>
				<PARAM_0_IS_DYNAMIC>True</PARAM_0_IS_DYNAMIC>
			</UseParametersFn>
			<UseParametersFn Name="ASOBO_Add_Set_Binding_Helper">
				<EVENT_ID>GEAR_UP</EVENT_ID>
				<PARAM_0>1 #SET_VAR_FORCE_TO_UP# 2</PARAM_0>
				<PARAM_0_IS_DYNAMIC>True</PARAM_0_IS_DYNAMIC>
			</UseParametersFn>
			<UseParametersFn Name="ASOBO_Add_Set_Binding_Helper">
				<EVENT_ID>GEAR_DOWN</EVENT_ID>
				<PARAM_0>0 #SET_VAR_FORCE_TO_UP# 0</PARAM_0>
				<PARAM_0_IS_DYNAMIC>True</PARAM_0_IS_DYNAMIC>
			</UseParametersFn>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Emergency_Gears_Settings_Config">
		<Parameters Type="Default">
			<USE_SIMPLE_TOGGLE_INTERACTION>True</USE_SIMPLE_TOGGLE_INTERACTION>
		</Parameters>
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Gears_Handle_Parameters"/>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Auto_Define_XStates_Variant_Helper"/>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>#SIMVAR_GEAR_EMERGENCY_HANDLE_POSITION#</SIMVAR>
			</UseParametersFn>
			<!-- Check position -->
			<POS_UP_CHECK>#GET_GEAR_EMERGENCY_HANDLE_POSITION# !</POS_UP_CHECK>
			<POS_DOWN_CHECK>#GET_GEAR_EMERGENCY_HANDLE_POSITION#</POS_DOWN_CHECK>
			<!-- Set position -->
			<POS_UP_CODE>0 #SET_GEAR_EMERGENCY_HANDLE_POSITION#</POS_UP_CODE>
			<POS_DOWN_CODE>1 #SET_GEAR_EMERGENCY_HANDLE_POSITION#</POS_DOWN_CODE>
			
			<USE_SIMPLE_TOGGLE_INTERACTION>#USE_SIMPLE_TOGGLE_INTERACTION#</USE_SIMPLE_TOGGLE_INTERACTION>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Parking_Brake_Settings_Config">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Parking_Brake_Parameters"/>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Auto_Define_XStates_Variant_Helper"/>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>#SIMVAR_PARKING_BRAKE_POSITION#</SIMVAR>
			</UseParametersFn>
			<!-- Check position -->
			<POS_RELEASED_CHECK>#GET_PARKING_BRAKE_POSITION# !</POS_RELEASED_CHECK>
			<POS_ON_CHECK>#GET_PARKING_BRAKE_POSITION#</POS_ON_CHECK>
			<!-- Set position -->
			<POS_RELEASED_CODE>0 #SET_PARKING_BRAKE_POSITION#</POS_RELEASED_CODE>
			<POS_ON_CODE>1 #SET_PARKING_BRAKE_POSITION#</POS_ON_CODE>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
</ModelBehaviors>