	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Light_Intensity_Settings_Config_Base">
		<Parameters Type="Default">
			<ID>1</ID>
			<HAS_AUTHORITY_OVER_SWITCH_STATE>False</HAS_AUTHORITY_OVER_SWITCH_STATE>
		</Parameters>
		<Parameters Type="Default">
			<LIGHT_ID>#ID#</LIGHT_ID>
			<START_AT_DEFAULT_LIGHT_INTENSITY>True</START_AT_DEFAULT_LIGHT_INTENSITY>
			<DEFAULT_LIGHT_INTENSITY>50</DEFAULT_LIGHT_INTENSITY>
			<NB_INCREMENT_FOR_FULL_RANGE>100</NB_INCREMENT_FOR_FULL_RANGE>
			<PFN_DELTA_THRESHOLD_VALUES>ASOBO_Drag_Threshold_Medium</PFN_DELTA_THRESHOLD_VALUES>
		</Parameters>
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Light_#LIGHT_TYPE_ID#_Parameters"/>
		</Parameters>
		<Parameters Type="Default">
			<Condition Valid="HAS_AUTHORITY_OVER_SWITCH_STATE">
				<SYNC_LIGHT_SWITCH_STATE>#GET_LIGHT# s0 #GET_LIGHT_INTENSITY# 0 &gt; != if{ l0 #DEFAULT_LIGHT_INTENSITY# * #SET_LIGHT_INTENSITY# }</SYNC_LIGHT_SWITCH_STATE>
			</Condition>
		</Parameters>
		<ReturnParameters>
			<VALUE_TYPE>PERCENT</VALUE_TYPE>
			<IE_GET_STATE>#GET_LIGHT_INTENSITY#</IE_GET_STATE>
			<!-- Key cannot handle lower values than 1% delta -->
			<NB_INCREMENT_FOR_FULL_RANGE Process="Int">#NB_INCREMENT_FOR_FULL_RANGE# 100 min</NB_INCREMENT_FOR_FULL_RANGE>
			<PFN_DELTA_THRESHOLD_VALUES>#PFN_DELTA_THRESHOLD_VALUES#</PFN_DELTA_THRESHOLD_VALUES>
			<Condition Valid="HAS_AUTHORITY_OVER_SWITCH_STATE">
				<True>
					<UseParametersFn Name="ASOBO_Watch_Simvars_Helper">
						<SaveParameters ID="Temp_Simvars_To_Watch">
							<SIMVAR_LIGHT_INTENSITY>#SIMVAR_LIGHT_INTENSITY#</SIMVAR_LIGHT_INTENSITY>
							<SIMVAR_LIGHT_SWITCH>#SIMVAR_LIGHT_SWITCH#</SIMVAR_LIGHT_SWITCH>
						</SaveParameters>
					</UseParametersFn>
					<UseParametersFn Name="ASOBO_Append_Init_Code_Helper">
						<INIT_CODE>#SYNC_LIGHT_SWITCH_STATE#</INIT_CODE>
					</UseParametersFn>
					<IE_SET_STATE>s0 #SET_LIGHT_INTENSITY# l0 near 0 &gt; #SET_LIGHT#</IE_SET_STATE>
				</True>
				<False>
					<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
						<SIMVAR>#SIMVAR_LIGHT_INTENSITY#</SIMVAR>
					</UseParametersFn>
					<IE_SET_STATE>#SET_LIGHT_INTENSITY#</IE_SET_STATE>
				</False>
			</Condition>
			<Condition Valid="START_AT_DEFAULT_LIGHT_INTENSITY">
				<UseParametersFn Name="ASOBO_Add_Update_Helper">
					<UPDATE_CODE>#DEFAULT_LIGHT_INTENSITY# #GET_LIGHT# * #SET_LIGHT_INTENSITY#</UPDATE_CODE>
					<UPDATE_ONCE>True</UPDATE_ONCE>
				</UseParametersFn>
			</Condition>
			<!-- Light category -->
			<SaveParameters ID="#IE_PARAMS#" Append="Default">
				<TT_LIGHT_CATEGORY>#TT_LIGHT_CATEGORY#</TT_LIGHT_CATEGORY>
			</SaveParameters>
		</ReturnParameters>
	</ParametersFn>
