	<!-- ################################## LIGHTING ################################## -->
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Light_State_Settings_Config_Base">
		<Parameters Type="Default">
			<ID>1</ID>
		</Parameters>
		<Parameters Type="Default">
			<LIGHT_ID>#ID#</LIGHT_ID>
		</Parameters>
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Light_#LIGHT_TYPE_ID#_Parameters"/>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Auto_Define_XStates_Variant_Helper"/>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>#SIMVAR_LIGHT_SWITCH#</SIMVAR>
			</UseParametersFn>
			<!-- Check position -->
			<POS_OFF_CHECK>#GET_LIGHT# !</POS_OFF_CHECK>
			<POS_ON_CHECK>#GET_LIGHT#</POS_ON_CHECK>
			<!-- Set position -->
			<POS_OFF_CODE>0 #SET_LIGHT#</POS_OFF_CODE>
			<POS_ON_CODE>1 #SET_LIGHT#</POS_ON_CODE>
			<!-- Light category -->
			<SaveParameters ID="#IE_PARAMS#" Append="Default">
				<TT_LIGHT_CATEGORY>#TT_LIGHT_CATEGORY#</TT_LIGHT_CATEGORY>
			</SaveParameters>
		</ReturnParameters>
	</ParametersFn>
