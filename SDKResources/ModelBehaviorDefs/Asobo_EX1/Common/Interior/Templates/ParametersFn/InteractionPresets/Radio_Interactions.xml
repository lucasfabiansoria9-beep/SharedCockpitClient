	<!-- GET_FREQUENCY -->
	<!-- SET_FREQUENCY_HZ -->
	<!-- SIMVAR_FREQUENCY -->
	<ParametersFn Name="ASOBO_Frequency_Settings_Config">
		<Parameters Type="Default">
			<VAR_TOOLTIP_VALUE_UNIT>MHz</VAR_TOOLTIP_VALUE_UNIT>
		</Parameters>
		<ReturnParameters>
			<IE_VALUE_UNITS>Hz</IE_VALUE_UNITS>
			<VALUE_TYPE>NUMBER</VALUE_TYPE>
			<VAR_TOOLTIP_VALUE_UNIT>#VAR_TOOLTIP_VALUE_UNIT#</VAR_TOOLTIP_VALUE_UNIT>
			<CLAMP_VALUE>#CLAMP_FREQUENCY#</CLAMP_VALUE>
			<IE_GET_STATE>#GET_FREQUENCY#</IE_GET_STATE>
			<Condition Check="SIMVAR_FREQUENCY">
				<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
					<SIMVAR>#SIMVAR_FREQUENCY#</SIMVAR>
				</UseParametersFn>				
			</Condition>
			<IE_SET_STATE>#SET_FREQUENCY_HZ#</IE_SET_STATE>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<!-- GET_COM_FREQUENCY -->
	<!-- SET_COM_FREQUENCY_HZ -->
	<!-- SIMVAR_COM_FREQUENCY -->
	<ParametersFn Name="ASOBO_COM_Frequency_KHz_Settings_Config">
		<Parameters Type="Default">
			<CARRY_INCREMENT>False</CARRY_INCREMENT>
		</Parameters>
		<Parameters Type="Override">
			<IE_INCREMENT>#IS_COM_SPACING_25KHZ# if{ @25KHz } els{ @5KHz }</IE_INCREMENT>	
			<Condition Valid="CARRY_INCREMENT">
				<False>
					<CLAMP_FREQUENCY>s0 #GET_COM_FREQUENCY# @Keep_Only_Mhz + l0 @Keep_Only_Mhz - #CLAMP_FREQUENCY#</CLAMP_FREQUENCY>
				</False>
			</Condition>
		</Parameters>
		<Parameters Type="Override">
			<CLAMP_FREQUENCY>
				sp0 #IS_COM_SPACING_25KHZ# if{ l0 } els{ 
					l0 @25KHz % @20KHz == if{ 
						l0 @5KHz + l0 @5KHz - p0 #GET_COM_FREQUENCY# &gt; ?
					} els{ l0 }
				} #CLAMP_FREQUENCY#
			</CLAMP_FREQUENCY>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Frequency_Settings_Config">
				<GET_FREQUENCY>#GET_COM_FREQUENCY#</GET_FREQUENCY>
				<SET_FREQUENCY_HZ>#SET_COM_FREQUENCY_HZ#</SET_FREQUENCY_HZ>
				<SIMVAR_FREQUENCY>#SIMVAR_COM_FREQUENCY#</SIMVAR_FREQUENCY>
			</UseParametersFn>
			<TOOLTIP_VALUE_FORMAT>'%.3f MHz'</TOOLTIP_VALUE_FORMAT>
			<IE_INCREMENT>#IE_INCREMENT#</IE_INCREMENT>
			<INCREMENT_PER_THRESHOLD>#IE_INCREMENT#</INCREMENT_PER_THRESHOLD>
		</ReturnParameters>
	</ParametersFn>
