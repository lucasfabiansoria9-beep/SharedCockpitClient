<ModelBehaviors>
	<!-- ################################### AIRFRAME ################################# -->
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Door_Settings_Config">
		<Parameters Type="Default">
			<POINT_ID>0</POINT_ID>
		</Parameters>
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Interactive_Point_Parameters"/>
		</Parameters>
		<ReturnParameters>
			<VALUE_TYPE>PERCENT</VALUE_TYPE>
			<IE_GET_STATE>#GET_INTERACTIVE_POINT_GOAL#</IE_GET_STATE>
			<IE_SET_STATE>#SET_INTERACTIVE_POINT_GOAL#</IE_SET_STATE>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>#SIMVAR_INTERACTIVE_POINT_GOAL#</SIMVAR>
			</UseParametersFn>
			<ANIM_LAG>0</ANIM_LAG>
			<ANIM_CODE>#GET_INTERACTIVE_POINT_OPEN#</ANIM_CODE>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Door_Toggle_Settings_Config">
		<ReturnParameters>
			<Condition>
				<Test>
					<Or>
						<Or>
							<Arg NotEmpty="SET_CONDITION"/>
							<Arg NotEmpty="RESET_CONDITION"/>
						</Or>
						<Arg NotEmpty="TOGGLE_CONDITION"/>
					</Or>
				</Test>
				<True>
					<UseParametersFn Name="ASOBO_Conditional_Door_Toggle_Settings_Config" />
				</True>
				<False>
					<UseParametersFn Name="ASOBO_Simple_Door_Toggle_Settings_Config" />
				</False>
			</Condition>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Simple_Door_Toggle_Settings_Config">
		<Parameters Type="Default">
			<POINT_ID>0</POINT_ID>
		</Parameters>
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Interactive_Point_Parameters"/>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Auto_Define_XStates_Variant_Helper"/>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>#SIMVAR_INTERACTIVE_POINT_GOAL#</SIMVAR>
			</UseParametersFn>
			<ANIM_LAG>0</ANIM_LAG>
			<ANIM_CODE>#GET_INTERACTIVE_POINT_OPEN#</ANIM_CODE>
			<POS_OPEN_CHECK>#GET_INTERACTIVE_POINT_GOAL# 50 &gt;</POS_OPEN_CHECK>
			<POS_CLOSED_CHECK>#GET_INTERACTIVE_POINT_GOAL# 50 &gt; !</POS_CLOSED_CHECK>
			<!-- Set position -->
			<POS_OPEN_CODE>100 #SET_INTERACTIVE_POINT_GOAL#</POS_OPEN_CODE>
			<POS_CLOSED_CODE>0 #SET_INTERACTIVE_POINT_GOAL#</POS_CLOSED_CODE>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Conditional_Door_Toggle_Settings_Config">
		<Parameters Type="Default">
			<POINT_ID>0</POINT_ID>
			<SET_CONDITION>1</SET_CONDITION>
			<RESET_CONDITION>1</RESET_CONDITION>
		</Parameters>
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Interactive_Point_Parameters"/>
		</Parameters>
		<Parameters Type="Override">
			<Condition NotEmpty="TOGGLE_CONDITION">
				<Parameters Type="Override">
					<SET_CONDITION>#TOGGLE_CONDITION#</SET_CONDITION>
					<RESET_CONDITION>#TOGGLE_CONDITION#</RESET_CONDITION>
				</Parameters>
			</Condition>
		</Parameters>
		<Parameters Type="Override">
			<SET_POINT>
				100 == if{ #SET_CONDITION# if{ 100 #SET_INTERACTIVE_POINT_GOAL# } }
				els{ #RESET_CONDITION# if{ 0 #SET_INTERACTIVE_POINT_GOAL# } }
			</SET_POINT>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Auto_Define_XStates_Variant_Helper"/>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>#SIMVAR_INTERACTIVE_POINT_GOAL#</SIMVAR>
			</UseParametersFn>
			<ANIM_LAG>0</ANIM_LAG>
			<ANIM_CODE>#GET_INTERACTIVE_POINT_OPEN#</ANIM_CODE>
			<POS_OPEN_CHECK>#GET_INTERACTIVE_POINT_GOAL# 50 &gt;</POS_OPEN_CHECK>
			<POS_CLOSED_CHECK>#GET_INTERACTIVE_POINT_GOAL# 50 &gt; !</POS_CLOSED_CHECK>
			<!-- Set position -->
			<POS_OPEN_CODE>100 #SET_POINT#</POS_OPEN_CODE>
			<POS_CLOSED_CODE>0 #SET_POINT#</POS_CLOSED_CODE>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Lock_Door_Settings_Config">
		<Parameters Type="Default">
			<POINT_ID>0</POINT_ID>
			<ANIM_INPUT_SET/>
		</Parameters>
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Interactive_Point_Parameters"/>
		</Parameters>
		<Parameters Type="Default">
			<LOCK_VAR>XMLVAR_DOOR_#POINT_ID#_LOCK</LOCK_VAR>
		</Parameters>
		<Parameters Type="Default">
			<IS_LOCKED>(L:1:#LOCK_VAR#)</IS_LOCKED>
			<SET_LOCK>(&gt;L:1:#LOCK_VAR#)</SET_LOCK>
			<IS_OPEN>#GET_INTERACTIVE_POINT_OPEN# 1 &gt;</IS_OPEN>
		</Parameters>
		<Parameters Type="Default">
			<CAN_MOVE>#IS_LOCKED# ! #IS_OPEN# or</CAN_MOVE>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Add_Update_Helper">
				<UPDATE_ONCE>True</UPDATE_ONCE>
				<Condition Valid="ANIM_INPUT_SET">
					<True>
						<UPDATE_CODE>#IS_OPEN# ! #SET_LOCK# #IS_LOCKED# ! (&gt;B:#ANIM_INPUT_SET#)</UPDATE_CODE>
					</True>
					<False>
						<UPDATE_CODE>#IS_OPEN# ! #SET_LOCK#</UPDATE_CODE>
					</False>
				</Condition>
			</UseParametersFn>
			<VALUE_TYPE>PERCENT</VALUE_TYPE>
			<IE_GET_STATE>#GET_INTERACTIVE_POINT_GOAL#</IE_GET_STATE>
			<IE_SET_STATE>#CAN_MOVE# if{ #SET_INTERACTIVE_POINT_GOAL# }</IE_SET_STATE>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>#SIMVAR_INTERACTIVE_POINT_GOAL#</SIMVAR>
			</UseParametersFn>
			<ANIM_LAG>0</ANIM_LAG>
			<ANIM_CODE>#GET_INTERACTIVE_POINT_OPEN#</ANIM_CODE>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Exit_Door_Settings_Config">
		<Parameters Type="Override">
			<EXIT_LOCAL_VAR>L:1:EXIT_AIRCRAFT</EXIT_LOCAL_VAR>
		</Parameters>
		<Parameters Type="Override">
			<GET_EXIT_LOCAL_VAR>(#EXIT_LOCAL_VAR#)</GET_EXIT_LOCAL_VAR>
			<SET_EXIT_LOCAL_VAR>(&gt;#EXIT_LOCAL_VAR#)</SET_EXIT_LOCAL_VAR>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Auto_Define_XStates_Variant_Helper"/>
			<!-- Check position -->
			<POS_ON_CHECK>#GET_EXIT_LOCAL_VAR#</POS_ON_CHECK>
			<POS_OFF_CHECK>#GET_EXIT_LOCAL_VAR# !</POS_OFF_CHECK>
			<!-- Set position -->
			<POS_ON_CODE>1 #SET_EXIT_LOCAL_VAR#</POS_ON_CODE>
			<POS_OFF_CODE>0 #SET_EXIT_LOCAL_VAR#</POS_OFF_CODE>
		</ReturnParameters>
	</ParametersFn>
</ModelBehaviors>