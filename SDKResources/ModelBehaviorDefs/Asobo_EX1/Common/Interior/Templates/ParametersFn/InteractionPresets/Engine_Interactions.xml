<ModelBehaviors>
	<!-- ################################## ENGINE #################################### -->
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Throttle_Settings_Config">
		<Parameters Type="Default">
			<ID>1</ID>
		</Parameters>
		<Parameters Type="Default">
			<ENG_ID>#ID#</ENG_ID>
			<THROTTLE_MIN_VALUE_16K>0</THROTTLE_MIN_VALUE_16K>
		</Parameters>
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Throttle_Parameters"/>
		</Parameters>
		<ReturnParameters>
			<VALUE_TYPE>U16K</VALUE_TYPE>
			<IE_GET_STATE>#GET_THROTTLE_16K#</IE_GET_STATE>
			<IE_SET_STATE>#SET_THROTTLE_16K#</IE_SET_STATE>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>#SIMVAR_THROTTLE#</SIMVAR>
			</UseParametersFn>
			<SaveParameters ID="#IE_PARAMS#" Append="Default">
				<TT_ENGINE_CATEGORY>THROTTLE</TT_ENGINE_CATEGORY>
			</SaveParameters>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Throttle_With_Reverser_Settings_Config_Parameters">
		<Parameters Type="Default">
			<ID>1</ID>
		</Parameters>
		<Parameters Type="Default">
			<ENG_ID>#ID#</ENG_ID>
			<HAS_IDLE_GATE>True</HAS_IDLE_GATE>
		</Parameters>
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Throttle_Parameters"/>
		</Parameters>
		<Parameters Type="Override">
			<THROTTLE_MIN_VALUE_16K_PROCESSED Process="Float">#THROTTLE_MIN_VALUE_16K#</THROTTLE_MIN_VALUE_16K_PROCESSED>
		</Parameters>
		<Parameters Type="Override">
			<FULL_RANGE Process="Float">#THROTTLE_MAX_VALUE_16K# #THROTTLE_MIN_VALUE_16K_PROCESSED# -</FULL_RANGE>
			<MIN_DIST>#THROTTLE_MIN_VALUE_16K_PROCESSED# abs</MIN_DIST>
		</Parameters>
		<Condition Valid="HAS_IDLE_GATE">
			<Parameters Type="Default">
				<IDLE_IE_PERCENT_SET Process="Float">#MIN_DIST# #FULL_RANGE# / 100 *</IDLE_IE_PERCENT_SET>
				<IDLE_ANIM_PERCENT>50</IDLE_ANIM_PERCENT>
				<IDLE_GATE_DIRECTION>Both</IDLE_GATE_DIRECTION>	
				<GET_GATES>Throttle_Gates</GET_GATES>
				<GET_GATES_CONFIG>Throttle_Gates_Config</GET_GATES_CONFIG>
				<GET_POSITION_PCT_OVER_100>(B:#IE_PRESET_ID_UC#, percent over 100)</GET_POSITION_PCT_OVER_100>
			</Parameters>
			<Parameters Type="Override">
				<SaveParameters ID="#GET_GATES#" Append="Default">
					<POINT_0>Idle</POINT_0>
				</SaveParameters>
				<SaveParameters ID="#GET_GATES_CONFIG#" Append="Default">
					<IDLE_IE_PERCENT_SET>#IDLE_IE_PERCENT_SET#</IDLE_IE_PERCENT_SET>
					<IDLE_ANIM_PERCENT>#IDLE_ANIM_PERCENT#</IDLE_ANIM_PERCENT>
					<IDLE_GATE_DIRECTION>#IDLE_GATE_DIRECTION#</IDLE_GATE_DIRECTION>
				</SaveParameters>
				<UseParametersFn Name="ASOBO_Gates_Input_Handling">
					<INPUT_NAME>#IE_PRESET_ID_UC#_DEC</INPUT_NAME>
					<OUT_PARAM_NAME>ON_BEFORE_DEC_CODE_VALUE_SET</OUT_PARAM_NAME>
				</UseParametersFn>
				<UseParametersFn Name="ASOBO_Gates_Input_Handling">
					<INPUT_NAME>#IE_PRESET_ID_UC#_INC</INPUT_NAME>
					<OUT_PARAM_NAME>ON_BEFORE_INC_CODE_VALUE_SET</OUT_PARAM_NAME>
				</UseParametersFn>
			</Parameters>
		</Condition>
		<ReturnParameters>
			<VALUE_TYPE>M16K</VALUE_TYPE>
			<MIN_VALUE>#THROTTLE_MIN_VALUE_16K_PROCESSED#</MIN_VALUE>
			<IE_GET_STATE>#GET_THROTTLE_16K#</IE_GET_STATE>
			<IE_SET_STATE>#SET_THROTTLE_16K#</IE_SET_STATE>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>#SIMVAR_THROTTLE#</SIMVAR>
			</UseParametersFn>
			<Condition Valid="HAS_IDLE_GATE">
				<PLOAD_AXIS_CURVE>#GET_GATES#</PLOAD_AXIS_CURVE>
				<PLOAD_AXIS_CURVE_CONFIG>#GET_GATES_CONFIG#</PLOAD_AXIS_CURVE_CONFIG>
				<UseParametersFn Name="ASOBO_Add_Update_Helper">
					<UPDATE_ONCE>True</UPDATE_ONCE>
					<UPDATE_CODE>#GET_POSITION_PCT_OVER_100# sp0 #IS_AT_ANY_GATE_CHECK# if{ 1 #SET_CAN_BYPASS_GATE# }</UPDATE_CODE>
				</UseParametersFn>
				<ON_BEFORE_DEC_CODE_VALUE_SET>#ON_BEFORE_DEC_CODE_VALUE_SET#</ON_BEFORE_DEC_CODE_VALUE_SET>
				<ON_BEFORE_INC_CODE_VALUE_SET>#ON_BEFORE_INC_CODE_VALUE_SET#</ON_BEFORE_INC_CODE_VALUE_SET>
			</Condition>
			<SaveParameters ID="#IE_PARAMS#" Append="Default">
				<TT_ENGINE_CATEGORY>THROTTLE</TT_ENGINE_CATEGORY>
			</SaveParameters>
		</ReturnParameters>
	</ParametersFn>
