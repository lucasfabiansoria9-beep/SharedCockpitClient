			<ENG_ID>#ID#</ENG_ID>
		</Parameters>
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Ignition_Parameters"/>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Auto_Define_XStates_Variant_Helper"/>
			<UseParametersFn Name="ASOBO_Append_Init_Code_Helper">
				<INIT_CODE>#GET_IGNITION_SWITCH# @Enum_Ignition_Auto == if{ @Enum_Ignition_On #SET_IGNITION_SWITCH# }</INIT_CODE>
			</UseParametersFn>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>#SIMVAR_IGNITION_SWITCH#</SIMVAR>
			</UseParametersFn>
			<!-- Check position -->
			<POS_OFF_CHECK>#GET_IGNITION_SWITCH# @Enum_Ignition_Off ==</POS_OFF_CHECK>
			<POS_ON_CHECK>#GET_IGNITION_SWITCH# @Enum_Ignition_On ==</POS_ON_CHECK>
			<!-- Set position -->
			<POS_OFF_CODE>@Enum_Ignition_Off #SET_IGNITION_SWITCH#</POS_OFF_CODE>
			<POS_ON_CODE>@Enum_Ignition_On #SET_IGNITION_SWITCH#</POS_ON_CODE>
		</ReturnParameters>
	</ParametersFn>
