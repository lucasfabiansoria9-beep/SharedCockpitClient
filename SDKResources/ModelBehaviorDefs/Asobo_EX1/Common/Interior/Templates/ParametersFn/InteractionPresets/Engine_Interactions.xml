			<IE_SET_STATE>@16K / #MIN_VALUE# * #SET_THROTTLE_16K#</IE_SET_STATE>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>#SIMVAR_THROTTLE#</SIMVAR>
			</UseParametersFn>
			<SaveParameters ID="#IE_PARAMS#" Append="Default">
