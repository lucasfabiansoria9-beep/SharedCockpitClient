	<ParametersFn Name="ASOBO_APU_On_Off_Settings_Config_Parameters">
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Auto_Define_XStates_Variant_Helper"/>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>APU SWITCH</SIMVAR>
			</UseParametersFn>
			<!-- Check position -->
			<POS_OFF_CHECK>(A:APU SWITCH, Bool) !</POS_OFF_CHECK>
			<POS_ON_CHECK>(A:APU SWITCH, Bool)</POS_ON_CHECK>
			<!-- Set position -->
			<POS_OFF_CODE>(&gt;K:APU_OFF_SWITCH)</POS_OFF_CODE>
			<POS_ON_CODE>(&gt;K:APU_STARTER)</POS_ON_CODE>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_APU_Off_On_Start_Settings_Config_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Enum_Parameters_Helper">
				<SaveParameters ID="Enum_Values">
					<POS_OFF>0</POS_OFF>
					<POS_ON>1</POS_ON>
					<POS_START>2</POS_START>
				</SaveParameters>
			</UseParametersFn>
		</Parameters>
		<Parameters Type="Default">
			<APU_GEN_STATE_TIED_TO_APU_SWITCH>True</APU_GEN_STATE_TIED_TO_APU_SWITCH>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Auto_Define_XStates_Variant_Helper"/>
			<UseParametersFn Name="ASOBO_Append_Init_Code_Helper">
				<INIT_CODE>(A:APU SWITCH, Bool) ! #IS_POS_START# and (A:APU SWITCH, Bool) #IS_POS_OFF# and or if{ #SET_POS_ON# }</INIT_CODE>
			</UseParametersFn>
			<Condition Valid="APU_GEN_STATE_TIED_TO_APU_SWITCH">
				<UseParametersFn Name="ASOBO_Append_Init_Code_Helper">
					<INIT_CODE>(A:APU SWITCH, Bool) s0 (A:APU GENERATOR SWITCH, Bool) != if{ 0 l0 (&gt;K:2:APU_GENERATOR_SWITCH_SET) }</INIT_CODE>
				</UseParametersFn>
			</Condition>
			<UseParametersFn Name="ASOBO_Watch_Simvars_Helper">
				<SaveParameters ID="Temp_Simvars_To_Watch">
					<APU_SWITCH>APU SWITCH</APU_SWITCH>
					<Condition Valid="APU_GEN_STATE_TIED_TO_APU_SWITCH">
						<APU_GEN_SIMVAR>APU GENERATOR SWITCH</APU_GEN_SIMVAR>						
					</Condition>
				</SaveParameters>
			</UseParametersFn>
			<!-- Check position -->
			<POS_OFF_CHECK>(A:APU SWITCH, Bool) ! #IS_POS_OFF# and</POS_OFF_CHECK>
			<POS_ON_CHECK>#IS_POS_ON#</POS_ON_CHECK>
			<POS_START_CHECK>(A:APU SWITCH, Bool) #IS_POS_START# and</POS_START_CHECK>
			<!-- Set position -->
			<POS_OFF_CODE>(&gt;K:APU_OFF_SWITCH) #SET_POS_OFF#</POS_OFF_CODE>
			<POS_ON_CODE>#SET_POS_ON#</POS_ON_CODE>
			<POS_START_CODE>(&gt;K:APU_STARTER) #SET_POS_START#</POS_START_CODE>
			<POS_START_MOMENTARY_RELEASE_TARGET>On</POS_START_MOMENTARY_RELEASE_TARGET>
		</ReturnParameters>
	</ParametersFn>
