	<ParametersFn Name="ASOBO_Incremental_TRIM_Settings_Positions">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Process_Trim_Limits_Parameters_Helper"/>
			<UseParametersFn Name="ASOBO_Get_Enum_Parameters_Helper">
				<SaveParameters ID="Enum_Values">
					<POS_NEUTRAL>0</POS_NEUTRAL>
					<POS_INC>1</POS_INC>
					<POS_DEC>2</POS_DEC>
				</SaveParameters>
			</UseParametersFn>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Auto_Define_XStates_Variant_Helper"/>
			<!-- Check position -->
			<POS_NEUTRAL_CHECK>#IS_POS_NEUTRAL#</POS_NEUTRAL_CHECK>
			<POS_INCR_CHECK>#IS_POS_INC#</POS_INCR_CHECK>
			<POS_DECR_CHECK>#IS_POS_DEC#</POS_DECR_CHECK>
			<!-- Set position -->
			<POS_NEUTRAL_CODE>#SET_POS_NEUTRAL# (&gt;B:PROCEDURE_#TRIM_TYPE#_Trim_Neutral)</POS_NEUTRAL_CODE>
			<POS_INCR_CODE>#SET_POS_INC# (&gt;B:PROCEDURE_#TRIM_TYPE#_Trim_Increment)</POS_INCR_CODE>
			<POS_DECR_CODE>#SET_POS_DEC# (&gt;B:PROCEDURE_#TRIM_TYPE#_Trim_Decrement)</POS_DECR_CODE>
			<!-- Tooltip -->
			<Condition Valid="TRIM_HAS_NO_ANGLE_INFORMATION">
				<True>
					<IE_TOOLTIP_VALUE>#GET_TRIM_PERCENT# '%.1f%%' @sprintf</IE_TOOLTIP_VALUE>
				</True>
				<False>
					<IE_TOOLTIP_VALUE>#GET_TRIM_DEGREES# '%.1f°' @sprintf</IE_TOOLTIP_VALUE>
				</False>
			</Condition>
		</ReturnParameters>
	</ParametersFn>
