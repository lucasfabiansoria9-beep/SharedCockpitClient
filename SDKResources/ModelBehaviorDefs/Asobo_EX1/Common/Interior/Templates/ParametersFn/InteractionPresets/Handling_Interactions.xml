				<TRIM_Y_AXIS_TYPE>LONGITUDINAL</TRIM_Y_AXIS_TYPE>
				<TRIM_X_AXIS_TYPE>LATERAL</TRIM_X_AXIS_TYPE>
			</UseParametersFn>
		</ReturnParameters>
	</ParametersFn>
	<ParametersFn Name="ASOBO_Increment_TRIM_Axis_Settings_Positions">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Process_Trim_Limits_Parameters_Helper">
				<OUT_PARAM_ID>X_AXIS</OUT_PARAM_ID>
				<TRIM_TYPE>#TRIM_X_AXIS_TYPE#</TRIM_TYPE>
			</UseParametersFn>
			<UseParametersFn Name="ASOBO_Get_Process_Trim_Limits_Parameters_Helper">
				<OUT_PARAM_ID>Y_AXIS</OUT_PARAM_ID>
				<TRIM_TYPE>#TRIM_Y_AXIS_TYPE#</TRIM_TYPE>
			</UseParametersFn>
			<UseParametersFn Name="ASOBO_Get_Enum_Parameters_Helper">
				<SaveParameters ID="Enum_Values">
					<POS_NEUTRAL>0</POS_NEUTRAL>
					<POS_DN>1</POS_DN>
					<POS_UP>2</POS_UP>
					<POS_L>3</POS_L>
					<POS_R>4</POS_R>
				</SaveParameters>
			</UseParametersFn>
		</Parameters>
		<Parameters Type="Default">
			<UseParametersFn Name="ASOBO_IE_Directional_Variables_Parameters"/>
			<Condition Valid="TRIM_X_AXIS_HAS_NO_ANGLE_INFORMATION">
				<True>
					<FORMAT_IE_TOOLTIP_X_AXIS>#GET_TRIM_X_AXIS_PERCENT# '%.1f%%' @sprintf</FORMAT_IE_TOOLTIP_X_AXIS>
				</True>
				<False>
					<FORMAT_IE_TOOLTIP_X_AXIS>#GET_TRIM_X_AXIS_DEGREES# '%.1f°' @sprintf</FORMAT_IE_TOOLTIP_X_AXIS>
				</False>
			</Condition>
			<Condition Valid="TRIM_Y_AXIS_HAS_NO_ANGLE_INFORMATION">
				<True>
					<FORMAT_IE_TOOLTIP_Y_AXIS>#GET_TRIM_Y_AXIS_PERCENT# '%.1f%%' @sprintf</FORMAT_IE_TOOLTIP_Y_AXIS>
				</True>
				<False>
					<FORMAT_IE_TOOLTIP_Y_AXIS>#GET_TRIM_Y_AXIS_DEGREES# '%.1f°' @sprintf</FORMAT_IE_TOOLTIP_Y_AXIS>
				</False>
			</Condition>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Add_Set_Binding_Helper">
				<ALIAS_ID>#TRIM_X_AXIS_TYPE#_Set</ALIAS_ID>
				<PARAM_0>p0</PARAM_0>
				<PARAM_1>#GET_Y_AXIS_POSITION#</PARAM_1>
				<PARAM_1_IS_DYNAMIC>True</PARAM_1_IS_DYNAMIC>
				<PARAM_0_IS_DYNAMIC>True</PARAM_0_IS_DYNAMIC>
			</UseParametersFn>
			<UseParametersFn Name="ASOBO_Add_Set_Binding_Helper">
				<ALIAS_ID>#TRIM_Y_AXIS_TYPE#_Set</ALIAS_ID>
				<PARAM_0>#GET_X_AXIS_POSITION#</PARAM_0>
				<PARAM_1>p0</PARAM_1>
				<PARAM_0_IS_DYNAMIC>True</PARAM_0_IS_DYNAMIC>
				<PARAM_1_IS_DYNAMIC>True</PARAM_1_IS_DYNAMIC>
			</UseParametersFn>
			<UseParametersFn Name="ASOBO_Auto_Define_XStates_Variant_Helper"/>
			<!-- Check position -->
			<POS_NEUTRAL_CHECK>#IS_POS_NEUTRAL#</POS_NEUTRAL_CHECK>
			<POS_UP_CHECK>#IS_POS_UP#</POS_UP_CHECK>
			<POS_DN_CHECK>#IS_POS_DN#</POS_DN_CHECK>
			<POS_L_CHECK>#IS_POS_L#</POS_L_CHECK>
			<POS_R_CHECK>#IS_POS_R#</POS_R_CHECK>
			<!-- Set position -->
			<POS_NEUTRAL_CODE>#SET_POS_NEUTRAL# (&gt;B:PROCEDURE_#TRIM_X_AXIS_TYPE#_Trim_Neutral) (&gt;B:PROCEDURE_#TRIM_Y_AXIS_TYPE#_Trim_Neutral)</POS_NEUTRAL_CODE>
			<POS_UP_CODE>#SET_POS_UP# (&gt;B:PROCEDURE_#TRIM_Y_AXIS_TYPE#_Trim_Decrement) (&gt;B:PROCEDURE_#TRIM_X_AXIS_TYPE#_Trim_Neutral)</POS_UP_CODE>
			<POS_DN_CODE>#SET_POS_DN# (&gt;B:PROCEDURE_#TRIM_Y_AXIS_TYPE#_Trim_Increment) (&gt;B:PROCEDURE_#TRIM_X_AXIS_TYPE#_Trim_Neutral)</POS_DN_CODE>
			<POS_L_CODE>#SET_POS_L# (&gt;B:PROCEDURE_#TRIM_X_AXIS_TYPE#_Trim_Decrement) (&gt;B:PROCEDURE_#TRIM_Y_AXIS_TYPE#_Trim_Neutral)</POS_L_CODE>
			<POS_R_CODE>#SET_POS_R# (&gt;B:PROCEDURE_#TRIM_X_AXIS_TYPE#_Trim_Increment) (&gt;B:PROCEDURE_#TRIM_Y_AXIS_TYPE#_Trim_Neutral)</POS_R_CODE>
			<!-- Tooltip -->
			<IE_TOOLTIP_VALUE>#FORMAT_IE_TOOLTIP_X_AXIS# #FORMAT_IE_TOOLTIP_Y_AXIS# '%s / %s' @Sprintf</IE_TOOLTIP_VALUE>
		</ReturnParameters>
	</ParametersFn>
	<ParametersFn Name="Get_Procedure_Trim_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Process_Trim_Limits_Parameters_Helper"/>
		</Parameters>
		<Condition Valid="TRIM_HAS_NO_ANGLE_INFORMATION">
			<True>
				<Parameters Type="Default">
					<INCREMENT_PER_SECOND_PCT>10</INCREMENT_PER_SECOND_PCT>
				</Parameters>
				<Parameters Type="Override">
					<INCREMENT_PER_SECOND_16K Process="Float">#INCREMENT_PER_SECOND_PCT# #TRIM_RANGE# / #TRIM_RANGE_16K# *</INCREMENT_PER_SECOND_16K>
				</Parameters>
			</True>
			<False>
				<Parameters Type="Default">
					<INCREMENT_PER_SECOND_DEGREES>1</INCREMENT_PER_SECOND_DEGREES>
				</Parameters>
				<Parameters Type="Override">
					<INCREMENT_PER_SECOND_16K Process="Float">#INCREMENT_PER_SECOND_DEGREES# #TRIM_RANGE# / #TRIM_RANGE_16K# *</INCREMENT_PER_SECOND_16K>
				</Parameters>
			</False>
		</Condition>
		<ReturnParameters>
			<UseParametersFn Name="Get_Procedure_Incremental_Switch_Parameters">
				<SURFACE_TYPE>#TRIM_TYPE#</SURFACE_TYPE>
				<GET_SURFACE_VALUE_16K>#GET_TRIM_16K#</GET_SURFACE_VALUE_16K>
				<SET_SURFACE_VALUE_16K>#SET_TRIM_16K#</SET_SURFACE_VALUE_16K>
			</UseParametersFn>
		</ReturnParameters>
	</ParametersFn>
	<ParametersFn Name="Get_Procedure_Incremental_Switch_Parameters">
		<Parameters Type="Default">
			<Set_Incremental_Switch_State>@Set_Incremental_Switch_State</Set_Incremental_Switch_State>
			<Get_Incremental_Switch_State>@Get_Incremental_Switch_State</Get_Incremental_Switch_State>
		</Parameters>
		<Parameters Type="Default">
			<INCREMENT_PER_SECOND_PCT>10</INCREMENT_PER_SECOND_PCT>
		</Parameters>
		<Parameters Type="Default">
			<INCREMENT_PER_SECOND_16K Process="Float">#INCREMENT_PER_SECOND_PCT# 100 / @16K *</INCREMENT_PER_SECOND_16K>
		</Parameters>
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Move_Interpolation_Helper">
				<START_VALUE>#GET_SURFACE_VALUE_16K#</START_VALUE>
				<SET_NEW_VALUE>#SET_SURFACE_VALUE_16K#</SET_NEW_VALUE>
				<END_VALUE>(O:_Target_Position, position 16k)</END_VALUE>
				<COEFFICIENT>#INCREMENT_PER_SECOND_16K# @TICK_Get_Delta_Time *</COEFFICIENT>
				<OUT_PARAM_NAME>UPDATE_SURFACE_VALUE</OUT_PARAM_NAME>
			</UseParametersFn>
		</Parameters>
		<Parameters Type="Override">
			<UPDATE_SURFACE_CODE>
				#Get_Incremental_Switch_State# @Incremental_Switch_Pos_Incr == if{ @16K sp0 g0 }
				#Get_Incremental_Switch_State# @Incremental_Switch_Pos_Decr == if{ -@16K sp0 g0 } els{ g1 }
				:0 l0 (&gt;O:_Target_Position, position 16k) #UPDATE_SURFACE_VALUE# :1
			</UPDATE_SURFACE_CODE>
		</Parameters>
		<ReturnParameters>
			<TICK_EVERY_FRAME>True</TICK_EVERY_FRAME>
			<ON_VALUE_CHANGED>#UPDATE_SURFACE_CODE#</ON_VALUE_CHANGED>
			<UseParametersFn Name="ASOBO_Append_Set_Code_Helper">
				<SET_CODE>p0 #Set_Incremental_Switch_State#</SET_CODE>
			</UseParametersFn>
			<UseParametersFn Name="ASOBO_Append_Value_Code_Helper">
				<VALUE_CODE>#Get_Incremental_Switch_State#</VALUE_CODE>
			</UseParametersFn>
			<SaveParameters ID="#IE_PARAMS#" Append="Override">
				<IE_SET_ARG_COUNT>1</IE_SET_ARG_COUNT>
				<SET_PARAM_0>p0</SET_PARAM_0>
				<SET_PARAM_0_TYPE>Float</SET_PARAM_0_TYPE>
				<SET_PARAM_0_IS_DYNAMIC>True</SET_PARAM_0_IS_DYNAMIC>
			</SaveParameters>
			<UseParametersFn Name="ASOBO_Add_Set_Binding_Helper">
				<ALIAS_ID>Neutral</ALIAS_ID>
				<PARAM_0>@Incremental_Switch_Pos_Neutral</PARAM_0>
				<PARAM_0_IS_DYNAMIC>False</PARAM_0_IS_DYNAMIC>
			</UseParametersFn>
			<UseParametersFn Name="ASOBO_Add_Set_Binding_Helper">
				<ALIAS_ID>Increment</ALIAS_ID>
				<PARAM_0>@Incremental_Switch_Pos_Incr</PARAM_0>
				<PARAM_0_IS_DYNAMIC>False</PARAM_0_IS_DYNAMIC>
			</UseParametersFn>
			<UseParametersFn Name="ASOBO_Add_Set_Binding_Helper">
				<ALIAS_ID>Decrement</ALIAS_ID>
				<PARAM_0>@Incremental_Switch_Pos_Decr</PARAM_0>
				<PARAM_0_IS_DYNAMIC>False</PARAM_0_IS_DYNAMIC>
			</UseParametersFn>
		</ReturnParameters>
	</ParametersFn>
