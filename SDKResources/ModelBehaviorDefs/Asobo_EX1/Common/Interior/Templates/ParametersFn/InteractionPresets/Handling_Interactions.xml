<ModelBehaviors>
	<!-- ################################## HANDLING ################################## -->
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Flaps_Settings_Config">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Flaps_Lever_Parameters"/>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Auto_Define_XStates_Variant_Helper"/>
			<XSTATES_TYPE>NUMERICAL</XSTATES_TYPE>
			<FORMAT_POSITION_TO_STR>'%d°'</FORMAT_POSITION_TO_STR>
			<BINDING_PREFIX>Deg_</BINDING_PREFIX>
			<IE_GET_STATE>#GET_FLAPS_HANDLE#</IE_GET_STATE>
			<IE_SET_STATE>#SET_FLAPS_HANDLE#</IE_SET_STATE>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>#SIMVAR_FLAPS_HANDLE#</SIMVAR>
			</UseParametersFn>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Flaps_States_Settings_Config">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Flaps_Lever_Parameters"/>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Auto_Define_XStates_Variant_Helper"/>
			<IE_GET_STATE>#GET_FLAPS_HANDLE#</IE_GET_STATE>
			<IE_SET_STATE>#SET_FLAPS_HANDLE#</IE_SET_STATE>
			<IE_INC_CODE>#GET_FLAPS_HANDLE# p0 + #SET_FLAPS_HANDLE#</IE_INC_CODE>
			<IE_DEC_CODE>#GET_FLAPS_HANDLE# p0 - #SET_FLAPS_HANDLE#</IE_DEC_CODE>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>#SIMVAR_FLAPS_HANDLE#</SIMVAR>
			</UseParametersFn>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Flaps_Switch_Settings_Config">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Flaps_Lever_Parameters">
			</UseParametersFn>
		</Parameters>
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Enum_Parameters_Helper">
				<SaveParameters ID="Enum_Values">
					<POS_NEUTRAL>0</POS_NEUTRAL>
					<POS_INC>1</POS_INC>
					<POS_DEC>2</POS_DEC>
				</SaveParameters>
			</UseParametersFn>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Auto_Define_XStates_Variant_Helper"/>
			<!-- Check position -->
			<POS_NEUTRAL_CHECK>#IS_POS_NEUTRAL#</POS_NEUTRAL_CHECK>
			<POS_INCR_CHECK>#IS_POS_INC#</POS_INCR_CHECK>
			<POS_DECR_CHECK>#IS_POS_DEC#</POS_DECR_CHECK>
			<!-- Set position -->
			<POS_NEUTRAL_CODE>#SET_POS_NEUTRAL# (&gt;B:PROCEDURE_FLAPS_Neutral)</POS_NEUTRAL_CODE>
			<POS_INCR_CODE>#SET_POS_INC# (&gt;B:PROCEDURE_FLAPS_Increment)</POS_INCR_CODE>
			<POS_DECR_CODE>#SET_POS_DEC# (&gt;B:PROCEDURE_FLAPS_Decrement)</POS_DECR_CODE>
			<!-- Tooltip -->
			<IE_TOOLTIP_VALUE>#GET_FLAPS_HANDLE_CONTINUOUS# '%.1f%%' @sprintf</IE_TOOLTIP_VALUE>
		</ReturnParameters>
	</ParametersFn>
