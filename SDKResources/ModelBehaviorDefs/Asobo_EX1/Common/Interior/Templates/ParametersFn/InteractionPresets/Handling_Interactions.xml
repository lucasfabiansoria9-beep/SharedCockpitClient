			<IE_GET_STATE>#GET_TRIM_16K#</IE_GET_STATE>
			<IE_SET_STATE>#SET_TRIM_16K#</IE_SET_STATE>
			<IE_INCREMENT>#DEGREE_INCREMENT_PER_INC_DEC_EVENT# #TRIM_RANGE# 2 / / @16K *</IE_INCREMENT>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>#TRIM_AXIS_VAR#</SIMVAR>
			</UseParametersFn>
			<INCREMENT_PER_THRESHOLD>#DEGREE_INCREMENT_PER_THRESHOLD# #TRIM_RANGE# /</INCREMENT_PER_THRESHOLD>			<!-- Process="Float" -->
			<IE_TOOLTIP_VALUE>#GET_TRIM_DEGREES# '%.1f°' @sprintf</IE_TOOLTIP_VALUE>
