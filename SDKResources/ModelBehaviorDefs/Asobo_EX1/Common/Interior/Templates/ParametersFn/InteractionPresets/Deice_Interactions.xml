<ModelBehaviors>
	<!-- ################################## DEICE ##################################### -->
	<!-- ############################################################################## -->
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Deice_Windshield_Settings_Config">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Windshield_Deice_Parameters"/>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Auto_Define_XStates_Variant_Helper"/>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>#SIMVAR_WINDSHIELD_DEICE#</SIMVAR>
			</UseParametersFn>
			<!-- Check position -->
			<POS_OFF_CHECK>#GET_WINDSHIELD_DEICE# !</POS_OFF_CHECK>
			<POS_ON_CHECK>#GET_WINDSHIELD_DEICE#</POS_ON_CHECK>
			<!-- Set position -->
			<POS_OFF_CODE>0 #SET_WINDSHIELD_DEICE#</POS_OFF_CODE>
			<POS_ON_CODE>1 #SET_WINDSHIELD_DEICE#</POS_ON_CODE>
		</ReturnParameters>
	</ParametersFn>
