<ModelBehaviors>
	<!-- ################################## AUTOPILOT ################################# -->
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Baro_Settings_Config">
		<Parameters Type="Default">
			<ID>1</ID>
			<MIN_BARO_MB>955</MIN_BARO_MB>
			<MAX_BARO_MB>1060</MAX_BARO_MB>
			<BARO_INC_HG>0.01</BARO_INC_HG>
		</Parameters>
		<Parameters Type="Default">
			<BARO_ID>#ID#</BARO_ID>
			<BARO_INC_MB Process="Float">#BARO_INC_HG# 33.863886667 *</BARO_INC_MB>
			<BARO_TOOLTIP_VALUE_TYPE>inHG</BARO_TOOLTIP_VALUE_TYPE>
			<NB_INCREMENT_FOR_FULL_RANGE>3000</NB_INCREMENT_FOR_FULL_RANGE>
		</Parameters>
		<Parameters Type="Default">
			<Switch Param="BARO_TOOLTIP_VALUE_TYPE">
				<Case Value="Both">
					<TOOLTIP_VALUE_FORMAT_MB>'%.1f HPA'</TOOLTIP_VALUE_FORMAT_MB>
					<TOOLTIP_VALUE_FORMAT_HG>'%.2f IN'</TOOLTIP_VALUE_FORMAT_HG>
					<Condition NotEmpty="IE_NAME_UID">
						<True>
							<TOOLTIP_VALUE_CHECK_DISPLAYED_UNIT>(L:1:XMLVAR_Baro_#IE_NAME_UID#_Tooltip_Displayed_Unit)</TOOLTIP_VALUE_CHECK_DISPLAYED_UNIT>
						</True>
						<False>
							<TOOLTIP_VALUE_CHECK_DISPLAYED_UNIT>(L:1:XMLVAR_Baro_#BARO_ID#_Tooltip_Displayed_Unit)</TOOLTIP_VALUE_CHECK_DISPLAYED_UNIT>
						</False>
					</Condition>
				</Case>
				<Case Value="Millibar">
					<TOOLTIP_VALUE_FORMAT>'%.1f HPA'</TOOLTIP_VALUE_FORMAT>
				</Case>
				<Default>
					<TOOLTIP_VALUE_FORMAT>'%.2f IN'</TOOLTIP_VALUE_FORMAT>
				</Default>
			</Switch>
		</Parameters>
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Baro_Parameters"/>
		</Parameters>
		<ReturnParameters>
			<NB_INCREMENT_FOR_FULL_RANGE>#NB_INCREMENT_FOR_FULL_RANGE#</NB_INCREMENT_FOR_FULL_RANGE>
			<VALUE_TYPE>NUMBER</VALUE_TYPE>
			<MIN_VALUE>#MIN_BARO_MB#</MIN_VALUE>
			<MAX_VALUE>#MAX_BARO_MB#</MAX_VALUE>
			<IE_INCREMENT>#BARO_INC_MB#</IE_INCREMENT>
			<IE_GET_STATE>#GET_BARO_MB#</IE_GET_STATE>
			<IE_SET_STATE>#SET_BARO_MB#</IE_SET_STATE>
			<IE_VALUE_UNITS>Millibar</IE_VALUE_UNITS>
			<Switch Param="BARO_TOOLTIP_VALUE_TYPE">
				<Case Value="Both">
					<IE_TOOLTIP_VALUE>#TOOLTIP_VALUE_CHECK_DISPLAYED_UNIT# @BARO_Unit_HPA == if{ #GET_BARO_MB# #TOOLTIP_VALUE_FORMAT_MB# } els{ #GET_BARO_HG# #TOOLTIP_VALUE_FORMAT_HG# } @sprintf</IE_TOOLTIP_VALUE>
				</Case>
				<Default>
					<VAR_TOOLTIP_VALUE_UNIT>#BARO_TOOLTIP_VALUE_TYPE#</VAR_TOOLTIP_VALUE_UNIT>
					<TOOLTIP_VALUE_FORMAT>#TOOLTIP_VALUE_FORMAT#</TOOLTIP_VALUE_FORMAT>
				</Default>
			</Switch>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>#SIMVAR_BARO_MB#</SIMVAR>
			</UseParametersFn>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<Macro Name="BARO_Unit_IN">0</Macro>
	<Macro Name="BARO_Unit_HPA">1</Macro>
	<ParametersFn Name="ASOBO_Baro_Units_Settings_Config">
		<Parameters Type="Default">
			<ID>1</ID>
		</Parameters>
		<Parameters Type="Default">
			<BARO_ID>#ID#</BARO_ID>
		</Parameters>
		<Parameters Type="Default">
			<Condition NotEmpty="IE_NAME_UID">
				<True>
					<VAR_TOOLTIP_VALUE_UNIT_NAME>XMLVAR_Baro_#IE_NAME_UID#_Tooltip_Displayed_Unit</VAR_TOOLTIP_VALUE_UNIT_NAME>
				</True>
				<False>
					<VAR_TOOLTIP_VALUE_UNIT_NAME>XMLVAR_Baro_#BARO_ID#_Tooltip_Displayed_Unit</VAR_TOOLTIP_VALUE_UNIT_NAME>
				</False>
			</Condition>
			<VAR_TOOLTIP_VALUE_UNIT_SCOPE>O</VAR_TOOLTIP_VALUE_UNIT_SCOPE>
		</Parameters>
		<Parameters Type="Override">
			<VAR_TOOLTIP_VALUE_UNIT>#VAR_TOOLTIP_VALUE_UNIT_SCOPE#:#VAR_TOOLTIP_VALUE_UNIT_NAME#</VAR_TOOLTIP_VALUE_UNIT>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Auto_Define_XStates_Variant_Helper"/>
			<UseParametersFn Name="ASOBO_Watch_LocalVar_Helper">
				<LOCAL_VAR>#VAR_TOOLTIP_VALUE_UNIT_NAME#</LOCAL_VAR>
				<VAR_SCOPE>#VAR_TOOLTIP_VALUE_UNIT_SCOPE#</VAR_SCOPE>
			</UseParametersFn>
			<!-- Tooltip -->
			<POS_IN_TOOLTIP_ID>@TT_Package.STATE.UNIT.IN</POS_IN_TOOLTIP_ID>
			<POS_HPA_TOOLTIP_ID>@TT_Package.STATE.UNIT.HPA</POS_HPA_TOOLTIP_ID>
			<!-- Check -->
			<POS_IN_CHECK>@BARO_Unit_IN (#VAR_TOOLTIP_VALUE_UNIT#) ==</POS_IN_CHECK>
			<POS_HPA_CHECK>@BARO_Unit_HPA (#VAR_TOOLTIP_VALUE_UNIT#) ==</POS_HPA_CHECK>
			<!-- Code -->
			<POS_IN_CODE>@BARO_Unit_IN (&gt;#VAR_TOOLTIP_VALUE_UNIT#)</POS_IN_CODE>
			<POS_HPA_CODE>@BARO_Unit_HPA (&gt;#VAR_TOOLTIP_VALUE_UNIT#)</POS_HPA_CODE>
		</ReturnParameters>
	</ParametersFn>
