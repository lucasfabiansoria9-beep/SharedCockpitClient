		</Parameters>
		<ReturnParameters>
			<IE_TOOLTIP_VALUE>#IE_TOOLTIP_VALUE#</IE_TOOLTIP_VALUE>
			<UseParametersFn Name="ASOBO_Auto_Define_Event_Param_Helper">
				<ON_EVENT_SET>#GET_ALTIMETER_ALTITUDE# #SET_ALTITUDE_LOCK# #ON_EVENT#</ON_EVENT_SET>
			</UseParametersFn>
			<IE_VALUE_UNITS>Bool</IE_VALUE_UNITS>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Autopilot_Airspeed_Mode_Setting_Config">
		<Parameters Type="Override">
			<AIRSPEED_MODE_SIMVAR>AUTOPILOT AIRSPEED HOLD</AIRSPEED_MODE_VA>
		</Parameters>
		<Parameters Type="Override">
			<SET_HOLD>(&gt;K:AP_AIRSPEED_HOLD)</SET_HOLD>
			<AIRSPEED_MODE>(A:#AIRSPEED_MODE_SIMVAR#, Bool)</AIRSPEED_MODE>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Auto_Define_XStates_Variant_Helper"/>
			<!-- Check position -->
			<POS_OFF_CHECK>#AIRSPEED_MODE# !</POS_OFF_CHECK>
			<POS_ON_CHECK>#AIRSPEED_MODE#</POS_ON_CHECK>
			<!-- Set position -->
			<POS_OFF_CODE>0 #SET_HOLD#</POS_OFF_CODE>
			<POS_ON_CODE>1 #SET_HOLD#</POS_ON_CODE>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>#AIRSPEED_MODE_SIMVAR#</SIMVAR>
			</UseParametersFn>
		</ReturnParameters>
	</ParametersFn>
