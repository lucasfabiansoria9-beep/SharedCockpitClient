	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Autopilot_Vertical_Speed_Increment_Event_Setting_Config_Parameters">
		<Parameters Type="Default">
			<DIRECTION>UP</DIRECTION>
			<ON_EVENT />
		</Parameters>
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Autopilot_Vertical_Speed_Increment_Parameters"/>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Auto_Define_Event_Param_Helper">
				<ON_EVENT_SET>#VERTICALSPEED_SETTING_CODE# #ON_EVENT#</ON_EVENT_SET>
			</UseParametersFn>
