			<ON_EVENT />
		</Parameters>
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Autopilot_Vertical_Speed_Parameters"/>
		</Parameters>
		<Parameters Type="Override">
			<Condition Check="DIRECTION" Match="DOWN">
				<True>
					<DIRECTION>-1</DIRECTION>
				</True>
				<False>
					<DIRECTION>1</DIRECTION>
				</False>
			</Condition>
		</Parameters>
		<ReturnParameters>
