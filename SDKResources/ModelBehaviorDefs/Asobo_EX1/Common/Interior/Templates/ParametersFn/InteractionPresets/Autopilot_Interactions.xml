	<ParametersFn Name="ASOBO_Autopilot_Vertical_Speed_Setting_Config_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Autopilot_Vertical_Speed_Parameters"/>
		</Parameters>
		<Parameters Type="Override">
			<Condition Valid="DECREMENT_ON_UP">
				<True>
					<SET_INTERACTION_DIRECTION>l0 0 &gt; if{ -1 sp1 } els{ 1 sp1 }</SET_INTERACTION_DIRECTION>
				</True>
				<False>
					<SET_INTERACTION_DIRECTION>l0 0 &gt; if{ 1 sp1 } els{ -1 sp1 }</SET_INTERACTION_DIRECTION>
				</False>
			</Condition>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="Asobo_Infinite_Var_Config_Parameters">
				<VAR_ID>VerticalSpeedKnob</VAR_ID>
				<ON_VALUE_SET>#SET_INTERACTION_DIRECTION# #VERTICALSPEED_SETTING_CODE#</ON_VALUE_SET>
			</UseParametersFn>
			<IE_TOOLTIP_VALUE>#IE_TOOLTIP_VALUE#</IE_TOOLTIP_VALUE>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Autopilot_Vertical_Speed_Event_Setting_Config_Parameters">
