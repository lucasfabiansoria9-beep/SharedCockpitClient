	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Autopilot_Leveler_Config_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_AP_Leveler_Parameters" />
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Auto_Define_XStates_Variant_Helper"/>
			<!-- Check position -->
			<POS_OFF_CHECK>#GET_LEVELER# ! </POS_OFF_CHECK>
			<POS_ON_CHECK>#GET_LEVELER#</POS_ON_CHECK>
			<!-- Set position -->
			<POS_OFF_CODE>
				0 #SET_LEVELER#
				#SET_PITCH_LEVELER_OFF#
				(O:APStateWhenLevelerEnabled) if{
					#SET_AP_ON#
				} els{
					#SET_AP_OFF#
				}
			</POS_OFF_CODE>
			<POS_ON_CODE>
				1 #SET_LEVELER#
				#GET_AP_ON# (&gt;O:APStateWhenLevelerEnabled)
				#GET_AP_ON# ! if{
					#SET_AP_ON#
				}
				#SET_PITCH_LEVELER_ON#
			</POS_ON_CODE>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>#SIMVAR_LEVELER#</SIMVAR>
			</UseParametersFn>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>#SIMVAR_AP_ON#</SIMVAR>
			</UseParametersFn>
			<UseParametersFn Name="ASOBO_Append_Init_Code_Helper">
				<INIT_CODE>#GET_AP_ON# ! #GET_LEVELER# and if{ 0 #SET_LEVELER# #SET_PITCH_LEVELER_OFF# }</INIT_CODE>
			</UseParametersFn>
		</ReturnParameters>
	</ParametersFn>
