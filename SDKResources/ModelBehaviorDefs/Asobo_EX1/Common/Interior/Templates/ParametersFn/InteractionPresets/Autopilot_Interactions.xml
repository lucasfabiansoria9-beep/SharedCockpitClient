	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Autopilot_Vertical_Speed_Increment_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_AP_Airspeed_Setting_Parameters" />
			<UseParametersFn Name="ASOBO_AP_VerticalSpeed_Setting_Parameters" />
			<UseParametersFn Name="ASOBO_AP_Pitch_Setting_Parameters" />
			<UseParametersFn Name="ASOBO_AP_FlightLevelChange_Parameters" />
		</Parameters>
		<Parameters Type="Override">
			<Condition Check="DIRECTION" Match="DOWN">
				<True>
					<SET_AIRSPEED_LOCK_VAR>#DEC_AIRSPEED_LOCK_VAR#</SET_AIRSPEED_LOCK_VAR>
					<SET_VS_LOCK_VAR>#DEC_VS_LOCK_VAR# (&gt;H:AP_DN)</SET_VS_LOCK_VAR>
					<SET_PITCH_LOCK_VAR>#DEC_PITCH_LOCK_VAR#</SET_PITCH_LOCK_VAR>
				</True>
				<False>
					<SET_AIRSPEED_LOCK_VAR>#INC_AIRSPEED_LOCK_VAR#</SET_AIRSPEED_LOCK_VAR>
					<SET_VS_LOCK_VAR>#INC_VS_LOCK_VAR# (&gt;H:AP_INC)</SET_VS_LOCK_VAR>
					<SET_PITCH_LOCK_VAR>#INC_PITCH_LOCK_VAR#</SET_PITCH_LOCK_VAR>
				</False>
			</Condition>
		</Parameters>
		<ReturnParameters>
			<VERTICALSPEED_SETTING_CODE>
				#GET_FLIGHT_LEVEL_CHANGE# if{
					#SET_AIRSPEED_LOCK_VAR#
				} els{
					#GET_VS_LOCK# if{
						#SET_VS_LOCK_VAR#
					} els{
						#SET_PITCH_LOCK_VAR#
					}
				}
			</VERTICALSPEED_SETTING_CODE>
			<IE_TOOLTIP_VALUE>
				#GET_FLIGHT_LEVEL_CHANGE# if{
					#GET_AIRSPEED_LOCK_VAR# '%dKts' @Sprintf
				} els{
					#GET_VS_LOCK# if{
						#GET_VS_LOCK_VAR# '%dft/m' @Sprintf
					} els{
						#GET_PITCH_LOCK_VAR# '%.1f&deg;' @Sprintf
					}
				}
			</IE_TOOLTIP_VALUE>
			<IE_VALUE_UNITS>number</IE_VALUE_UNITS>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Autopilot_Vertical_Speed_Parameters">
		<Parameters Type="Default">
			<MIN_VALUE>-5000</MIN_VALUE>
			<MAX_VALUE>5000</MAX_VALUE>
			<INCREMENT>100</INCREMENT>
			<MAX_PITCH>25</MAX_PITCH>
			<SPEED_INCREMENT>1</SPEED_INCREMENT>
			<VS_INCREMENT>100</VS_INCREMENT>
			<PITCH_INCREMENT>0.2</PITCH_INCREMENT>
		</Parameters>
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_AP_Airspeed_Setting_Parameters" />
			<UseParametersFn Name="ASOBO_AP_VerticalSpeed_Setting_Parameters" />
			<UseParametersFn Name="ASOBO_AP_Pitch_Setting_Parameters" />
			<UseParametersFn Name="ASOBO_AP_FlightLevelChange_Parameters" />
		</Parameters>
		<ReturnParameters>
			<VERTICALSPEED_SETTING_CODE>
				#GET_FLIGHT_LEVEL_CHANGE# if{
					#GET_AIRSPEED_LOCK_VAR# #SPEED_INCREMENT# l1 * + 0 max #SET_AIRSPEED_LOCK_VAR#
				} els{
					#GET_VS_LOCK# if{
						#GET_VS_LOCK_VAR# #VS_INCREMENT# l1 * + #SET_VS_LOCK_VAR#
					} els{
						#GET_PITCH_LOCK_VAR# #PITCH_INCREMENT# l1 * + #MAX_PITCH# / 1 min -1 max 16384 * #SET_PITCH_LOCK_VAR#
					}
				}
			</VERTICALSPEED_SETTING_CODE>
			<IE_TOOLTIP_VALUE>
				#GET_FLIGHT_LEVEL_CHANGE# if{
					#GET_AIRSPEED_LOCK_VAR# '%dKts' @Sprintf
				} els{
					#GET_VS_LOCK# if{
						#GET_VS_LOCK_VAR# '%dft/m' @Sprintf
					} els{
						#GET_PITCH_LOCK_VAR# '%.1f&deg;' @Sprintf
					}
				}
			</IE_TOOLTIP_VALUE>
			<IE_INCREMENT>#INCREMENT#</IE_INCREMENT>
			<IE_VALUE_UNITS>number</IE_VALUE_UNITS>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Autopilot_Altitude_Setting_Config_Parameters">
		<Parameters Type="Default">
			<ALTITUDE_ID>1</ALTITUDE_ID>
			<MIN_VALUE>0</MIN_VALUE>
			<MAX_VALUE>50000</MAX_VALUE>
			<INCREMENT>100</INCREMENT>
			<VAR_ID>Altitude_Setting</VAR_ID>
			<VALUE_TYPE>PERCENT</VALUE_TYPE>
		</Parameters>
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_AP_Altitude_Setting_Parameters" />
			<GET_LOCALVAR>(L:1:XMLVAR_#VAR_ID#)</GET_LOCALVAR>
			<SET_LOCALVAR>(&gt;L:1:XMLVAR_#VAR_ID#)</SET_LOCALVAR>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="Asobo_Infinite_Var_Config_Parameters">
				<ON_VALUE_SET>
					#GET_LOCALVAR# #SET_ALTITUDE_LOCK#
					p0 0 &gt; if{ (&gt;H:AP_KNOB_Up) } els{ (&gt;H:AP_KNOB_Down) }
				</ON_VALUE_SET>
			</UseParametersFn>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>#SIMVAR_ALTITUDE_LOCK#</SIMVAR>
			</UseParametersFn>
			<UseParametersFn Name="ASOBO_Append_Init_Code_Helper">
				<INIT_CODE>#GET_ALTITUDE_LOCK# #SET_LOCALVAR#</INIT_CODE>
			</UseParametersFn>
			<INTERACTION_TYPE_VARIANT>Infinite</INTERACTION_TYPE_VARIANT>
			<IE_INCREMENT>#INCREMENT#</IE_INCREMENT>
			<INCREMENT_PER_THRESHOLD>#INCREMENT#</INCREMENT_PER_THRESHOLD>
			<PFN_DELTA_THRESHOLD_VALUES>ASOBO_Drag_Threshold_Medium</PFN_DELTA_THRESHOLD_VALUES>
			<IE_TOOLTIP_VALUE>#GET_LOCALVAR# '%d ft' @Sprintf</IE_TOOLTIP_VALUE>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Autopilot_Altitude_Sync_Event_Setting_Config_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_AP_Altitude_Setting_Parameters" />
