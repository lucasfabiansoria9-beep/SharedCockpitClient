			</UseParametersFn>
			<!-- Check position -->
			<POS_OFF_CHECK>#GET_APU_GENERATOR_SWITCH# !</POS_OFF_CHECK>
			<POS_ON_CHECK>#GET_APU_GENERATOR_SWITCH#</POS_ON_CHECK>
