<ModelBehaviors>
	<!-- ################################## ELECTRICAL ################################ -->
	<!-- ############################################################################## -->
	<!-- Battery -->
	<ParametersFn Name="ASOBO_Battery_State_Settings_Config">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Battery_Parameters"/>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Auto_Define_XStates_Variant_Helper"/>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>#SIMVAR_BATTERY_SWITCH#</SIMVAR>
			</UseParametersFn>
			<!-- Check position -->
			<POS_OFF_CHECK>#GET_BATTERY_SWITCH# !</POS_OFF_CHECK>
			<POS_ON_CHECK>#GET_BATTERY_SWITCH#</POS_ON_CHECK>
			<!-- Set position -->
			<POS_OFF_CODE>0 #SET_BATTERY#</POS_OFF_CODE>
			<POS_ON_CODE>1 #SET_BATTERY#</POS_ON_CODE>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<!-- Circuit connection -->
	<ParametersFn Name="ASOBO_Circuit_Power_Settings_Config">
		<Parameters Type="Default">
			<UseParametersFn Name="ASOBO_Circuit_Parameters"/>
		</Parameters>
		<ReturnParameters>
			<VALUE_TYPE>Percent</VALUE_TYPE>
			<IE_GET_STATE>#GET_CIRCUIT_POWER_SETTING#</IE_GET_STATE>
			<IE_SET_STATE>#SET_CIRCUIT_POWER_SETTING#</IE_SET_STATE>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>#SIMVAR_CIRCUIT_POWER#</SIMVAR>
			</UseParametersFn>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Circuit_Switch_Settings_Config">
