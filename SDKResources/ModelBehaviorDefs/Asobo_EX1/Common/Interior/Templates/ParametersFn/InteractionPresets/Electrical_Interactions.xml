		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Auto_Define_XStates_Variant_Helper"/>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>#SIMVAR_CIRCUIT_ON#</SIMVAR>
			</UseParametersFn>
			<!-- Check position -->
			<POS_OFF_CHECK>#GET_CIRCUIT_SWITCH# !</POS_OFF_CHECK>
			<POS_ON_CHECK>#GET_CIRCUIT_SWITCH#</POS_ON_CHECK>
			<!-- Set position -->
			<POS_OFF_CODE>0 #SET_CIRCUIT#</POS_OFF_CODE>
			<POS_ON_CODE>1 #SET_CIRCUIT#</POS_ON_CODE>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<!-- Circuit connection -->
	<ParametersFn Name="ASOBO_Circuit_State_Settings_Config">
		<Parameters Type="Default">
