		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Auto_Define_XStates_Variant_Helper"/>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>#SIMVAR_CONNECTION#</SIMVAR>
			</UseParametersFn>
			<!-- Check position -->
			<POS_OFF_CHECK>#GET_CONNECTION# !</POS_OFF_CHECK>
			<POS_ON_CHECK>#GET_CONNECTION#</POS_ON_CHECK>
			<!-- Set position -->
			<POS_OFF_CODE>0 #SET_CONNECTION#</POS_OFF_CODE>
			<POS_ON_CODE>1 #SET_CONNECTION#</POS_ON_CODE>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<!-- Bus connection -->
	<ParametersFn Name="ASOBO_Bus_State_Settings_Config">
		<Parameters Type="Default">
			<UseParametersFn Name="ASOBO_Bus_Connection_Parameters"/>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Auto_Define_XStates_Variant_Helper"/>
			<!-- Check position -->
			<POS_OFF_CHECK>#GET_CONNECTION# !</POS_OFF_CHECK>
			<POS_ON_CHECK>#GET_CONNECTION#</POS_ON_CHECK>
			<!-- Set position -->
			<POS_OFF_CODE>0 #SET_CONNECTION#</POS_OFF_CODE>
			<POS_ON_CODE>1 #SET_CONNECTION#</POS_ON_CODE>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<!-- Bus connection swap -->
	<ParametersFn Name="ASOBO_Bus_Swap_Settings_Config">
		<Parameters Type="Default">
			<ID>1</ID>
		</Parameters>
		<Parameters Type="Default">
			<BUS_ID_1>#ID#</BUS_ID_1>
			<BUS_ID_2>#ID#</BUS_ID_2>
		</Parameters>
		<Parameters Type="Default">
			<UseParametersFn Name="ASOBO_Bus_Connection_Parameters">
				<BUS_ID>#BUS_ID_1#</BUS_ID>
				<OUT_PARAM_SUFFIX>_1</OUT_PARAM_SUFFIX>
			</UseParametersFn>
			<UseParametersFn Name="ASOBO_Bus_Connection_Parameters">
				<BUS_ID>#BUS_ID_2#</BUS_ID>
				<OUT_PARAM_SUFFIX>_2</OUT_PARAM_SUFFIX>
			</UseParametersFn>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Auto_Define_XStates_Variant_Helper"/>
			<!-- Check position -->
			<POS_OFF_CHECK>#GET_1_CONNECTION# ! #GET_2_CONNECTION# and</POS_OFF_CHECK>
			<POS_ON_CHECK>#GET_1_CONNECTION# #GET_2_CONNECTION# ! and</POS_ON_CHECK>
			<!-- Set position -->
			<POS_OFF_CODE>0 #SET_1_CONNECTION# 1 #SET_2_CONNECTION#</POS_OFF_CODE>
			<POS_ON_CODE>1 #SET_1_CONNECTION# 0 #SET_2_CONNECTION#</POS_ON_CODE>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
