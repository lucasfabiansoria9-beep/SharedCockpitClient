<ModelBehaviors>
	<!-- ################################## FUEL ###################################### -->
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Fuel_Valve_Settings_Config">
		<Parameters Type="Default">
			<ID>1</ID>
		</Parameters>
		<Parameters Type="Default">
			<VALVE_ID>#ID#</VALVE_ID>
		</Parameters>
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Fuel_Valve_Parameters"/>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Auto_Define_XStates_Variant_Helper"/>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>#SIMVAR_VALVE#</SIMVAR>
			</UseParametersFn>
			<!-- Check position -->
			<POS_CLOSED_CHECK>#GET_VALVE# !</POS_CLOSED_CHECK>
			<POS_OPEN_CHECK>#GET_VALVE#</POS_OPEN_CHECK>
			<!-- Set position -->
			<POS_CLOSED_CODE>0 #SET_VALVE#</POS_CLOSED_CODE>
			<POS_OPEN_CODE>1 #SET_VALVE#</POS_OPEN_CODE>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Fuel_Trigger_Crossfeed_Cutoff_Settings_Config">
		<Parameters Type="Default">
			<ID>1</ID>
		</Parameters>
		<Parameters Type="Default">
			<TRIGGER_ID>#ID#</TRIGGER_ID>
			<VALVE_ID>#ID#</VALVE_ID>
		</Parameters>
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Fuel_Trigger_Parameters"/>
			<UseParametersFn Name="ASOBO_Fuel_Valve_Parameters"/>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Auto_Define_XStates_Variant_Helper"/>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>#SIMVAR_TRIGGER#</SIMVAR>
			</UseParametersFn>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>#SIMVAR_VALVE#</SIMVAR>
			</UseParametersFn>
			<!-- Check position -->
			<POS_ON_CHECK>#GET_TRIGGER# ! #GET_VALVE# and</POS_ON_CHECK>
			<POS_CROSSFEED_CHECK>#GET_TRIGGER# #GET_VALVE# and</POS_CROSSFEED_CHECK>
			<POS_OFF_CHECK>#GET_VALVE# !</POS_OFF_CHECK>
			<!-- Set position -->
			<POS_ON_CODE>0 #SET_TRIGGER# 1 #SET_VALVE#</POS_ON_CODE>
			<POS_CROSSFEED_CODE>1 #SET_TRIGGER# 1 #SET_VALVE#</POS_CROSSFEED_CODE>
			<Condition NotEmpty="BLOCKER_IE">
				<True>
					<POS_OFF_CODE>(B:#BLOCKER_IE#) if{ 0 #SET_VALVE# }</POS_OFF_CODE>
					<SHOW_INTERMEDIARY_POS_CONDITION>(B:#BLOCKER_IE#) and</SHOW_INTERMEDIARY_POS_CONDITION>
				</True>
				<False>
					<POS_OFF_CODE>0 #SET_VALVE#</POS_OFF_CODE>
				</False>
			</Condition>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Fuel_Pump_Settings_Config">
		<Parameters Type="Default">
			<ID>1</ID>
		</Parameters>
		<Parameters Type="Default">
			<PUMP_ID>#ID#</PUMP_ID>
		</Parameters>
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Fuel_Pump_Parameters"/>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Auto_Define_XStates_Variant_Helper"/>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>#SIMVAR_PUMP#</SIMVAR>
			</UseParametersFn>
			<!-- Check position -->
			<POS_OFF_CHECK>#GET_PUMP# !</POS_OFF_CHECK>
			<POS_ON_CHECK>#GET_PUMP#</POS_ON_CHECK>
			<!-- Set position -->
			<POS_OFF_CODE>0 #SET_PUMP#</POS_OFF_CODE>
			<POS_ON_CODE>1 #SET_PUMP#</POS_ON_CODE>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
</ModelBehaviors>