<ModelBehaviors>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Liquid_Dropping_Lever_Settings_Config">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Liquid_Dropping_Door_Parameters"/>
		</Parameters>
		<ReturnParameters>
			<VALUE_TYPE>U16K</VALUE_TYPE>
			<MIN_VALUE>0</MIN_VALUE>
			<MAX_VALUE>@16K</MAX_VALUE>
			<IE_GET_STATE>#GET_LIQUID_DROPPING_DOOR_TARGET_POSITION_16K#</IE_GET_STATE>
			<IE_SET_STATE>#SET_LIQUID_DROPPING_DOOR_TARGET_POSITION_16K#</IE_SET_STATE>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>#SIMVAR_LIQUID_DROPPING_DOOR_TARGET_POSITION#</SIMVAR>
			</UseParametersFn>
			<UseParametersFn Name="ASOBO_Add_Set_Binding_Helper">
				<ALIAS_ID>TOGGLE</ALIAS_ID>
				<PARAM_0>#GET_LIQUID_DROPPING_DOOR_TARGET_POSITION_16K# 0 &gt; if{ 0 } els{ @16K }</PARAM_0>
				<PARAM_0_IS_DYNAMIC>True</PARAM_0_IS_DYNAMIC>
			</UseParametersFn>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Liquid_Dropping_Command_Group_Lever_Settings_Config">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Liquid_Dropping_Door_Parameters"/>
			<!-- Command group does not have a set, so requires a DOOR_ID to get the state of one of the doors in the command group -->
			<UseParametersFn Name="ASOBO_Liquid_Dropping_Command_Group_Parameters"/>
		</Parameters>
		<ReturnParameters>
			<VALUE_TYPE>U16K</VALUE_TYPE>
			<MIN_VALUE>0</MIN_VALUE>
			<MAX_VALUE>@16K</MAX_VALUE>
			<IE_GET_STATE>#GET_LIQUID_DROPPING_DOOR_TARGET_POSITION_16K#</IE_GET_STATE>
			<IE_SET_STATE>#SET_LIQUID_DROPPING_COMMAND_GROUP_TARGET_POSITION_16K#</IE_SET_STATE>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>#SIMVAR_LIQUID_DROPPING_DOOR_TARGET_POSITION#</SIMVAR>
			</UseParametersFn>
			<UseParametersFn Name="ASOBO_Add_Set_Binding_Helper">
				<ALIAS_ID>TOGGLE</ALIAS_ID>
				<PARAM_0>#GET_LIQUID_DROPPING_DOOR_TARGET_POSITION_16K# 0 &gt; if{ 0 } els{ @16K }</PARAM_0>
				<PARAM_0_IS_DYNAMIC>True</PARAM_0_IS_DYNAMIC>
			</UseParametersFn>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Liquid_Dropping_Command_Group_Button_Settings_Config">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Liquid_Dropping_Door_Parameters"/>
			<!-- Command group does not have a set, so requires a DOOR_ID to get the state of one of the doors in the command group -->
			<UseParametersFn Name="ASOBO_Liquid_Dropping_Command_Group_Parameters"/>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Auto_Define_XStates_Variant_Helper"/>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>#SIMVAR_LIQUID_DROPPING_DOOR_TARGET_POSITION#</SIMVAR>
			</UseParametersFn>
			<!-- Check position -->
			<POS_CLOSED_CHECK>#GET_LIQUID_DROPPING_DOOR_TARGET_POSITION_16K# 0 ==</POS_CLOSED_CHECK>
			<POS_OPEN_CHECK>#GET_LIQUID_DROPPING_DOOR_TARGET_POSITION_16K# 0 &gt;</POS_OPEN_CHECK>
			<!-- Set position -->
			<POS_CLOSED_CODE>0 #SET_LIQUID_DROPPING_COMMAND_GROUP_TARGET_POSITION_16K#</POS_CLOSED_CODE>
			<POS_OPEN_CODE>@16K #SET_LIQUID_DROPPING_COMMAND_GROUP_TARGET_POSITION_16K#</POS_OPEN_CODE>
			<UseParametersFn Name="ASOBO_Add_Set_Binding_Helper">
				<ALIAS_ID>TOGGLE</ALIAS_ID>
				<PARAM_0>#GET_LIQUID_DROPPING_DOOR_TARGET_POSITION_16K# 0 &gt; if{ 0 } els{ @16K }</PARAM_0>
				<PARAM_0_IS_DYNAMIC>True</PARAM_0_IS_DYNAMIC>
			</UseParametersFn>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="Get_Procedure_Camera_Pitch_Parameters">
		<ReturnParameters>
			<UseParametersFn Name="Get_Procedure_Camera_Parameters">
				<INCREMENT_PER_SECOND_PCT>100</INCREMENT_PER_SECOND_PCT>
				<CAMERA_AXIS>PITCH</CAMERA_AXIS>
			</UseParametersFn>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="Get_Procedure_Camera_Yaw_Parameters">
		<ReturnParameters>
			<UseParametersFn Name="Get_Procedure_Camera_Parameters">
				<CAMERA_AXIS>YAW</CAMERA_AXIS>
				<WRAP_CODE>l0 abs @16K &gt;= if{ neg }</WRAP_CODE>
				<!-- Slight offset to escape locking if we stop right at +/-16K -->
				<TARGET_VALUE>@16K 1 +</TARGET_VALUE>
			</UseParametersFn>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="Get_Procedure_Camera_Parameters">
		<Parameters Type="Default">
			<TARGET_VALUE>@16K</TARGET_VALUE>
			<WRAP_CODE/>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="Get_Procedure_Incremental_Switch_Parameters">
				<SURFACE_TYPE>#CAMERA_AXIS#</SURFACE_TYPE>
				<GET_SURFACE_VALUE_16K>(A:DIRECTORS CUT CAMERA #CAMERA_AXIS#, percent) 100 / #TARGET_VALUE# *</GET_SURFACE_VALUE_16K>
				<SET_SURFACE_VALUE_16K>#WRAP_CODE# 100 * @16K / (&gt;A:DIRECTORS CUT CAMERA #CAMERA_AXIS#, percent)</SET_SURFACE_VALUE_16K>
			</UseParametersFn>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Incremental_Camera_Control_Settings_Config">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Enum_Parameters_Helper">
				<SaveParameters ID="Enum_Values">
					<POS_NEUTRAL>0</POS_NEUTRAL>
					<POS_DN>1</POS_DN>
					<POS_UP>2</POS_UP>
					<POS_L>3</POS_L>
					<POS_R>4</POS_R>
				</SaveParameters>
			</UseParametersFn>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Add_Set_Binding_Helper">
				<ALIAS_ID>Yaw_Increase</ALIAS_ID>
				<PARAM_0>1</PARAM_0>
			</UseParametersFn>
			<UseParametersFn Name="ASOBO_Add_Set_Binding_Helper">
				<ALIAS_ID>Yaw_Decrease</ALIAS_ID>
				<PARAM_0>-1</PARAM_0>
			</UseParametersFn>
			<UseParametersFn Name="ASOBO_Add_Set_Binding_Helper">
				<ALIAS_ID>Pitch_Increase</ALIAS_ID>
				<PARAM_0>0</PARAM_0>
				<PARAM_1>1</PARAM_1>
			</UseParametersFn>
			<UseParametersFn Name="ASOBO_Add_Set_Binding_Helper">
				<ALIAS_ID>Pitch_Decrease</ALIAS_ID>
				<PARAM_0>0</PARAM_0>
				<PARAM_1>-1</PARAM_1>
			</UseParametersFn>
			<UseParametersFn Name="ASOBO_Add_Set_Binding_Helper">
				<ALIAS_ID>Neutral</ALIAS_ID>
			</UseParametersFn>

			<UseParametersFn Name="ASOBO_Auto_Define_XStates_Variant_Helper"/>
			<!-- Check position -->
			<POS_NEUTRAL_CHECK>#IS_POS_NEUTRAL#</POS_NEUTRAL_CHECK>
			<POS_UP_CHECK>#IS_POS_UP#</POS_UP_CHECK>
			<POS_DN_CHECK>#IS_POS_DN#</POS_DN_CHECK>
			<POS_L_CHECK>#IS_POS_L#</POS_L_CHECK>
			<POS_R_CHECK>#IS_POS_R#</POS_R_CHECK>
			<!-- Set position -->
			<POS_NEUTRAL_CODE>#SET_POS_NEUTRAL# (&gt;B:PROCEDURE_CAMERA_YAW_Neutral) (&gt;B:PROCEDURE_CAMERA_PITCH_Neutral)</POS_NEUTRAL_CODE>
			<POS_UP_CODE>#SET_POS_UP# (&gt;B:PROCEDURE_CAMERA_PITCH_Increment) (&gt;B:PROCEDURE_CAMERA_YAW_Neutral)</POS_UP_CODE>
			<POS_DN_CODE>#SET_POS_DN# (&gt;B:PROCEDURE_CAMERA_PITCH_Decrement) (&gt;B:PROCEDURE_CAMERA_YAW_Neutral)</POS_DN_CODE>
			<POS_L_CODE>#SET_POS_L# (&gt;B:PROCEDURE_CAMERA_YAW_Decrement) (&gt;B:PROCEDURE_CAMERA_PITCH_Neutral)</POS_L_CODE>
			<POS_R_CODE>#SET_POS_R# (&gt;B:PROCEDURE_CAMERA_YAW_Increment) (&gt;B:PROCEDURE_CAMERA_PITCH_Neutral)</POS_R_CODE>
			<!-- Tooltip -->
			<!--IE_TOOLTIP_VALUE>#FORMAT_IE_TOOLTIP_X_AXIS# #FORMAT_IE_TOOLTIP_Y_AXIS# '%s / %s' @Sprintf</IE_TOOLTIP_VALUE-->
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Lead_Pole_Lever_Settings_Config">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Lead_Pole_Parameters"/>
		</Parameters>
		<ReturnParameters>
			<VALUE_TYPE>U16K</VALUE_TYPE>
			<MIN_VALUE>0</MIN_VALUE>
			<MAX_VALUE>@16K</MAX_VALUE>
			<IE_GET_STATE>#GET_LEAD_POLE_SET# @16K *</IE_GET_STATE>
			<IE_SET_STATE>#SET_LEAD_POLE_SET_16K#</IE_SET_STATE>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>#SIMVAR_LEAD_POLE_SET#</SIMVAR>
			</UseParametersFn>
			<UseParametersFn Name="ASOBO_Add_Set_Binding_Helper">
				<ALIAS_ID>TOGGLE</ALIAS_ID>
				<PARAM_0>#GET_LEAD_POLE_SET# 0 &gt; if{ 0 } els{ @16K }</PARAM_0>
				<PARAM_0_IS_DYNAMIC>True</PARAM_0_IS_DYNAMIC>
			</UseParametersFn>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Grapple_Hook_Settings_Config">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Grapple_Hook_Parameters"/>
		</Parameters>
		<ReturnParameters>
			<UseParametersFn Name="ASOBO_Auto_Define_XStates_Variant_Helper"/>
			<UseParametersFn Name="ASOBO_Watch_Simvar_Helper">
				<SIMVAR>#SIMVAR_GRAPPLE_HOOK_SET#</SIMVAR>
			</UseParametersFn>
			<!-- Check position -->
			<POS_OFF_CHECK>#GET_GRAPPLE_HOOK_SET# !</POS_OFF_CHECK>
			<POS_ON_CHECK>#GET_GRAPPLE_HOOK_SET#</POS_ON_CHECK>
			<!-- Set position -->
			<POS_OFF_CODE>0 #SET_GRAPPLE_HOOK_SET#</POS_OFF_CODE>
			<POS_ON_CODE>1 #SET_GRAPPLE_HOOK_SET#</POS_ON_CODE>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
</ModelBehaviors>