<ModelBehaviors>
	<!-- ################################## HANDLING ################################## -->
	<!-- ############################################################################## -->
	<!-- Yoke/Stick -->
	<!-- IN (opt): PART_ID_ONLY (If true, will only create the part_id for highlights) -->
	<!-- IN (opt): ANIM_NAME_YOKE_X (Left rudder node name) -->
	<!-- IN (opt): ANIM_NAME_YOKE_Y (Right rudder node name) -->
	<!-- IN: See ASOBO_Yoke_Animation_Parameters -->
	<Template Name="ASOBO_IT_HANDLING_Yoke_SubTemplate">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Common_Parameters_Helper"/>
		</Parameters>
		<Parameters Type="Override">
			<Condition Valid="PART_ID_ONLY">
				<False>
					<UseParametersFn Name="ASOBO_PFN_Call_Overridable_ParametersFn_Helper">
						<PFN_PARAM_DEFAULT>ASOBO_Yoke_Animation_Parameters</PFN_PARAM_DEFAULT>
						<PFN_PARAM_NAME>HANDLING_YOKE_ANIMATION_PARAMETERS_FN</PFN_PARAM_NAME>
					</UseParametersFn>
				</False>
			</Condition>
		</Parameters>
		<Parameters Type="Default">
			<Condition Valid="PART_ID_ONLY">
				<False>
					<Switch>
						<Case Valid="ALWAYS_USE_LINEAR_ANIMATION_SIMVARS">
							<ANIM_CODE_X>#ANIM_SIMVAR_X_LINEAR_FORMATTED#</ANIM_CODE_X>
						</Case>
						<Default>
							<ANIM_CODE_X>@E_IsInVR if{ #ANIM_SIMVAR_X_LINEAR_FORMATTED# } els{ #ANIM_SIMVAR_X_FORMATTED# }</ANIM_CODE_X>
						</Default>
					</Switch>
					<ANIM_CODE_Y>#ANIM_SIMVAR_Y_FORMATTED#</ANIM_CODE_Y>
				</False>
			</Condition>
		</Parameters>
		<Component ID="#NODE_ID#" Node="#NODE_ID#">
			<UseTemplate Name="@BasePartIdTemplate"/>
			<Condition Valid="PART_ID_ONLY">
				<False>
					<UseTemplate Name="ASOBO_GT_Anim_Code_Template">
						<ANIM_NAME>#ANIM_NAME_YOKE_X#</ANIM_NAME>
						<ANIM_CODE>#ANIM_CODE_X#</ANIM_CODE>
					</UseTemplate>
					<UseTemplate Name="ASOBO_GT_Anim_Code_Template">
						<ANIM_NAME>#ANIM_NAME_YOKE_Y#</ANIM_NAME>
						<ANIM_CODE>#ANIM_CODE_Y#</ANIM_CODE>
					</UseTemplate>
				</False>
			</Condition>
		</Component>
	</Template>
	<!-- ############################################################################## -->
	<!-- Rudder pedals -->
	<!-- IN (opt): PART_ID_ONLY (If true, will only create the part_id for highlights) -->
	<!-- IN (opt): NODE_ID_LEFT (Left rudder node name) -->
	<!-- IN (opt): NODE_ID_RIGHT (Right rudder node name) -->
	<!-- IN: See ASOBO_Rudder_Pedals_Animation_Parameters -->
	<Template Name="ASOBO_IT_HANDLING_Rudder_Pedals_SubTemplate">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Common_Parameters_Helper"/>
		</Parameters>
		<Parameters Type="Override">
			<Condition Valid="PART_ID_ONLY">
				<False>
					<UseParametersFn Name="ASOBO_PFN_Call_Overridable_ParametersFn_Helper">
						<PFN_PARAM_DEFAULT>ASOBO_Rudder_Pedals_Animation_Parameters</PFN_PARAM_DEFAULT>
						<PFN_PARAM_NAME>HANDLING_RUDDER_PEDALS_ANIMATION_PARAMETERS_FN</PFN_PARAM_NAME>
					</UseParametersFn>
				</False>
			</Condition>
		</Parameters>
		<Component ID="#NODE_ID_LEFT#" Node="#NODE_ID_LEFT#">
			<UseTemplate Name="@BasePartIdTemplate"/>
			<Condition Valid="PART_ID_ONLY">
				<False>
					<UseTemplate Name="ASOBO_GT_Anim_Simvar_Code_Template">
						<ANIM_NAME>#ANIM_NAME#</ANIM_NAME>
						<ANIM_SIMVAR>#SIMVAR_RUDDER_PEDALS#</ANIM_SIMVAR>
						<ANIM_SIMVAR_UNITS>#ANIM_SIMVAR_UNITS_PEDALS#</ANIM_SIMVAR_UNITS>
						<ANIM_SIMVAR_BIAS>#ANIM_SIMVAR_BIAS_PEDALS#</ANIM_SIMVAR_BIAS>
						<ANIM_SIMVAR_SCALE>#ANIM_SIMVAR_SCALE_PEDALS#</ANIM_SIMVAR_SCALE>
					</UseTemplate>
					<Condition Valid="USE_BRAKES">
						<UseTemplate Name="ASOBO_IT_HANDLING_Brakes_Animation_Template"/>
					</Condition>
				</False>
			</Condition>
		</Component>
		<Component ID="#NODE_ID_RIGHT#" Node="#NODE_ID_RIGHT#">
			<UseTemplate Name="@BasePartIdTemplate"/>
		</Component>
	</Template>
	<!-- ############################################################################## -->
	<!-- IN (opt): DIFFERENTIATE_BRAKE_ANIM_L_R (If true, will use different animations for left and right brakes) -->
	<!-- IN (if DIFFERENTIATE_BRAKE_ANIM_L_R) (opt): BRAKE_ANIM_NAME_L (Left brake animation name) -->
	<!-- IN (if DIFFERENTIATE_BRAKE_ANIM_L_R) (opt): BRAKE_ANIM_NAME_R (Right brake animation name) -->
	<!-- IN (if NOT DIFFERENTIATE_BRAKE_ANIM_L_R)) (opt): BRAKE_ANIM_NAME (brakes animation name) -->
	<!-- IN: See ASOBO_Brakes_Animation_Parameters -->
	<Template Name="ASOBO_IT_HANDLING_Brakes_Animation_SubTemplate">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_PFN_Call_Overridable_ParametersFn_Helper">
				<PFN_PARAM_DEFAULT>ASOBO_Brakes_Animation_Parameters</PFN_PARAM_DEFAULT>
				<PFN_PARAM_NAME>HANDLING_BRAKES_ANIMATION_PARAMETERS_FN</PFN_PARAM_NAME>
			</UseParametersFn>
		</Parameters>
		<Parameters Type="Override">
			<ANIM_SIMVAR_UNITS>#ANIM_SIMVAR_UNITS_BRAKES#</ANIM_SIMVAR_UNITS>
			<ANIM_SIMVAR_BIAS>#ANIM_SIMVAR_BIAS_BRAKES#</ANIM_SIMVAR_BIAS>
			<ANIM_SIMVAR_SCALE>#ANIM_SIMVAR_SCALE_BRAKES#</ANIM_SIMVAR_SCALE>
		</Parameters>
		<Condition Valid="DIFFERENTIATE_BRAKE_ANIM_L_R">
			<True>
				<UseTemplate Name="ASOBO_GT_Anim_Simvar_Code_Template">
					<ANIM_NAME>#BRAKE_ANIM_NAME_L#</ANIM_NAME>
					<ANIM_SIMVAR>#SIMVAR_RUDDER_PEDALS_BRAKE_L#</ANIM_SIMVAR>
				</UseTemplate>
				<UseTemplate Name="ASOBO_GT_Anim_Simvar_Code_Template">
					<ANIM_NAME>#BRAKE_ANIM_NAME_R#</ANIM_NAME>
					<ANIM_SIMVAR>#SIMVAR_RUDDER_PEDALS_BRAKE_R#</ANIM_SIMVAR>
				</UseTemplate>
			</True>
			<False>
				<UseTemplate Name="ASOBO_GT_Anim_Simvar_Code_Template">
					<ANIM_NAME>#BRAKE_ANIM_NAME#</ANIM_NAME>
					<ANIM_SIMVAR>#SIMVAR_RUDDER_PEDALS_BRAKE#</ANIM_SIMVAR>
				</UseTemplate>
			</False>
		</Condition>
	</Template>
	<!-- ############################################################################## -->
	<!-- IN: See ASOBO_Trim_Indicator_Parameters -->
	<Template Name="ASOBO_IT_HANDLING_Trim_Indicator_SubTemplate">
		<Parameters Type="Default">
			<ANIM_NAME>HANDLING_#TRIM_TYPE#_Trim_Indicator</ANIM_NAME>
		</Parameters>
		<UseTemplate Name="ASOBO_GT_Anim_Code_Template">
			<UseParametersFn Name="ASOBO_Trim_Indicator_Animation_Parameters"/>
		</UseTemplate>
	</Template>
	<!-- ############################################################################## -->
	<Template Name="ASOBO_IT_HANDLING_Tiller_SubTemplate">
		<Parameters Type="Default">
			<IN_BASE_NAME>HANDLING_Tiller</IN_BASE_NAME>
		</Parameters>
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Common_Parameters_Helper"/>
		</Parameters>
		<UseTemplate Name="ASOBO_GT_Anim_Code_Template">
			<UseParametersFn Name="ASOBO_Tiller_Parameters"/>
		</UseTemplate>
	</Template>
	<!-- ############################################################################## -->
</ModelBehaviors>