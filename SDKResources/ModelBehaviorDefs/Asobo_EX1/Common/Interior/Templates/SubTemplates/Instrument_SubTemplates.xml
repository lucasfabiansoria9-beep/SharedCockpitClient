	<Template Name="ASOBO_IT_Hourmeter_Indicator_SubTemplate">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Hourmeter_Parameters"/>
		</Parameters>
		<Parameters Type="Default">
			<ANIM_LAG>100</ANIM_LAG>
			<DECIMAL_SHOWN>1</DECIMAL_SHOWN>
			<Condition Valid="USE_HOBBS_TIME">
				<True>
					<GET_ELAPSED_TIME>#GET_ELAPSED_ENG_HOBBS_TIME#</GET_ELAPSED_TIME>
				</True>
				<False>
					<GET_ELAPSED_TIME>#GET_ELAPSED_ENG_TIME#</GET_ELAPSED_TIME>
				</False>
			</Condition>
		</Parameters>
		<Parameters Type="Override">
			<DIGIT_PRECISION Process="Int">#DECIMAL_SHOWN# 1 +</DIGIT_PRECISION>
		</Parameters>
			<Loop>
				<Setup>
					<Param>DIGIT_INDEX</Param>
					<Value>ANIM_NAME</Value>
					<Parameters>
						<LoadParameters ID="Hourmeter_Indicator_DIGIT_INDEX_TO_ANIM_NAME"/>
						<RemoveSavedParameters ID="Hourmeter_Indicator_DIGIT_INDEX_TO_ANIM_NAME"/>
					</Parameters>
				</Setup>
				<Do>
					<Parameters Type="Override">
						<PRV_DIGIT_INDEX Process="Int">#DIGIT_INDEX# 1 -</PRV_DIGIT_INDEX>
						<NXT_DIGIT_INDEX Process="Int">#DIGIT_INDEX# 1 +</NXT_DIGIT_INDEX>
					</Parameters>
					<Parameters Type="Override">
						<PMOD_FACTOR Process="Float">10 #NXT_DIGIT_INDEX# #DIGIT_PRECISION# - pow</PMOD_FACTOR>
						<DIV_FACTOR Process="Float">10 #DIGIT_INDEX# #DIGIT_PRECISION# - pow</DIV_FACTOR>
					</Parameters>
					<Parameters Type="Override" Lifetime="Loop">
						<CUR_VAL>l0 #PMOD_FACTOR# pmod #DIV_FACTOR# /</CUR_VAL>
					</Parameters>
					<Parameters Type="Override" Lifetime="Loop">
						<Condition Check="ELAPSED_TO_DIGITS_VAL">
							<False>
								<ELAPSED_TO_DIGITS_VAL>#CUR_VAL# sp#DIGIT_INDEX#</ELAPSED_TO_DIGITS_VAL>
								<DIGITS_VAL_TO_ANIM_POS>0 l#DIGIT_INDEX# +</DIGITS_VAL_TO_ANIM_POS>
							</False>
							<True>
								<ELAPSED_TO_DIGITS_VAL>#ELAPSED_TO_DIGITS_VAL# #CUR_VAL# flr sp#DIGIT_INDEX#</ELAPSED_TO_DIGITS_VAL>
								<DIGITS_VAL_TO_ANIM_POS>#DIGITS_VAL_TO_ANIM_POS# sp#PRV_DIGIT_INDEX# l#PRV_DIGIT_INDEX# 9 10 0 1 (F:MapRange) l#DIGIT_INDEX# +</DIGITS_VAL_TO_ANIM_POS>
							</True>
						</Condition>
					</Parameters>
					<UseTemplate Name="ASOBO_GT_Anim_Template">
						<ANIM_CODE>
							#GET_ELAPSED_TIME# sp0
							#ELAPSED_TO_DIGITS_VAL#
							#DIGITS_VAL_TO_ANIM_POS#
							10 *
						</ANIM_CODE>
						<ANIM_WRAP>True</ANIM_WRAP>
					</UseTemplate>
				</Do>
			</Loop>
	</Template>
