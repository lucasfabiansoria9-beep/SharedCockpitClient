	</Template>
	<!-- ############################################################################## -->
	<Template Name="ASOBO_ET_COMMON_Dirt_Multiple_Template">
		<Parameters Type="Override">
			<TEMPLATE_TO_USE>ASOBO_ET_COMMON_Dirt_Template</TEMPLATE_TO_USE>
		</Parameters>
		<Loop>
			<Setup>
				<Param>NODE_ID</Param>
				<Parameters>
					<LoadParameters ID="Dirt_NODE_IDs"/>
					<RemoveSavedParameters ID="Dirt_NODE_IDs"/>
				</Parameters>
			</Setup>
			<Do>
				<UseTemplate Name="ASOBO_GT_Component_Template"/>
			</Do>
		</Loop>
	</Template>
