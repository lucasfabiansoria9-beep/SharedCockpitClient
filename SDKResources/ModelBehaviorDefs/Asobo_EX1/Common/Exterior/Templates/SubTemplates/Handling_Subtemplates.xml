			<UseParametersFn Name="ASOBO_Get_Spoilers_Animation_Parameters"/>
		</Parameters>
		<Condition NotEmpty="ANIM_NAME_L">
			<UseTemplate Name="ASOBO_GT_Anim_Template">
				<ANIM_NAME>#ANIM_NAME_L#</ANIM_NAME>
				<ANIM_CODE>#ANIM_CODE_L#</ANIM_CODE>
			</UseTemplate>
		</Condition>
		<Condition NotEmpty="ANIM_NAME_R">
			<UseTemplate Name="ASOBO_GT_Anim_Template">
