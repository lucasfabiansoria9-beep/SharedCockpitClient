		<UseTemplate Name="ASOBO_GT_Anim_Template">
			<ANIM_CODE>#GET_SKI_ANIMATION#</ANIM_CODE>
		</UseTemplate>
	</Template>
	<!-- ############################################################################## -->
</ModelBehaviors>