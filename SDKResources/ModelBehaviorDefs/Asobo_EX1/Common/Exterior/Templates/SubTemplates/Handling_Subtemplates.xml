<ModelBehaviors>
	<!-- ################################## HANDLING ################################## -->
	<!-- ############################################################################## -->
	<Template Name="ASOBO_ET_HANDLING_Surface_SubTemplate">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Surface_Animation_Parameters"/>
		</Parameters>
		<Condition Valid="USE_DIFFERENT_L_R_ANIMATIONS">
			<True>
				<Condition NotEmpty="ANIM_NAME_L">
					<UseTemplate Name="ASOBO_GT_Anim_Template">
						<ANIM_NAME>#ANIM_NAME_L#</ANIM_NAME>
						<ANIM_CODE>#ANIM_CODE_L#</ANIM_CODE>
					</UseTemplate>
				</Condition>
				<Condition NotEmpty="ANIM_NAME_R">
					<UseTemplate Name="ASOBO_GT_Anim_Template">
						<ANIM_NAME>#ANIM_NAME_R#</ANIM_NAME>
						<ANIM_CODE>#ANIM_CODE_R#</ANIM_CODE>
					</UseTemplate>
				</Condition>
			</True>
			<False>
				<UseTemplate Name="ASOBO_GT_Anim_Template"/>
			</False>
		</Condition>
	</Template>
	<!-- ############################################################################## -->
	<Template Name="ASOBO_ET_HANDLING_Flaps_Slats_SubTemplate">
