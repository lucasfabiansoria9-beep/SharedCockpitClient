				<ANIM_NAME>#ANIM_NAME_L#</ANIM_NAME>
				<ANIM_CODE>#ANIM_CODE_L#</ANIM_CODE>
			</UseTemplate>
		</Condition>
		<Condition NotEmpty="ANIM_NAME_R">
			<UseTemplate Name="ASOBO_GT_Anim_Template">
				<ANIM_NAME>#ANIM_NAME_R#</ANIM_NAME>
				<ANIM_CODE>#ANIM_CODE_R#</ANIM_CODE>
			</UseTemplate>
		</Condition>
	</Template>
	<!-- ############################################################################## -->
	<Template Name="ASOBO_ET_HANDLING_Spoilers_SubTemplate">
