		</Condition>
	</Template>
	<!-- ############################################################################## -->
	<Template Name="ASOBO_ET_HANDLING_Water_Rudder_SubTemplate">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Water_Rudder_Animation_Parameters"/>
		</Parameters>
		<UseTemplate Name="ASOBO_GT_Anim_Template">
			<ANIM_CODE>#GET_WATER_RUDDER#</ANIM_CODE>
		</UseTemplate>
	</Template>
	<!-- ############################################################################## -->
	<Template Name="ASOBO_ET_HANDLING_Ski_SubTemplate">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Ski_Animation_Parameters"/>
