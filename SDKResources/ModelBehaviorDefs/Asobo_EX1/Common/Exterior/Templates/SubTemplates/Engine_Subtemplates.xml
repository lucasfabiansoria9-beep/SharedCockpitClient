<ModelBehaviors>
	<!-- ################################## ENGINE #################################### -->
	<!-- ############################################################################## -->
	<Template Name="ASOBO_ET_ENGINE_Animation_SubTemplate">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Engine_Animation_Parameters"/>
		</Parameters>
		<UseTemplate Name="ASOBO_GT_Anim_Template">
			<ANIM_CODE>#ANIM_CODE_ROTATION#</ANIM_CODE>
		</UseTemplate>
		<Condition>
			<Test>
				<Arg NotEmpty="ANIM_NAME_REVERSER"/>
			</Test>
			<True>
				<UseTemplate Name="ASOBO_GT_Anim_Template">
					<ANIM_NAME>#ANIM_NAME_REVERSER#</ANIM_NAME>
					<ANIM_CODE>#ANIM_CODE_REVERSER#</ANIM_CODE>
				</UseTemplate>
			</True>
		</Condition>
		<UseTemplate Name="ASOBO_ET_COMMON_Collection_Visibility_Template">
			<SAVED_NODE_IDS>STILL_NODE_IDs</SAVED_NODE_IDS>
			<VISIBILITY_CODE>#VISIBILITY_CODE_STILL#</VISIBILITY_CODE>
		</UseTemplate>
		<UseTemplate Name="ASOBO_ET_COMMON_Collection_Visibility_Template">
			<SAVED_NODE_IDS>SLOW_NODE_IDs</SAVED_NODE_IDS>
			<VISIBILITY_CODE>#VISIBILITY_CODE_SLOW#</VISIBILITY_CODE>
		</UseTemplate>
		<UseTemplate Name="ASOBO_ET_COMMON_Collection_Visibility_Template">
			<SAVED_NODE_IDS>BLURRED_NODE_IDs</SAVED_NODE_IDS>
			<VISIBILITY_CODE>#VISIBILITY_CODE_BLURRED#</VISIBILITY_CODE>
		</UseTemplate>
	</Template>
	<!-- ############################################################################## -->
	<Template Name="ASOBO_ET_ENGINE_Reverser_Animation_SubTemplate">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Engine_Animation_Parameters"/>
		</Parameters>
		<UseTemplate Name="ASOBO_GT_Anim_Template">
			<ANIM_CODE>#ANIM_CODE_REVERSER#</ANIM_CODE>
		</UseTemplate>
	</Template>
	<!-- ############################################################################## -->
	<Template Name="ASOBO_ET_ENGINE_Beta_Animation_SubTemplate">
		<Parameters Type="Default">
			<MIN_BETA>0</MIN_BETA>
			<MAX_BETA>360</MAX_BETA>
			<ANIM_LENGTH>100</ANIM_LENGTH>
		</Parameters>
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Prop_RPM_Parameters"/>
		</Parameters>
		<UseTemplate Name="ASOBO_GT_Anim_Template">
			<ANIM_CODE>#GET_PROP_BETA# #MIN_BETA# #MAX_BETA# 0 #ANIM_LENGTH# (F:MapRange)</ANIM_CODE>
		</UseTemplate>
	</Template>
	<!-- ############################################################################## -->
	<Template Name="ASOBO_ET_ENGINE_Components_Animation_SubTemplate">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Engine_Components_Animation_Parameters"/>
		</Parameters>
		<UseTemplate Name="ASOBO_GT_Anim_Template">
			<ANIM_CODE>#ANIM_CODE_PROP_ROTATION#</ANIM_CODE>
			<ANIM_LENGTH>#ANIM_PROP_ROTATION_LENGTH#</ANIM_LENGTH>
			<ANIM_WRAP>True</ANIM_WRAP>
		</UseTemplate>
		<Loop>
			<Setup>
				<Param>CUR_ID</Param>
				<From>1</From>
				<Inc>1</Inc>
				<To>#PROP_MAX_COUNT#</To>
			</Setup>
			<Do>
				<Parameters Type="Override" Lifetime="Loop">
					<MIN_RPM_BLADES Process="Param">MIN_RPM_BLADES_#CUR_ID#</MIN_RPM_BLADES>
					<MAX_RPM_BLADES Process="Param">MAX_RPM_BLADES_#CUR_ID#</MAX_RPM_BLADES>
				</Parameters>
				<Parameters Type="Default" Lifetime="Loop">
					<Condition CheckSavedParameters="BLADES_NODE_IDs_#CUR_ID#">
						<False>
							<SaveParameters ID="BLADES_NODE_IDs_#CUR_ID#">
								<Prop_#ENG_ID#_Blades_#CUR_ID#/>
							</SaveParameters>
						</False>
					</Condition>
					<Condition CheckSavedParameters="CONE_NODE_IDs_#CUR_ID#">
						<False>
							<SaveParameters ID="CONE_NODE_IDs_#CUR_ID#" Append="Default">
								<Prop_#ENG_ID#_Cone_#CUR_ID#/>
							</SaveParameters>
						</False>
					</Condition>
					<Condition CheckSavedParameters="SIDE_NODE_IDs_#CUR_ID#">
						<False>
							<SaveParameters ID="SIDE_NODE_IDs_#CUR_ID#" Append="Default">
								<Prop_#ENG_ID#_Side_#CUR_ID#/>
							</SaveParameters>
						</False>
					</Condition>
					<Condition Valid="COPY_RPM_BLADES_TO_CONE">
						<MIN_RPM_CONE_#CUR_ID# Process="Param">MIN_RPM_BLADES_#CUR_ID#</MIN_RPM_CONE_#CUR_ID#>
						<MAX_RPM_CONE_#CUR_ID# Process="Param">MAX_RPM_BLADES_#CUR_ID#</MAX_RPM_CONE_#CUR_ID#>
					</Condition>
					<Condition Valid="COPY_RPM_BLADES_TO_SIDE">
						<MIN_RPM_SIDE_#CUR_ID# Process="Param">MIN_RPM_BLADES_#CUR_ID#</MIN_RPM_SIDE_#CUR_ID#>
						<MAX_RPM_SIDE_#CUR_ID# Process="Param">MAX_RPM_BLADES_#CUR_ID#</MAX_RPM_SIDE_#CUR_ID#>
					</Condition>
				</Parameters>
				<Parameters Type="Override" Lifetime="Loop">
					<MIN_RPM_CONE Process="Param">MIN_RPM_CONE_#CUR_ID#</MIN_RPM_CONE>
					<MAX_RPM_CONE Process="Param">MAX_RPM_CONE_#CUR_ID#</MAX_RPM_CONE>
					<MIN_RPM_SIDE Process="Param">MIN_RPM_SIDE_#CUR_ID#</MIN_RPM_SIDE>
					<MAX_RPM_SIDE Process="Param">MAX_RPM_SIDE_#CUR_ID#</MAX_RPM_SIDE>
				</Parameters>
				<Condition>
					<Test>
						<LowerOrEqual>
							<Number>#CUR_ID#</Number>
							<Number>#PROP_BLADES_COUNT#</Number>
						</LowerOrEqual>
					</Test>
					<True>
						<UseTemplate Name="ASOBO_ET_COMMON_Collection_Visibility_Template">
							<SAVED_NODE_IDS>BLADES_NODE_IDs_#CUR_ID#</SAVED_NODE_IDS>
							<UseParametersFn Name="ASOBO_Get_Engine_Visibility_Limits_Parameters">
								<MIN_RPM>#MIN_RPM_BLADES#</MIN_RPM>
								<MAX_RPM>#MAX_RPM_BLADES#</MAX_RPM>
							</UseParametersFn>
						</UseTemplate>
					</True>
				</Condition>
				<Condition>
					<Test>
						<LowerOrEqual>
							<Number>#CUR_ID#</Number>
							<Number>#PROP_CONE_COUNT#</Number>
						</LowerOrEqual>
					</Test>
					<True>
						<UseTemplate Name="ASOBO_ET_COMMON_Collection_Visibility_Template">
							<SAVED_NODE_IDS>CONE_NODE_IDs_#CUR_ID#</SAVED_NODE_IDS>
							<UseParametersFn Name="ASOBO_Get_Engine_Visibility_Limits_Parameters">
								<MIN_RPM>#MIN_RPM_CONE#</MIN_RPM>
								<MAX_RPM>#MAX_RPM_CONE#</MAX_RPM>
							</UseParametersFn>
						</UseTemplate>
					</True>
				</Condition>
				<Condition>
					<Test>
						<LowerOrEqual>
							<Number>#CUR_ID#</Number>
							<Number>#PROP_SIDE_COUNT#</Number>
						</LowerOrEqual>
					</Test>
					<True>
						<UseTemplate Name="ASOBO_ET_COMMON_Collection_Visibility_Template">
							<SAVED_NODE_IDS>SIDE_NODE_IDs_#CUR_ID#</SAVED_NODE_IDS>
							<UseParametersFn Name="ASOBO_Get_Engine_Visibility_Limits_Parameters">
								<MIN_RPM>#MIN_RPM_SIDE#</MIN_RPM>
								<MAX_RPM>#MAX_RPM_SIDE#</MAX_RPM>
							</UseParametersFn>
						</UseTemplate>
					</True>
				</Condition>
			</Do>
		</Loop>
	</Template>
	<!-- ############################################################################## -->
	<Template Name="ASOBO_ET_ROTOR_Animation_SubTemplate">
		<Condition Check="ANIM_NAME_CONE">
			<UseTemplate Name="ASOBO_ET_ROTOR_Coning_Animation_SubTemplate">
				<ANIM_NAME>#ANIM_NAME_CONE#</ANIM_NAME>
			</UseTemplate>
		</Condition>
		<Condition Check="ANIM_NAME_SWASHPLATE">
			<UseTemplate Name="ASOBO_ET_ROTOR_Swashplate_Animation_SubTemplate">
				<ANIM_NAME>#ANIM_NAME_SWASHPLATE#</ANIM_NAME>
			</UseTemplate>
		</Condition>
		<Condition Check="ANIM_NAME_PITCH">
			<UseTemplate Name="ASOBO_ET_ROTOR_Tail_Blade_Pitch_Animation_SubTemplate">
				<ANIM_NAME>#ANIM_NAME_PITCH#</ANIM_NAME>
			</UseTemplate>
		</Condition>
	</Template>
	<!-- ############################################################################## -->
	<Template Name="ASOBO_ET_ROTOR_Coning_Animation_SubTemplate">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Rotor_Coning_Animation_Parameters"/>
		</Parameters>
		<UseTemplate Name="ASOBO_GT_Anim_Template">
			<ANIM_CODE>#ANIM_CODE_CONING#</ANIM_CODE>
		</UseTemplate>
	</Template>
	<!-- ############################################################################## -->
	<Template Name="ASOBO_ET_ROTOR_Swashplate_Animation_SubTemplate">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Rotor_Swashplate_Animation_Parameters"/>
		</Parameters>
		<UseTemplate Name="ASOBO_GT_Anim_Template">
			<ANIM_CODE>#ANIM_CODE_SWASHPLATE#</ANIM_CODE>
		</UseTemplate>
	</Template>
	<!-- ############################################################################## -->
	<Template Name="ASOBO_ET_ROTOR_Tail_Blade_Pitch_Animation_SubTemplate">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Rotor_Tail_Blade_Pitch_Animation_Parameters"/>
		</Parameters>
		<UseTemplate Name="ASOBO_GT_Anim_Template">
			<ANIM_CODE>#ANIM_CODE_BLADE_PITCH#</ANIM_CODE>
		</UseTemplate>
	</Template>
	<!-- ############################################################################## -->
	<Template Name="ASOBO_ET_ENGINE_Rotor_Model_Swap_RPM_SubTemplate">
		<Parameters Type="Default">
			<ROTOR>PRIMARY</ROTOR>
			<RPM_THRESHOLD>20</RPM_THRESHOLD>
		</Parameters>
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_ROTOR_RPM_Parameters"/>
		</Parameters>
		<Parameters Type="Override">
			<Switch Param="ROTOR">
				<Case Value="PRIMARY">
					<GET_ROTOR_RPM>#GET_PRIMARY_ROTOR_RPM#</GET_ROTOR_RPM>
				</Case>
				<Case Value="SECONDARY">
					<GET_ROTOR_RPM>#GET_SECONDARY_ROTOR_RPM#</GET_ROTOR_RPM>
				</Case>
			</Switch>
		</Parameters>
		<Parameters Type="Default">
			<IS_HIGH_RPM>#GET_ROTOR_RPM# #RPM_THRESHOLD# &gt;</IS_HIGH_RPM>
		</Parameters>
		<UseTemplate Name="ASOBO_CT_Swapable_Mesh_Template">
			<MESH_VISIBLE_WHEN_TRUE>#NODE_VISIBLE_AT_HIGH_RPM#</MESH_VISIBLE_WHEN_TRUE>
			<MESH_VISIBLE_WHEN_FALSE>#NODE_VISIBLE_AT_LOW_RPM#</MESH_VISIBLE_WHEN_FALSE>
			<IS_VISIBLE>#IS_HIGH_RPM#</IS_VISIBLE>
		</UseTemplate>
	</Template>
	<!-- ############################################################################## -->
	<Template Name="ASOBO_ET_ENGINE_Propeller_Model_Swap_RPM_SubTemplate">
		<Parameters Type="Default">
			<ENG_ID>1</ENG_ID>
			<RPM_THRESHOLD>20</RPM_THRESHOLD>
		</Parameters>
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Prop_RPM_Parameters"/>
		</Parameters>
		<Parameters Type="Default">
			<IS_HIGH_RPM>#GET_PROP_RPM# #RPM_THRESHOLD# &gt;</IS_HIGH_RPM>
		</Parameters>
		<UseTemplate Name="ASOBO_CT_Swapable_Mesh_Template">
			<MESH_VISIBLE_WHEN_TRUE>#NODE_VISIBLE_AT_HIGH_RPM#</MESH_VISIBLE_WHEN_TRUE>
			<MESH_VISIBLE_WHEN_FALSE>#NODE_VISIBLE_AT_LOW_RPM#</MESH_VISIBLE_WHEN_FALSE>
			<IS_VISIBLE>#IS_HIGH_RPM#</IS_VISIBLE>
		</UseTemplate>
	</Template>
</ModelBehaviors>