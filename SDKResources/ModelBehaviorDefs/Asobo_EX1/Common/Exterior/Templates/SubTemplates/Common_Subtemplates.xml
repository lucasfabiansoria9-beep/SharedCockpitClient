<ModelBehaviors>
	<!-- ################################## COMMON #################################### -->
	<!-- ############################################################################## -->
	<Template Name="ASOBO_ET_COMMON_WIPER_SubTemplate">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Wiper_Animation_Parameters"/>
		</Parameters>
		<UseTemplate Name="ASOBO_GT_Anim_Template"/>
	</Template>
	<!-- ############################################################################## -->
	<Template Name="ASOBO_ET_COMMON_Nozzle_SubTemplate">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Nozzle_Animation_Parameters"/>
		</Parameters>
		<UseTemplate Name="ASOBO_GT_Anim_Template"/>
	</Template>
	<!-- ############################################################################## -->
	<Template Name="ASOBO_ET_COMMON_LaunchBar_SubTemplate">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_LaunchBar_Animation_Parameters"/>
		</Parameters>
		<UseTemplate Name="ASOBO_GT_Anim_Template"/>
	</Template>
	<!-- ############################################################################## -->
	<Template Name="ASOBO_ET_COMMON_Door_SubTemplate">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Door_Animation_Parameters"/>
		</Parameters>
		<UseTemplate Name="ASOBO_GT_Anim_Template"/>
	</Template>
	<!-- ############################################################################## -->
	<Template Name="ASOBO_ET_COMMON_Dirt_SubTemplate">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Dirt_Visibility_Parameters"/>
		</Parameters>
		<UseTemplate Name="ASOBO_Visibility_Template"/>
	</Template>
	<!-- ############################################################################## -->
	<Template Name="ASOBO_ET_COMMON_Helicraft_Yaw_String_SubTemplate">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Helicraft_Yaw_String_Parameters"/>
		</Parameters>
		<Parameters Type="Default">
			<ANIM_LAG_360>150</ANIM_LAG_360>
			<ANIM_LAG_LR_END>500</ANIM_LAG_LR_END>
			<ANIM_LAG_IDLE_TO_STAND>300</ANIM_LAG_IDLE_TO_STAND>
			<ANIM_LAG_L_R>500</ANIM_LAG_L_R>
		</Parameters>
		<Condition Check="ANIM_NAME_360">
			<UseTemplate Name="ASOBO_GT_Anim_Template">
				<ANIM_NAME>#ANIM_NAME_360#</ANIM_NAME>
				<ANIM_CODE>#YAW_STRING_ROTATION_ANIMATION#</ANIM_CODE>
				<ANIM_WRAP>True</ANIM_WRAP>
				<Condition Valid="ANIM_LAG_360">
					<ANIM_LAG>#ANIM_LAG_360#</ANIM_LAG>
				</Condition>
			</UseTemplate>
		</Condition>
		<Condition Check="ANIM_NAME_L_R_END">
			<UseTemplate Name="ASOBO_GT_Anim_Template">
				<ANIM_NAME>#ANIM_NAME_L_R_END#</ANIM_NAME>
				<ANIM_CODE>#YAW_STRING_L_R_END_ANIMATION#</ANIM_CODE>
				<ANIM_WRAP>True</ANIM_WRAP>
				<Condition Valid="ANIM_LAG_LR_END">
					<ANIM_LAG>#ANIM_LAG_LR_END#</ANIM_LAG>
				</Condition>
			</UseTemplate>
		</Condition>
		<Condition Check="ANIM_NAME_IDLE_TO_STAND">
			<UseTemplate Name="ASOBO_GT_Anim_Template">
				<ANIM_NAME>#ANIM_NAME_IDLE_TO_STAND#</ANIM_NAME>
				<ANIM_CODE>#YAW_STRING_IDLE_TO_STAND_ANIMATION#</ANIM_CODE>
				<Condition Valid="ANIM_LAG_IDLE_TO_STAND">
					<ANIM_LAG>#ANIM_LAG_IDLE_TO_STAND#</ANIM_LAG>
				</Condition>
			</UseTemplate>
		</Condition>
		<Condition Check="ANIM_NAME_L_R">
			<UseTemplate Name="ASOBO_GT_Anim_Template">
				<ANIM_NAME>#ANIM_NAME_L_R#</ANIM_NAME>
				<ANIM_CODE>#YAW_STRING_L_R_ANIMATION#</ANIM_CODE>
				<Condition Valid="ANIM_LAG_L_R">
					<ANIM_LAG>#ANIM_LAG_L_R#</ANIM_LAG>
				</Condition>
			</UseTemplate>
		</Condition>
	</Template>
	<!-- ############################################################################## -->
	<Template Name="ASOBO_ET_COMMON_Glider_Yaw_String_SubTemplate">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Glider_Yaw_String_Parameters"/>
		</Parameters>
		<Parameters Type="Default">
			<ANIM_LAG_360>50</ANIM_LAG_360>
			<ANIM_LAG_NOISE>800</ANIM_LAG_NOISE>
			<ANIM_LAG_L_R>200</ANIM_LAG_L_R>
			<REDUCE_NOISE_WITH_AIRSPEED>True</REDUCE_NOISE_WITH_AIRSPEED>
		</Parameters>
		<Condition Check="ANIM_NAME_360">
			<UseTemplate Name="ASOBO_GT_Anim_Template">
				<ANIM_NAME>#ANIM_NAME_360#</ANIM_NAME>
				<ANIM_CODE>#YAW_STRING_ROTATION_ANIMATION#</ANIM_CODE>
				<ANIM_WRAP>True</ANIM_WRAP>
				<Condition Valid="ANIM_LAG_360">
					<ANIM_LAG>#ANIM_LAG_360#</ANIM_LAG>
				</Condition>
			</UseTemplate>
		</Condition>
		<Condition Check="ANIM_NAME_NOISE">
			<UseTemplate Name="ASOBO_GT_Anim_Template">
				<ANIM_NAME>#ANIM_NAME_NOISE#</ANIM_NAME>
				<ANIM_CODE>#YAW_STRING_NOISE_ANIMATION#</ANIM_CODE>
				<ANIM_WRAP>True</ANIM_WRAP>
				<Condition Valid="ANIM_LAG_NOISE">
					<ANIM_LAG>#ANIM_LAG_NOISE#</ANIM_LAG>
				</Condition>
			</UseTemplate>
		</Condition>
		<Condition Check="ANIM_NAME_L_R">
			<UseTemplate Name="ASOBO_GT_Anim_Template">
				<ANIM_NAME>#ANIM_NAME_L_R#</ANIM_NAME>
				<ANIM_CODE>#YAW_STRING_L_R_ANIMATION#</ANIM_CODE>
				<Condition Valid="ANIM_LAG_L_R">
					<ANIM_LAG>#ANIM_LAG_L_R#</ANIM_LAG>
				</Condition>
			</UseTemplate>
		</Condition>
	</Template>
	<!-- ############################################################################## -->
	<Template Name="ASOBO_ET_COMMON_Cover_Interaction_And_Visibility_SubTemplate">
		<UseTemplate Name="ASOBO_ET_COMMON_Cover_Visibility_SubTemplate">
			<COMPONENT_ID>#IN_BASE_NAME#_Visibility</COMPONENT_ID>
			<NODE_ID>#IN_BASE_NAME#</NODE_ID>
		</UseTemplate>
		<UseTemplate Name="ASOBO_ET_COMMON_Toggle_Cover_Interaction_SubTemplate"/>
	</Template>
	<!-- ############################################################################## -->
	<Template Name="ASOBO_ET_COMMON_Cover_Visibility_SubTemplate">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Get_Cover_Visibility_Parameters"/>
		</Parameters>
		<UseTemplate Name="ASOBO_GT_Component_Visibility_Template"/>
	</Template>
	<!-- ############################################################################## -->
	<Template Name="ASOBO_ET_COMMON_Toggle_Cover_Interaction_SubTemplate">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Cover_Parameters"/>
		</Parameters>
		<UseTemplate Name="ASOBO_IT_Simple_Interaction_Template">
			<ON_EVENT>#TOGGLE_COVER#</ON_EVENT>
		</UseTemplate>
	</Template>
	<!-- ############################################################################## -->
</ModelBehaviors>