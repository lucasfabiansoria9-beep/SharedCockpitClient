<ModelBehaviors>
	<!-- ################################## LANDING ################################### -->
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Get_Gear_Animation_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Gear_Parameters"/>
		</Parameters>
		<Parameters Type="Default">
			<ANIM_CODE>#GET_GEAR_ANIMATION_POSITION#</ANIM_CODE>
		</Parameters>
		<ReturnParameters>
			<ANIM_CODE>#ANIM_CODE#</ANIM_CODE>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Get_Gear_Tire_Animation_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Wheel_Parameters"/>
		</Parameters>
		<Parameters Type="Default">
			<MIN_RPM_FOR_BLUR>400</MIN_RPM_FOR_BLUR>
		</Parameters>
		<Parameters Type="Default">
			<ANIM_CODE>#GET_WHEEL_ROTATION_ANGLE# dnor 360 / 100 *</ANIM_CODE>
			<VISIBILITY_CODE_STILL>#GET_WHEEL_RPM# #MIN_RPM_FOR_BLUR# &lt;</VISIBILITY_CODE_STILL>
			<VISIBILITY_CODE_BLURRED>#GET_WHEEL_RPM# #MIN_RPM_FOR_BLUR# &gt;</VISIBILITY_CODE_BLURRED>
		</Parameters>
		<ReturnParameters>
			<ANIM_CODE>#ANIM_CODE#</ANIM_CODE>
			<VISIBILITY_CODE_STILL>#VISIBILITY_CODE_STILL#</VISIBILITY_CODE_STILL>
			<VISIBILITY_CODE_BLURRED>#VISIBILITY_CODE_BLURRED#</VISIBILITY_CODE_BLURRED>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Get_Gear_Steering_Animation_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Gear_Parameters"/>
		</Parameters>
		<Parameters Type="Default">
			<ANIM_CODE>#GET_GEAR_STEER_ANGLE# dnor 360 / 100 *</ANIM_CODE>
		</Parameters>
		<ReturnParameters>
			<ANIM_CODE>#ANIM_CODE#</ANIM_CODE>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Get_Contact_Point_Steering_Animation_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Contact_Point_Parameters"/>
		</Parameters>
		<Parameters Type="Default">
			<ANIM_CODE>#GET_CONTACT_POINT_STEER_ANGLE# dnor 360 / 100 *</ANIM_CODE>
		</Parameters>
		<ReturnParameters>
			<ANIM_CODE>#ANIM_CODE#</ANIM_CODE>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Get_Contact_Point_Animation_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Contact_Point_Parameters"/>
		</Parameters>
		<Parameters Type="Default">
			<Switch Param="TYPE">
				<Case Value="EXTENSION_AND_COMPRESSION">
					<ANIM_CODE>#GET_CONTACT_POINT_POSITION# #GET_CONTACT_POINT_COMPRESSION# + 2 /</ANIM_CODE>
				</Case>
				<Case Value="COMPRESSION">
					<ANIM_CODE>#GET_CONTACT_POINT_COMPRESSION#</ANIM_CODE>
				</Case>
				<Default>
					<ANIM_CODE>#GET_CONTACT_POINT_POSITION#</ANIM_CODE>
				</Default>
			</Switch>
		</Parameters>
		<ReturnParameters>
			<ANIM_CODE>#ANIM_CODE#</ANIM_CODE>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
</ModelBehaviors>