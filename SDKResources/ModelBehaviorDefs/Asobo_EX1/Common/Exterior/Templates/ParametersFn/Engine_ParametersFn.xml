<ModelBehaviors>
	<!-- ################################## ENGINE #################################### -->
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Get_Engine_Animation_Parameters">
		<Parameters Type="Default">
			<MIN_RPM_FOR_SLOW>188</MIN_RPM_FOR_SLOW>
			<MIN_RPM_FOR_BLUR>375</MIN_RPM_FOR_BLUR>
		</Parameters>
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Engine_Thrust_Parameters"/>
		</Parameters>
		<Parameters Type="Default">
			<ANIM_CODE_REVERSER>#GET_TURB_THRUST_REVERSER_POS#</ANIM_CODE_REVERSER>
			<ANIM_CODE_ROTATION>(O:XMLVAR_ENG_#ENG_ID#_ROTATION_ANGLE) #GET_ENGINE_RPM# 60 / 360 * @E_AnimationDeltaTime * + dnor s0 (&gt;O:XMLVAR_ENG_#ENG_ID#_ROTATION_ANGLE) l0 360 / 100 *</ANIM_CODE_ROTATION>
			<VISIBILITY_CODE_STILL>#GET_ENGINE_RPM# #MIN_RPM_FOR_SLOW# &lt;=</VISIBILITY_CODE_STILL>
			<VISIBILITY_CODE_SLOW>#GET_ENGINE_RPM# s0 #MIN_RPM_FOR_SLOW# &gt; l0 #MIN_RPM_FOR_BLUR# &lt;= and</VISIBILITY_CODE_SLOW>
			<VISIBILITY_CODE_BLURRED>#GET_ENGINE_RPM# #MIN_RPM_FOR_BLUR# &gt;</VISIBILITY_CODE_BLURRED>
		</Parameters>
		<ReturnParameters>
			<ANIM_CODE_REVERSER>#ANIM_CODE_REVERSER#</ANIM_CODE_REVERSER>
			<ANIM_CODE_ROTATION>#ANIM_CODE_ROTATION#</ANIM_CODE_ROTATION>
			<VISIBILITY_CODE_STILL>#VISIBILITY_CODE_STILL#</VISIBILITY_CODE_STILL>
			<VISIBILITY_CODE_SLOW>#VISIBILITY_CODE_SLOW#</VISIBILITY_CODE_SLOW>
			<VISIBILITY_CODE_BLURRED>#VISIBILITY_CODE_BLURRED#</VISIBILITY_CODE_BLURRED>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Get_Engine_Components_Animation_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Prop_RPM_Parameters"/>
		</Parameters>
		<Parameters Type="Default">
			<Condition NotEmpty="PROP_BLADES_COUNT">
				<False>
					<UseParametersFn Name="ASOBO_Get_Max_Param_Iteration_Helper">
						<PARAM_PREFIX_0>MAX_RPM_BLADES_</PARAM_PREFIX_0>
						<PARAM_PREFIX_1>MIN_RPM_BLADES_</PARAM_PREFIX_1>
						<OUTGOING_PARAM>PROP_BLADES_COUNT</OUTGOING_PARAM>
					</UseParametersFn>
				</False>
			</Condition>
			<Condition NotEmpty="PROP_SIDE_COUNT">
				<False>
					<UseParametersFn Name="ASOBO_Get_Max_Param_Iteration_Helper">
						<PARAM_PREFIX_0>MAX_RPM_SIDE_</PARAM_PREFIX_0>
						<PARAM_PREFIX_1>MIN_RPM_SIDE_</PARAM_PREFIX_1>
						<OUTGOING_PARAM>PROP_SIDE_COUNT</OUTGOING_PARAM>
					</UseParametersFn>
				</False>
			</Condition>
			<Condition NotEmpty="PROP_CONE_COUNT">
				<False>
					<UseParametersFn Name="ASOBO_Get_Max_Param_Iteration_Helper">
						<PARAM_PREFIX_0>MAX_RPM_CONE_</PARAM_PREFIX_0>
						<PARAM_PREFIX_1>MIN_RPM_CONE_</PARAM_PREFIX_1>
						<OUTGOING_PARAM>PROP_CONE_COUNT</OUTGOING_PARAM>
					</UseParametersFn>
				</False>
			</Condition>
			<ANIM_PROP_ROTATION_LENGTH>360</ANIM_PROP_ROTATION_LENGTH>
		</Parameters>
		<ReturnParameters>
			<PROP_MAX_COUNT Process="Int">#PROP_BLADES_COUNT# #PROP_SIDE_COUNT# #PROP_CONE_COUNT# max max</PROP_MAX_COUNT>
			<PROP_BLADES_COUNT>#PROP_BLADES_COUNT#</PROP_BLADES_COUNT>
			<PROP_SIDE_COUNT>#PROP_SIDE_COUNT#</PROP_SIDE_COUNT>
			<PROP_CONE_COUNT>#PROP_CONE_COUNT#</PROP_CONE_COUNT>
			<SIMVAR_PROP_ROTATION>#SIMVAR_PROP_ROTATION#</SIMVAR_PROP_ROTATION>
			<ANIM_CODE_PROP_ROTATION>#GET_PROP_ROTATION#</ANIM_CODE_PROP_ROTATION>
			<ANIM_PROP_ROTATION_LENGTH>#ANIM_PROP_ROTATION_LENGTH#</ANIM_PROP_ROTATION_LENGTH>
			<GET_PROP_RPM>#GET_PROP_RPM#</GET_PROP_RPM>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Get_Engine_Visibility_Limits_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Engine_Thrust_Parameters"/>
		</Parameters>
		<Parameters Type="Default">
			<MIN_RPM_CODE/>
			<MAX_RPM_CODE/>
			<AND_CODE/>
		</Parameters>
		<Parameters Type="Override">
			<Condition NotEmpty="MIN_RPM">
				<MIN_RPM_CODE>#GET_PROP_RPM# #MIN_RPM# &gt;=</MIN_RPM_CODE>
				<HAS_MIN_MAX>True</HAS_MIN_MAX>
			</Condition>
			<Condition NotEmpty="MAX_RPM">
				<MAX_RPM_CODE>#GET_PROP_RPM# #MAX_RPM# &lt;</MAX_RPM_CODE>
				<HAS_MIN_MAX>True</HAS_MIN_MAX>
			</Condition>
			<Condition>
				<Test>
					<And>
						<Arg NotEmpty="MIN_RPM"/>
						<Arg NotEmpty="MAX_RPM"/>
					</And>
				</Test>
				<True>
					<AND_CODE>and</AND_CODE>
				</True>
			</Condition>
		</Parameters>
		<ReturnParameters>
			<VISIBILITY_CODE>#MIN_RPM_CODE# #MAX_RPM_CODE# #AND_CODE#</VISIBILITY_CODE>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Get_Rotor_Animation_Parameters">
		<Parameters Type="Default">
			<MIN_RPM_FOR_SLOW>188</MIN_RPM_FOR_SLOW>
			<MIN_RPM_FOR_BLUR>375</MIN_RPM_FOR_BLUR>
		</Parameters>
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_ROTOR_RPM_Parameters"/>
		</Parameters>
		<Parameters Type="Override">
			<Switch Param="ROTOR">
				<Case Value="PRIMARY">
					<GET_ROTOR_RPM>#GET_PRIMARY_ROTOR_RPM#</GET_ROTOR_RPM>
				</Case>
				<Case Value="SECONDARY">
					<GET_ROTOR_RPM>#GET_SECONDARY_ROTOR_RPM#</GET_ROTOR_RPM>
				</Case>
			</Switch>
		</Parameters>
		<Parameters Type="Default">
			<ANIM_CODE_ROTATION>(O:XMLVAR_ROTOR_#ROTOR#_ROTATION_ANGLE) #GET_ROTOR_RPM# 60 / 360 * @E_AnimationDeltaTime * + dnor s0 (&gt;O:XMLVAR_ROTOR_#ROTOR#_ROTATION_ANGLE) l0 360 / 100 *</ANIM_CODE_ROTATION>
			<VISIBILITY_CODE_STILL>#GET_ROTOR_RPM# #MIN_RPM_FOR_SLOW# &lt;=</VISIBILITY_CODE_STILL>
			<VISIBILITY_CODE_SLOW>#GET_ROTOR_RPM# s0 #MIN_RPM_FOR_SLOW# &gt; l0 #MIN_RPM_FOR_BLUR# &lt;= and</VISIBILITY_CODE_SLOW>
			<VISIBILITY_CODE_BLURRED>#GET_ROTOR_RPM# #MIN_RPM_FOR_BLUR# &gt;</VISIBILITY_CODE_BLURRED>
		</Parameters>
		<ReturnParameters>
			<ANIM_CODE_ROTATION>#ANIM_CODE_ROTATION#</ANIM_CODE_ROTATION>
			<VISIBILITY_CODE_STILL>#VISIBILITY_CODE_STILL#</VISIBILITY_CODE_STILL>
			<VISIBILITY_CODE_SLOW>#VISIBILITY_CODE_SLOW#</VISIBILITY_CODE_SLOW>
			<VISIBILITY_CODE_BLURRED>#VISIBILITY_CODE_BLURRED#</VISIBILITY_CODE_BLURRED>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Get_Rotor_Coning_Animation_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_ROTOR_Disk_Parameters"/>
		</Parameters>
		<Parameters Type="Override">
			<Switch Param="ROTOR">
				<Case Value="PRIMARY">
					<GET_ROTOR_CONING_PCT>#GET_PRIMARY_ROTOR_DISK_CONING_PCT#</GET_ROTOR_CONING_PCT>
				</Case>
				<Case Value="SECONDARY">
					<GET_ROTOR_CONING_PCT>#GET_SECONDARY_ROTOR_DISK_CONING_PCT#</GET_ROTOR_CONING_PCT>
				</Case>
			</Switch>
		</Parameters>
		<ReturnParameters>
			<ANIM_CODE_CONING>#GET_ROTOR_CONING_PCT# 100 + 2 /</ANIM_CODE_CONING>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Get_Rotor_Swashplate_Animation_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_ROTOR_Collective_Parameters"/>
		</Parameters>
		<ReturnParameters>
			<ANIM_CODE_SWASHPLATE>#GET_ROTOR_COLLECTIVE#</ANIM_CODE_SWASHPLATE>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Get_Rotor_Tail_Blade_Pitch_Animation_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_ROTOR_Tail_Parameters"/>
		</Parameters>
		<ReturnParameters>
			<ANIM_CODE_BLADE_PITCH>#GET_ROTOR_TAIL_BLADE_PITCH# 100 + 2 /</ANIM_CODE_BLADE_PITCH>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
</ModelBehaviors>