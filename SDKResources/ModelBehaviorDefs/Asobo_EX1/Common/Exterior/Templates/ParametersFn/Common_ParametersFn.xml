	<ParametersFn Name="ASOBO_Get_Cover_Visibility_Parameters">
		<Parameters Type="Override">
			<UseParametersFn Name="ASOBO_Cover_Parameters" />
		</Parameters>
		<Parameters Type="Default">
			<VISIBILITY_CODE>#IS_COVER_ON#</VISIBILITY_CODE>
		</Parameters>
		<ReturnParameters>
			<VISIBILITY_CODE>#VISIBILITY_CODE#</VISIBILITY_CODE>
		</ReturnParameters>
	</ParametersFn>
