<ModelBehaviors>
	<!-- ################################## COMMON #################################### -->
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Get_Wiper_Animation_Parameters">
		<Parameters Type="Default">
			<GET_WIPER_POSITION>(L:1:XMLVAR_WIPER_POSITION)</GET_WIPER_POSITION>
			<POSITION_SCALE>100</POSITION_SCALE>
		</Parameters>
		<Parameters Type="Default">
			<FULL_WIPE_DISTANCE Process="Int">#POSITION_SCALE# 2 /</FULL_WIPE_DISTANCE>
		</Parameters>
		<Parameters Type="Default">
			<ANIM_CODE>100 #GET_WIPER_POSITION# #FULL_WIPE_DISTANCE# - abs #FULL_WIPE_DISTANCE# / 100 * -</ANIM_CODE>
		</Parameters>
		<ReturnParameters>
			<ANIM_CODE>#ANIM_CODE#</ANIM_CODE>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Get_Nozzle_Animation_Parameters">
		<Parameters Type="Default">
			<ENGINE_ID>1</ENGINE_ID>
		</Parameters>
		<Parameters Type="Default">
			<GET_NOZZLE_POSITION>(A:TURB ENG PRIMARY NOZZLE PERCENT:#ENGINE_ID#, Percent)</GET_NOZZLE_POSITION>
		</Parameters>
		<Parameters Type="Default">
			<ANIM_CODE>#GET_NOZZLE_POSITION#</ANIM_CODE>
		</Parameters>
		<ReturnParameters>
			<ANIM_CODE>#ANIM_CODE#</ANIM_CODE>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Get_LaunchBar_Animation_Parameters">
		<Parameters Type="Default">
			<GET_LAUNCHBAR_POSITION>(A:LAUNCHBAR POSITION, Percent)</GET_LAUNCHBAR_POSITION>
		</Parameters>
		<Parameters Type="Default">
			<ANIM_CODE>#GET_LAUNCHBAR_POSITION#</ANIM_CODE>
		</Parameters>
		<ReturnParameters>
			<ANIM_CODE>#ANIM_CODE#</ANIM_CODE>
		</ReturnParameters>
	</ParametersFn>
	<!-- ############################################################################## -->
	<ParametersFn Name="ASOBO_Get_Door_Animation_Parameters">
		<Parameters Type="Default">
			<ID>0</ID>
		</Parameters>
		<Parameters Type="Default">
			<DOOR_SIMVAR>INTERACTIVE POINT OPEN:#ID#</DOOR_SIMVAR>
		</Parameters>
		<Parameters Type="Default">
			<GET_DOOR_POS>(A:#DOOR_SIMVAR#, Percent)</GET_DOOR_POS>
		</Parameters>
		<Parameters Type="Default">
			<ANIM_CODE>#GET_DOOR_POS#</ANIM_CODE>
		</Parameters>
