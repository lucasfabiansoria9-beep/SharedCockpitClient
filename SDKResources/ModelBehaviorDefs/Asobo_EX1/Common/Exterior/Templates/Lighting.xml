<ModelBehaviors>
	<!-- ################################## LANDING ################################### -->
	<!-- ############################################################################## -->
	<Include RelativeFile="ParametersFn\\Lighting_ParametersFn.xml"/>
	<!-- ############################################################################## -->
	<Template Name="ASOBO_ET_Lighting_AOA_Template">
		<Parameters Type="Default">
			<Switch Param="TYPE">
				<Case Value="GREEN">
					<MIN_AOA>8.8</MIN_AOA>
					<MAX_AOA>99</MAX_AOA>
				</Case>
				<Case Value="YELLOW">
					<MIN_AOA>6.9</MIN_AOA>
					<MAX_AOA>9.3</MAX_AOA>
				</Case>
				<Case Value="RED">
					<MIN_AOA>-99</MIN_AOA>
					<MAX_AOA>7.4</MAX_AOA>
				</Case>
			</Switch>
			<BRIGHTNESS_VAR>1</BRIGHTNESS_VAR>
		</Parameters>
		<Parameters Type="Default">
			<UseParametersFn Name="ASOBO_Get_Angle_Of_Attack_Parameters"/>
		</Parameters>
		<UseTemplate Name="ASOBO_IT_Emissive_Brightness_Template">
			<EMISSIVE_CODE>
				#GET_INCIDENCE_ALPHA# s1 #MIN_AOA# &gt; l1 #MAX_AOA# &lt; and #BRIGHTNESS_VAR# *
			</EMISSIVE_CODE>
		</UseTemplate>
	</Template>
	<!-- ############################################################################## -->
</ModelBehaviors>